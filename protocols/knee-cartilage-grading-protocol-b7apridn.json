er_of_steps":4,"number_of_bookmarks":0,"number_of_comments":0,"number_of_bookmarked_comments":0,"number_of_steps_comments":0,"number_of_protocol_comments":0,"number_of_exports":0,"number_of_runs":0,"number_of_votes":0,"number_of_reagents":0,"number_of_equipments":0,"number_of_collections":0,"number_of_forks":{"private":0,"public":0},"number_of_accessible_forks":0},"status":{"id":1,"info":"We use this protocol and it’s working"},"steps":[{"id":1379206,"guid":"B12F7E80B6AD11ECB1CF4DE68A7C1D54","previous_id":0,"previous_guid":null,"section":"\u003cp\u003eGross Morphological Assessment Sheet\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"6mika\",\"text\":\"Macroscopic scores (1-4) are assigned to cartilage surfaces during tissue harvesting and recorded on the Gross Morphological Assessment Sheet (See Description). These scores use a modified Outerbridge classification grading system.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"2s4h9\",\"text\":\"\\nScore of 1 = normal, intact surface\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"1qqjt\",\"text\":\"Score of 2 = minimum fibrillation\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"c7t7l\",\"text\":\"Score of 3 = overt fibrillation, fissuring\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"dirvb\",\"text\":\"Score of 4 = erosion\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":60463,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"1","cases":[],"critical":null},{"id":1379207,"guid":"BD4CFEE0B6AD11ECB1CF4DE68A7C1D54","previous_id":1379206,"previous_guid":"B12F7E80B6AD11ECB1CF4DE68A7C1D54","section":"\u003cp\u003eGross Morphological Assessment Sheet\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":true,"step":"{\"blocks\":[{\"key\":\"9qbmg\",\"text\":\"Each grid space on the assessment sheet is assigned a score, as seen above 1-4, for cartilage on the Patella, Trochlea, Medial Femoral Condyle, Lateral Femoral Condyle, Medial Tibial Plateau, and Lateral Tibial Plateau.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":60463,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"1.1","cases":[],"critical":null},{"id":1379208,"guid":"C13A1790B6AD11ECB1CF4DE68A7C1D54","previous_id":1379207,"previous_guid":"BD4CFEE0B6AD11ECB1CF4DE68A7C1D54","section":"\u003cp\u003eGross Morphological Assessment Sheet\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"19fsh\",\"text\":\"The individual grid scores for each knee are added together ( left and right knees are added up separately). Each sum will range from 48 to 192.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":60463,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"2","cases":[],"critical":null},{"id":1379209,"guid":"C6E33820B6AD11ECB1CF4DE68A7C1D54","previous_id":1379208,"previous_guid":"C13A1790B6AD11ECB1CF4DE68A7C1D54","section":"\u003cp\u003eAssessing the Total Knee Cartilage Grade\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"dup8k\",\"text\":\"The total score for each knee will determine the grade category with normal (Grades 0 or I) or OA (Grades 3 or 4).\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"bsbuu\",\"text\":\"\\nKnee Cartilage Grades 0-IV:\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"UNDERLINE\",\"offset\":1,\"length\":26}],\"entityRanges\":[],\"data\":{}},{\"key\":\"89ep4\",\"text\":\"Grade 0 = normal (total score of 48)\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"felhn\",\"text\":\"Grade I = minimal change (total score 49-72)\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"1q9uo\",\"text\":\"Grade II = mild change (total score 73-96)\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"2tneh\",\"text\":\"Grade III = moderate change (total score 97-120)\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"7nv2f\",\"text\":\"Grade IV = severe change (total score 121+)\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":60463,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"3","cases":[],"critical":null}],"template_id":4,"title":"Knee Cartilage Grading Protocol ","title_html":"\u003cp\u003eKnee Cartilage Grading Protocol \u003c/p\u003e","type_id":1,"units":[{"id":1,"type_id":3,"name":"µL","can_manage":0},{"id":2,"type_id":3,"name":"mL","can_manage":0},{"id":3,"type_id":3,"name":"L","can_manage":0},{"id":4,"type_id":3,"name":"µg","can_manage":0},{"id":5,"type_id":3,"name":"mg","can_manage":0},{"id":6,"type_id":3,"name":"g","can_manage":0},{"id":7,"type_id":3,"name":"kg","can_manage":0},{"id":8,"type_id":3,"name":"ng","can_manage":0},{"id":9,"type_id":3,"name":"Hz","can_manage":0},{"id":10,"type_id":24,"name":"°C","can_manage":0},{"id":11,"type_id":24,"name":"°К","can_manage":0},{"id":12,"type_id":24,"name":"°F","can_manage":0},{"id":13,"type_id":25,"name":"Mass Percent","can_manage":0},{"id":14,"type_id":25,"name":"% volume","can_manage":0},{"id":15,"type_id":25,"name":"Mass / % volume","can_manage":0},{"id":16,"typ