section_color":"#94EBFF","section_duration":720,"critical":null,"critical_id":null,"duration":0},{"id":1229618,"guid":"EBE46430F92211EB92CD2122EAD9352C","previous_id":1229604,"previous_guid":"DC824020F92211EB92CD2122EAD9352C","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"85D7DD0171224B31BB40C62DD718AFB1","order_id":1,"type_id":6,"title":"Section","source":{"title":"Obtaining shaved hair "}},{"id":1054724,"guid":"B4F66EF8814D432FBB0E0517DFEF6F8B","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">The hair should be shaved close without nicking the skin, as this can cause falsely elevated concentration of cortisol due to blood being present in the sample. It is essential that hair be collected from the same body region across all animals for a given study.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":720,"critical":null,"critical_id":null,"duration":60},{"id":1229620,"guid":"4F3EF630F92311EB92CD2122EAD9352C","previous_id":1229618,"previous_guid":"EBE46430F92211EB92CD2122EAD9352C","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"8B4877910376462D8CBA1116EE615DA4","order_id":1,"type_id":6,"title":"Section","source":{"title":"Obtaining shaved hair "}},{"id":1054724,"guid":"668E368AEF1A4D6787A4DD70084A3496","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">The distal end of each sample is then wrapped in masking tape, leaving 1\u20131.5 cm of the proximal hair exposed. The sample can be placed in an envelope and stored at -20 \u00b0C (-4 \u00b0F) or room temperature out of direct sunlight until further processing. <\/div><div style = \"text-align :; float : ;\"><img style = \"\" src = \"https:\/\/s3.amazonaws.com\/protocols-files\/files\/d94xbmm2x.jpg\" \/><\/div><br\/><br\/><\/div>"}},{"id":1054725,"guid":"56BC