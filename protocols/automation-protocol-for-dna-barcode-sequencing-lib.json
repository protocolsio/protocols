n_download":1,"is_locked":0},"guid":"CD67C840DBD111EA91A17B65518DB6EC","state_version_id":193,"steps":[{"id":1000443,"guid":"00D23520DBD311EAB52CC3E06E9DB8ED","previous_id":null,"previous_guid":null,"modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"871645B61E934D2A86D0826717630AB9","order_id":1,"type_id":6,"title":"Section","source":{"title":"Preparation of Magnetic Bead\/PEG-NaCl Stock"}},{"id":1054724,"guid":"227A43BB5A114C43A99627035FA726CD","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Add 9 g PEG-8000 to a new 50 mL sterile conical tube.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1000444,"guid":"00D3E2D0DBD311EAB52CC3E06E9DB8ED","previous_id":1000443,"previous_guid":"00D23520DBD311EAB52CC3E06E9DB8ED","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"39AC9EEE5AB44095A63C6A85FE8220A6","order_id":1,"type_id":6,"title":"Section","source":{"title":"Preparation of Magnetic Bead\/PEG-NaCl Stock"}},{"id":1054724,"guid":"4491BED027E04272A1D866EC0D05DF3C","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Add 10 mL 5 mol\/L NaCl.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1000445,"guid":"00D653D0DBD311EAB52CC3E06E9DB8ED","previous_id":1000444,"previous_guid":"00D3E2D0DBD311EAB52CC3E06E9DB8ED","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"DCFAF5658574417E9662A4B84C1F2847","order_id":1,"type_id":6,"title":"Section","source":{"title":"Preparation of Magnetic Bead\/PEG-NaCl Stock"}},{"id":1054724,"guid":"8CBB140DF0D240B3837A01F8A8C0B097","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Add 500 \u00b5L 1 mol\/L Tris-HCl, pH8.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1000446,"guid":"00D89DC0DBD311EAB52CC3E06E9DB8ED","previous_id":1000445,"previous_guid":"00D653D0DBD311EAB52CC3E06E9DB8ED","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"AE439D4D729146D1B556D20D048FBA3A","order_id":1,"type_id":6,"title":"Section","source":{"title":"Preparation of Magnetic Bead\/PEG-NaCl Stock"}},{"id":1054724,"guid":"EDED3953115C44B79A460F053428232E","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Add 500 \u00b5L 0.5 mol\/L EDTA.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1000447,"guid":"00DBAB00DBD311EAB52CC3E06E9DB8ED","previous_id":1000446,"previous_guid":"00D89DC0DBD311EAB52CC3E06E9DB8ED","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"C1295122AE5B4033B7786E75192B33FF","order_id":1,"type_id":6,"title":"Section","source":{"title":"Preparation of Magnetic Bead\/PEG-NaCl Stock"}},{"id":1054724,"guid":"5FE9034EE68E4FC083672FC2B942B6F8","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Fill 50 mL tube to 45 mL using sterile nuclease free water (vendor), mix by vortexing every 1-2 minutes until PEG goes into solution. <\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1000448,"guid":"00DEB840DBD311EAB52CC3E06E9DB8ED","previous_id":1000447,"previous_guid":"00DBAB00DBD311EAB52CC3E06E9DB8ED","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"6DEEA9DFDCBC476699AED0B9D65A637D","order_id":1,"type_id":6,"title":"Section","source":{"title":"Preparation of Magnetic Bead\/PEG-NaCl Stock"}},{"id":1054724,"guid":"F840CAE59760492F8721CF90A59712FA","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Add 27.5 \u00b5L Tween-20 and mix gently by inverting 4-6 times.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1000449,"guid":"00E2AFE0DBD311EAB52CC3E06E9DB8ED","previous_id":1000448,"previous_guid":"00DEB840DBD311EAB52CC3E06E9DB8ED","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"28BCDC89FAE144F3801EDBDD21DEBDDC","order_id":1,"type_id":6,"title":"Section","source":{"title":"Preparation of Magnetic Bead\/PEG-NaCl Stock"}},{"id":1054724,"guid":"271C5D6824094A23AC9094A85AB3FA31","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Vortex Sera-mag SpeedBeads to resuspend and transfer 1 mL to each of two 2 mL micro-centrifuge tubes. <\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1000450,"guid":"00E65960DBD311EAB52CC3E06E9DB8ED","previous_id":1000449,"previous_guid":"00E2AFE0DBD311EAB52CC3E06E9DB8ED","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"85C68E7A6176454FAE9626C80D5C7F81","order_id":1,"type_id":6,"title":"Section","source":{"title":"Preparation of Magnetic Bead\/PEG-NaCl Stock"}},{"id":1054724,"guid":"B07444F6240C43C49AB7ABAE1914646B","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Place SpeedBeads solutions on a magnetic separation rack until beads are drawn to the magnet and solutions are clear.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1000451,"guid":"00EA9F20DBD311EAB52CC3E06E9DB8ED","previous_id":1000450,"previous_guid":"00E65960DBD311EAB52CC3E06E9DB8ED","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"104248C7DAB04FD6B83B8A1FF1570CB9","order_id":1,"type_id":6,"title":"Section","source":{"title":"Preparation of Magnetic Bead\/PEG-NaCl Stock"}},{"id":1054724,"guid":"78F65C6861F14E86865983E9E4A6CEDC","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Remove supernatant.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1000452,"guid":"00F01D60DBD311EAB52CC3E06E9DB8ED","previous_id":1000451,"previous_guid":"00EA9F20DBD311EAB52CC3E06E9DB8ED","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"23FE4D0EAAE34CC5B7938DA70CDB5DE8","order_id":1,"type_id":6,"title":"Section","source":{"title":"Preparation of Magnetic Bead\/PEG-NaCl Stock"}},{"id":1054724,"guid":"2E57C83F4E8843D384363663C1D7D56A","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Add 1 mL 1x TE Buffer (Thermo Scientific, cat. #12090015 pH 8.0) to each micro-centrifuge tube containing beads, remove from magnetic stand, and mix by vortexing. <\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1000453,"guid":"00F4FF60DBD311EAB52CC3E06E9DB8ED","previous_id":1000452,"previous_guid":"00F01D60DBD311EAB52CC3E06E9DB8ED","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"AF694DCD0B4F423986FF27659653FA08","order_id":1,"type_id":6,"title":"Section","source":{"title":"Preparation of Magnetic Bead\/PEG-NaCl Stock"}},{"id":1054724,"guid":"BF60D1DD1E944992BC32B9781E3312CD","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Repeated steps 8-10 two additional times.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1000454,"guid":"00FA7DA0DBD311EAB52CC3E06E9DB8ED","previous_id":1000453,"previous_guid":"00F4FF60DBD311EAB52CC3E06E9DB8ED","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"863CA11AE8A84ED3BCA380FA409407E4","order_id":1,"type_id":6,"title":"Section","source":{"title":"Preparation of Magnetic Bead\/PEG-NaCl Stock"}},{"id":1054724,"guid":"ACE1A938793A4AC8B42E1ECA6453ACC4","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">After mixing, add both 1 mL SpeedBeads solutions to the 50 mL conical tube containing the PEG\/NaCl solution.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1000455,"guid":"00FFFBE0DBD311EAB52CC3E06E9DB8ED","previous_id":1000454,"previous_guid":"00FA7DA0DBD311EAB52CC3E06E9DB8ED","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"240D619A97D1422CA33E46AC821C04C0","order_id":1,"type_id":6,"title":"Section","source":{"title":"Preparation of Magnetic Bead\/PEG-NaCl Stock"}},{"id":1054724,"guid":"F7C1F0B3884642AEA883268EB3A59825","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Fill conical tube to the 50 mL mark with sterile nuclease free water, and gently mix until solution is a uniformed brown color.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1000456,"guid":"010727D0DBD311EAB52CC3E06E9DB8ED","previous_id":1000455,"previous_guid":"00FFFBE0DBD311EAB52CC3E06E9DB8ED","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"A9585F5A0F4940E2BE11F290152FECA7","order_id":1,"type_id":6,"title":"Section","source":{"title":"Preparation of Magnetic Bead\/PEG-NaCl Stock"}},{"id":1054724,"guid":"88A58443FBC04AD1B9C328C748AED9A9","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Wrap in foil (or place in dark container) and store at 4 \u00b0C until use.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1000458,"guid":"23934D60DBD311EAB52CC3E06E9DB8ED","previous_id":1000456,"previous_guid":"010727D0DBD311EAB52CC3E06E9DB8ED","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"6D53F5EECD424A58BEB2E12A580B5262","order_id":1,"type_id":6,"title":"Section","source":{"title":"Preparation of DNA barcode sequencing samples"}},{"id":1054724,"guid":"03CCF0C2DC034FDB8014476C06509ECF","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Transfer 35 \u00b5L of each plasmid DNA sample to a PCR plate (Bio-Rad, HSP9645).<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1000459,"guid":"239A0420DBD311EAB52CC3E06E9DB8ED","previous_id":1000458,"previous_guid":"23934D60DBD311EAB52CC3E06E9DB8ED","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"9C874AA6E73149E0B04F204D088DB0BC","order_id":1,"type_id":6,"title":"Section","source":{"title":"Preparation of DNA barcode sequencing samples"}},{"id":1054724,"guid":"97B56AB6237940C3BDAF23D022B381CD","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Add 5 \u00b5L ApaI Restriction Enzyme Solution for plasmid DNA linearization; mix 3x by repeated aspiration and dispense.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1000460,"guid":"23A0BAE0DBD311EAB52CC3E06E9DB8ED","previous_id":1000459,"previous_guid":"239A0420DBD311EAB52CC3E06E9DB8ED","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"A088DF3689D8407B8C8901ACB3BDE289","order_id":1,"type_id":6,"title":"Section","source":{"title":"Preparation of DNA barcode sequencing samples"}},{"id":1054724,"guid":"5BE691FB645249E0BD20E184B2001F2B","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Incubate samples in PCR plate in automated thermocycler at 37 \u00b0C for 15 minutes.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1000461,"guid":"23A7BFC0DBD311EAB52CC3E06E9DB8ED","previous_id":1000460,"previous_guid":"23A0BAE0DBD311EAB52CC3E06E9DB8ED","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"7B97F0EB43E9462EB24FC34AC06E17B2","order_id":1,"type_id":6,"title":"Section","source":{"title":"Preparation of DNA barcode sequencing samples"}},{"id":1054724,"guid":"786DFD109CFF4BC5858FA8682C94C75C","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Add 2.5 \u00b5L Forward Index Primer and 2.5 \u00b5L Reverse Index Primer to each sample (20 \u00b5mol\/L stock primer solutions, all primer sequences are listed in supplementary files). The Forward and Reverse Index Primers attached sample multiplexing tags to the resulting amplicons so that the different samples could be distinguished when they are pooled and run on the same sequencing flow cell. Eight different Forward Index Primers and 12 different Reverse Index Primers are used to uniquely label the amplicons from each sample across 24 different chemical environments and four time points used for barcode sequencing.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1000462,"guid":"23AEC4A1DBD311EAB52CC3E06E9DB8ED","previous_id":1000461,"previous_guid":"23A7BFC0DBD311EAB52CC3E06E9DB8ED","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"D7C59E12255D4BB194842B04FC78A2A7","order_id":1,"type_id":6,"title":"Section","source":{"title":"Preparation of DNA barcode sequencing samples"}},{"id":1054724,"guid":"72D6D7AAA8E8429F8722F5AFBA9606DD","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Add 45 \u00b5L 2x PCR Master Mix 1 (Thermo Scientific Phusion Flash High-Fidelity PCR Master Mix, Cat. #F548L) to each sample; mix 3x by repeated aspiration and dispense.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1000463,"guid":"23B63EB0DBD311EAB52CC3E06E9DB8ED","previous_id":1000462,"previous_guid":"23AEC4A1DBD311EAB52CC3E06E9DB8ED","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"BA4F42134CA243F5A389390A9042B7F1","order_id":1,"type_id":6,"title":"Section","source":{"title":"Preparation of DNA barcode sequencing samples"}},{"id":1054724,"guid":"01F9C01E63664D1B9D1B85B0646DAD9F","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Run the first PCR reaction in automated thermocycler with the following conditions:<\/div><div class = \"text-block\">    a. Initial denaturation: 98 \u00b0C for 60 s<\/div><div class = \"text-block\">    b. 3 cycles: <\/div><div class = \"text-block\">        i.   Denaturation: 98 \u00b0C for 10 s <\/div><div class = \"text-block\">        ii.  Annealing: 58 \u00b0C for 20 s <\/div><div class = \"text-block\">        iii. Elongation: 72 \u00b0C for 20 s <\/div><div class = \"text-block\">    c. Final Extension: 72 \u00b0C for 60 s <\/div><div class = \"text-block\">    d. Cooling: 20 \u00b0C for 15 s<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1000466,"guid":"66A5DD70DBD311EAB52CC3E06E9DB8ED","previous_id":1000463,"previous_guid":"23B63EB0DBD311EAB52CC3E06E9DB8ED","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"B61EB7FEEA0C4413A1417051CC724D95","order_id":1,"type_id":6,"title":"Section","source":{"title":"Preparation of DNA barcode sequencing samples"}},{"id":1054724,"guid":"810EE0A0AE32499584719D894DFAC43A","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">During the first PCR reaction, pipette 48 \u00b5L Magnetic Bead\/PEG-NaCl Stock into each of 24 wells in a 96-well midi plate (Abgene, AB-0765); mix bead stock well by repeated aspiration and dispense before each transfer.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1000467,"guid":"66AFC880DBD311EAB52CC3E06E9DB8ED","previous_id":1000466,"previous_guid":"66A5DD70DBD311EAB52CC3E06E9DB8ED","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"733B4C65FC1C43F5B56962028F8A1420","order_id":1,"type_id":6,"title":"Section","source":{"title":"Preparation of DNA barcode sequencing samples"}},{"id":1054724,"guid":"2CD7921F5D5341D7A08C6C27CE513652","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">When the first PCR reaction is finished, transfer 80 \u00b5L from each PCR reaction to a well in the midi plate; mix PCR reaction solution and bead stock 10x by repeated aspiration and dispense; wait for 5 minutes. During this step, the ratio of Bead Stock to PCR reaction volume is 0.6x. Consequently, because of the relatively low PEG concentration in the mixture, the 5.5kb plasmid template binds to the beads.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1000468,"guid":"66B98C81DBD311EAB52CC3E06E9DB8ED","previous_id":1000467,"previous_guid":"66AFC880DBD311EAB52CC3E06E9DB8ED","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"203F1C20415C4D1DA8A88133A1B0D41C","order_id":1,"type_id":6,"title":"Section","source":{"title":"Preparation of DNA barcode sequencing samples"}},{"id":1054724,"guid":"4EFE4D1A738D4D6E94317A7E2DD0AB6E","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Move the midi plate to 96-well magnet base to pull the magnetic beads out of suspension; wait for 2 minutes. <\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1000469,"guid":"66C26620DBD311EAB52CC3E06E9DB8ED","previous_id":1000468,"previous_guid":"66B98C81DBD311EAB52CC3E06E9DB8ED","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"760A4F39D0614961B41FB96BD6FFAC7A","order_id":1,"type_id":6,"title":"Section","source":{"title":"Preparation of DNA barcode sequencing samples"}},{"id":1054724,"guid":"77717CF65F6C489AAED932E9069FA729","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Transfer 128 \u00b5L of supernatant from each sample to a clean well in the midi plate (still on the magnet base); wait an additional 3 minutes for final bead separation.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1000470,"guid":"66CC7840DBD311EAB52CC3E06E9DB8ED","previous_id":1000469,"previous_guid":"66C26620DBD311EAB52CC3E06E9DB8ED","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"F4D33019B894446090FFF71113D4B21D","order_id":1,"type_id":6,"title":"Section","source":{"title":"Preparation of DNA barcode sequencing samples"}},{"id":1054724,"guid":"9B5B4B44DCDE4903840048BC528D5A17","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Transfer 118 \u00b5L of supernatant (containing the 218 bp PCR amplicon and primers)from each sample to another clean well in the midi plate; move the midi plate from the magnet base to automated shaker.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1000471,"guid":"66D6B170DBD311EAB52CC3E06E9DB8ED","previous_id":1000470,"previous_guid":"66CC7840DBD311EAB52CC3E06E9DB8ED","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"7D6579D7D6F74A14A5A2B2B20C465544","order_id":1,"type_id":6,"title":"Section","source":{"title":"Preparation of DNA barcode sequencing samples"}},{"id":1054724,"guid":"13F3019589AB495689EC0E6A4783D0AD","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Add 70.8 \u00b5L Magnetic Bead Stock (0.6x of 118 \u00b5L supernatant volume) to each sample; mix supernatant and bead stock 10x by repeated aspiration and dispense; wait for 5 minutes. During this step, only the 206 bp PCR amplicon binds to the beads; the 70 bp primers remain in the supernatant.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1000472,"guid":"66E111B0DBD311EAB52CC3E06E9DB8ED","previous_id":1000471,"previous_guid":"66D6B170DBD311EAB52CC3E06E9DB8ED","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"6B8F1E5A28E447A1A650F0B7A8D161BF","order_id":1,"type_id":6,"title":"Section","source":{"title":"Preparation of DNA barcode sequencing samples"}},{"id":1054724,"guid":"80F83B7B902C433CA5E2B811A478DB49","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Move the midi plate back to the magnet base; wait for 5 minutes.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1000473,"guid":"66EBC010DBD311EAB52CC3E06E9DB8ED","previous_id":1000472,"previous_guid":"66E111B0DBD311EAB52CC3E06E9DB8ED","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"5A37028879724E3D97D4597C9F37F8A7","order_id":1,"type_id":6,"title":"Section","source":{"title":"Preparation of DNA barcode sequencing samples"}},{"id":1054724,"guid":"ADA82261FFB0484CBC4393E36DE36F4F","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Remove and discard the supernatant from each well.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1000474,"guid":"66F7CE00DBD311EAB52CC3E06E9DB8ED","previous_id":1000473,"previous_guid":"66EBC010DBD311EAB52CC3E06E9DB8ED","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"6A1C0F3F11A3458C86D28FB9E72A11D6","order_id":1,"type_id":6,"title":"Section","source":{"title":"Preparation of DNA barcode sequencing samples"}},{"id":1054724,"guid":"B9E4D3BCDD494B4BB8A0900A3A93D5B2","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Add 200 \u00b5L 80 % ethanol to each magnetic bead pellet; wait 30 s.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1000475,"guid":"6703DBF0DBD311EAB52CC3E06E9DB8ED","previous_id":1000474,"previous_guid":"66F7CE00DBD311EAB52CC3E06E9DB8ED","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"ACB68A4BAFAF48628C662E5A3014CD38","order_id":1,"type_id":6,"title":"Section","source":{"title":"Preparation of DNA barcode sequencing samples"}},{"id":1054724,"guid":"9DF381C6F69C4C0691D9505A10B8F549","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Remove and discard the ethanol supernatant; then, using 50 \u00b5L tips, remove residual supernatant from the bottom of each well. <\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1000476,"guid":"670F2690DBD311EAB52CC3E06E9DB8ED","previous_id":1000475,"previous_guid":"6703DBF0DBD311EAB52CC3E06E9DB8ED","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"51CEC23E2D2F4E7096E549067C1AEB3F","order_id":1,"type_id":6,"title":"Section","source":{"title":"Preparation of DNA barcode sequencing samples"}},{"id":1054724,"guid":"95BE30C59E0B46A0BBDBEA45549DD1A8","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Move the midi plate from magnet base to the automated shaker; allow magnetic bead pellets to dry for 5 minutes at room temperature.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1000477,"guid":"671ABF50DBD311EAB52CC3E06E9DB8ED","previous_id":1000476,"previous_guid":"670F2690DBD311EAB52CC3E06E9DB8ED","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"B2ACBA2FFF64473F8E4605A02E950064","order_id":1,"type_id":6,"title":"Section","source":{"title":"Preparation of DNA barcode sequencing samples"}},{"id":1054724,"guid":"C9EAAC3C74914A5FBD8197741A930A72","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Add 35 \u00b5L nuclease-free water to each sample; resuspend beads by 5x repeated aspiration and dispense.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1000478,"guid":"6726F450DBD311EAB52CC3E06E9DB8ED","previous_id":1000477,"previous_guid":"671ABF50DBD311EAB52CC3E06E9DB8ED","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"A2C60FAB6DAB4E0091B5ABFBC3D53E95","order_id":1,"type_id":6,"title":"Section","source":{"title":"Preparation of DNA barcode sequencing samples"}},{"id":1054724,"guid":"11CB550180AC4D41B898734F17FFECA0","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Mix samples by shaking at 1800 rpm for 10 s; wait for 5 minutes.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1000479,"guid":"6733C590DBD311EAB52CC3E06E9DB8ED","previous_id":1000478,"previous_guid":"6726F450DBD311EAB52CC3E06E9DB8ED","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"913FC858FE6F4691A9B2ECB443F8E9F1","order_id":1,"type_id":6,"title":"Section","source":{"title":"Preparation of DNA barcode sequencing samples"}},{"id":1054724,"guid":"8C8E5D4D66614C6D94B0877E81EB11B5","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Move the midi plate back to the magnet base; wait for 5 minutes.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1000480,"guid":"6741CF50DBD311EAB52CC3E06E9DB8ED","previous_id":1000479,"previous_guid":"6733C590DBD311EAB52CC3E06E9DB8ED","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"996BB7888FC14259A77B4082EAD0D256","order_id":1,"type_id":6,"title":"Section","source":{"title":"Preparation of DNA barcode sequencing samples"}},{"id":1054724,"guid":"DBAAB69BE886404FA0CD8F0921EAF7A5","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">During 5-minute wait, pipette 36 \u00b5L PCR Master Mix 2 (containing primers) into each of 24 wells in a PCR plate.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1000481,"guid":"674FB200DBD311EAB52CC3E06E9DB8ED","previous_id":1000480,"previous_guid":"6741CF50DBD311EAB52CC3E06E9DB8ED","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"E61B249869A54F498B1981C7C3B929AA","order_id":1,"type_id":6,"title":"Section","source":{"title":"Preparation of DNA barcode sequencing samples"}},{"id":1054724,"guid":"C61A0665C9EE43A29B389F6637025EAF","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">After 5-minute wait, transfer 30 \u00b5L of each supernatant from the midi plate (on magnet base) to the PCR plate.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1000482,"guid":"675D4690DBD311EAB52CC3E06E9DB8ED","previous_id":1000481,"previous_guid":"674FB200DBD311EAB52CC3E06E9DB8ED","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"B5D8D81544B74183B0FB85355A4536B5","order_id":1,"type_id":6,"title":"Section","source":{"title":"Preparation of DNA barcode sequencing samples"}},{"id":1054724,"guid":"811A1EB882EA42AA80FBED16CA3EAF6D","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Run the second PCR reaction in the automated thermocycler with the following conditions:<\/div><div class = \"text-block\">    a. Initial denaturation: 98 \u00b0C for 60 s<\/div><div class = \"text-block\">    b. 15 cycles: <\/div><div class = \"text-block\">        i.  Denaturation: 98 \u00b0C for 10 s <\/div><div class = \"text-block\">        ii. Annealing and Elongation: 72 \u00b0C for 30 s <\/div><div class = \"text-block\">    c. Final Extension: 72 \u00b0C for 120 s <\/div><div class = \"text-block\">    d. Cooling: 15 \u00b0C for 20 s<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1000484,"guid":"8A683910DBD311EAB52CC3E06E9DB8ED","previous_id":1000482,"previous_guid":"675D4690DBD311EAB52CC3E06E9DB8ED","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"87234BE5108E47998C8E563BDE8440C4","order_id":1,"type_id":6,"title":"Section","source":{"title":"Preparation of DNA barcode sequencing samples"}},{"id":1054724,"guid":"ECB5F9F28E0148F187C8E6E7B1B8ED2D","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">During the second PCR reaction, pipette 66 \u00b5L (1.1x of the 60 \u00b5L PCR volume) Magnetic Bead Stock into each of 24 clean wells in the midi plate (on the automated shaker); mix bead stock well by repeated aspiration and dispense before each transfer.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1000485,"guid":"8A7FDFC0DBD311EAB52CC3E06E9DB8ED","previous_id":1000484,"previous_guid":"8A683910DBD311EAB52CC3E06E9DB8ED","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"644D633170C2432691C1DBC1B92C7644","order_id":1,"type_id":6,"title":"Section","source":{"title":"Preparation of DNA barcode sequencing samples"}},{"id":1054724,"guid":"AC856B6F6B0D42D0B09DBB8203E111C5","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">When the second PCR reaction is finished, transfer 60 \u00b5L from each PCR reaction to a well in the midi plate; mix the PCR reaction solution and bead stock 10x by repeated aspiration and dispensing; wait for 5 minutes. During this step, with a Bead Stock to PCR reaction volume ratio of 1.1, the 306 bp PCR amplicon binds to the beads while the 70 bp primers remain in the supernatant.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1000486,"guid":"8A8E5EB0DBD311EAB52CC3E06E9DB8ED","previous_id":1000485,"previous_guid":"8A7FDFC0DBD311EAB52CC3E06E9DB8ED","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"A2AC58DF2F2344C6A3B2D012B7625D32","order_id":1,"type_id":6,"title":"Section","source":{"title":"Preparation of DNA barcode sequencing samples"}},{"id":1054724,"guid":"67F89A50BEBB4F00BA3E61784D5612FE","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Move the midi plate back to the magnet base; wait for 4 minutes.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"s