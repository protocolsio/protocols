:{},"shared_access_id":0,"should_acknowledge":false,"show_comparison":false,"sign_info":null,"space_access":{"can_preview":false,"can_view":false,"can_edit":false,"can_remove":false,"can_add":false,"can_publish":true,"can_get_doi":true,"can_share":false,"can_share_organization":false,"can_move":false,"can_move_outside":false,"can_transfer":false,"can_download":false,"limited_run":false,"limited_private_links":false,"limited_blind_links":false,"is_locked":false},"space_id":22979,"state_version_id":21,"stats":{"is_voted":false,"number_of_views":67,"number_of_steps":14,"number_of_bookmarks":0,"number_of_comments":0,"number_of_bookmarked_comments":0,"number_of_steps_comments":0,"number_of_protocol_comments":0,"number_of_exports":0,"number_of_runs":0,"number_of_votes":0,"number_of_reagents":0,"number_of_equipments":0,"number_of_collections":0,"number_of_forks":{"private":0,"public":0},"number_of_accessible_forks":0},"status":{"id":1,"info":""},"steps":[{"id":1163049,"guid":"9F0971009FC211EB9FD0C9799A38E3DB","previous_id":0,"previous_guid":null,"section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"eaee7\",\"text\":\"Euthanize mice (12 week, male, C57BL/6 for kidney; 10 week, female, MMTV-PyMT for mammary tumor, both from Jackson Laboratory, Bar Harbor, ME) by CO2 inhalation.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":49211,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"1","cases":[],"critical":null},{"id":1163050,"guid":"9F0AA9809FC211EB9FD0C9799A38E3DB","previous_id":1163049,"previous_guid":"9F0971009FC211EB9FD0C9799A38E3DB","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"70a3p\",\"text\":\"Dissect kidneys or mammary tumor and mince into ~1 mm3  pieces.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":49211,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"2","cases":[],"critical":null},{"id":1163051,"guid":"9F0B6CD09FC211EB9FD0C9799A38E3DB","previous_id":1163050,"previous_guid":"9F0AA9809FC211EB9FD0C9799A38E3DB","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"44pp4\",\"text\":\"Process samples using microfluidic device platform.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":49211,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"3","cases":[],"critical":null},{"id":1163052,"guid":"9F0C7E409FC211EB9FD0C9799A38E3DB","previous_id":1163051,"previous_guid":"9F0B6CD09FC211EB9FD0C9799A38E3DB","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"evbqu\",\"text\":\"After device processing, centrifuge recovered cells (400xg, 5 min) and treat with 100 Units of DNase I for 5 min at 37°C, and wash by centrifugation into PBS+1% BSA (PBS+).\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":49211,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4","cases":[],"critical":null},{"id":1163053,"guid":"9F0DB6C09FC211EB9FD0C9799A38E3DB","previous_id":1163052,"previous_guid":"9F0C7E409FC211EB9FD0C9799A38E3DB","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"ep2f2\",\"text\":\"Incubate samples with RBC lysis buffer for 5 min on ice, centrifuge, and resuspend in PBS+.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":49211,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"5","cases":[],"critical":null},{"id":1163054,"guid":"9F0F16509FC211EB9FD0C9799A38E3DB","previous_id":1163053,"previous_guid":"9F0DB6C09FC211EB9FD0C9799A38E3DB","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"2ehdr\",\"text\":\"Stain cells with SytoxBlue (Life Technologies, Carlsbad, CA, USA) prior to FACS (FACSAria Fusion, BD Biosciences, Franklin Lakes, NJ) to remove dead cells and ambient RNA.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":49211,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"6","cases":[],"critical":null},{"id":1163055,"guid":"9F109CF09FC211EB9FD0C9799A38E3DB","previous_id":1163054,"previous_guid":"9F0F16509FC211EB9FD0C9799A38E3DB","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"6fct2\",\"text\":\"Centrifuge sorted live single cells (SytoxBlue-neg) and resuspend at a concentration of 1000 cells/µL in PBS with 0.04% BSA. ‘\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":49211,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"7","cases":[],"critical":null},{"id":1163056,"guid":"9F1271B09FC211EB9FD0C9799A38E3DB","previous_id":1163055,"previous_guid":"9F109CF09FC211EB9FD0C9799A38E3DB","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"fbr4c\",\"text\":\"Use the 10x Chromium system (10x Genomics, Pleasanton, CA) for droplet-enabled scRNA-seq. Load oil, cells, reagents, and beads onto an eight-channel microfluidic chip. Load lanes with ~17,000 cells from each sample, as determined using an automated cell counter (Countess II, Invitrogen, Carlsbad, CA).\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":49211,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"8","cases":[],"critical":null},{"id":1163057,"guid":"9F1446709FC211EB9FD0C9799A38E3DB","previous_id":1163056,"previous_guid":"9F1271B09FC211EB9FD0C9799A38E3DB","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"bs4n7\",\"text\":\"Perform library generation for 10x Genomics Single Cell Expression v3 chemistry according to manufacturer’s instructions.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":49211,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"9","cases":[],"critical":null},{"id":1163058,"guid":"9F1669509FC211EB9FD0C9799A38E3DB","previous_id":1163057,"previous_guid":"9F1446709FC211EB9FD0C9799A38E3DB","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"bhkcs\",\"text\":\"Sequence the samples at the desired depth using an Illumina NovaSeq 6000 platform (Illumina, San Diego, CA).\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":49211,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"10","cases":[],"critical":null},{"id":1163059,"guid":"9F188C319FC211EB9FD0C9799A38E3DB","previous_id":1163058,"previous_guid":"9F1669509FC211EB9FD0C9799A38E3DB","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"4mu1d\",\"text\":\"Align sequencing fastq files using 10x Genomics Cell Ranger software (version 3.1.0) to an indexed mm10 reference genome.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":49211,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"11","cases":[],"critical":null},{"id":1163060,"guid":"9F1AFD309FC211EB9FD0C9799A38E3DB","previous_id":1163059,"previous_guid":"9F188C319FC211EB9FD0C9799A38E3DB","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"eddff\",\"text\":\"Use Cell Ranger Aggr to normalize the mapped reads for cells across the libraries for each data set.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":49211,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"12","cases":[],"critical":null},{"id":1163061,"guid":"9F1D6E319FC211EB9FD0C9799A38E3DB","previous_id":1163060,"previous_guid":"9F1AFD309FC211EB9FD0C9799A38E3DB","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"1vco6\",\"text\":\"Discard genes that were not detected in at least 3 cells from further analysis. Discard cells with low (3000 for kidney; \\u003e4000 for mammary tumor) unique genes expressed, as these potentially represent low quality or doublet cells, respectively. Discard cells with high mitochondrial gene percentages (\\u003e50% for kidney and \\u003e25% for mammary tumor), as these can also represent low quality or dying cells.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":49211,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"13","cases":[],"critical":null},{"id":1163062,"guid":"9F202D509FC211EB9FD0C9799A38E3DB","previous_id":1163061,"previous_guid":"9F1D6E319FC211EB9FD0C9799A38E3DB","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"52uoj\",\"text\":\"Use the Seurat pipeline for cluster identification. Perform principle component analysis (PCA) using genes that are highly variable. Perform density clustering to identify groups and use Uniform Manifold Approximation and Projection (UMAP) plots to visualize the groupings.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":49211,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"14","cases":[],"critical":null}],"table_of_contents":[],"template_id":1,"title":"Single cell RNA Sequencing for tissue samples processed with microfluidic device platform","title_html":"\u003cp\u003eSingle cell RNA Sequencing for tissue samples processed with microfluidic device platform\u003c/p\u003e","translations":[],"troubleshooting_items":[],"type_id":1,"units":[{"id":1,"type_id":3,"name":"µL","can_manage":0,"read_only":0},{"id":2,"type_id":3,"name":"mL","can_manage":0,"read_only":0},{"id":3,"type_id":3,"name":"L","can_manage":0,"read_only":0},{"id":4,"type_id":3,"name":"µg","can_manage":0,"read_only":0},{"id":5,"type_id":3,"name":"mg","can_manage":0,"read_only":0},{"id":6,"type_id":3,"name":"g","can_manage":0,"read_only":0},{"id":7,"type_id":3,"name":"kg","can_manage":0,"read_only":0},{"id":8,"type_id":3,"name":"ng","can_manage":0,"read_only":0},{"id":9,"type_id":3,"name":"Hz","can_manage":0,"read_only":0},{"id":10,"type_id":24,"name":"°C","can_manage":0,"read_only":0},{"id":11,"type_id":24,"name":"°К","can_manage":0,"read_only":0},{"id":12,"type_id":24,"name":"°F","can_manage":0,"read_only":0},{"id":13,"type_id":25,"name":"Mass Percent","can_manage":0,"read_only":0},{"id":14,"type_id":25,"name":"% volume","can_manage":0,"read_only":0},{"id":15,"type_id":25,"name":"Mass / % volume","can_manage":0,"read_only":0},{"id":16,"type_id":25,"name":"Parts per Million (PPM)","can_manage":0,"read_only":0},{"id":17,"type_id":25,"name":"Parts per Billion (PPB)","can_manage":0,"read_only":0},{"id":18,"type_id":25,"name":"Parts per Trillion (PPT)","can_manage":0,"read_only":0},{"id":19,"type_id":25,"name":"Mole Fraction","can_manage":0,"read_only":0},{"id":20,"type_id":25,"name":"Mole Percent","can_manage":0,"read_only":0},{"id":21,"type_id":25,"name":"Molarity (M)","can_manage":0,"read_only":1},{"id":22,"type_id"