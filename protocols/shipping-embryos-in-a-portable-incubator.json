r_of_views":0,"number_of_steps":0,"number_of_bookmarks":0,"number_of_comments":0,"number_of_exports":0,"number_of_runs":0,"number_of_votes":0,"is_voted":0},"parent_protocols":[],"parent_collections":[],"cited_protocols":[]},"created_on":1616443277,"resolve_on":0,"resolved_user":{"name":" ","affiliation":null,"affiliations":[],"username":null,"note":null,"link":null,"image":{"source":null,"placeholder":null},"badges":[],"verified":0,"is_verified_user":false,"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false},"shared":false}}],"is_owner":1,"has_subprotocols":0,"is_subprotocol":0,"is_bookmarked":0,"can_claim_authorship":0,"can_accept_authorship":0,"can_be_copied":1,"can_remove_fork":1,"fork_id":null,"url":"https:\/\/www.protocols.io\/view\/shipping-embryos-in-a-portable-incubator-bth4nj8w","forks_count":{"private":0,"public":0},"access":{"can_view":1,"can_remove":0,"can_add":0,"can_edit":0,"can_publish":0,"can_get_doi":0,"can_share":1,"can_move":1,"can_move_outside":1,"can_transfer":1,"can_download":1,"is_locked":0},"guid":"1B0C5EC08A7E11EB85471F738719EFDB","state_version_id":102,"steps":[{"id":1148383,"guid":"27E4F1C08A7E11EB85471F738719EFDB","previous_id":null,"previous_guid":null,"modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"58F5C3D2C6154CCAA54402CD2E9E0910","order_id":1,"type_id":6,"title":"Section","source":{"title":" "}},{"id":1054724,"guid":"5D3E3E100744403897D1EE650C530CF4","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Test the shipping incubator and make sure that it can remain powered for at least 24 hours while operating with the battery. We currently use TO 42 from WTA <\/div><div class = \"text-block\"><a href=\"https:\/\/www.wtavet.com.br\/?lang=en\" style = \"text-decoration:underline;color:blue;cursor:pointer;\"><span style = \":;\">https:\/\/www.wtavet.com.br\/?lang=en<\/span><\/a><\/div><div class = \"text-block\"> but others work well also. The temperature is set to 38.5\u00b0C<\/div><\/div>"}},{"id":1054725,"guid":"488B4AE08A7F11EB85471F738719EFDB","order_id":2,"type_id":23,"title":"file","source":{"source":"https:\/\/s3.amazonaws.com\/protocols-files\/files\/c8p38p8n.jpg","placeholder":"","original_name":"Picture2.jpg"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1148385,"guid":"3E2B00508A7E11EB85471F738719EFDB","previous_id":1148383,"previous_guid":"27E4F1C08A7E11EB85471F738719EFDB","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"FD7D9F3AD2A64390BCE7D2C9D0DC92FD","order_id":1,"type_id":6,"title":"Section","source":{"title":" "}},{"id":1054724,"guid":"C302DB44E78948BDB32ACD1366C4B415","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Start charging shipping incubator 1 day before shipping so that it is fully charged at the time tubes containing embryos are added to the incubator.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1148386,"guid":"5F85AAC08A7E11EB85471F738719EFDB","previous_id":1148385,"previous_guid":"3E2B00508A7E11EB85471F738719EFDB","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"D0A9F5B02E424710B2E250137C6413EA","order_id":1,"type_id":6,"title":"Section","source":{"title":" "}},{"id":1054724,"guid":"43DCF8158C8E4CDBB4A7AD5FE5F9607B","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Make shipping label. We routinely use Fedex service with Priority Overnight Service. Fedex states that these packages arrive by 10:30 AM but it is more like noon for locations not close to a Fedex facility.  First Overnight Service is supposed to arrive by 8:00 am but arrival before 10:30 AM is more like it when the location is remote.  Note First Overnight is expensive!  We deliver the package to Fedex for shipping by 4:30 PM<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1148388,"guid":"85093E608A7E11EB85471F738719EFDB","previous_id":1148386,"previous_guid":"5F85AAC08A7E11EB85471F738719EFDB","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"753AFA5992574578910A895A8AD24565","order_id":1,"type_id":6,"title":"Section","source":{"title":" "}},{"id":1054724,"guid":"B10ED1A4E47B42A29A45B2A62477D8A6","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><div class = \"justify\" style = \"text-align:left\">Two methods have been used successfully.<\/div><\/div><div class = \"text-block\"><ul style = \"list-style-type:disc;\"><li style = \"counter-reset:ol0;list-style-type:disc;\">Use 1 ml Corning cryogenic vials with orange caps filled with HEPES-TALP supplemented with 10% (v\/v) fetal bovine serum (R&D Systems) and 50 \u00b5M dithithreitol. We have successfully shipped up to 15 embryos using this method.  <\/li><li style = \"counter-reset:ol0;list-style-type:disc;\"><span>Add up to \u223c50 embryos per tube in polypropylene-stoppered 10 \u00d7 63 mm polystyrene tubes containing 1 ml SOF-BE2 supplemented with 1 mM HEPES, pH 7.5, and 50 \u03bcM dithiothreitol. Tubes are equilibrated overnight in a humidified incubator at 38.5\u00b0C and 5% O<\/span><span style = \"vertical-align:sub;\">2<\/span><span>, 5% CO<\/span><span style = \"vertical-align:sub;\">2<\/span><span>, and 90% N<\/span><span style = \"vertical-align:sub;\">2<\/span><span>. After addition of embryos to the medium, \u223c1.5 ml mineral oil was layered over the medium and tubes were placed in the incubator. <\/span><\/li><\/ul><\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1148389,"guid":"9B603CE08A7E11EB85471F738719EFDB","previous_id":1148388,"previous_guid":"85093E608A7E11EB85471F738719EFDB","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"85B6737CFF0F4E809C0297055F48F7A1","order_id":1,"type_id":6,"title":"Section","source":{"title":" "}},{"id":1054724,"guid":"A766974AD7D842A8A89F1CD1E80FE27A","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">We have had plastic shipping containers custom-made for us by Benson Box (<\/div><div class = \"text-block\"><a href=\"https:\/\/bensonbox.com\" style = \"text-decoration:underline;color:blue;cursor:pointer;\"><span style = \":;\">https:\/\/bensonbox.com<\/span><\/a><\/div><div class = \"text-block\">) to protect the incubator during shipment. These are lined with Styrofoam to cushion the incubator and have lots of labeling to recommend caution with handling. The one currently used for the WTA incubator was designed for a large incubator and so additional padding is added to the inside of the container. Images are shown on the next page.Benson Boxes are very durable. <\/div><div class = \"text-block\"><div class = \"justify\" style = \"text-align:left\"><\/div><\/div><div class = \"text-block\"><div class = \"justify\" style = \"text-align:justify\"><\/div><style>\n\t\t\t\t\t\t\t  .justify:after {\n\t\t\t\t\t\t\t    content: \"\";\n\t\t\t\t\t\t\t    display:inline-block;\n\t\t\t\t\t\t\t    width: 100%;\n\t\t\t\t\t\t\t  }\n\t\t\t\t\t\t\t<\/style><\/div><div class = \"text-block\"><div class = \"justify\" style = \"text-align:justify\"><\/div><style>\n\t\t\t\t\t\t\t  .justify:after {\n\t\t\t\t\t\t\t    content: \"\";\n\t\t\t\t\t\t\t    display:inline-block;\n\t\t\t\t\t\t\t    width: 100%;\n\t\t\t\t\t\t\t  }\n\t\t\t\t\t\t\t<\/style><\/div><\/div>"}},{"id":1054725,"guid":"358302308A7F11EB85471F738719EFDB","order_id":2,"type_id":23,"title":"file","source":{"source":"https:\/\/s3.amazonaws.com\/protocols-files\/files\/c8p28p8n.png","placeholder":"","original_name":"Picture1.png"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0}],"document":"","materials":[],"description":"<div class = \"text-blocks\"><div class = \"text-block\">This is a protocol to describe how to ship non-frozen embryos<\/div><\/div>","changed_on":1616431997}