epth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":82671,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"5","cases":[],"critical":null},{"id":1697674,"guid":"ABBBF880FF7611EDA49FDF75CDED1C42","previous_id":1697673,"previous_guid":"ABBAE711FF7611EDA49FDF75CDED1C42","section":"\u003cp\u003eProcedure\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"ak47u\",\"text\":\"Place a digital camcorder in front of the 600-ml glass beaker towards the glass beaker and mirrors. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"2k6pi\",\"text\":\" \",\"type\":\"atomic\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[{\"key\":0,\"offset\":0,\"length\":1}],\"data\":{}},{\"key\":\"funag\",\"text\":\"\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{\"0\":{\"type\":\"notes\",\"mutability\":\"IMMUTABLE\",\"data\":{\"blocks\":[{\"data\":{},\"depth\":0,\"entityRanges\":[],\"inlineStyleRanges\":[],\"key\":\"3cm2\",\"text\":\"Ensure the glass cylinder is not overlapping in the mirror.\",\"type\":\"unstyled\"}],\"entityMap\":{}}}}}","data":null,"protocol_id":82671,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"6","cases":[],"critical":null},{"id":1697675,"guid":"ABBCE2E1FF7611EDA49FDF75CDED1C42","previous_id":1697674,"previous_guid":"ABBBF880FF7611EDA49FDF75CDED1C42","section":"\u003cp\u003eProcedure\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"h4jk\",\"text\":\"Place a test mouse into the 600-ml glass beaker and start recording.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":82671,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"7","cases":[],"critical":null},{"id":1697676,"guid":"ABBE1B61FF7611EDA49FDF75CDED1C42","previous_id":1697675,"previous_guid":"ABBCE2E1FF7611EDA49FDF75CDED1C42","section":"\u003cp\u003eProcedure\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"44jfv\",\"text\":\"After 10 minutes, stop recording and return the test mouse immediately to its home cage.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":82671,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"8","cases":[],"critical":null},{"id":1697677,"guid":"ABBFF020FF7611EDA49FDF75CDED1C42","previous_id":1697676,"previous_guid":"ABBE1B61FF7611EDA49FDF75CDED1C42","section":"\u003cp\u003eProcedure\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"el47o\",\"text\":\"Clean the 600-ml glass beaker with 70% ethanol and water.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":82671,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"9","cases":[],"critical":null},{"id":1697678,"guid":"B799FEE0FF7611EDA49FDF75CDED1C42","previous_id":1697677,"previous_guid":"ABBFF020FF7611EDA49FDF75CDED1C42","section":"\u003cp\u003eAnalysis\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"7llch\",\"text\":\"Analyze videos offline by a person blinded to the treatments.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":82671,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"10","cases":[],"critical":null},{"id":1697679,"guid":"C4456760FF7611EDA49FDF75CDED1C42","previous_id":1697678,"previous_guid":"B799FEE0FF7611EDA49FDF75CDED1C42","sec