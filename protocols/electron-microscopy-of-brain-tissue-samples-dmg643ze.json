s_text":"","ownership_history":null,"parent_collections":[],"parent_protocols":[],"peer_reviewed":false,"protocol_references":"","public":true,"public_fork_note":"","published_on":1726594461,"references":[],"related_equipments":[],"related_materials":[],"reserved_doi":"","retraction_reason":null,"samples":{},"shared_access_id":265,"show_comparison":false,"sign_info":null,"space_access":{"can_view":false,"can_edit":false,"can_remove":false,"can_add":false,"can_publish":true,"can_get_doi":true,"can_share":false,"can_move":false,"can_move_outside":false,"can_transfer":false,"can_download":false,"limited_run":false,"limited_private_links":false,"limited_blind_links":false,"is_locked":false},"space_id":8782,"state_version_id":193,"stats":{"is_voted":false,"number_of_views":0,"number_of_steps":16,"number_of_bookmarks":0,"number_of_comments":0,"number_of_bookmarked_comments":0,"number_of_steps_comments":0,"number_of_protocol_comments":0,"number_of_exports":0,"number_of_runs":0,"number_of_votes":0,"number_of_reagents":0,"number_of_equipments":0,"number_of_collections":0,"number_of_forks":{"private":0,"public":0},"number_of_accessible_forks":0},"status":{"id":1,"info":"We use this protocol and it's working"},"steps":[{"id":2232958,"guid":"6450D15D5566471EAD5F2E236F0BB9EF","previous_id":0,"previous_guid":null,"section":"\u003cp\u003eBrain perfusion\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"7lsuq\",\"text\":\"Mice were anesthetized with 150 uL pentobarbital (Euthasol), and their hearts were punctured.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"9a5aj\",\"text\":\"\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":107774,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"1","cases":[],"critical":null},{"id":2232959,"guid":"7C35FEFD98C74780A969A85FA3DD2A05","previous_id":2232958,"previous_guid":"6450D15D5566471EAD5F2E236F0BB9EF","section":"\u003cp\u003eBrain perfusion\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"f8uns\",\"text\":\"Mice were perfused with 50 mL of 37°C PBS followed by 50 mL of 37°C 4% paraformaldehyde (PFA) at a flow rate of 6 mL/min.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":107774,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"2","cases":[],"critical":null},{"id":2232961,"guid":"8B2ACB45FDFA4E1DAD0089A4DE60241A","previous_id":2232959,"previous_guid":"7C35FEFD98C74780A969A85FA3DD2A05","section":"\u003cp\u003eBrain perfusion\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"4niho\",\"text\":\"     \\nBrains were dissected and immediately placed in a cold (4°C) fixative solution of 3% glutaraldehyde, 1% paraformaldehyde (Electron Microscopy Sciences, EMS), 5% sucrose in 0.1M sodium cacodylate\\ntrihydrate.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":107774,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"3","cases":[],"critical":null},{"id":2232962,"guid":"523CE4D649D6431B805A72D09A89857A","previous_id":2232961,"previous_guid":"8B2ACB45FDFA4E1DAD0089A4DE60241A","section":"\u003cp\u003eSamples preparation\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"blb5r\",\"text\":\"     \\nTissue blocks were transferred to brass high-pressure freezing planchettes (Ted Pella, Inc.) prefilled with buffer containing 10% 70kD Ficoll (extracellular cryoprotectant).\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":107774,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4","cases":[],"critical":null},{"id":2232963,"guid":"EE4AFB11FEA44D9F9550ACF67FC21509","previous_id":2232962,"previous_guid":"523CE4D649D6431B805A72D09A89857A","section":"\u003cp\u003eSamples preparation\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"uesg\",\"text\":\"     \\nPlanchettes were placed into a high-pressure freezing machine (Bal-Tec HPM010) and ultra-rapidly frozen.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":107774,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"5","cases":[],"critical":null},{"id":2232964,"guid":"19B40ABF38A64D2782203651BFD50578","previous_id":2232963,"previous_guid":"EE4AFB11FEA44D9F9550ACF67FC21509","section":"\u003cp\u003eSamples preparation\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"elqim\",\"text\":\"     \\nPlanchettes with vitrified tissue samples were transferred under liquid nitrogen to cryotubes (Nunc) containing a frozen solution of 2% Osmium tetroxide (EMS), 0.05% uranyl acetate (EMS) in acetone.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":107774,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"6","cases":[],"critical":null},{"id":2232965,"guid":"BA5699032F9D4E0A9641524C499AC2CB","previous_id":2232964,"previous_guid":"19B40ABF38A64D2782203651BFD50578","section":"\u003cp\u003eSamples preparation\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"3up32\",\"text\":\"     \\nTubes were placed in a freeze-substitution machine (Leica Microsystems AFS2) and freeze-substituted as follows:\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"1u2gt\",\"text\":\"     -90°C for 72 hrs.\",\"type\":\"ordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"58ktc\",\"text\":\"     Warm to -20°C over 24 hours.\",\"type\":\"ordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"84jdb\",\"text\":\"     Hold at -20°C for 12 hours\",\"type\":\"ordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"cough\",\"text\":\"     Warm to 4°C over 1 hour.\",\"type\":\"ordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":107774,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"7","cases":[],"critical":null},{"id":2232966,"guid":"16ECFF7BA05541F3A20FC692E4D26639","previous_id":2232965,"previous_guid":"BA5699032F9D4E0A9641524C499AC2CB","section":"\u003cp\u003eSamples preparation\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"7eri3\",\"text\":\"     \\nSamples, still within planchettes, were rinsed 3x with cold (4°C) acetone and brought to room temperature.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":107774,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"8","cases":[],"critical":null},{"id":2232967,"guid":"04D27117545C445AAB69CF072C72726A","previous_id":2232966,"previous_guid":"16ECFF7BA05541F3A20FC692E4D26639","section":"\u003cp\u003eSamples preparation\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"ermdg\",\"text\":\"     Samples were infiltrated into Epon-Araldite resin (EMS) as follows:\",\"type\":\"ordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"a06l8\",\"text\":\"     2:1 (Acetone:Resin) for 1 hour\",\"type\":\"ordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"35t1i\",\"text\":\"     1:1 for 1 hour\",\"type\":\"ordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"8gbah\",\"text\":\"     1:2 for 1 hour\",\"type\":\"ordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":107774,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"9","cases":[],"critical":null},{"id":2232968,"guid":"2CF8A8738CBB4B189DD17AA06E666A41","previous_id":2232967,"previous_guid":"04D27117545C445AAB69CF072C72726A","section":"\u003cp\u003eSamples preparation\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"o5s3\",\"text\":\"     \\nSamples transferred into 100% resin and removed from the planchettes.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":107774,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"10","cases":[],"critical":null},{"id":2232969,"guid":"2F2C75F536A94C37AA3BF8AA7E37E3C3","previous_id":2232968,"previous_guid":"2CF8A8738CBB4B189DD17AA06E666A41","section":"\u003cp\u003eSamples preparation\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"2ko7o\",\"text\":\"  Samples were allowed to infiltrate into resin for 24 hours, with gentle agitation.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":107774,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"11","cases":[],"critical":null},{"id":2232970,"guid":"82A5EA011D424F61AED970C7F36A2398","previous_id":2232969,"previous_guid":"2F2C75F536A94C37AA3BF8AA7E37E3C3","section":"\u003cp\u003eSamples preparation\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"9l33s\",\"text\":\"     \\nBrain tissue samples were placed into resin containing accelerator (DMP30) and flat-embedded between two Teflon-coated glass microscope slides.  Resin was polymerized for 24-48 hrs at 60°C.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":107774,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"12","cases":[],"critical":null},{"id":2232971,"guid":"288D9C9EBEBE44F58B6315EDCC09B495","previous_id":2232970,"previous_guid":"82A5EA011D424F61AED970C7F36A2398","section":"\u003cp\u003eSamples preparation\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"7907n\",\"text\":\"     \\nEmbedded brain tissue blocks were examined with a dissecting microscope to select optimum regions.  These were excised with scalpel and reglued onto plastic sectioning stubs.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":107774,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"13","cases":[],"critical":null},{"id":2232972,"guid":"48AC02D36CB7461D90AF8C2011842DA7","previous_id":2232971,"previous_guid":"288D9C9EBEBE44F58B6315EDCC09B495","section":"\u003cp\u003eSamples preparation\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"2chsi\",\"text\":\"     \\nSemi-thin (170 nm) sections were cut with a ultramicrotome (Leica Microsytems UC6) using a diamond knife (Diatome, Ltd.).  Sections were collected onto formvar-coated copper-rhodium 1mm slot grids (EMS) and stained with 3% uranyl acetate and lead citrate.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":107774,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"14","cases":[],"critical":null},{"id":2232977,"guid":"EFBE4601996F4308BD7745C0AE4C5038","previous_id":2232972,"previous_guid":"48AC02D36CB7461D90AF8C2011842DA7","section":"\u003cp\u003eImaging\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"b54ji\",\"text\":\"Grids were imaged with a transmission electron microscope (Thermo-Fisher Tecnai T12-G2, 120k eV) equipped with a Gatan US1000 2k x 2k CMOS camera.  Large-area montaged images of brain regions were collected automatically using the SerialEM software package.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":107774,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"15","cases":[],"critical":null},{"id":2232982,"guid":"FC50FC3BDD58445DA334BBA4CA8F4EF1","previous_id":2232977,"previous_guid":"EFBE4601996F4308BD7745C0AE4C5038","section":"\u003cp\u003eImaging\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"86ogn\",\"text\":\"     \\nImages were subsequently aligned and analyzed using the IMOD software package and AIVIA image analysis software (version 10.5.0; Leica Microsystems) with pixel classifier machine learning software to identify mitochondria.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":107774,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"16","cases":[],"critical":null}],"template_id":0,"title":"Electron Microscopy of Brain Tissue Samples","title_html":"\u003cp\u003eElectron Microscopy of Brain Tissue Samples\u003c/p\u003e","type_id":1,"units":[{"id":1,"type_id":3,"name":"µ