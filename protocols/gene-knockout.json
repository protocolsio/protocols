duration":0},{"id":1061395,"guid":"52FE94C0175F11EBA9A97364D7D98425","previous_id":1061394,"previous_guid":"4C35FA70175F11EBA9A97364D7D98425","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"5DC56CD7E3F24EB1BF12071DFDE851EF","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"BE6E070254274194A03DCD585BF23CC6","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Collect cells at 4200 rpm for 10 min in a 4\u2103 refrigerated centrifuge.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1061396,"guid":"590083B0175F11EBA9A97364D7D98425","previous_id":1061395,"previous_guid":"52FE94C0175F11EBA9A97364D7D98425","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"C472CA4B8A1749B89DCA558BECC96DC0","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"808B77D5E448437DA8CC2C943BA7CFDA","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Resuspend the cells gentlely with 10 ml of pre-cooled and sterilized 10% glycerol on ice.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1061397,"guid":"64080030175F11EBA9A97364D7D98425","previous_id":1061396,"previous_guid":"590083B0175F11EBA9A97364D7D98425","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"01AA341E187A406F81B08AA80545A67E","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"048DF09AEF7545039EC6A903ABCA4405","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Repeat steps 2.3 and 2.4 three times, using 10 mL of pre-cooled and sterilized 10% glycerol for the first two washes. The last time it was resuspended with 0.5 mL of glycerol.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1061398,"guid":"6AA85200175F11EBA9A97364D7D98425","previous_id":1061397,"previous_guid":"64080030175F11EBA9A97364D7D98425","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"722C8C0B5D86406E8DA17B14DDEFD2A9","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"ADD3D8DD37E443B6B4484CFF86B38B72","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Dispense in 100 \u03bcL\/tube into a pre-cooled centrifuge tube, it can be used for electroporation or immediately placed in a -80\u2103 refrigerator. The bacteria can be stored at -80\u2103 for half a year.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1061399,"guid":"73281230175F11EBA9A97364D7D98425","previous_id":1061398,"previous_guid":"6AA85200175F11EBA9A97364D7D98425","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"D98C314958AB4801B593AEB5F0D85061","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"B61D433ACEF74749A3D02F869EF10A2D","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span>Prepare target segment: Clone upstream and downstream sequences of <\/span><span style = \"font-style:italic;\">yhaK<\/span><span> by PCR to about 500 bp each, then connect upstream and downstream homology arms together by overlap.<\/span><\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1061400,"guid":"7744ED70175F11EBA9A97364D7D98425","previous_id":1061399,"previous_guid":"73281230175F11EBA9A97364D7D98425","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"E5601FF822B84705B220C663853C22DD","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"4EF7A422090A46739B679CEECED7A4C8","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">pTarget-yhaK vector construction (Str resistance): Download the yhaK gene sequence, Find the appropriate gRNA affinity site and replace 1961 to 1980 bases of the pTarget plasmid by digestion and ligation or overlap.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1061401,"guid":"7B9275A0175F11EBA9A97364D7D98425","previous_id":1061400,"previous_guid":"7744ED70175F11EBA9A97364D7D98425","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"E19190AF8FD64BA5BA6405CAA6E4A78A","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"9956125F88084483B4A0BF9480917749","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Electroporation: <\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1061402,"guid":"7F4EE160175F11EBA9A97364D7D98425","previous_id":1061405,"previous_guid":"8F27E320175F11EBA9A97364D7D98425","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"011318A5162148AD9418FDCFEE41DA0F","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"C4619252B7BF437195397FFEA384324D","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span>Knockout verification: Primers were designed at 700~1000 bp upstream and downstream of the <\/span><span style = \"font-style:italic;\">yhaK<\/span><span> in the genome to perform colony PCR validation on clones on the plates.<\/span><\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1061403,"guid":"83DCA6E0175F11EBA9A97364D7D98425","previous_id":1061401,"previous_guid":"7B9275A0175F11EBA9A97364D7D98425","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"0A5BEBA2D6E54A51B35F51E3F007A803","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"4042FF5D0CA54931B5A5FBB51636DDD4","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Add >200ng target fragment and >40ng pTarget-yhaK plasmid to 100\u03bcL competent cells on ice for 10~30 min, the plasmid and the ligation product should not exceed 5\u03bcL;<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1061404,"guid":"89B514D0175F11EBA9A97364D7D98425","previous_id":1061403,"previous_guid":"83DCA6E0175F11EBA9A97364D7D98425","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"14BAA1CC2B76465ABBDA262F848116FC","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"F1AE4EB14E7942D4B27F2445B72D6E75","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">UV-sterilize the washed and dried 2 mm electroporation cuvette for 20 min and pre-cooled on ice, then quickly transfer the competent cells to the bottom of the electroporation cuvette; <\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1061405,"guid":"8F27E320175F11EBA9A97364D7D98425","previous_id":1061404,"previous_guid":"89B514D0175F11EBA9A97364D7D98425","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"FAA309B722EC475384EDE6B0C4086AEA","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"9359642058C54F7E87886581C678C1BA","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Dry the outer wall of the cup. Immediately after the electric shock, add 900~1000\u03bcL of 37 \u00b0C preheated LB medium, mix gently, transfer to a 1.5 mL centrifuge tube, shake at 30 \u00b0C, 150 rpm, 45 ~ 60 min, then apply an appropriate amount of bacterial solution to the plate containing Kana and Str.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1061406,"guid":"9974E940175F11EBA9A97364D7D98425","previous_id":1061402,"previous_guid":"7F4EE160175F11EBA9A97364D7D98425","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"C3A0D89FCAC34B23A23DF1EFC3D60951","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"E45927BF523948269613607C6A2DACA1","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">pTarget-yhaK elimination: Take the correct clone, add 1 mM IPTG at 30\u2103 for 12 h, streak to isolate the monoclone. The clones were tested for the presence of Str resistance, clones with pTarget eliminated is not resistant to str. The pCas9 plasmid can be eliminated by incubation at 37\u2103.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1061407,"guid":"9D4AA960175F11EBA9A97364D7D98425","previous_id":1061406,"pr