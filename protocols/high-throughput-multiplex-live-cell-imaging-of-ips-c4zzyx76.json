sion_id":341,"stats":{"is_voted":false,"number_of_views":12,"number_of_steps":4,"number_of_bookmarks":0,"number_of_comments":0,"number_of_bookmarked_comments":0,"number_of_steps_comments":0,"number_of_protocol_comments":0,"number_of_exports":0,"number_of_runs":0,"number_of_votes":0,"number_of_reagents":0,"number_of_equipments":0,"number_of_collections":0,"number_of_forks":{"private":0,"public":0},"number_of_accessible_forks":0},"status":{"id":1,"info":"We use this protocol and it's working"},"steps":[{"id":1857743,"guid":"FEBC08B35E264257BF483BBF32A26330","previous_id":0,"previous_guid":null,"section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"9k7cd\",\"text\":\"Live-cell multiplexed imaging was performed in 96-well plates (PerkinElmer). Cortical neurons were plated at a density of 20,000 - 40,000 cells/well after at least 50 days of neural induction. Neurons were then maintained until imaging (\\u003e70 days) with twice weekly feeding.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90905,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"1","cases":[],"critical":null},{"id":1857744,"guid":"51DEF1D6279540018809469D4DC868A2","previous_id":1857743,"previous_guid":"FEBC08B35E264257BF483BBF32A26330","section":"","section_color":null,"section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"40u5c\",\"text\":\"Cells were then washed with Hank’s balanced salt solution (HBSS; Invitrogen)\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90905,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"2","cases":[],"critical":null},{"id":1857745,"guid":"C3A0BECE4D574000BBD3108FE3A24058","previous_id":1857744,"previous_guid":"51DEF1D6279540018809469D4DC868A2","section":"","section_color":null,"section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"298rb\",\"text\":\"Cells were then incubated with 10 μM Hoechst (62249, ThermoFisher Scientific), 25 nM TMRM (T668, Thermo Fisher Scientific) and 250 nM LysoTracker Deep Red (L23492, Thermo Fisher Scientific); 500 nM SYTOX green (S7020, Thermo Fisher Scientific) in HBSS for 30 minutes at room temperature.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[{\"key\":0,\"offset\":156,\"length\":6}],\"data\":{}}],\"entityMap\":{\"0\":{\"type\":\"link\",\"mutability\":\"MUTABLE\",\"data\":{\"guid\":\"919216F0893111EE86870A58A9FEAC02\",\"url\":\"https://www.ncbi.nlm.nih.gov/nuccore/L23492\"}}}}","data":null,"protocol_id":90905,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"3","cases":[],"critical":null},{"id":1857747,"guid":"F51466B00E2A4F4BBCD474657EE27F3B","previous_id":1857745,"previous_guid":"C3A0BECE4D574000BBD3108FE3A24058","section":"","section_color":null,"section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"211oo\",\"text\":\"Live-cell images were acquired using an Opera Phenix High-Content Screening System (PerkinElmer). Hoechst was imaged using the 405 nm laser, SYTOX green labelling was imaged using the 488 nm laser, TMRM was imaged using the 516 nm laser, and LysoTracker was imaged using the 647 nm laser. Live-cell imaging was performed in HBSS. ~20 fields of view were taken per well with each field taken at 5 focal planes in the z-axis.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"italic\",\"offset\":416,\"length\":1}],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90905,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4","cases":[],"critical":null}],"template_id":4,"title":"High-Throughput Multiplex Live-Cell Imaging of iPSC-derived neurons","title_html":"High-Throughput Multiplex Live-Cell Imaging of iPSC-derived neurons","type_id":1,"units":[{"id":1,"t