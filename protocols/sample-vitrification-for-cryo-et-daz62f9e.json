ng"},"steps":[{"id":1986931,"guid":"733E942813F64C10800E8711B31D0A1B","previous_id":0,"previous_guid":null,"section":"\u003cp\u003evitrification for cells grown on the grids\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"1es59\",\"text\":\"Cells were seeded on holey carbon-coated 200 mesh gold EM grids (Quantifoil Micro Tools, Jena, Germany) in 35 mm cell culture dishes. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":97054,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"1","cases":[],"critical":null},{"id":1986932,"guid":"158AA3AF8DD34ABE849B9373A41BD934","previous_id":1986931,"previous_guid":"733E942813F64C10800E8711B31D0A1B","section":"\u003cp\u003evitrification for cells grown on the grids\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"cg6ac\",\"text\":\"Allow to properly attach before transfection with GFP-polyQ and RFP-reporters. Cells were incubated for 1 day before media change, and followed by another day of incubation before drug treatments.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":97054,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"2","cases":[],"critical":null},{"id":1986937,"guid":"C4425D39F20242C2AD896557C16E2C34","previous_id":1986932,"previous_guid":"158AA3AF8DD34ABE849B9373A41BD934","section":"\u003cp\u003evitrification for cells grown on the grids\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"8vjqo\",\"text\":\"Prior to vitrification, cells were applied with DMEM containing 10 % glycerol as a cryo-protectant and Dynabeads (Invitrogen) at 1:40 dilution for 3D CLEM workflow. Dynabeads maybe skipped of the fluorescent target is large, such as the polyQ-GFP aggregates which span several microns and are visible in SEM and FIB views for targeted milling.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[{\"key\":0,\"offset\":83,\"length\":15}],\"data\":{}}],\"entityMap\":{\"0\":{\"type\":\"link\",\"mutability\":\"MUTABLE\",\"data\":{\"guid\":\"4BC81749E77811EEBC660A58A9FEAC02\",\"url\":\"https://www.sciencedirect.com/topics/biochemistry-genetics-and-molecular-biology/cryoprotectant\"}}}}","data":null,"protocol_id":97054,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"3","cases":[],"critical":null},{"id":1986938,"guid":"7472FB99B2F4487680E130D14465A4DD","previous_id":1986937,"previous_guid":"C4425D39F20242C2AD896557C16E2C34","section":"\u003cp\u003evitrification for cells grown on the grids\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"fh84t\",\"text\":\"The grids were then mounted on Vitrobot Mark IV (Thermo), blotted from the back side using FEI Vitrobot PerforatedFilter Paper (Whatman) with force 10 for 15 seconds (this setting is specific to our vitrobot and may vary from place to place) at room temperature, and plunged into a 2:1 ethane:propane mixture cooled down to liquid nitrogen temperature.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":97054,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4","cases":[],"critical":null},{"id":1986942,"guid":"3FE0E60EA7814D2A9B1B0108DD42DA19","previous_id":1986938,"previous_guid":"7472FB99B2F4487680E130D14465A4DD","section":"\u003cp\u003evitrification for cells grown on the grids\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"6pcb8\",\"text\":\"Plunge-frozen grids were then clipped into Autogrid support frames modified with a cut-out (Thermo), stored in liquid nitrogen, and maintained at ≤−170°C for all steps.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":97054,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"5","cases":[],"critical":null},{"id":1986943,"guid":"F249F796C807467181B4E02C9830ADC5","previous_id":1986942,"previous_guid":"3FE0E60EA7814D2A9B1B0108DD42DA19","section":"\u003cp\u003evitrification for isolated vesicles containing polyQ-GFP and RFP-LC3B\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"2696d\",\"text\":\"The vesicles (~200ul collected from 1 of 6-well containing cells) were re-suspended in 1X PBS and passed through membrane filter (Corning) to remove large\\nclumps prior to vitrification. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":97054,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"6","cases":[],"critical":null},{"id":1986996,"guid":"6C403B7C99E44E09878CF919458FA82F","previous_id":1986943,"previous_guid":"F249F796C807467181B4E02C9830ADC5","section":"\u003cp\u003evitrification for isolated vesicles containing polyQ-GFP and RFP-LC3B\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"9uhp3\",\"text\":\"4 ml of the sample was applied to glow discharged holey carbon-coated 200 mesh copper EM grids (Quantifoil Micro Tools), vitrified as above at 4 °C and 100 % humidity.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":97054,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"7","cases":[],"critical":null}],"template_id":0,"title":"Sample vitrification for cryo-ET","title_html":"Sample vitrification for cryo-ET","type_id":1,"units":[{"id":1,"t