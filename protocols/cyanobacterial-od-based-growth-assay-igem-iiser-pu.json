:[],"verified":0,"is_verified_user":false,"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false},"shared":false}}],"is_owner":1,"has_subprotocols":0,"is_subprotocol":0,"is_bookmarked":0,"can_claim_authorship":0,"can_accept_authorship":0,"can_be_copied":1,"can_remove_fork":1,"fork_id":null,"url":"https:\/\/www.protocols.io\/view\/cyanobacterial-od-based-growth-assay-igem-iiser-pu-bw8pphvn","forks_count":{"private":0,"public":0},"access":{"can_view":1,"can_remove":1,"can_add":0,"can_edit":0,"can_publish":0,"can_get_doi":1,"can_share":1,"can_move":1,"can_move_outside":1,"can_transfer":1,"can_download":1,"is_locked":0},"guid":"3DC5FFE0F8FE11EBAF60C7B094634AF4","state_version_id":804,"steps":[{"id":1230469,"guid":"50202E00F99D11EBAF60C7B094634AF4","previous_id":null,"previous_guid":null,"modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"EA20527B1C144BCC8C28CD1D2304FAAE","order_id":1,"type_id":6,"title":"Section","source":{"title":"Sample Preparation"}},{"id":1054724,"guid":"7CA9503EFDC0477685F918E7DDA25CDF","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Start with a single colony of Synechococcus elongatus UTEX 2973 and inoculate it in <\/div><div class = \"text-block\"> of liquid BG-11 media containing <\/div><div class = \"text-block\"> of NaCl.<\/div><br\/><\/div>"}},{"id":1054725,"guid":"012F5E90F99F11EBAF60C7B094634AF4","order_id":2,"type_id":3,"title":"amount","source":{"amount":10,"unit":"mL","title":""}},{"id":1054726,"guid":"1F68CBD0F99F11EBAF60C7B094634AF4","order_id":3,"type_id":25,"title":"concentration","source":{"concentration":150,"unit":"Milimolar (mM)","title":""}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1230475,"guid":"6CF872B0F99F11EBAF60C7B094634AF4","previous_id":1230469,"previous_guid":"50202E00F99D11EBAF60C7B094634AF4","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"CA0C135097B7432BB417315E010CDCCD","order_id":1,"type_id":6,"title":"Section","source":{"title":"Sample Preparation"}},{"id":1054724,"guid":"72EDD8EBE27E40BF9F4E03EFD0CB0395","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Allow the colony to grow to a dark green color for 1-2 days in a shaker incubator at 150 rpm, at the temperature for which you want a growth curve.<\/div><br\/><div class = \"text-block\">We are using the Thermo Scientific MaxQ 6000 shaking incubator.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1230476,"guid":"89594F60F99F11EBAF60C7B094634AF4","previous_id":1230475,"previous_guid":"6CF872B0F99F11EBAF60C7B094634AF4","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"7C31BA47AB8C49BC9A66B36D64376868","order_id":1,"type_id":6,"title":"Section","source":{"title":"Sample Preparation"}},{"id":1054724,"guid":"8BD91880712F40C9928E49BE8BF8FAED","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Transfer the growing mixture into <\/div><div class = \"text-block\"> of liquid BG-11 media containing <\/div><div class = \"text-block\"> of NaCl.<\/div><br\/><\/div>"}},{"id":1054725,"guid":"A5242DA0F99F11EBAF60C7B094634AF4","order_id":2,"type_id":3,"title":"amount","source":{"amount":90,"unit":"mL","title":""}},{"id":1054726,"guid":"BC876C00F99F11EBAF60C7B094634AF4","order_id":3,"type_id":25,"title":"concentration","source":{"concentration":150,"unit":"Milimolar (mM)","title":""}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1230477,"guid":"D2422A80F99F11EBAF60C7B094634AF4","previous_id":1230476,"previous_guid":"89594F60F99F11EBAF60C7B094634AF4","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"4618CB224C3B495B9E95CD87882F582F","order_id":1,"type_id":6,"title":"Section","source":{"title":"Sample Preparation"}},{"id":1054724,"guid":"A8F3E59A4B3E449A9004ED9107B04C79","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Allow the <\/div><div class = \"text-block\"> the growing mixture to grow for 24 hours <\/div><br\/><\/div>"}},{"id":1054725,"guid":"DBEE35B0F99F11EBAF60C7B094634AF4","order_id":2,"type_id":3,"title":"amount","source":{"amount":100,"unit":"mL","title":""}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1230492,"guid":"0FED0FD0F9A011EBAF60C7B094634AF4","previous_id":1230477,"previous_guid":"D2422A80F99F11EBAF60C7B094634AF4","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"9A9516341F474B918E53FF529F2B2947","order_id":1,"type_id":6,"title":"Section","source":{"title":"Sample Preparation"}},{"id":1054724,"guid":"DB635D6BEB4A45629E460C1E7B473DF0","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Take <\/div><div class = \"text-block\"> of the culture and measure its optical density in a spectrophotometer at a wavelength of 730 nm, since it lies outside of the absorption spectrum of cyanobacterial pigments (Blankenship et al., 2011), after calibrating the spectrophotometer to the BG-11 media with 150 mM NaCl. <\/div><br\/><div class = \"text-block\">We are using the Eppendorf Biospectrometer Basic spectrophotometer.<\/div><br\/><\/div>"}},{"id":1054725,"guid":"1DECE4C0F9A011EBAF60C7B094634AF4","order_id":2,"type_id":3,"title":"amount","source":{"amount":1,"unit":"mL","title":""}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1230493,"guid":"A6B670F0F9A011EBAF60C7B094634AF4","previous_id":1230492,"previous_guid":"0FED0FD0F9A011EBAF60C7B094634AF4","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"B2130E82FB1948E2B51BC61C4AD7E9DA","order_id":1,"type_id":6,"title":"Section","source":{"title":"Sample Preparation"}},{"id":1054724,"guid":"01343EAF540C4455BC65020A0B081DC6","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">If the OD of the culture is of a high-density (OD= 0.8-1) proceed with the next step. If not, allow the culture to grow for some more time before taking an OD measurement again.<\/div><br\/><div class = \"text-block\">If the OD of this culture is >1, subculture <\/div><div class = \"text-block\">  of the culture into <\/div><div class = \"text-block\">  of the BG-11 media with <\/div><div class = \"text-block\">  NaCl and allow it to grow again before taking an OD measurement.<\/div><br\/><br\/><br\/><br\/><\/div>"}},{"id":1054725,"guid":"83BE14B0F9A311EBAF60C7B094634AF4","order_id":2,"type_id":3,"title":"amount","source":{"amount":10,"unit":"mL","title":""}},{"id":1054726,"guid":"8D91A510F9A311EBAF60C7B094634AF4","order_id":3,"type_id":3,"title":"amount","source":{"amount":90,"unit":"mL","title":""}},{"id":1054727,"guid":"9A5EED70F9A311EBAF60C7B094634AF4","order_id":4,"type_id":25,"title":"concentration","source":{"concentration":150,"unit":"Milimolar (mM)","title":""}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1230494,"guid":"DF3FDC40F9A011EBAF60C7B094634AF4","previous_id":1230493,"previous_guid":"A6B670F0F9A011EBAF60C7B094634AF4","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"4E036A84A296462FA33F19EACE27F8E2","order_id":1,"type_id":6,"title":"Section","source":{"title":"Sample Preparation"}},{"id":1054724,"guid":"8D1AA2CC2666435B973AD35276EBF6C2","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">After obtaining the OD from the previous step, use the formula O1.V1 = O2.V2 (where O is OD and V is the volume of the culture at that OD). O1 is the OD value at which you want to start measuring the growth and O2 is the OD value of the high-density culture (OD = 0.8 -1). V1 is the volume of culture that has the OD=O1. V2 is the volume of the high-density culture that needs to be diluted to V1 in order to obtain a culture with OD=O1.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1230495,"guid":"438AE8C0F9A111EBAF60C7B094634AF4","previous_id":1230494,"previous_guid":"DF3FDC40F9A011EBAF60C7B094634AF4","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"0E7D15628DC0431AA5864F389C11CBC7","order_id":1,"type_id":6,"title":"Section","source":{"title":"Sample Preparation"}},{"id":1054724,"guid":"515725C0A14943F190C8807651A08B19","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">For the purposes of this experiment, we want to start measuring growth from a <\/div><div class = \"text-block\"> culture of OD = 0.2. Let's say we measured the OD of the high-density culture to be 0.8. Thus, by the above formula; 0.2x100 = 0.8xV2<\/div><div class = \"text-block\">V2 = 25 ml. Thus we need to dilute 25 ml of the high-density culture to <\/div><div class = \"text-block\">  with the BG-11 media containing <\/div><div class = \"text-block\"> NaCl.<\/div><br\/><\/div>"}},{"id":1054725,"guid":"00C4FCA0F9A211EBAF60C7B094634AF4","order_id":2,"type_id":3,"title":"amount","source":{"amount":100,"unit":"mL","title":""}},{"id":1054726,"guid":"28889530F9A211EBAF60C7B094634AF4","order_id":3,"type_id":3,"title":"amount","source":{"amount":100,"unit":"mL","title":""}},{"id":1054727,"guid":"6A92B5F0F9A211EBAF60C7B094634AF4","order_id":4,"type_id":25,"title":"concentration","source":{"concentration":150,"unit":"Milimolar (mM)","title":""}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1230496,"guid":"37F40810F9A211EBAF60C7B094634AF4","previous_id":1230495,"previous_guid":"438AE8C0F9A111EBAF60C7B094634AF4","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"03DAE1BE2E6E40678E457BAE4F572EC7","order_id":1,"type_id":6,"title":"Section","source":{"title":"Making Measurements"}},{"id":1054724,"guid":"A541A8910E8E4C7C81F8102571D08376","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Once the <\/div><div class = \"text-block\"> culture of OD=0.2 is obtained, we can start making measurements. Take <\/div><div class = \"text-block\"> of this culture and record the absorbance value in the spectrophotometer. <\/div><br\/><\/div>"}},{"id":1054725,"guid":"4E2AD460F9A211EBAF60C7B094634AF4","order_id":2,"type_id":3,"title":"amount","source":{"amount":100,"unit":"mL","title":""}},{"id":1054726,"guid":"903FF1A0F9A211EBAF60C7B094634AF4","order_id":3,"type_id":3,"title":"amount","source":{"amount":1,"unit":"mL","title":""}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1230497,"guid":"7F9A1810F9A411EBAF60C7B094634AF4","previous_id":1230496,"previous_guid":"37F40810F9A211EBAF60C7B094634AF4","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"234DCDDB8D374EEAA0BA1F00483E5FDE","order_id":1,"type_id":6,"title":"Section","source":{"title":"Making Measurements"}},{"id":1054724,"guid":"6D6C16966F794478AE9D3C2058699E59","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Take <\/div><div class = \"text-block\"> of the culture and make absorbance measurements every 3 hours till an OD=1 is obtained.<\/div><br\/><\/div>"}},{"id":1054725,"guid":"838498B0F9A411EBAF60C7B094634AF4","order_id":2,"type_id":3,"title":"amount","source":{"amount":1,"unit":"mL","title":""}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0}],"document":"","materials":[],"description":"<div class = \"text-blocks\"><div class = \"text-block\">This protocol can be used to measure the optical density (OD) based growth of Cyanobacteria to obtain a growth curve.<\/div><\/div>","changed_on":1629216592}