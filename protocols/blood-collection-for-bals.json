s = \"text-block\">Label 1.5 mL microcentrifuge tubes with corresponding animal IDs.<\/div><\/div>"}},{"id":1054725,"guid":"E219D5B28AA44CAF81323A8B903BA7F2","order_id":2,"type_id":26,"title":"notes","source":{"id":0,"parent_id":0,"uri":"","title":"","body":"<div class = \"text-blocks\"><div class = \"text-block\">Animals must be bled within ~30 minutes of passive vapor shut off to prevent lowering of BALs before blood collection.<\/div><\/div>","created_on":0,"changed_on":0,"creator":{"name":" ","affiliation":null,"affiliations":[],"username":"","link":null,"image":{"source":"","placeholder":""},"badges":[],"verified":0,"is_verified_user":false,"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false},"comments":[]}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":[],"critical_id":null,"duration":0},{"id":914879,"guid":"BC302C307D2F11EA8F3CC1621144AA4C","previous_id":914878,"previous_guid":"7EBE31307D2F11EA8F3CC1621144AA4C","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"54E976DB725540BA8BA8FB5CC9811DCB","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"39244D5F6E1040098124E7F2E2F47DED","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Open microcentrifuge tube and place in a tube rack that can hold all samples.<\/div><\/div>"}},{"id":1054725,"guid":"489BBA45140A47299651C43E5EF5A3F3","order_id":2,"type_id":26,"title":"notes","source":{"id":0,"parent_id":0,"uri":"","title":"","body":"<div class = \"text-blocks\"><div class = \"text-block\">** The samples will be dropped off at TSRI in this tube rack. Label tube rack on tape with your name, \"George,\" date, and number of samples. **<\/div><\/div>","created_on":0,"changed_on":0,"creator":{"name":" ","affiliation":null,"affiliations":[],"username":"","link":null,"image":{"source":"","placeholder":""},"badges":[],"verified":0,"is_verified_user":false,"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false},"comments":[]}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":[],"critical_id":null,"duration":0},{"id":914880,"guid":"DEE986D07D3011EA8F3CC1621144AA4C","previous_id":914879,"previous_guid":"BC302C307D2F11EA8F3CC1621144AA4C","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"6802B06A152948B3BE44E5172322B6D7","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"1821275866FB4F9CBDE9078B96C09E49","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Pick up the animal and firmly hold its body under your armpit to restrain the animal, leaving its tail exposed.<\/div><\/div>"}},{"id":1054725,"guid":"7490373D3FA44A2DBF9F33C941A9CD2A","order_id":2,"type_id":26,"title":"notes","source":{"id":0,"parent_id":0,"uri":"","title":"","body":"<div class = \"text-blocks\"><div class = \"text-block\">Animal can also be restrained by wrapping the body in a towel or disposable underpad.<\/div><\/div>","created_on":0,"changed_on":0,"creator":{"name":" ","affiliation":null,"affiliations":[],"username":"","link":null,"image":{"source":"","placeholder":""},"badges":[],"verified":0,"is_verified_user":false,"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false},"comments":[]}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":[],"critical_id":null,"duration":0},{"id":914881,"guid":"7E7110B07D3111EA8F3CC1621144AA4C","previous_id":914880,"previous_guid":"DEE986D07D3011EA8F3CC1621144AA4C","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"083799A704124523A6FD83A4F8EE71CD","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"CA0CAC94DCDA45B7BE61F874CC7DD35C","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Place the distal tail on a flat surface and use an 18g needle to perpendicularly pierce ~1-2 cm from the tail tip all the way through the tail.<\/div><\/div>"}},{"id":1054725,"guid":"463AFB19BD0A42FDAA480CD832A6E649","order_id":2,"type_id":26,"title":"notes","source":{"id":0,"parent_id":0,"uri":"","title":"","body":"<div class = \"text-blocks\"><div class = \"text-block\">Use a new needle every few animals to maintain a clean puncture.<\/div><\/div>","created_on":0,"changed_on":0,"creator":{"name":" ","affiliation":null,"affiliations":[],"username":"","link":null,"image":{"source":"","placeholder":""},"badges":[],"verified":0,"is_verified_user":false,"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false},"comments":[]}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":[],"critical_id":null,"duration":0},{"id":914883,"guid":"25A4D8F07D4411EA8F3CC1621144AA4C","previous_id":914881,"previous_guid":"7E7110B07D3111EA8F3CC1621144AA4C","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"2BDFDD635CDC4FBE892BCD1E70F1BF05","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"63D1741875414C4ABDFCCD9AA43B482E","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Immediately after puncturing, draw blood from the tail by firmly dragging fingers from the base of the tail to the end and repeat until ~100 ul of blood is collected.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":[],"critical_id":null,"duration":0},{"id":914884,"guid":"4E0BF2907D4611EA8F3CC1621144AA4C","previous_id":914883,"previous_guid":"25A4D8F07D4411EA8F3CC1621144AA4C","modifie