,"protocol_id":0,"components":[{"id":1054723,"guid":"3D62DAAB146941848D1F81C6316D13B8","order_id":1,"type_id":6,"title":"Section","source":{"title":"Setting up the centrifugation"}},{"id":1054724,"guid":"1F3500BA6C7F443C81A7C8485C213339","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Place 80 mL of FBS and one new 500 mL bottle of media in the 37 \u02daC water bath and allow FBS to thaw.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":979332,"guid":"E73B0C90C52511EA842CE937DE8E2115","previous_id":null,"previous_guid":null,"modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"EC3C23D8D55D48BBBBDD9998E9F15524","order_id":1,"type_id":6,"title":"Section","source":{"title":"Setting up the centrifugation"}},{"id":1054724,"guid":"08C03AFD2BAA4DA09C86CCF6022CECC0","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Turn on and log into the ultracentrifuge using the account and password specific to the device.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":979333,"guid":"F3F34650C52511EA842CE937DE8E2115","previous_id":979332,"previous_guid":"E73B0C90C52511EA842CE937DE8E2115","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"78E6491920E94F55AC16E6EBD1485FD0","order_id":1,"type_id":6,"title":"Section","source":{"title":"Setting up the centrifugation"}},{"id":1054724,"guid":"123D80461BF54E0D8DBB2CE4FDFA1BC5","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Write your information on any sign-up sheets associated with the device.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":979335,"guid":"108433B0C52611EA842CE937DE8E2115","previous_id":979331,"previous_guid":"BE61E550C52511EA842CE937DE8E2115","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"A4F12969325C4335A4084DB2F72423CE","order_id":1,"type_id":6,"title":"Section","source":{"title":"Setting up the centrifugation"}},{"id":1054724,"guid":"68F6244A0D064C958232FD1AB38CF2B0","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">While the FBS is thawing inspect the refrigerated rotor to ensure there are no cracks or other structural issues and that all o-ring seals are intact and greased with vacuum grease.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":979336,"guid":"30430870C52611EA842CE937DE8E2115","previous_id":979335,"previous_guid":"108433B0C52611EA842CE937DE8E2115","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"88AFF7CE30C94240A8A126AC19B02E07","order_id":1,"type_id":6,"title":"Section","source":{"title":"Setting up the centrifugation"}},{"id":1054724,"guid":"27B5AB23FE0846E5BBAA80676D5F4415","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Obtain a set of six sterilized polycarbonate ultracentrifuge tubes with rubbers stoppers and red aluminum caps. Inspect all the tubes to ensure they look clean and there are no cracks. Generally the tubes will crack after 10 uses, due to stress induced by autoclaving.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":979337,"guid":"BB8FBD10C52611EA842CE937DE8E2115","previous_id":979336,"previous_guid":"30430870C52611EA842CE937DE8E2115","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"36AF778FE4E446829B060E1B1C11CCB3","order_id":1,"type_id":6,"title":"Section","source":{"title":"Setting up the centrifugation"}},{"id":1054724,"guid":"38382C52941B48FC8578817CF4921EBD","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Sterilize then transport the thawed FBS, media, and a ultracentrifuge tube rack into a sterile hood.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":979338,"guid":"DA350B80C52611EA842CE937DE8E2115","previous_id":979337,"previous_guid":"BB8FBD10C52611EA842CE937DE8E2115","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"98593F91925D439C8F8CE2EE3823463C","order_id":1,"type_id":6,"title":"Section","source":{"title":"Setting up the centrifugation"}},{"id":1054724,"guid":"CF771E1938374C63A4389683ADBBFAF5","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Pipette 52 mL of media into each tube, avoiding all bubbles and drips on the outside of the tubes (any should be quickly wiped up.)<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":979339,"guid":"EE893520C52611EA842CE937DE8E2115","previous_id":979338,"previous_guid":"DA350B80C52611EA842CE937DE8E2115","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"93D1C6ECD3FB47A7B9146B9D18117E53","order_id":1,"type_id":6,"title":"Section","source":{"title":"Setting up the centrifugation"}},{"id":1054724,"guid":"A34300524FDE4DBC8F4B0824921CB2D3","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Pipette 13 mL of FBS into each tube, avoiding bubbles and drips on the outside of the tubes.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":979340,"guid":"043C7990C52711EA842CE937DE8E2115","previous_id":979339,"previous_guid":"EE893520C52611EA842CE937DE8E2115","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"CCDD5FF2040F4B7A9793E00356AA59C2","order_id":1,"type_id":6,"title":"Section","source":{"title":"Setting up the centrifugation"}},{"id":1054724,"guid":"7A07B5A1623745649B85DD94122E7390","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Carefully inspect each tube to ensure the exact same volume is in each tube as even small differences can lead to severe rotor or centrifuge damage.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":979341,"guid":"15663E40C52711EA842CE937DE8E2115","previous_id":979340,"previous_guid":"043C7990C52711EA842CE937DE8E2115","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"4612BA5CCF65451BBD71384868D61762","order_id":1,"type_id":6,"title":"Section","source":{"title":"Setting up the centrifugation"}},{"id":1054724,"guid":"A453282BF1D6444DA977F83A0CD7ED57","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Fasten the stoppers and caps onto the tubes very tightly.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":979342,"guid":"245AFA80C52711EA842CE937DE8E2115","previous_id":979341,"previous_guid":"15663E40C52711EA842CE937DE8E2115","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"540C8CC2ACC7424D9FED84B5DE936834","order_id":1,"type_id":6,"title":"Section","source":{"title":"Setting up the centrifugation"}},{"id":1054724,"guid":"746A5480395D4BDB84DD09649116D7B1","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Insert the tubes in the rotor and secure the rotor cap tightly, using a metal rod to tighten the top if necessary.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":979343,"guid":"2E01D590C52711EA842CE937DE8E2115","previous_id":979342,"previous_guid":"245AFA80C52711EA842CE937DE8E2115","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"9E4EEEC54D4F409A80B162EF91ED2353","order_id":1,"type_id":6,"title":"Section","source":{"title":"Setting up the centrifugation"}},{"id":1054724,"guid":"EBE0305C91854B1AB97151E40D8E549F","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Transport the filled rotor to the centrifuge.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":979344,"guid":"3934A7D0C52711EA842CE937DE8E2115","previous_id":979343,"previous_guid":"2E01D590C52711EA842CE937DE8E2115","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"D5993C7CA07B4580834E0AC1F6488132","order_id":1,"type_id":6,"title":"Section","source":{"title":"Setting up the centrifugation"}},{"id":1054724,"guid":"D64746457CC24024BAD510A2AD3D4612","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">The vacuum button may need to be pressed to release pressure before the door of the centrifuge can be oppened.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":979345,"guid":"4CF12550C52711EA842CE937DE8E2115","previous_id":979344,"previous_guid":"3934A7D0C52711EA842CE937DE8E2115","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"696D72AA0E914DDDA02D84B5C7B49132","order_id":1,"type_id":6,"title":"Section","source":{"title":"Setting up the centrifugation"}},{"id":1054724,"guid":"DE9CFD9B69CB4CE3B35FF3A32F7A0809","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Insert the filled rotor into the ultracentrifuge.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":979346,"guid":"5D2E8E30C52711EA842CE937DE8E2115","previous_id":979345,"previous_guid":"4CF12550C52711EA842CE937DE8E2115","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"E0A08F28F092479082EDB806A4D678E2","order_id":1,"type_id":6,"title":"Section","source":{"title":"Setting up the centrifugation"}},{"id":1054724,"guid":"8E0A6930E10C4DF4AB3F2B3D258AF7AB","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Select the rotor with its specific serial number (etched onto the rotor bottom) in the rotor selection menu.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":979348,"guid":"72560D60C52711EA842CE937DE8E2115","previous_id":979346,"previous_guid":"5D2E8E30C52711EA842CE937DE8E2115","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"1B0F6BB42C74400591110A3B06C2032F","order_id":1,"type_id":6,"title":"Section","source":{"title":"Setting up the centrifugation"}},{"id":1054724,"guid":"45E073F765BF4AEBA45FA60AD32357C6","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Create a run at 100,000 x g for 18 hours at 4 \u02daC and press vacuum.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":979351,"guid":"A734C030C52711EA842CE937DE8E2115","previous_id":979352,"previous_guid":"C874B6B0C52711EA842CE937DE8E2115","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"F1292EF117C4400D8254A875B1FA067C","order_id":1,"type_id":6,"title":"Section","source":{"title":"Setting up the centrifugation"}},{"id":1054724,"guid":"5784407F83F44A54A470F968F7FDF442","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Stay with the ultracentrifuge until it reaches its final speed and stop the centrifuge if you have even a slight concern about an unusual sound or movement indicating an imbalance. The ultracentrifuge will hum as it reaches speed but not rattle.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":979352,"guid":"C874B6B0C52711EA842CE937DE8E2115","previous_id":979348,"previous_guid":"72560D60C52711EA842CE937DE8E2115","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"F9ECB1A568CC4B6E92E2ECEFF2457921","order_id":1,"type_id":6,"title":"Section","source":{"title":"Setting up the centrifugation"}},{"id":1054724,"guid":"5B5CC89ADEA34473853648A51B1FA236","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">After the device has sufficiently held a vaccum, press the green Start or Go button.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":979354,"guid":"DBFE3D50C52711EA842CE937DE8E2115","previous_id":979351,"previous_guid":"A734C030C52711EA842CE937DE8E2115","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"2E598D37ABCB46AEB1E87713675E970F","order_id":1,"type_id":6,"title":"Section","source":{"title":"Harvesting the media"}},{"id":1054724,"guid":"499498070C964F97A42475B78FD82F72","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Obtain a new bottle of media which can be cold unless you would like to use the EV-depleted media immediatley.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":979355,"guid":"069715F0C52811EA842CE937DE8E2115","previous_id":979354,"previous_guid":"DBFE3D50C52711EA842CE937DE8E2115","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"72EB8828947048C0B0C5DE4ED4EC1B7F","order_id":1,"type_id":6,"title":"Section","source":{"title":"Harvesting the media"}},{"id":1054724,"guid":"01D9BDCFAF904D379703523CF87DEF33","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">The moment that the rotor is done spinning, carefully remove the rotor and place it on the centrifuge or adjacent table. If more than 10 minutes elapses between when the rotor stops spinning and you are able to harvest the media you should consider re-centrifuging it as the supernatant may not be depleted of particles any longer.<\/div><div class = \"text-block\">Though the EV pellet is not visible you should handle the rotor and tubes with the understanding that there is a fragile pellet of EVs at the bottom of each of the tubes and any significant movement or josteling can result in part of the pellet becoming resuspended in the supernatant before it is harvested.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":979356,"guid":"2D7111D0C52811EA842CE937DE8E2115","previous_id":979355,"previous_guid":"069715F0C52811EA842CE937DE8E2115","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"C56782F2429746C88E418CAB6B76986C","order_id":1,"type_id":6,"title":"Section","source":{"title":"Harvesting the media"}},{"id":1054724,"guid":"D6A734664EFB486F9B463BC9F50DFDC3","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Use tweezers or other metal instrument to slowly and gently lift each ultracentrifuge tube from its chamber and place onto the metal rack, minimizing movement and bumping.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":979357,"guid":"3CA12960C52811EA842CE937DE8E2115","previous_id":979356,"previous_guid":"2D7111D0C52811EA842CE937DE8E2115","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"27EBC861960B4ECC96B8ED916CCD8325","order_id":1,"type_id":6,"title":"Section","source":{"title":"Harvesting the media"}},{"id":1054724,"guid":"E8221F05130745C797C66924E2D9453C","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Inspect all tubes and tube slots to ensure none were cracked and had their sterility compromised.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":979358,"guid":"49A84670C52811EA842CE937DE8E2115","previous_id":979357,"previous_guid":"3CA12960C52811EA842CE937DE8E2115","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"AA19D2DCA8C54F12A07D4A3F568CB42E","order_id":1,"type_id":6,"