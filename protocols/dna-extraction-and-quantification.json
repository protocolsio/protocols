ritical":null,"critical_id":null,"duration":0},{"id":1096199,"guid":"E6EA129043BA11EBBBF4B531B85ED195","previous_id":1096198,"previous_guid":"E6E7055043BA11EBBBF4B531B85ED195","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"03D30E40EADB47FF86929984A4B280F8","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"A8206299865747F8A2F995E2CB0461A3","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Add 1 \u00b5L of lysozyme (100mg\/ml) shortly v\u00f3rtex, and incubate for 1 h at 37 \u00b0C.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1096200,"guid":"E6ECAAA043BA11EBBBF4B531B85ED195","previous_id":1096199,"previous_guid":"E6EA129043BA11EBBBF4B531B85ED195","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"CB04C5FB3AF34B58BAEE62257014CC21","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"FA132DF7BE814AE99C6D77263AC80F0B","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Add 67,5 \u00b5L of SDS 10 % (pre-heated at 65 \u00b0C) and 2,5 \u00b5L of Proteinase K (10 mg\/mL) and shortly v\u00f3rtex, and incubate at 65 \u00b0C for 10 min.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1096201,"guid":"E6F07B3043BA11EBBBF4B531B85ED195","previous_id":1096200,"previous_guid":"E6ECAAA043BA11EBBBF4B531B85ED195","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"31870C1495794EDBBBFF59F64E310E55","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"FDAFEDFDA0314150803859C72814FBBA","order_id":1,"type_id":1,"title":"description","source":{"descr