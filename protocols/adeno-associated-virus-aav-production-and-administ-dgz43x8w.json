guid":null,"section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"bh070\",\"text\":\"**Transfection of HEK293T Cells**\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":103196,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"1","cases":[],"critical":null},{"id":2134026,"guid":"0E54A0456FA143B994FE8C1CB5B6C55A","previous_id":2134025,"previous_guid":"1CB6E8ACDB604F15B703AC846FC24FE0","section":"","section_color":"","section_duration":0,"is_substep":true,"step":"{\"blocks\":[{\"key\":\"apo1k\",\"text\":\"- Transfect HEK293T cells with the following plasmids: pAd-DELTA F6, serotype plasmid AAV PHP.eB, and AAV plasmid (pZac2.1-GfaABC1D-Ezrin-BioID2-HA or pZac2.1-GfaABC1D-Ezrin T567A-BioID2-HA).\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":103196,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"1.1","cases":[],"critical":null},{"id":2134027,"guid":"18B0EF7824814608999A09F04D737770","previous_id":2134026,"previous_guid":"0E54A0456FA143B994FE8C1CB5B6C55A","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"eulvg\",\"text\":\"**Cell Lysis and AAV Purification**\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":103196,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"2","cases":[],"critical":null},{"id":2134028,"guid":"8CAE79610A9949618B9FA274778DBD63","previous_id":2134027,"previous_guid":"18B0EF7824814608999A09F04D737770","section":"","section_color":"","section_duration":0,"is_substep":true,"step":"{\"blocks\":[{\"key\":\"fmgkg\",\"text\":\"- Three days after transfection, collect cells in a lysis buffer (15 mM NaCl, 5 mM Tris-HCl, pH 8.5).\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":103196,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"2.1","cases":[],"critical":null},{"id":2134029,"guid":"7916790A83CF417FB02F829667BEDE02","previous_id":2134028,"previous_guid":"8CAE79610A9949618B9FA274778DBD63","section":"","section_color":"","section_duration":0,"is_substep":true,"step":"{\"blocks\":[{\"key\":\"csorh\",\"text\":\"- Perform repeat freeze-thaw cycles followed by treatment with Benzonase at 37°C for 30 minutes.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":103196,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"2.2","cases":[],"critical":null},{"id":2134030,"guid":"47D02491756A4783B6FA40CD608D0062","previous_id":2134029,"previous_guid":"7916790A83CF417FB02F829667BEDE02","section":"","section_color":"","section_duration":0,"is_substep":true,"step":"{\"blocks\":[{\"key\":\"dh9bl\",\"text\":\"- Centrifuge lysed cells to pellet debris, and collect the supernatant containing AAVs.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":103196,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"2.3","cases":[],"critical":null},{"id":2134031,"guid":"A78A33B911C74F82A51EF688AAB57640","previous_id":2134030,"previous_guid":"47D02491756A4783B6FA40CD608D0062","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"a50nj\",\"text\":\"**Optiprep Density Gradient Centrifugation**\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":103196,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"3","cases":[],"critical":null},{"id":2134032,"guid":"C9D87627C127475E8760E889142828D1","previous_id":2134031,"previous_guid":"A78A33B911C74F82A51EF688AAB57640","section":"","section_color":"","section_duration":0,"is_substep":true,"step":"{\"blocks\":[{\"key\":\"4qqn1\",\"text\":\"- Apply the supernatant containing AAVs to an Optiprep density gradient (15%, 25%, 40%, and 60% iodixanol).\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":103196,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"3.1","cases":[],"critical":null},{"id":2134033,"guid":"88FDDC14DFA64B7CA9CC01171DC71727","previous_id":2134032,"previous_guid":"C9D87627C127475E8760E889142828D1","section":"","section_color":"","section_duration":0,"is_substep":true,"step":"{\"blocks\":[{\"key\":\"cc7v6\",\"text\":\"- Centrifuge at 67,000 rpm using a Beckman Ti-70 rotor for 1 hour.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":103196,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"3.2","cases":[],"critical":null},{"id":2134034,"guid":"33247A3AC1964B3D81AFD260DF6475A3","previous_id":2134033,"previous_guid":"88FDDC14DFA64B7CA9CC01171DC71727","section":"","section_color":"","section_duration":0,"is_substep":true,"step":"{\"blocks\":[{\"key\":\"eruae\",\"text\":\"- Collect the AAV-enriched fraction from between the 40% and 60% iodixanol layers.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":103196,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"3.3","cases":[],"critical":null},{"id":2134035,"guid":"5485400DCAE7432AB6B0543742F322AC","previous_id":2134034,"previous_guid":"33247A3AC1964B3D81AFD260DF6475A3","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"d9ln\",\"text\":\"**AAV Concentration**\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":103196,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4","cases":[],"critical":null},{"id":2134036,"guid":"78E7AA7E5E2F42638857D8322453A407","previous_id":2134035,"previous_guid":"5485400DCAE7432AB6B0543742F322AC","section":"","section_color":"","section_duration":0,"is_substep":true,"step":"{\"blocks\":[{\"key\":\"7j666\",\"text\":\"- Concentrate the AAV fraction by repeated washes with sterile PBS using an Amicon Ultra-15 filtration unit (100 kDa NMWL) to a final volume of approximately 100 μl.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":103196,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4.1","cases":[],"critical":null},{"id":2134037,"guid":"7B94112789664CED9354370A2A0C95F6","previous_id":2134036,"previous_guid":"78E7AA7E5E2F42638857D8322453A407","section":"","section_color":"","section_duration":0,"is_substep":true,"step":"{\"blocks\":[{\"key\":\"911ur\",\"text\":\"- Aliquot the concentrated AAVs for storage at −80°C.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":103196,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4.2","cases":[],"critical":null},{"id":2134038,"guid":"E264AED5CFA24E38A36DA54C2A540CAC","previous_id":2134037,"previous_guid":"7B94112789664CED9354370A2A0C95F6","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"4i4a0\",\"text\":\"**Mouse Anesthesia and Surgery**\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":103196,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"5","cases":[],"critical":null},{"id":2134039,"guid":"490D4D3E33D844F790A6B24FE98EDB52","previous_id":2134038,"previous_guid":"E264AED5CFA24E38A36DA54C2A540CAC","section":"","section_color":"","section_duration":0,"is_substep":true,"step":"{\"blocks\":[{\"key\":\"9uoa5\",\"text\":\"- Anesthetize 9-week-old WT or LRRK2 G2019Ski/ki mice using 1.5% isoflurane gas in a stereotaxic frame.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":103196,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"5.1","cases":[],"critical":null},{"id":2134040,"guid":"B87C08E292DD4DC6842063EB1B576473","previous_id":2134039,"previous_guid":"490D4D3E33D844F790A6B24FE98EDB52","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"97sj8\",\"text\":\"**Intravenous AAV Injection**\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":103196,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"6","cases":[],"critical":null},{"id":2134041,"guid":"9A8F0668652643C2A040481FFFE3485D","previous_id":2134040,"previous_guid":"B87C08E292DD4DC6842063EB1B576473","section":"","section_color":"","section_duration":0,"is_substep":true,"step":"{\"blocks\":[{\"key\":\"5aoi5\",\"text\":\"- Inject 10 μl of purified AAVs (titer of ~1 x 10^12 GC/ml) into the mouse brain intravenously by injection into the retro-orbital sinus.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":103196,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"6.1","cases":[],"critical":null},{"id":2134042,"guid":"008F299CB2F542E9B2F02DDB8568F8F2","previous_id":2134041,"previous_guid":"9A8F0668652643C2A040481FFFE3485D","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"aavs3\",\"text\":\"**Post-Injection Care and Tissue Preparation**\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":103196,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"7","cases":[],"critical":null},{"id":2134043,"guid":"E8DDDA05939B439680035BA131C2E99F","previous_id":2134042,"previous_guid":"008F299CB2F542E9B2F02DDB8568F8F2","section":"","section_color":"","section_duration":0,"is_substep":true,"step":"{\"blocks\":[{\"key\":\"2mbhp\",\"text\":\"- Allow mice to recover for 3 weeks until they reach 12 weeks of age.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":103196,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"7.1","cases":[],"critical":null},{"id":2134044,"guid":"88EF46F1349043B4915BBBA6BA4CA7F2","previous_id":2134043,"previous_guid":"E8DDDA05939B439680035BA131C2E99F","section":"","section_color":"","section_duration":0,"is_substep":true,"step":"{\"blocks\":[{\"key\":\"7i6u8\",\"text\":\"- Anesthetize mice with 200 mg/kg Tribromoethanol (Avertin) and perform transcardial perfusion with TBS/Heparin followed by 4% paraformaldehyde (PFA) at room temperature (RT).\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":103196,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"7.2","cases":[],"critical":null},{"id":2134045,"guid":"2886073DEFDB4F76ADCEEB3F4D045B47","previous_id":2134044,"previous_guid":"88EF46F1349043B4915BBBA6BA4CA7F2","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"bedn1\",\"text\":\"**Brain Processing**\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":103196,"case_id":0,"c