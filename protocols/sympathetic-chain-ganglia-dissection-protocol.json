itical":[],"critical_id":null,"duration":0},{"id":857334,"guid":"46D810001AB111EA9B931B003A461DF7","previous_id":857333,"previous_guid":"94D5D7701AB011EA9B931B003A461DF7","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"08C1AA382969485A8594636CE650EC6C","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"E13FB5C0AB274736ABE9EA0F1D624DFD","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span style = \"font-weight:bold;\">Perfusion<\/span><span> - mice will be perfused with cold, sterile saline to remove the blood from the animal tissues. <\/span><\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":[],"critical_id":null,"duration":0},{"id":857335,"guid":"681435F01AB111EA9B931B003A461DF7","previous_id":857334,"previous_guid":"46D810001AB111EA9B931B003A461DF7","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"D741CED28B7D42FC8F330BB5D3681B01","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"E1F4EA5B3B6240A58E88520D776F4448","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><div class = \"justify\" style = \"text-align:justify\">Prepare the 50 ml syringe for perfusion by filling up with at least 30 ml ice-cold sterile saline, and connecting it with the vaculet by attaching the tubing to the tip of the syringe. Once attached, depress the plunger on the syringe to expel any air bubbles in the tubing. You should clearly see a jet of saline shoot from the tip of the butterfly needle.<\/div><style>\n\t\t\t\t\t\t\t  .justify:after {\n\t\t\t\t\t\t\t    content: \"\";\n\t\t\t\t\t\t\t    display:inline-block;\n\t\t\t\t\t\t\t    width: 100%;\n\t\t\t\t\t\t\t  }\n\t\t\t\t\t\t\t<\/style><\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":[],"critical_id":null,"duration":0},{"id":857342,"guid":"E0EE52301AB111EA9B931B003A461DF7","previous_id":857335,"previous_guid":"681435F01AB111EA9B931B003A461DF7","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"EFBCD7DD16F14991BE1AA196CFAFA6C0","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"789C95560BA84501AC39227D44E2C4D6","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Spray the mouse belly with 70% ethanol.\nMake an incision using the medium scissors by pulling up part of the mouse\u2019s skin above the abdomen. Be careful to not cut too deep; only cut deep enough to open up the mouse\u2019s organ space (peritoneum, you can see the mouse\u2019s intact intestines if done properly).<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":[],"critical_id":null,"duration":0},{"id":857343,"guid":"E7235F101AB111EA9B931B003A461DF7","previous_id":857342,"previous_guid":"E0EE52301AB111EA9B931B003A461DF7","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"0B56CC6CF2EF4C3A85D82DEEF2CFA079","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"549DE88A3FEB47A6A27CC25504BC5ADA","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Continue to open the peritoneum by sliding the scissors along and cutting up the sides of the mouse until you reach and can clearly see its entire rib cage.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":[],"critical_id":null,"duration":0},{"id":857344,"guid":"EB2252101AB111EA9B931B003A461DF7","previous_id":857343,"previous_guid":"E7235F101AB111EA9B931B003A461DF7","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"BE8C52DE380E431199D27DC4C8B559E4","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"33F57C9043D24EDDBF53F6D1E613EF19","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Pierce the diaphragm using small scissors. Be careful not to poke\/pierce the heart as you do this!<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":[],"critical_id":null,"duration":0},{"id":857345,"guid":"EE5801A01AB111EA9B931B003A461DF7","previous_id":857344,"previous_guid":"EB2252101AB111EA9B931B003A461DF7","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"38182357466F47869053B97A9C3E8710","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"7BA62FB7F04B45669A7C399268199477","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Cut each side of the rib cage to completely open up the chest cavity and clamp the sternum with a mosquito forceps\/hemostat to retract it up and away from the heart. The heart should be completely exposed and clearly visible.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":[],"critical_id":null,"duration":0},{"id":857346,"guid":"F6580A801AB111EA9B931B003A461DF7","previous_id":857345,"previous_guid":"EE5801A01AB111EA9B931B003A461DF7","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"D98E904AE1004A8DACFADB8A686A8BF8","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"EC3615017359433CA32A459CD9A58B05","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">With the other mosquito forceps\/hemostat gently grasp the heart to position the tip of the left ventricle so that it just barely peeks out from the edge of the forceps\/hemostat. (It is important to not clamp too much of the heart as this will impede flow and reduce the effectiveness of the perfusion.)<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":[],"critical_id":null,"duration":0},{"id":857349,"guid":"20E60DB01AB211EA9B931B003A461DF7","previous_id":858220,"previous_guid":"178D4A001B5F11EAB4C17D5B4902E3CF","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"857B9B15AC5D4E0FB4D961B50B70813C","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"FF5CE2A119B944EE8A7D9EBA5BA8A5EF","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">With your free hand, insert the butterfly needle into the tip of the left ventricle. Insert needle as shallow as possible as to not stab through the heart and destroy the integrity of the left ventricle\/atrium. <\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":[],"critical_id":null,"duration":0},{"id":857350,"guid":"21297F501AB211EA9B931B003A461DF7","previous_id":857349,"previous_guid":"20E60DB01AB211EA9B931B003A461DF7","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"FCC6024EDA4F4197AA7102011DE608C0","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"8BAB628B6B4B46378C5C2A0DFD273D3F","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Slowly depress the plunger of the 50mL syringe to perfuse at least 30mL of ice-cold saline. Monitor the liver until it becomes completely clear to ensure proper perfusion. It should slowly change from deep red to a grey\/beige color.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":[],"critical_id":null,"duration":0},{"id":857351,"guid":"534393E01AB211EA9B931B003A461DF7","previous_id":857350,"previous_guid":"21297F501AB211EA9B931B003A461DF7","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"04BFBCA98F924AA4BE34AC7A9A0B264D","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"DDC5D1A670FB4998BCDFBC4CBABBF8DA","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Once you have witnessed the color change in the liver, remove the needle from the left ventricle. Remove the set of mosquito forceps\/hemostat clamped to the sternum.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":[],"critical_id":null,"duration":0},{"id":857355,"guid":"AB753EB01AB211EA9B931B003A461DF7","previous_id":857351,"previous_guid":"534393E01AB211EA9B931B003A461DF7","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"AC4CBAD1258D4ACA88EA7B6782F317EA","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"952D420D246E4101A87A9E16D95431CD","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Dispose of the perfusion run off (blood, and saline) mixture that has collected in the appropriate waste container.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":[],"critical_id":null,"duration":0},{"id":857356,"guid":"B954E1701AB211EA9B931B003A461DF7","previous_id":857355,"previous_guid":"AB753EB01AB211EA9B931B003A461DF7","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"5B54195F617043DCB000B01B7EDE1094","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"7D4E806253BB427B9EE6B0782277E994","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span style = \"font-weight:bold;\">Tissue dissection and collection<\/span><\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":[],"critical_id":null,"duration":0},{"id":857367,"guid":"CBFC93901AB211EA9B931B003A461DF7","previous_id":857356,"previous_guid":"B954E1701AB211EA9B931B003A461DF7","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"95CDDB7C7A8D4644ADC72F1201BEECFB","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"7D7508D02EE240659E50B6D3CBB976ED","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Remove all the inner organs below diaphragm, except the kidneys. The kidneys will act as a landmark to help identify relevant structures while dissecting. If you wish to keep the organs, store in designated tube and drop into liquid nitrogen tank. <\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":[],"critical_id":null,"duration":0},{"id":857368,"guid":"CD2F20201AB211EA9B931B003A461DF7","previous_id":857367,"previous_guid":"CBFC93901AB211EA9B931B003A461DF7","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"8D85B5D5CDBE4CE0B38F47A3A2E8DE0A","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"C5646DD3D69B41DABA5F740961DAAF2D","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Remove the heart and lungs. If you wish to keep the organs, store in designated tube and drop into liquid nitrogen tank. <\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":[],"critical_id":null,"duration":0},{"i