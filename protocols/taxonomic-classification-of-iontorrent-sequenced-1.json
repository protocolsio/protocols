revious_id":974473,"previous_guid":"F2D77F5DBCE94ABAB578044B487F0D7C","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"E323D576DFFB443081348B070A4FE0D7","order_id":1,"type_id":6,"title":"Section","source":{"title":"Pre-processing"}},{"id":1054724,"guid":"3DADBD5CEF6447DEA40EA3245D9292EB","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Import fastq files.<\/div><div class = \"text-block\">Trim primers.<\/div><div class = \"text-block\">Remove sequences shorter than 100 bp.<\/div><div class = \"text-block\">Suggested software:<\/div><div class = \"text-block\">(or python scripts).<\/div><\/div>"}},{"id":1054725,"guid":"AD6D28A0BB8811EAA48F0BBB3230F3E2","order_id":2,"type_id":8,"title":"software","source":{"name":"Geneious","developer":"Biomatters Ltd","repository":"","link":"https:\/\/www.geneious.com\/geneious\/","os_name":"","os_version":""}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":974475,"guid":"3492D162432541A694869D56C90D977E","previous_id":974474,"previous_guid":"55609C39427442028A17109102347991","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"BC81689CFF3C4A67A51F18EE3241F772","order_id":1,"type_id":6,"title":"Section","source":{"title":"Pre-processing"}},{"id":1054724,"guid":"1390F1C25C774496BA3A0142BC365BAC","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">(Optional) Check the Phred quality score of the sequences.<\/div><div class = \"text-block\">Suggested software: <\/div><\/div>"}},{"id":1054725,"guid":"41075090BB8E11EAA48F0BBB3230F3E2","order_id":2,"type_id":8,"title":"software","source":{"name":"FastQC","developer":"Simon Andrews","repository":null,"link":"http:\/\/www.bioinformatics.babraham.ac.uk\/projects\/fastqc\/","os_name":null,"os_version":null}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":974476,"guid":"A77D5275A04348B8B5AC6F5D4C3C7CAC","previous_id":974475,"previous_guid":"3492D162432541A694869D56C90D977E","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"CC3B567CDCC64DBABD4246E25EC9A6F3","order_id":1,"type_id":6,"title":"Section","source":{"title":"On Qiime2"}},{"id":1054724,"guid":"632D2822CAA04FF7AD5F4D1B0D49F0FD","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span style = \"font-weight:bold;\">To train a Na\u00efve Bayes classifier<\/span><span>: Download the Greengenes 13_8 99% OTU database. Upload the files \"99_otus.fasta\" and \"99_otu_taxonomy.txt\" onto a folder accesible by the Qiime2 environment. With Qiime2 activated and inside the folder containing those files, type in the command line:<\/span><\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":974477,"guid":"AEDC41DAD572461FA28A0AF7528A6BED","previous_id":974476,"previous_guid":"A77D5275A04348B8B5AC6F5D4C3C7CAC","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"316458BDA4954245A7D5A8078A84490A","order_id":1,"type_id":6,"title":"Section","source":{"title":"On Qiime2"}},{"id":1054724,"guid":"C59F8902501C4D07956ED1092CDC2EE0","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">>qiime tools import --type 'FeatureData[Sequence]' --input-path 99_otus.fasta --output-path otus.qza<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":974478,"guid":"91CB17E79FA14DEB9B2FA6084AA2DEC6","previous_id":974477,"previous_guid":"AEDC41DAD572461FA28A0AF7528A6BED","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"149108FF3D0D4E97B6AE9A6762F612F7","order_id":1,"type_id":6,"title":"Section","source":{"title":"On Qiime2"}},{"id":1054724,"guid":"EFBE8D42C3F442A09C6B5130B8EB9004","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">>qiime tools import --type 'FeatureData[Taxonomy]' --input-format HeaderlessTSVTaxonomyFormat --input-path 99_otu_taxonomy.txt --output-path taxonomy.qza<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":974479,"guid":"6983F5E64CE64CDA971A0BAE702244A3","previous_id":974478,"previous_guid":"91CB17E79FA14DEB9B2FA6084AA2DEC6","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"53A7B63026E343BEA59599FF7818E1DF","order_id":1,"type_id":6,"title":"Section","source":{"title":"On Qiime2"}},{"id":1054724,"guid":"37541E86461E4CFAA2651A422E8526A6","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">>qiime feature-classifier extract-reads --i-sequences otus.qza --p-f-primer AGAGTTTGATCMTGGCTCAG --p-r-primer CYNACTGCTGCCTCCCGTAG --o-reads ref-seqs.qza<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":974480,"guid":"DD91E53CF25648468489DE928C35F49B","previous_id":974479,"previous_guid":"6983F5E64CE64CDA971A0BAE702244A3","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"0DC7391577B34DE495629D1BE98F5B2A","order_id":1,"type_id":6,"title":"Section","source":{"title":"On Qiime2"}},{"id":1054724,"guid":"1290E3647D0341C3BD09DD2225415742","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">>qiime feature-classifier fit-classifier-naive-bayes --i-reference-reads ref-seqs.qza --i-reference-taxonomy taxonomy.qza --o-classifier classifierp.qza<\/div><\/div>"}},{"id":1054725,"guid":"C3F2CF461FEB4B949379DB95D6A2E0F2","order_id":2,"type_id":17,"title":"result","source":{"body":"<div class = \"text-blocks\"><div class = \"text-block\">The artifact \"classifier.qza\" can be copied to any other folder for the following steps.<\/div><div class = \"text-block\"><span>Example of <\/span><span style = \"font-weight:bold;\">classifier trained on the Greengenes 99% OTU database for the V1-V2 region of the 16S rRNA gene: <\/span><\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":974481,"guid":"D53465C2F9694D95B51ED243C2741C76","previous_id":974480,"previous_guid":"DD91E53CF25648468489DE928C35F49B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"B681BD23F92F44158D291152CA3DEB78","order_id":1,"type_id":6,"title":"Section","source":{"title":"On Qiime2"}},{"id":1054724,"guid":"5031D13697F3426DBB2C765EAED5C3BE","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span style = \"font-weight:bold;\">To perform taxonomy classification<\/span><span>: In a new folder, upload the artifact \"classifier.qza\" and the file with the metadata corresponding to the fastq files in the correct format. Example of file:<\/span><\/div><div class = \"text-block\">Also create a subfolder \"fastq\" containing the compressed fastq files in the correct nominal format (eg. \"fcm1_1_L001_R1_001.fastq.qz\" and \"fcm2_2_L001_R1_001.fastq.qz\") and the classifier artifact.<\/div><div class = \"text-block\">In the command line, type in:<\/div><\/div>"}},{"id":1054725,"guid":"6A2E0640BC7811EAA48F0BBB3230F3E2","order_id":2,"type_id":23,"title":"file","source":{"source":"https:\/\/s3.amazonaws.com\/protocols-files\/public\/e44a164fa5aa6c4397c4313b772950c1df5ffa599d264bf69e00602ae7aab626\/cdr6bejt7.tsv","placeholder":"\/img\/extensions\/file.png","original_name":"metadata.tsv"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":974482,"guid":"EAC8C5BD7A444F47A333D79CFBDDC85E","previous_id":974481,"previous_guid":"D53465C2F9694D95B51ED243C2741C76","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"41D61DE31C3049A4ACE1A13EC3B19F04","order_id":1,"type_id":6,"title":"Section","source":{"title":"On Qiime2"}},{"id":1054724,"guid":"A76377B17583450397F4383F7DD5A825","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">>qiime tools import --type 'SampleData[SequencesWithQuality]' --input-path fastq --input-format CasavaOneEightSingleLanePerSampleDirFmt --output-path demux-single-end.qza<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":974483,"guid":"D4DAA304F51847F58F49244663A4A688","previous_id":974482,"previous_guid":"EAC8C5BD7A444F47A333D79CFBDDC85E","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"9A1C2DC6710F473494DDC347BE454763","order_id":1,"type_id":6,"title":"Section","source":{"title":"On Qiime2"}},{"id":1054724,"guid":"1422CC6EB09A4D31ABCC1C1717001C4D","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">>qiime vsearch dereplicate-sequences --i-sequences demux-single-end.qza --o-dereplicated-table dereplicatedtable.qza --o-dereplicated-sequences rep-seqs.qza<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":974484,"guid":"B60DFAF411D945FF93BA838998761BDF","previous_id":974483,"previous_guid":"D4DAA304F51847F58F49244663A4A688","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"53770E1394CE420E9AFC1CBB8B42E1A3","order_id":1,"type_id":6,"title":"Section","source":{"title":"On Qiime2"}},{"id":1054724,"guid":"F968C8E40F794E7998980F2EE060314D","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">>qiime feature-classifier classify-sklearn --i-classifier classifier.qza --i-reads rep-seqs.qza --o-classification taxonomy.qza<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":974485,"guid":"0DE28ED176DE4201B8A3217E05D29DBC","previous_id":974484,"previous_guid":"B60DFAF411D945FF93BA838998761BDF","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"9A0C55C246F7464CB1D9CF6AA672B254","order_id":1,"type_id":6,"title":"Section","source":{"title":"On Qiime2"}},{"id":1054724,"guid":"6E5D5094D17344DBB9B40D3F8BF2E2C3","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">>qiime taxa barplot --i-table dereplicatedtable.qza --i-taxonomy taxonomy.qza --m-metadata-file sample-metadata.tsv --o-visualization taxa-bar-plots.qzv<\/div><\/div>"}},{"id":1054725,"guid":"057841D11D994F4C98C4B58E1C5B1EAC","order_id":2,"type_id":17,"title":"result","source":{"body":"<div class = \"text-blocks\"><div class = \"text-block\"><span>The artifact \"taxa-bar-plots.qzv\" can be interactively visualized in <\/span><a href=\"https:\/\/view.qiime2.org\" style = \"text-decoration:underline;color:blue;cursor:pointer;\"><span style = \":;\">https:\/\/view.qiime2.org\/<\/span><\/a><\/div><div class = \"text-block\">Example of artifact:<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":974486,"guid":"5CB10CA6AF0F4327A7FC7819B19F308F","previous_id":974485,"previous_guid":"0DE28ED176DE4201B8A3217E05D29DBC","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"0CA52E7B1F434FD0AB76A1015589CDA0","order_id":1,"type_id":6,"title":"Section","source":{"title":"On Qiime2"}},{"id":1054724,"guid":"4A4DF9D6F3954DC6B6E9409A974FEA2E","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">The components of the biom table (the OTU abundance table and the taxonomy table) can be obtained from the \"taxa-bar-plots.qzv\" artifact or with the following commands:<\/div><div class = \"text-block\">>qiime tools export --input-path dereplicatedtable.qza --output-path exported-feature-table<\/div><div class = \"text-block\">>cd exported-feature-table<\/div><div class = \"text-block\">>biom convert -i feature-table.biom -o otutable.tsv --to-tsv<\/div><div class = \"text-block\">>qiime tools export --input-path taxonomy.qza --output-path taxonomy<\/div><\/div>"}},{"id":1054725,"guid":"3CA7BBD40E2B49569F868223812933F4","order_id":2,"type_id":17,"title":"result","source":{"body":"<div class = \"text-blocks\"><div class = \"text-block\">The files \"taxonomy.tsv\" and \"otutable.tsv\" should be downloaded and can be manually converted into .csv files and adapted for the analysis with R. The additional file \"metadata.csv\" is also necessary.<\/div><div class = \"text-block\">Example of files:<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":974487,"guid":"7DE097FFA0E743EABE3E59A0E240BA85","previous_id":974486,"previous_guid":"5CB10CA6AF0F4327A7FC7819B19F308F","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"3D0DA3116B294DC2A1F4E32B2BB6A551","order_id":1,"type_id":6,"title":"Section","source":{"title":"Analysis with R"}},{"id":1054724,"guid":"0D43A7BD43A44939949E423BB848D14A","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span>Within R and activating the <\/span><span style = \"font-style:italic;\">phyloseq <\/span><span>package, different types of analysis can be easily performed.<\/span><\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#84CE84","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":974488,"guid":"ED682C5A6A804449B0EE3575020086E7","previous_id":974487,"previous_guid":"7DE097FFA0E743EABE3E59A0E240BA85","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"CCA7C93E949E439983E8F831289E38DD","order_id":1,"type_id":6,"title":"Section","source":{"title":"Analysis with R"}},{"id":1054724,"guid":"DBCE6DCA9CA444A298841B634929C40B","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">The first step is the creation of the phyloseq object using the type of files shown in step 5.5:<\/div><div class = \"text-block\">> otu_table <- read.csv(\"otutable.csv\",sep=\",\",row.names=1)<\/div><div class = \"text-block\">> tax_table <- read.csv(\"taxonomy.csv\",sep=\",\",row.names=1)<\/div><div class = \"text-block\">> OTU <- otu_table(otu_table,taxa_are_rows=TRUE)<\/div><div class = \"text-block\">> tax_matrix<- as.matrix(tax_table)<\/div><div class = \"text-block\">> TAX <- tax_table(tax_matrix)<\/div><div class = \"text-block\">> metadata <- read.csv(\"metadata.csv\",sep=\",\",row.names=1)<\/div><div class = \"text-block\">> meta <- sample_data(metadata)<\/div><div class = \"text-block\">> fcm <- phyloseq(OTU, TAX, meta)<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#84CE84","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":974489,"guid":"10DA28D44ACF44E0BC2A20B2DD10C5C5","previous_id":974488,"previous_guid":"ED682C5A6A804449B0EE3575020086E7","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"1F8FB08C558D411F834D76FAE97F8416","order_id":1,"type_id":6,"title":"Section","source":{"title":"Analysis with R"}},{"id":1054724,"guid":"AD8AACE48E18422CAC5A5639FDA27B95","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Basic operations such as tax_glom are used to prepare the data for further analyses. For example, with the following commands a table containing the taxonomic abundance for the families present in each sample is created:<\/div><div class = \"text-block\">> fcm.family <- tax_glom(fcm,\"Family\", NArm= FALSE)<\/div><div class = \"text-block\">> fcm.family.m <- psmelt(fcm.family)<\/div><div class = \"text-block\">> write.table(x=fcm.family.m, sep = \",\", file = \"fcm_family.csv\")<\/div><\/div>"}},{"id":1054725,"guid":"F5FDAEABA15340469116FAB06AF512D8","order_id":2,"type_id":17,"title":"result","source":{"body":"<div class = \"text-blocks\"><div class = \"text-block\">At this point a table with the taxonomic abundance of each family is produced. Example of file:<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#84CE84","section_duration":0,"critical":null,"critical_id":null,"duration":0}],"document":null,"materials":[],"description":null,"changed_on":1594141498}