1062490,"previous_guid":"70BD3860183E11EB9DF40556602F1E62","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"CE7309AC40974178B0FDE653ACB0E1F8","order_id":1,"type_id":6,"title":"Section","source":{"title":"Execution"}},{"id":1054724,"guid":"92192D257B864208A14639CFFC53B456","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Spread 2-3 mL of freshly fragmented protonema on the cellophane on the petri dish.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1062492,"guid":"70BE97F1183E11EB9DF40556602F1E62","previous_id":1062491,"previous_guid":"70BDFBB1183E11EB9DF40556602F1E62","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"94B8B89B301B48E6B7FE9CA4A6C2C94C","order_id":1,"type_id":6,"title":"Section","source":{"title":"Execution"}},{"id":1054724,"guid":"E3FC5757C4AF4F92B6659F1657AA6A26","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Seal off with micropore tape to allow gas exchange.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1062493,"guid":"70BFA960183E11EB9DF40556602F1E62","previous_id":1062492,"previous_guid":"70BE97F1183E11EB9DF40556602F1E62","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"420BE4E20A94427CAD9AFD048D4A0AFF","order_id":1,"type_id":6,"title":"Section","source":{"title":"Execution"}},{"id":1054724,"guid":"D2ACFD304B53494E926EA83A81B962A9","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Incubate plates at 24\u00b0C with a light regime of 16 h light : 8 h darkness at 80 \u03bcmol\u00d7m\u20132\u00d7s\u20131<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1062494,"guid":"70C06CB1183E11EB9DF40556602F1E62","previous_id":1062493,"previous_guid":"70BFA960183E11EB9DF40556602F1E62","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"5A2F08D108C5445EA0A55830A63A5190","order_id":1,"type_id":6,"title":"Section","source":{"title":"Execution"}},{"id":1054724,"guid":"7FE6290A370E4199A3309AC610151CA4","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">For next harvest gently scrape surface of cellophane disks after 7 \u2013 10 days (before culture starts to turn brown). Repeat cycle. PPNH4 liquid medium with 300 mg\/L Cefotaxim or 250 mg\/L Vancomycin is recommended for storage as the moss stays available for culture for >6 months.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1062495,"guid":"907185D0183E11EB9DF40556602F1E62","previous_id":1062503,"previous_guid":"FDD461E0184011EB9DF40556602F1E62","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"C1430BD3E9274BB2A2D3BE575272D886","order_id":1,"type_id":6,"title":"Section","source":{"title":"Prepare in advance"}},{"id":1054724,"guid":"139E86E6CD3F489986B78A5BEE429C17","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Solid PPNH4 medium on sterile petri dishes<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1062499,"guid":"0E3988F0183F11EB9DF40556602F1E62","previous_id":1062504,"previous_guid":"07CFC590184111EB9DF40556602F1E62","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"4141A3A649D34FD3974C290E21A1CBEA","order_id":1,"type_id":6,"title":"Section","source":{"title":"Prepare in advance"}},{"id":1054724,"guid":"F5407B15C8ED408790309530E3FAD91F","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Cellophane disks with the appropriate size for the petri dishes. Wet the cellophane disks<\/div><div class = \"text-block\">with sterile ultrapure water to avoid creases.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1062500,"guid":"181EA580183F11EB9DF40556602F1E62","previous_id":1062499,"previous_guid":"0E3988F0183F11EB9DF40556602F1E62","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"3F657F6168514A7DAD5AF7FC99B2CCA7","order_id":1,"type_id":6,"title":"Section","source":{"title":"Prepare in advance"}},{"id":1054724,"guid":"77C95D756FF4400190AABBBB628C0DAB","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Overlay the solid medium on the petri dishes with fitting cellophane disks to prevent<\/div><div class = \"text-block\">the mosses from growing into the medium.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1062501,"guid":"1F25A860183F11EB9DF40556602F1E62","previous_id":null,"previous_guid":null,"modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"D9C676C6A041455CBBB7BD80725AA861","order_id":1,"type_id":6,"title":"Section","source":{"title":"Prepare in advance"}},{"id":1054724,"guid":"70992B671E7D4394955D6923654619DB","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Liquid PPNH4 medium with 300 mg\/L Cefotaxim or 250 mg\/L Vancomycin.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1062503,"guid":"FDD461E0184011EB9DF40556602F1E62","previous_id":1062501,"previous_guid":"1F25A860183F11EB9DF40556602F1E62","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"41FA04B4CD3B4C8AA91A9595F673541B","order_id":1,"type_id":6,"title":"Section","source":{"title":"Prepare in advance"}},{"id":1054724,"guid":"A4082F62384C48A884E2001296781888","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Add 10 mL of a macro element-solution, 1 mL of a micro element solution, 1 mL of the<\/div><div class = \"text-block\">potassium phosphate solution and 2,7 mL of the ammonium tartrate solution for each liter of<\/div><div class = \"text-block\">medium.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1062504,"guid":"07CFC590184111EB9DF40556602F1E62","previous_id":1062495,"previous_guid":"907185D0183E11EB9DF40556602F1E62","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"BB831E938042491BB0F01CD3CC5DA5D1","order_id":1,"type_id":6,"title":"Section","source":{"title":"Prepare in advance"}},{"id":1054724,"guid":"6F4838A4F5E7416EB73E750754B14D97","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Add 7,2 g agar per liter of PPNH4-liquid medium. Add further 300 mg cefotaxime or 250 mg Vancomycin per liter of<\/div><div class = \"text-block\">medium to suppress bacterial growth.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0}],"document":null,"materials":[],"description":null,"changed_on":1603795622