er_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span style = \"font-weight:bold;\">Repeat Step 5 <\/span><span>(Step 5: Re-insert column into collection tube. Add 200 \u03bcl DNA Wash Buffer and spin for 1 minute at 16,000 x g.\u00a0Discarding flow-through is optional). <\/span><\/div><\/div>"}},{"id":1054725,"guid":"A721A075FA7641219B3CB0AF367FFE51","order_id":2,"type_id":4,"title":"duration","source":{"duration":60,"title":""}}],"cases":[],"data":null,"html":null,"section":null,"section_color":null,"section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1084929,"guid":"9F08D34A10D24809868FBF03B041FF8E","previous_id":1084928,"previous_guid":"2EFADC68F5E046B3B43941B5DFAC34C1","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"BBC8AF9AB9DE419DAA0CA7E290F147B1","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"9D12ED9497594ACBBE6497CD92CA5F2F","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span style = \"font-weight:bold;\">Transfer column to a clean 1.5 ml microfuge tube.\u00a0<\/span><span>Use care to ensure that\u00a0the tip of the column does not come into contact with the flow-through. If in doubt, re-spin for 1 minute.  <\/span><\/div><\/div>"}},{"id":1054725,"guid":"DC28F9EC256B4989875045CD7323ABE3","order_id":2,"type_id":26,"title":"notes","source":{"id":0,"parent_id":0,"uri":"","title":"","body":"<div class = \"text-blocks\"><div class = \"text-block\">Use care to ensure that the tip of the column does not come into contact with the flow-through. If in doubt, re-spin for 1 minute.<\/div><\/div>","created_on":0,"changed_on":0,"creator":{"name":" ","affiliation":null,"affiliations":[],"username":"","note":null,"link":null,"image":{"source":"","placeholder":""},"badges":[],"verified":0,"is_verified_user":false,"research_interests"