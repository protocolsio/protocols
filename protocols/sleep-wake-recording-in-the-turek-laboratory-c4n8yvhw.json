,{"id":202,"type_id":32,"name":"µm","can_manage":0,"read_only":0},{"id":203,"type_id":32,"name":"nm","can_manage":0,"read_only":0},{"id":204,"type_id":25,"name":"mg/mL","can_manage":0,"read_only":0},{"id":205,"type_id":25,"name":"µg/µL","can_manage":0,"read_only":0},{"id":206,"type_id":25,"name":"% (v/v)","can_manage":0,"read_only":0},{"id":207,"type_id":3,"name":"V","can_manage":0,"read_only":0},{"id":1324,"type_id":30,"name":"rcf","can_manage":0,"read_only":0},{"id":1359,"type_id":35,"name":"Bar","can_manage":0,"read_only":0},{"id":1360,"type_id":35,"name":"Pa","can_manage":0,"read_only":0}],"uri":"sleep-wake-recording-in-the-turek-laboratory-c4n8yvhw","url":"https://www.protocols.io/view/sleep-wake-recording-in-the-turek-laboratory-c4n8yvhw","version_class":90560,"version_data":{"id":0,"code":"c4n8yvhw","version_class":90560,"parent_id":null,"parent_uri":null,"is_same_owner":false,"is_parent_public":false,"has_pending_merge_request":false,"has_approved_merge_request":false,"merge_request":null},"version_id":0,"version_uri":"sleep-wake-recording-in-the-turek-laboratory-yxmvm3z29l3p/v1","versions":[{"id":90560,"title":"Sleep-Wake Recording in the Turek Laboratory","title_html":"Sleep-Wake Recording in the Turek Laboratory","image":{"source":"https://www.protocols.io/img/default_protocol.png","webp_source":null,"placeholder":"https://www.protocols.io/img/default_protocol.png","webp_placeholder":null},"doi":"dx.doi.org/10.17504/protocols.io.yxmvm3z29l3p/v1","uri":"sleep-wake-recording-in-the-turek-laboratory-c4n8yvhw","published_on":1718912544,"modified_on":1718912544,"version_class":90560,"version_id":0,"version_code":"c4n8yvhw","version_uri":"sleep-wake-recording-in-the-turek-laboratory-yxmvm3z29l3p/v1","created_on":1699377803,"categories":null,"type_id":1,"creator":{"name":"Keith C Summa","affiliation":"Northwestern University","affiliation_url":"","username":"keith-c-summa","link":"","image":{"source":"/img/avatars/004.png","placeholder":"/img/avatars/004.png","w