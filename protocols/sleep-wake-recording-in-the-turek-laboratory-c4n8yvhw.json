{"access":{"can_view":true,"can_edit":false,"can_remove":false,"can_add":false,"can_publish":false,"can_get_doi":false,"can_share":true,"can_move":true,"can_move_outside":true,"can_transfer":true,"can_download":true,"limited_run":false,"limited_private_links":false,"limited_blind_links":false,"is_locked":false},"authors":[{"name":"Keith C Summa","affiliation":"Northwestern University","affiliation_url":"","username":"keith-c-summa","link":"","image":{"source":"/img/avatars/004.png","placeholder":"/img/avatars/004.png","webp_source":""},"note":"","is_verified_user":true}],"before_start":"","book_chapter":null,"can_accept_authorship":false,"can_be_copied":true,"can_claim_authorship":false,"can_manage_keywords":true,"can_remove_fork":false,"can_sign":false,"child_steps":{},"cited_protocols":[],"collection_items":[],"created_on":1699377803,"creator":{"name":"Keith C Summa","affiliation":"Northwestern University","affiliation_url":"","username":"keith-c-summa","link":"","image":{"source":"/img/avatars/004.png","placeholder":"/img/avatars/004.png","webp_source":""},"badges":[{"id":2,"name":"Author","image":{"source":"/img/badges/bronze.svg","placeholder":"/img/badges/bronze.svg"}},{"id":6,"name":"Science accelerator","image":{"source":"/img/badges/accelerator.svg","placeholder":"/img/badges/accelerator.svg"}}],"affiliations":[{"affiliation":"Northwestern University","url":"","job_title":null,"is_default":true}]},"cross_cloud_origin":null,"description":"{\"blocks\":[{\"key\":\"767ra\",\"text\":\"Surgery and recording protocol for measuring sleep-wake behavior in mice   \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","disclaimer":"","document":"","documents":null,"doi":"dx.doi.org/10.17504/protocols.io.yxmvm3z29l3p/v1","doi_status":1,"ethics_statement":"{\"blocks\":[{\"key\":\"70fsl\",\"text\":\"Prior to use, this protocol must be approved by the Institutional Animal Care and Use Committee (IACUC) or the appropriate equivalent ethics committee(s). \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","fork_id":null,"fork_info":null,"forks":[],"funders":[{"funder_name":"ASAP","grant_id":"020551"},{"funder_name":"DOD","grant_id":"W81XWH2110582"}],"groups":[],"guid":"994F590A31664F1A9C5F27E103A606DC","guidelines":"","has_references":false,"has_step_reagents":false,"has_versions":false,"id":90560,"image":{"source":"https://www.protocols.io/img/default_protocol.png","webp_source":null,"placeholder":"https://www.protocols.io/img/default_protocol.png","webp_placeholder":null},"image_attribution":"","in_trash":false,"is_bookmarked":false,"is_contact_suspended":false,"is_content_confidential":false,"is_content_warning":false,"is_doi_reserved":true,"is_in_pending_publishing":false,"is_in_transfer":false,"is_owner":true,"is_research":true,"is_retracted":false,"is_shared_directly":false,"is_subprotocol":null,"is_unlisted":false,"item_id":1184713,"journal":null,"journals":[],"keywords":"ASAPCRN","last_modified":1718912544,"link":"","location":null,"manuscript_citation":"","materials":[],"materials_text":"","ownership_history":null,"parent_collections":[],"parent_protocols":[],"peer_reviewed":false,"protocol_references":"","public":true,"public_fork_note":"","published_on":1718912544,"references":[],"related_equipments":[],"related_materials":[],"reserved_doi":"10.17504/protocols.io.yxmvm3z29l3p/v1","retraction_reason":null,"samples":{},"shared_access_id":265,"show_comparison":false,"sign_info":null,"space_access":{"can_view":false,"can_edit":false,"can_remove":false,"can_add":false,"can_publish":true,"can_get_doi":true,"can_share":false,"can_move":false,"can_move_outside":false,"can_transfer":false,"can_download":false,"limited_run":false,"limited_private_links":false,"limited_blind_links":false,"is_locked":false},"space_id":8782,"state_version_id":375,"stats":{"is_voted":false,"number_of_views":41,"number_of_steps":14,"number_of_bookmarks":0,"number_of_comments":0,"number_of_bookmarked_comments":0,"number_of_steps_comments":0,"number_of_protocol_comments":0,"number_of_exports":0,"number_of_runs":0,"number_of_votes":0,"number_of_reagents":0,"number_of_equipments":0,"number_of_collections":0,"number_of_forks":{"private":0,"public":0},"number_of_accessible_forks":0},"status":{"id":1,"info":"We use this protocol and it's working"},"steps":[{"id":1841248,"guid":"D2D3FC8173E540AF93C4A435AEC1D683","previous_id":0,"previous_guid":null,"section":"\u003cp\u003eSurgery\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"5d0c7\",\"text\":\"The mouse is anesthetized by intraperitoneal injection with a ketamine/xylazine cocktail. Once the animal is anesthetized (as assessed by toe pinch), the fur on the top of the head is wetted with diluted alcohol and clipped.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90560,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"1","cases":[],"critical":null},{"id":1841249,"guid":"5160B946E70E4A40AD75218FF77052A3","previous_id":1841248,"previous_guid":"D2D3FC8173E540AF93C4A435AEC1D683","section":"\u003cp\u003eSurgery\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"bpgo2\",\"text\":\"The animal is positioned in the stereotaxis apparatus (David Kopf Instruments).\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90560,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"2","cases":[],"critical":null},{"id":1841250,"guid":"34A24EF6E67D47C2A0806860043754E6","previous_id":1841249,"previous_guid":"5160B946E70E4A40AD75218FF77052A3","section":"\u003cp\u003eSurgery\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"9ot0r\",\"text\":\"A single dose of meloxicam is given while the mouse is anesthetized. Sterile eye ointment is applied to the eyes to prevent corneal drying. Heat support during surgery is provided by small re-usable gel bead disks heated to 37 degrees Celsius.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90560,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"3","cases":[],"critical":null},{"id":1841251,"guid":"EAC7D6C007BA4ED88A3D74A66188CA99","previous_id":1841250,"previous_guid":"34A24EF6E67D47C2A0806860043754E6","section":"\u003cp\u003eSurgery\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"998rn\",\"text\":\"The incision area is cleaned with 3 alternative washes of betadine scrub and alcohol, and sterile drapes are positioned over the animal's back.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90560,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4","cases":[],"critical":null},{"id":1841252,"guid":"41EDC0F10C0945EBAC5CE4E2342FB5D8","previous_id":1841251,"previous_guid":"EAC7D6C007BA4ED88A3D74A66188CA99","section":"\u003cp\u003eSurgery\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"4v2av\",\"text\":\"An approximately 1 - 1.5 cm incision is made at the midline over the top of the skull. No skin is\\nremoved.            \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90560,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"5","cases":[],"critical":null},{"id":1841253,"guid":"6A199B00F4654C86A788189D0D25D14C","previous_id":1841252,"previous_guid":"41EDC0F10C0945EBAC5CE4E2342FB5D8","section":"\u003cp\u003eSurgery\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"a7td0\",\"text\":\"Mice are surgically implanted with a headmount (Part #8201, Pinnacle Technologies,\\nLawrence, KS) for sleep recording. The headmount consists of three electroencephalographic\\n(EEG) electrodes and 2 electromyographic (EMG) electrodes connected to a printed circuit board,\\nwhich is affixed to the exposed skull via a small amount of cyanoacrylate (Vet-bond or similar)\\nand four stainless steel screws (Part #8209: 0.10” screws for anterior positions; Part #8212: 0.12”\\nscrews for posterior positions) that serve as the EEG leads. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90560,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"6","cases":[],"critical":null},{"id":1841254,"guid":"0ECA49E399CF49CA9E177016C29DF19D","previous_id":1841253,"previous_guid":"6A199B00F4654C86A788189D0D25D14C","section":"\u003cp\u003eSurgery\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"66sih\",\"text\":\"Using the stereotaxic apparatus (David Kopf Instruments), the implant is positioned on the skull such that the front edge of the implant is placed 3.0-3.5 mm anterior to bregma. This configuration places all four screws over the cerebral cortex region in adult mice. Silver epoxy (Pinnacle\\nTechnologies, Lawrence, KS) may be applied to insure a secure electrical connection to the\\nscrews is achieved. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90560,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"7","cases":[],"critical":null},{"id":1841255,"guid":"6B06C13B5A2E4327A00BE1EAB1D32E2F","previous_id":1841254,"previous_guid":"0ECA49E399CF49CA9E177016C29DF19D","section":"\u003cp\u003eSurgery\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"7np85\",\"text\":\"A small pocket is made in the nuchal muscles using a pair of forceps for the\\nplacement of the EMG wires. Dental acrylic (Lang Dental Manufacturing Co., Wheeling, IL) is\\nused to seal and protect the headmount, and the skin is closed around the headmount with a\\nsingle suture using 4-0 or 5-0 chromic gut (United States Surgical, Norwalk, CT) at the front and\\nback of the implant. Absorbable suture is used to minimize risk of damage of the fragile wires\\nthat could occur if sutures needed to be removed.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90560,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"8","cases":[],"critical":null},{"id":1841256,"guid":"E2A77586D4604D9EB596A5734EC27400","previous_id":1841255,"previous_guid":"6B06C13B5A2E4327A00BE1EAB1D32E2F","section":"\u003cp\u003eSurgery\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"6f6jf\",\"text\":\"After surgery, a circulating water blanket is used for heat support as animals recover from anesthesia. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90560,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"9","cases":[],"critical":null},{"id":1841257,"guid":"642A203799BD4850BE8A27FFBB582C66","previous_id":1841256,"previous_guid":"E2A77586D4604D9EB596A5734EC27400","section":"\u003cp\u003eSurgery\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"4gevr\",\"text\":\"An additional dose of meloxicam is given for analgesia approximately 24 hours later. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90560,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"10","cases":[],"critical":null},{"id":1841258,"guid":"364CD44646584105916252BD58C5031C","previous_id":1841257,"previous_guid":"642A203799BD4850BE8A27FFBB582C66","section":"\u003cp\u003eRecording\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"fqo8i\",\"text\":\"Supplies and software utilized for sleep-wake recordings are from Pinnacle Technologies (Lawrence, KS). At least one week after surgery, mice are put into sleep recording cages (Product #8228). A preamp (Part #8406) plugs into the implant (Part #8201), and wires attached to the preamp connect to a commutator (Part #8408), which then plugs into an A/D converter (Part #8401).\\t\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90560,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"11","cases":[],"critical":null},{"id":1841259,"guid":"EE9CE881EFA04C9D88F7F5863B332BD3","previous_id":1841258,"previous_guid":"364CD44646584105916252BD58C5031C","section":"\u003cp\u003eRecording\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"9vs07\",\"text\":\"Pinnacle software PAL 8400 is used to record. Recordings are obtained at a sampling rate of 250\\nHz. A bandpass filter is applied between 0.5 Hz and 100 Hz on the EEG channels and between 1\\nHz and 100 Hz on the EMG channel.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90560,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"12","cases":[],"critical":null},{"id":1841260,"guid":"0D275284BEC74A0FA4EE3ED1B8FBA1B7","previous_id":1841259,"previous_guid":"EE9CE881EFA04C9D88F7F5863B332BD3","section":"\u003cp\u003eRecording\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"ch8t9\",\"text\":\"Recordings are obtained after at least 5 days of habituation to the sleep-wake chamber and continued over 48-72 hours.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90560,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"13","cases":[],"critical":null},{"id":1841261,"guid":"CC50BCB090A443BFBF0D1839CDDD6216","previous_id":1841260,"previous_guid":"0D275284BEC74A0FA4EE3ED1B8FBA1B7","section":"\u003cp\u003eRecording\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"6gfph\",\"text\":\"Mice are maintained on a constant 12-hr light:12-hr dark light:dark (LD) cycle for the duration of the experimental protocol. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90560,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"14","cases":[],"critical":null}],"template_id":1,"title":"Sleep-Wake Recording in the Turek Laboratory","title_html":"Sleep-Wake Recording in the Turek Laboratory","type_id":1,"units":[{"id":1,"type_id":3,"name":"µL","can_manage":0,"read_only":0},{"id":2,"type_id":3,"name":"mL","can_manage":0,"read_only":0},{"id":3,"type_id":3,"name":"L","can_manage":0,"read_only":0},{"id":4,"type_id":3,"name":"µg","can_manage":0,"read_only":0},{"id":5,"type_id":3,"name":"mg","can_manage":0,"read_only":0},{"id":6,"type_id":3,"name":"g","can_manage":0,"read_only":0},{"id":7,"type_id":3,"name":"kg","can_manage":0,"read_only":0},{"id":8,"type_id":3,"name":"ng","can_manage":0,"read_only":0},{"id":9,"type_id":3,"name":"Hz","can_manage":0,"read_only":0},{"id":10,"type_id":24,"name":