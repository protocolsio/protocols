,"can_move":false,"can_move_outside":false,"can_transfer":false,"can_download":false,"limited_run":false,"limited_private_links":false,"limited_blind_links":false,"is_locked":false},"space_id":1707,"state_version_id":958,"stats":{"is_voted":false,"number_of_views":19,"number_of_steps":9,"number_of_bookmarks":0,"number_of_comments":1,"number_of_bookmarked_comments":0,"number_of_steps_comments":1,"number_of_protocol_comments":0,"number_of_exports":1,"number_of_runs":0,"number_of_votes":0,"number_of_reagents":0,"number_of_equipments":0,"number_of_collections":0,"number_of_forks":{"private":0,"public":0},"number_of_accessible_forks":0},"status":{"id":1,"info":"We use this protocol and it's working"},"steps":[{"id":1544311,"guid":"C72DA5A077EF11ED950EBF4E4A967C7C","previous_id":1856783,"previous_guid":"0FFC8B59850F44E2AC15C3D49B64CBFB","section":"\u003cp\u003eSetup\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"29or2\",\"text\":\"Take out standards (1 and 2) and Qubit buffer from cabinet above NGS bench. Also get the Qubit reagent from the labled blue box in the drawer under the NGS bench. Retrieve DNA extractions and defrost (if they were frozen previously). Be sure to vortex and centrifuge samples. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":73799,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"2","cases":[],"critical":null},{"id":1544312,"guid":"D7BB516077EF11ED950EBF4E4A967C7C","previous_id":1544311,"previous_guid":"C72DA5A077EF11ED950EBF4E4A967C7C","section":"\u003cp\u003eSetup\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"eb93q\",\"text\":\"Clean the NGS area well, including the Qubit machine, vortex, and small centrifuge (if it is not already there, bring it to the NGS side of the lab bench - the blue one with the plastic lid).\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":73799,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"3","cases":[],"critical":null},{"id":1686125,"guid":"09602180F35811EDA3D56F84BFE64FA6","previous_id":1544312,"previous_guid":"D7BB516077EF11ED950EBF4E4A967C7C","section":"\u003cp\u003eSetup\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"1dfa7\",\"text\":\"Get out a number of Qubit tubes (glass cabinet above the NGS workspace) equal to your number of samples + 2 for the standards (N tubes + 2). Label each with an alcohol resistant marker, and write down each sample in your lab notebook (if you are using an abbreviated label, note this as well).\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":73799,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4","cases":[],"critical":null},{"id":1856778,"guid":"8473B7B96BA14BB4BD14132F8C8687AA","previous_id":1686125,"previous_guid":"09602180F35811EDA3D56F84BFE64FA6","section":"\u003cp\u003eMaster Mix\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"afc3i\",\"text\":\"Each time you use the Qubit, you will need to prepare enough of the master mix for all the samples. Prepare 199 uL Qubit buffer + 1 uL of Qubit reagent * (N samples + 2 standards + 1 extra). For example, if you have 6 samples, you will need 199 uL Qubit buffer + 1 uL of Qubit reagent * (9) for all the tubes. Get out an appropriate number of tubes to fit the master mix and label them. Combine the amount of buffer and reagent necessary, then vortex and spin the master mix.  \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"30bf1\",\"text\":\"\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":73799,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"5","cases":[],"critical":null},{"id":1856783,"guid":"0FFC8B59850F44E2AC15C3D49B64CBFB","previous_id":0,"previous_guid":null,"section":"\u003cp\u003eScale Protocol\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"eo1n2\",\"text\":\"Double check that you have appropriately scaled this protocol. After clicking \\\"run\\\" above, you will need to enter your number of samples + 2 standards + 1 extra for pipette error (e.g. if you are running 6 samples, scale this protocol for 9).\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":73799,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"1","cases":[],"critical":null},{"id":2646151,"guid":"EF970C14682B441084059A77214F0460","previous_id":1856778,"previous_guid":"8473B7B96BA14BB4BD14132F8C8687AA","section":"\u003cp\u003eMaking Tubes\u003c/p\u003e","section_color":"#A1EDFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"aj3j4\",\"text\":\"For Standard tubes, pipette: 190 uL of Master Mix + 10 uL of standard into each tube. Do this for each separate standard, switching pipette tips for each one. Vortex and spin once done.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":73799,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"6","cases":[],"critical":null},{"id":2646152,"guid":"6B993D2DD8104CB4BE0033D3A893E7BF","previous_id":2646151,"previous_guid":"EF970C14682B441084059A77214F0460","section":"\u003cp\u003eMaking Tubes\u003c/p\u003e","section_color":"#A1EDFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"2e99p\",\"text\":\"For DNA tubes: pipette 198 uL of Master Mix + 2 uL of extraction sample. Repeat for each sample, changing pipette tips in between each sample. Vortex and spin tubes once done.  \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":73799,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"7","cases":[],"critical":null},{"id":2646163,"guid":"ACEF3D9B752C49B39B93709B56DD099D","previous_id":2646152,"previous_guid":"6B993D2DD8104CB4BE0033D3A893E7BF","section":"\u003cp\u003eQubit\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"9kk46\",\"text\":\"Start up the Qubit machine. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"drf9k\",\"text\":\"Select the necessary path, which usually is: DNA \\u003e dsDNA broadrange. When told to run new standards, click yes\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"bold\",\"offset\":107,\"length\":3}],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":73799,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"8","cases":[],"critical":null},{"id":2646166,"guid":"161782DE748F47E3A133A29EF1BC9862","previous_id":2646163,"previous_guid":"ACEF3D9B752C49B39B93709B56DD099D","section":"\u003cp\u003eQubit\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"8kip0\",\"text\":\"Enter standard 1 when prompted, then run. Enter standard 2 when prompted, then run. Repeat with DNA samples until done. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":73799,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"9","cases":[],"critical":null}],"template_id":1,"title":"Qubit","title_htm