},"number_of_accessible_forks":0},"status":{"id":2,"info":"We are still developing and optimizing this protocol"},"steps":[{"id":1732656,"guid":"4E559BB020A511EE9CFEAB29212CDB1A","previous_id":0,"previous_guid":null,"section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"25m14\",\"text\":\"Working on ice, aspirate cell culture media and gently wash each well with 10 volumes of cold (2-8 °C) PBS (Table 1). Be careful not to disrupt the basement membrane matrix containing organoids.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":84904,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"1","cases":[],"critical":null},{"id":1732688,"guid":"2422225020AA11EE9CFEAB29212CDB1A","previous_id":1732656,"previous_guid":"4E559BB020A511EE9CFEAB29212CDB1A","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"fra7b\",\"text\":\"Aspirate the PBS, and add 10 volumes of cold (2-8 °C) Cultrex™ Organoid Harvesting Solution to each well (Table 1).\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"3ps2s\",\"text\":\"\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"cgg6t\",\"text\":\" \",\"type\":\"atomic\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[{\"key\":0,\"offset\":0,\"length\":1}],\"data\":{}},{\"key\":\"3qph\",\"text\":\"\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"8u47b\",\"text\":\"\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{\"0\":{\"type\":\"tables\",\"mutability\":\"IMMUTABLE\",\"data\":{\"cellsMeta\":{},\"colTitles\":[\"\",\"\",\"\"],\"colWidths\":[100,\"147\",100],\"data\":[[\"Plate Type\",\"Volume of membrane Matrix\",\"Volumen of PBS and organoids harvesting solution\"],[\"96-well plate\",\"5µl\",\"50µl\"],[\"48-well plate\",\"25µl\",\"250µl\"],[\"24-well plate\",\"50µl\",\"500µl\"]],\"guid\":\"8446F2D020AC11EE9CFEAB29212CDB1A\",\"isJexcelDataFormat\":true,\"legend\":\"\",\"mergeCells\":{},\"printData\":[[\"Plate Type\",\"Volume of membrane Matrix\",\"Volumen of PBS and organoids harvesting solution\"],[\"96-well plate\",\"5µl\",\"50µl\"],[\"48-well plate\",\"25µl\",\"250µl\"],[\"24-well plate\",\"50µl\",\"500µl\"]],\"rowHeights\":[23,23,23,23]}}}}","data":null,"protocol_id":84904,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"2","cases":[],"critical":null},{"id":1732697,"guid":"35CC76C020AC11EE9CFEAB29212CDB1A","previous_id":1732688,"previous_guid":"2422225020AA11EE9CFEAB29212CDB1A","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"99pmc\",\"text\":\"Incubate the plate at 2-8 °C on a cold pack into a Polystyrene box for 60 minutes with moderate shaking. This incubation is complete when the basement membrane matrix dome is no longer visible at the bottom of the well and the organoids are seen floating at the bottom of the well. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"brgrs\",\"text\":\"Note: Dislodging the dome with a cell scraper or pipet may accelerate this process.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"3met0\",\"text\":\"\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":84904,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"3","cases":[],"critical":null},{"id":1732698,"guid":"4872F06020AC11EE9CFEAB29212CDB1A","previous_id":1732697,"previous_guid":"35CC76C020AC11EE9CFEAB29212CDB1A","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"fdr14\",\"text\":\"Once the matrix depolymerizes, transfer the contents of the well into a tube on ice. Single wells may be transferred to a microtube while multiple domes may necessitate a 15 mL or 50 mL conical tube. Centrifuge the tube at 500 x g for 5 minutes at 2-8 °C in a swinging bucket rotor to pellet the organoids. Aspirate the supernatant.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":84904,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4","cases":[],"critical":null},{"id":1732699,"guid":"4E0358D020AC11EE9CFEAB29212CDB1A","previous_id":1732698,"previous_guid":"4872F06020AC11EE9CFEAB29212CDB1A","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"3efpu\",\"text\":\"Wash organoids with 10 volumes of cold (2-8 °C) PBS, and repeat centrifugation at 500 x g for 5 minutes at 2-8 °C in a  swinging bucket rotor to pellet the organoids. Aspirate the PBS.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":84904,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"5","cases":[],"critical":null},{"id":1732700,"guid":"51A0A3D020AC11EE9CFEAB29212CDB1A","previous_id":1732699,"previous_guid":"4E0358D020AC11EE9CFEAB29212CDB1A","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"doejs\",\"text\":\"Isolated organoids may be:\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"21ak9\",\"text\":\"a. Resuspended in basement membrane matrix for further organoid culture.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"5b02h\",\"text\":\"b. Resuspended in freezing medium for cryopreservation.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"2qrvm\",\"text\":\"c. Processed for biochemical analysis (such as RT-PCR, MS-PCR, sequencing, Western Blot, ELISA, or IHC)\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":84904,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"6","cases":[],"critical":null}],"template_id":4,"title":"Whole Organoids Harvesting Procedure (Cultrex)","title_html":"Whole Organoids Harvesting Procedure (Cultrex)","type_id":1,"units":[{"id":1,"type_id":3,"name":"µL","can_manage":0,"read_only":0},{"id":2,"type_id":3,"name":"mL","can_manage":0,"read_only":0},{"id":3,"type_id":3,"name":"L","can_manage":0,"read_only":0},{"id":4,"type_id":3,"name":"µg","can_manage":0,"read_only":0},{"id":5,"type_id":3,"name":"mg","can_manage":0,"read_only":0},{"id":6,"type_id":3,"name":"g","can_manage":0,"read_only":0},{"id":7,"type_id":3,"name":"kg","can_manage":0,"read_only":0},{"id":8,"type_id":3,"name":"ng","can_manage":0,"read_only":0},{"id":9,"type_id":3,"name":"Hz","can_manage":0,"read_only":0},{"id":10,"type_id":24,"name":"°C","can_manage":0,"read_only":0},{"id":11,"type_id":24,"name":"°К","can_manage":0,"read_only":0},{"id":12,"type_id":24,"name":"°F","can_manage":0,"read_only":0},{"id":13,"type_id":25,"name":"Mass Percent","can_manage":0,"read_only":0},{"id":14,"type_id":25,"name":"% volume","can_manage":0,"read_only":0},{"id":15,"type_id":25,"name":"Mass / % volume","can_manage":0,"read_only":0},{"id":16,"type_id":25,"name":"Parts per Million (PPM)","can_manage":0,"read_only":0},{"id":17,"type_id":25,"name":"Parts per Billion (PPB)","can_manage":0,"read_only":0},{"id":18,"type_id":25,"name":"Parts per Trillion (PPT)","can_manage":0,"read_only":0},{"id":19,"type_id":25,"name":"Mole Fraction","can_manage":0,"read_only":0},{"id":20,"type_id":25,"name":"Mole Percent","can_manage":0,"read_only":0},{"id":21,"type_id":25,"name":"Molarity (M