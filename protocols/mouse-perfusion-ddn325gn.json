r":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"er3h4\",\"text\":\"Insert 22g needle into the left ventricle and secure with hemostats.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":99771,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"6","cases":[],"critical":null},{"id":2053687,"guid":"F54FB0F20F76480D892E56798E2C642B","previous_id":2053675,"previous_guid":"75E4A9BA3FB7442389E6D5130977C519","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"8ffqu\",\"text\":\"Make small incision in the right ventricle.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":99771,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"7","cases":[],"critical":null},{"id":2053689,"guid":"BBCDE26D35E04EFBBAAB974C05AFE6DE","previous_id":2053687,"previous_guid":"F54FB0F20F76480D892E56798E2C642B","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"8ol1n\",\"text\":\"immediately start peristaltic pump, flowing at a rate of ~8-10mL per min.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":99771,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"8","cases":[],"critical":null},{"id":2053692,"guid":"E20DA2EB4FE1461EA94673723E4A155F","previous_id":2053689,"previous_guid":"BBCDE26D35E04EFBBAAB974C05AFE6DE","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"4hnim\",\"text\":\"Once the perfusate runs clear, stop pump, switch to 4% PFA solution, and resume flow.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_