rsion_id":123,"stats":{"is_voted":false,"number_of_views":0,"number_of_steps":5,"number_of_bookmarks":0,"number_of_comments":0,"number_of_bookmarked_comments":0,"number_of_steps_comments":0,"number_of_protocol_comments":0,"number_of_exports":0,"number_of_runs":0,"number_of_votes":0,"number_of_reagents":0,"number_of_equipments":0,"number_of_collections":0,"number_of_forks":{"private":0,"public":0},"number_of_accessible_forks":0},"status":{"id":1,"info":"We use this protocol and it's working"},"steps":[{"id":2250987,"guid":"D198813F518E4F5A87942F8C30ED1D91","previous_id":0,"previous_guid":null,"section":"\u003cp\u003eRelative Telomere Length Measurement\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"6iqpd\",\"text\":\"DNA isolation\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":108714,"case_id":0,"critical_ids":"1,41","duration":0,"original_id":0,"number":"1","cases":[],"critical":[{"id":1,"title":"Incubation","icon":"IncubationIcon"},{"id":41,"title":"Temperature","icon":"Temperature"}]},{"id":2250989,"guid":"E0824CD3C8B24FC59AF3049959C30A67","previous_id":2250987,"previous_guid":"D198813F518E4F5A87942F8C30ED1D91","section":"\u003cp\u003eRelative Telomere Length Measurement\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":true,"step":"{\"blocks\":[{\"key\":\"2qv5p\",\"text\":\"Genomic\\nDNA was extracted using the DNeasy Blood and Tissue Kit\\n(Qiagen, Hilden, Germany\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"bold\",\"offset\":36,\"length\":27}],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":108714,"case_id":0,"critical_ids":"1,2,5","duration":0,"original_id":0,"number":"1.1","cases":[],"critical":[{"id":1,"title":"Incubation","icon":"IncubationIcon"},{"id":2,"title":"Centrifugation","icon":"CentrifugationIcon"},{"id":5,"title":"Wash","icon":"WashIcon"}]},{"id":2250994,"guid":"961EB90B2037438B9F70F3A24D07E047","previous_id":2250989,"previous_guid":"E0824CD3C8B24FC59AF3049959C30A67","section":"\u003cp\u003eqPCR\u003c/p\u003e","section_color":"#7794FE","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"c8bv1\",\"text\":\"10 ng/μL DNA was amplified in a 5x EvaGreen‱ mix (No ROX) (Bio\\u0026Sell, Nurnberg, Germany). The thermal cycling protocol of  Telomere A: 5′-CGGTTTGTTTGGGTTTGGGTTTGGGTTTGGGTTTGGGTT-3′, Telomere B: 5′-GGCTTGCCTTACCCTTACCCTTACCCTTACCCTTACCCT-3′ and B2M was: 95°C for 15 min, followed by 40 cycles of 95°C for 15 s, 56°C for 20 s, and 72°C for20 s. The thermal cycling protocol of Telomere Primer Pair 1 was:95°C for 15 min, followed by 2 cycles of 15 s at 94°C, 1 cycle of 15 s at 49°C,40 cycles of 15 s at 94°C, 1 cycle 10 s at 62°C, 1 cycle 15 s at 74°C with signal acquisition, 10 s at 84°C, and 15 s at 88°C with signal acquisition. A no-template control (NTC) and the all templates were run in triplicate for each\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"bold\",\"offset\":243,\"length\":3},{\"style\":\"bold\",\"offset\":374,\"length\":23}],\"entityRanges\":[{\"key\":0,\"offset\":43,\"length\":1}],\"data\":{}},{\"key\":\"f18eh\",\"text\":\"reaction\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{\"0\":{\"type\":\"emoji\",\"mutability\":\"MUTABLE\",\"data\":{\"guid\":\"3928059B7FDA11EFBA9F0A58A9FEAC02\",\"name\":\":registered:\"}}}}","data":null,"protocol_id":108714,"case_id":0,"critical_ids":"4","duration":0,"original_id":0,"number":"2","cases":[],"critical":[{"id":4,"title":"PCR","icon":"PCRIcon"}]},{"id":2250997,"guid":"D39B9F784CC649318037246A1F44A120","previous_id":2250994,"previous_guid":"961EB90B2037438B9F70F3A24D07E047","section":"\u003cp\u003eqPCR\u003c/p\u003e","section_color":"#7794FE","section_duration":0,"is_substep":true,"step":"{\"blocks\":[{\"key\":\"fkq69\",\"text\":\"Primer sequences\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"482ek\",\"text\":\"Relative telomere length (RTL) was determined using two different primer pairs for telomere amplification and a primer pair for the single-copy reference gene beta-2-microglobulin(B2M). The following primers were employed: Telomere Primer Pair 1 (Hermann C, 2023): Telg: 5′-ACACTAAGGTTTGGGTTTGGGTTTGGGTTTGGGTTAGTGT-3′,Telc:\\n5′-TGTTAGGTATCCCTATCCCTATCCCTATCCCTATCCCTAACA-3′. Telomere Primer Pair 2 (Mugdha, 2020): Telomere A: 5′-CGGTTTGTTTGGGTTTGGGTTTGGGTTTGGGTTTGGGTT-3′, Telomere B: 5′-GGCTTGCCTTACCCTTACCCTTACCCTTACCCTTACCCT-3′. Single Copy Gene: Beta-2-microglobulin (B2M): Forward (F): 5′-CAAGACACCCGCCAGAAGAT-3′, Reverse (R): 5′-CAGCGTGGGACAGAAGGTAG-3′ (Designed by our lab).\",\"type\":\"align-left\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"bold\",\"offset\":159,\"length\":25},{\"style\":\"bold\",\"offset\":223,\"length\":42},{\"style\":\"bold\",\"offset\":374,\"length\":39},{\"style\":\"bold\",\"offset\":531,\"length\":46}],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":108714,"case_id":0,"critical_ids":"4","duration":0,"original_id":0,"number":"2.1","cases":[],"critical":[{"id":4,"title":"PCR","icon":"PCRIcon"}]},{"id":2250999,"guid":"C831544682E2422AA0AE9EE86B40AED3","previous_id":2250997,"previous_guid":"D39B9F784CC649318037246A1F44A120","section":"\u003cp\u003eData analysis\u003c/p\u003e","section_color":"#A1EDFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"c2c8q\",\"text\":\"Data were analyzed using ViiA7 software (ThermoFisher, MA, USA), and RTL was calculated using the ΔΔCt method: ΔΔCt = (Sample B2M Ct - Sample Telomere Ct) - (Control B2M Ct - Control Telomere Ct).\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"bold\",\"offset\":25,\"length\":15}],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":nul