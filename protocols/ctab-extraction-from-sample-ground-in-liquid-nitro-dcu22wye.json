\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"f7rqu\",\"text\":\"Liquid nitrogen\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"3vges\",\"text\":\"500µl CTAB\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"9ed6\",\"text\":\"50µl Sarkosyl\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"1rki6\",\"text\":\"50µl Proteinase K\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"74n80\",\"text\":\"200ul and 1ml tips\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"e8lb3\",\"text\":\"300µl Phenol chloroform\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"c1b5\",\"text\":\"1ml SEVAC\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"1kuar\",\"text\":\"400µl Isopropanol\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"2t1o0\",\"text\":\"500µl 70% Ethanol\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"1mogn\",\"text\":\"Nuclease free water\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","ownership_history":null,"parent_collections":[],"parent_protocols":[],"peer_reviewed":false,"protocol_references":"","public":true,"public_fork_note":"","published_on":1724349232,"references":[],"related_equipments":[],"related_materials":[],"reserved_doi":"10.17504/protocols.io.e6nvw157zlmk/v1","retraction_reason":null,"samples":{},"shared_access_id":0,"show_comparison":false,"sign_info":null,"space_access":{"can_view":false,"can_edit":false,"can_remove":false,"can_add":false,"can_publish":true,"can_get_doi":true,"can_share":false,"can_move":false,"can_move_outside":false,"can_transfer":false,"can_download":false,"limited_run":false,"limited_private_links":false,"limited_blind_links":false,"is_locked":false},"space_id":51982,"state_version_id":1157,"stats":{"is_voted":false,"number_of_views":2,"number_of_steps":21,"number_of_bookmarks":0,"number_of_comments":0,"number_of_bookmarked_comments":0,"number_of_steps_comments":0,"number_of_protocol_comments":0,"number_of_exports":0,"number_of_runs":0,"number_of_votes":0,"number_of_reagents":0,"number_of_equipments":0,"number_of_collections":0,"number_of_forks":{"private":0,"public":0},"number_of_accessible_forks":0},"status":{"id":1,"info":"We use this protocol and it's working"},"steps":[{"id":2034304,"guid":"395D0E247B284E54BEC5930610E60E55","previous_id":0,"previous_guid":null,"section":"\u003cp\u003eCryogenic Grinding and CTAB Extraction\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"86bb8\",\"text\":\"Dispense some liquid nitrogen in a small dewer (small thermos size)\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":98938,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"1","cases":[],"critical":null},{"id":2035014,"guid":"3276F5FEE5D44C8BAFF95852930021B9","previous_id":2034304,"previous_guid":"395D0E247B284E54BEC5930610E60E55","section":"\u003cp\u003eCryogenic Grinding and CTAB Extraction\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"92efl\",\"text\":\"Fill a mortar with liquid nitrogen to cool it and let it boil off. When it has completely boiled away, add a small amount more into the mortar and grind with pestle to a fine powder. Scrape the powder into a 1.5ml tube.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":98938,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"2","cases":[],"critical":null},{"id":2047387,"guid":"FCB39D60092F44568ECC56FDCBDCB38C","previous_id":2035014,"previous_guid":"3276F5FEE5D44C8BAFF95852930021B9","section":"\u003cp\u003eCryogenic Grinding and CTAB Extraction\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"6oek4\",\"text\":\"Add 500µl CTAB, 50µl sarkosyl and 50µl Proteinase K to the powder and vortex to mix.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":98938,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"3","cases":[],"critical":null},{"id":2047414,"guid":"97BCC8CD65AC453CADD6B5005793420E","previous_id":2047387,"previous_guid":"FCB39D60092F44568ECC56FDCBDCB38C","section":"\u003cp\u003eCryogenic Grinding and CTAB Extraction\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"3rgps\",\"text\":\"Place tube in a hot block at 60°C overnight or if in a rush 60-90 minutes.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":98938,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4","cases":[],"critical":null},{"id":2047438,"guid":"1E46F93EF98F45DB83A5B350FB3B3A88","previous_id":2047414,"previous_guid":"97BCC8CD65AC453CADD6B5005793420E","section":"\u003cp\u003eCryogenic Grinding and CTAB Extraction\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"ap9sg\",\"text\":\"After incubation add 300µl of phenol chloroform (This extracts the DNA into an aqueous layer).\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":98938,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"5","cases":[],"critical":null},{"id":2047439,"guid":"5853D41503C84B9BA9BAE8517C1CF577","previous_id":2047438,"previous_guid":"1E46F93EF98F45DB83A5B350FB3B3A88","section":"\u003cp\u003eCryogenic Grinding and CTAB Extraction\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"albou\",\"text\":\"Mix by inversion for 15 minutes on a rotator if you need high molecular weight. If you don't need high molecular weight, vortex well.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":98938,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"6","cases":[],"critical":null},{"id":2047440,"guid":"6D183050DFF440E89DABB06B2C0D887E","previous_id":2047439,"previous_guid":"5853D41503C84B9BA9BAE8517C1CF577","section":"\u003cp\u003eCryogenic Grinding and CTAB Extraction\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"fn4oc\",\"text\":\"Centrifuge for 3 minutes at 16 rpm (max speed) and label a new tube.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":98938,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"7","cases":[],"critical":null},{"id":2047441,"guid":"25BE05F37FDA48CFB9DED60842426B24","previous_id":2047440,"previous_guid":"6D183050DFF440E89DABB06B2C0D887E","section":"\u003cp\u003eCryogenic Grinding and CTAB Extraction\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"e3pmn\",\"text\":\"Carefully remove the top aqueous layer and transfer to the new tube. Avoid disturbing any of the white protein layer.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":98938,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"8","cases":[],"critical":null},{"id":2047442,"guid":"EF7830A5E7684E3DB5900D00D4AC1D5E","previous_id":2047441,"previous_guid":"25BE05F37FDA48CFB9DED60842426B24","section":"\u003cp\u003eCryogenic Grinding and CTAB Extraction\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"ajpdh\",\"text\":\"Add 500µl SEVAC and mix by placing on a rotator for 15 minutes (or vortex well if you don't require high molecular weight DNA).\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":98938,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"9","cases":[],"critical":null},{"id":2047443,"guid":"CE2082E2520240618D3DBA40FD0EBB7A","previous_id":2047442,"previous_guid":"EF7830A5E7684E3DB5900D00D4AC1D5E","section":"\u003cp\u003eCryogenic Grinding and CTAB Extraction\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"7ipdo\",\"text\":\"Centrifuge for 3 minutes at 16 rpm (max speed) and label a new tube.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":98938,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"10","cases":[],"critical":null},{"id":2047444,"guid":"A3C7DCFD0B954C9399CA6D615DAA21FB","previous_id":2047443,"previous_guid":"CE2082E2520240618D3DBA40FD0EBB7A","section":"\u003cp\u003eCryogenic Grinding and CTAB Extraction\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"2ran6\",\"text\":\"Carefully remove the top aqueous layer and transfer to the new tube. Avoid disturbing any of the white protein layer.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":98938,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"11","cases":[],"critical":null},{"id":2047445,"guid":"DA483F55C40C41C3AB185A64BD287F7A","previous_id":2047444,"previous_guid":"A3C7DCFD0B954C9399CA6D615DAA21FB","section":"\u003cp\u003eCryogenic Grinding and CTAB Extraction\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"3v9vo\",\"text\":\"Repeat steps 9-11.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":98938,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"12","cases":[],"critical":null},{"id":2047446,"guid":"67B40CCB3AF54855BA1EC192595D9B70","previous_id":2047445,"previous_guid":"DA483F55C40C41C3AB185A64BD287F7A","section":"\u003cp\u003eCryogenic Grinding and CTAB Extraction\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"37v6v\",\"text\":\"Add 400µl isopropanol and mix by inverting - DO NOT VORTEX! You may see DNA precipitate out.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":98938,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"13","cases":[],"critical":null},{"id":2047447,"guid":"A13834B989374A14892C4D5FA65E3CA7","previous_id":2047446,"previous_guid":"67B40CCB3AF54855BA1EC192595D9B70","section":"\u003cp\u003eCryogenic Grinding and CTAB Extraction\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"dd3tq\",\"text\":\"Centrifuge for 3 minutes at 16 rpm (max speed), you might see a pellet.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":98938,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"14","cases":[],"critical":null},{"id":2047448,"guid":"5FBD0409B970456686D2C448D1C67AB5","previous_id":2047447,"previous_guid":"A13834B989374A14892C4D5FA65E3CA7","section":"\u003cp\u003eCryogenic Grinding and CTAB Extraction\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"4buuv\",\"text\":\"Remove as much of the liquid as you can without disturbing the pellet.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":98938,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"15","cases":[],"critical":null},{"id":2047449,"guid":"3387E4DE5B044971896089085F479F54","previous_id":2047448,"previous_guid":"5FBD0409B970456686D2C448D1C67AB5","section":"\u003cp\u003eCryogenic Grinding and CTAB Extraction\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"3rbba\",\"text\":\"Add 500µl 70% ethanol (to wash away isopropanol)\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":98938,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"16","cases":[],"critical":null},{"id":2047450,"guid":"E09612B092F1499B9B89F034A3F14614","previous_id":2047449,"previous_guid":"3387E4DE5B044971896089085F479F54","section":"\u003cp\u003eCryogenic Grinding and CTAB Extraction\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"8r5tp\",\"text\":\"Wash pellet by inverting several times\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":98938,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"17","cases":[],"critical":null},{"id":2047451,"guid":"756BAC0A181A4F498DD25872361403D0","previous_id":2047450,"previous_guid":"E09612B092F1499B9B89F034A3F14614","section":"\u003cp\u003eCryogenic Grinding and CTAB Extraction\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"ut88\",\"text\":\"Centrifuge for 3 minutes at 16 rpm (max speed) and remove as much ethanol as possible without disturbing the pellet.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":98938,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"18","cases":[],"critical":null},{"id":2047452,"guid":"A48B3CA0E0244E4F96A105DCB72463A4","previous_id":2047451,"previous_guid":"756BAC0A181A4F498DD25872361403D0","section":"\u003cp\u003eCryogenic Grinding and CTAB Extraction\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"50uu9\",\"text\":\"Place tube on a hot block with the lid open at 60°C for 5 minutes to evaporate off any remaining ethanol.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":98938,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"19","cases":[],"critical":null},{"id":2047453,"guid":"86838F1E007E408B9FB6ACF17A94B695","previous_id":2047452,"previous_guid":"A48B3CA0E0244E4F96A105DCB72463A4","section":"\u003cp\u003eCryogenic Grinding and CTAB Extraction\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"3v13f\",\"text\":\"Resuspend in 30-50µl sterile water (flick the tube to help the pellet dissolve and if stubborn, pop back on the hot block for a few minutes).\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":98938,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"20","cases":[],"critical":null},{"id":2047454,"guid"