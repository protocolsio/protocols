":null,"section_color":"#EA94FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1131849,"guid":"525B68E0758311EBA34BE36E50EF3D5C","previous_id":1131838,"previous_guid":"87D965C0757F11EBA34BE36E50EF3D5C","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"BA19F614C7224F199B7AD6D0F43AFB70","order_id":1,"type_id":6,"title":"Section","source":{"title":"Exonuclease treatment"}},{"id":1054724,"guid":"386910B181B54311BC843732D36B33EE","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Add 5 ul of Exo I to each 50 ul PCR reaction<\/div><div class = \"text-block\">Incubation at 37\u00baC for 30 minutes<\/div><div class = \"text-block\">Heat inactivation at 80\u00baC for 20 minutes<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1131850,"guid":"360DD0A0758411EBA34BE36E50EF3D5C","previous_id":1131849,"previous_guid":"525B68E0758311EBA34BE36E50EF3D5C","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"0201E3B08F124DABA4D1EEE8BF10B352","order_id":1,"type_id":6,"title":"Section","source":{"title":"Test PCR II"}},{"id":1054724,"guid":"4844ABBBDA5F427F880AE21339677EF6","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Test optimal PCR II cycle number in a 25ul reaction\/test by adding<\/div><div class = \"text-block\">2.5ul of 1\/10 diluted PCR I product<\/div><div class = \"text-block\">2.5ul Accuprime buffer<\/div><div class = \"text-block\">2.5ul 10uM Sol I primer<\/div><div class = \"text-block\">2.5ul 10uM Sol II primer<\/div><div class = \"text-block\">0.25ul Accuprime Pfx HF enzyme<\/div><div class = \"text-block\">14.75ul H20<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1132278,"guid":"FF1BC230760711EBAF22F38412057E6A","previous_id":1131850,"previous_guid":"360DD0A0758411EBA34BE36E50EF3D5C","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"1D379CFAF12044C39BB7CEFBEFC2D560","order_id":1,"type_id":6,"title":"Section","source":{"title":"Test PCR II"}},{"id":1054724,"guid":"CFDD346319B94443A37E836E1923D0E0","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">For pooled target sites, run 14, 17, 20, 23, 26 and 29 cycles<\/div><div class = \"text-block\">For pooled injection sites, run 11, 14, 17, 20, and 23 cycles<\/div><div class = \"text-block\">Perform thermal cycling as follows:<\/div><div class = \"text-block\">Initial denaturation at<\/div><div class = \"text-block\">95\u00baC 2 minutes<\/div><div class = \"text-block\">Run different cycles of <\/div><div class = \"text-block\">95\u00baC 15 seconds<\/div><div class = \"text-block\">68\u00baC 1 minutes<\/div><div class = \"text-block\">Final extension at<\/div><div class = \"text-block\">68\u00baC 5 minutes<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1132291,"guid":"315A5940760911EBAF22F38412057E6A","previous_id":1132278,"previous_guid":"FF1BC230760711EBAF22F38412057E6A","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"E0D3D09E1B964811834C31CB06DEF59F","order_id":1,"type_id":6,"title":"Section","source":{"title":"Test PCR II"}},{"id":1054724,"guid":"4F280DF5308B44D3B3924365540B5B54","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Load test PCR II products on 2% agarose gel to find out the lowest cycle number which could generate a single and clean 230bp band.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1132299,"guid":"AF89F7D0760911EBAF22F38412057E6A","previous_id":1132291,"previous_guid":"315A5940760911EBAF22F38412057E6A","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"32CD380935B04399AE70166BA531792A","order_id":1,"type_id":6,"title":"Section","source":{"title":"PCR II"}},{"id":1054724,"guid":"5D835F287EFD47C18CF269E0E7D109E0","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">For pooled injection sites, use 12 ml of PCR II reaction<\/div><div class = \"text-block\">For pooled target sites, use 3 ml of PCR II reaction for <12 samples, 6 ml for <40 samples, and 12 ml for >40 samples.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#84CE84","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1132315,"guid":"643AD5A0760A11EBAF22F38412057E6A","previous_id":1132316,"previous_guid":"8024AD90760A11EBAF22F38412057E6A","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"40501695B2A14C60BD43EB98486E9594","order_id":1,"type_id":6,"title":"Section","source":{"title":"PCR II"}},{"id":1054724,"guid":"875C29DC7A6848DC8AB5096712C62783","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Perform thermal cycling as follows:<\/div><div class = \"text-block\">Initial denaturation at<\/div><div class = \"text-block\">95\u00baC 2 minutes<\/div><div class = \"text-block\">Run optimal cycles of <\/div><div class = \"text-block\">95\u00baC 15 seconds<\/div><div class = \"text-block\">68\u00baC 2.5 minutes<\/div><div class = \"text-block\">Final extension at<\/div><div class = \"text-block\">68\u00baC 5 minutes<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#84CE84","section_duration":0,"c