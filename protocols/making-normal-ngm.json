:0},{"id":1192219,"guid":"512CE9F1C79311EB8F64339741ABBFC1","previous_id":1192218,"previous_guid":"512B8A60C79311EB8F64339741ABBFC1","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"D8A670FAF98840C5856C0EC391E126A6","order_id":1,"type_id":6,"title":"Section","source":{"title":"Pre-autoclave"}},{"id":1054724,"guid":"74EA42099EA34ED496114D31FBC9DC53","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Add equivalent amount of water to the empty probe bottle.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1192220,"guid":"512E7091C79311EB8F64339741ABBFC1","previous_id":1192219,"previous_guid":"512CE9F1C79311EB8F64339741ABBFC1","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"A09D0B3E6592410EAACBD2DA7DBC2547","order_id":1,"type_id":6,"title":"Section","source":{"title":"Pre-autoclave"}},{"id":1054724,"guid":"34949AC532C44309BD2087EE5D20AD95","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Make sure the lid of your media bottle is not screwed completely when placing it inside the autoclave machine. Label with autoclave tape (\u2018NGM agar Rm 5020\u2019), and place over the lid to prevent it coming off in the autoclave.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1192221,"guid":"65DC7A00C79311EB8F64339741ABBFC1","previous_id":1192220,"previous_guid":"512E7091C79311EB8F64339741ABBFC1","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"820C5CEBB88742939547EAC207D1EAB5","order_id":1,"type_id":6,"title":"Section","source":{"title":"Using the autoclave"}},{"id":1054724,"guid":"C6516CC54FFD42A49C0014ED83F50612","order_id":1,