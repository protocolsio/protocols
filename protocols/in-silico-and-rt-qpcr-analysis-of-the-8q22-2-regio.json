":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1032842,"guid":"D5F5F1F0F80D11EA8561E7896FE90824","previous_id":1032840,"previous_guid":"54800200F80D11EA8561E7896FE90824","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"FCD0A6AB06B5476CB314478D43C508BF","order_id":1,"type_id":6,"title":"Section","source":{"title":"data download"}},{"id":1054724,"guid":"2E2B7C9088A848318A59CA9C5C0D102D","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Open the XENA browser (<\/div><div class = \"text-block\"><a href=\"https:\/\/xenabrowser.net\" style = \"text-decoration:underline;color:blue;cursor:pointer;\"><span style = \":;\">https:\/\/xenabrowser.net\/<\/span><\/a><\/div><div class = \"text-block\"><span style = \":UNDERLINE;\">) and select mutations<\/span><\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1032862,"guid":"170FDAC0F81811EA8561E7896FE90824","previous_id":1032842,"previous_guid":"D5F5F1F0F80D11EA8561E7896FE90824","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"E2E8B944D60F45EC92F4108E80C80866","order_id":1,"type_id":6,"title":"Section","source":{"title":"exclusion criteria"}},{"id":1054724,"guid":"4ECE0FF4E51649F2817B29D17D2101D2","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Apply the following exclusion criteria to the dataset: neoadjuvant chemotherapy, unknown tumor stage (Tx) or tumor stage <T2. <\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1033605,"guid":"D19051D0F8A511EABBE347F38446F4C3","previous_id":1032862,"previous_guid":"170FDAC