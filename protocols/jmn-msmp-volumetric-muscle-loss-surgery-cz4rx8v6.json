reagents":false,"has_versions":false,"id":87921,"image":{"source":"https://www.protocols.io/img/default_protocol.png","webp_source":null,"placeholder":"https://www.protocols.io/img/default_protocol.png","webp_placeholder":null},"image_attribution":"","in_trash":false,"is_bookmarked":false,"is_contact_suspended":false,"is_content_confidential":false,"is_content_warning":false,"is_doi_reserved":false,"is_entry_review_required":null,"is_in_pending_publishing":false,"is_in_transfer":false,"is_owner":true,"is_research":true,"is_retracted":false,"is_shared_directly":false,"is_subprotocol":null,"is_unlisted":false,"item_id":1162965,"journal":null,"journals":[],"keywords":"","last_modified":1695040697,"link":"","location":null,"manuscript_citation":"","materials":[],"materials_text":"","ownership_history":null,"parent_collections":[],"parent_protocols":[],"peer_reviewed":false,"protocol_references":"","public":true,"public_fork_note":"","published_on":1695040697,"references":[],"related_equipments":[],"related_materials":[],"reserved_doi":"","retraction_reason":null,"samples":{},"shared_access_id":106,"show_comparison":false,"sign_info":null,"state_version_id":55,"stats":{"is_voted":false,"number_of_views":0,"number_of_steps":21,"number_of_bookmarks":0,"number_of_comments":0,"number_of_bookmarked_comments":0,"number_of_steps_comments":0,"number_of_protocol_comments":0,"number_of_exports":0,"number_of_runs":0,"number_of_votes":0,"number_of_reagents":0,"number_of_equipments":0,"number_of_collections":0,"number_of_forks":{"private":0,"public":0},"number_of_accessible_forks":0},"status":{"id":1,"info":"We use this protocol and it's working"},"steps":[{"id":1791287,"guid":"DB97DC7AD8A142B389B8B635D4DE7D79","previous_id":0,"previous_guid":null,"section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"9kh22\",\"text\":\"Disinfect all surfaces and anesthetic equipment (induction chamber, nose cone) with Vimoba (Quip Laboratories VIMTAB).\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":87921,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"1","cases":[],"critical":null},{"id":1791288,"guid":"A7B51598348B43639A45E11B5D3D7F5C","previous_id":1791287,"previous_guid":"DB97DC7AD8A142B389B8B635D4DE7D79","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"3fuas\",\"text\":\"Prepare sterile surgical area and a separate surgical prep area (shaving, Rimadyl injection). Anesthesia should be available in both areas.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":87921,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"2","cases":[],"critical":null},{"id":1791289,"guid":"4407DAC13BDC4A638BA8DA4B3DABFDCC","previous_id":1791288,"previous_guid":"A7B51598348B43639A45E11B5D3D7F5C","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"9h1p8\",\"text\":\"Anesthetize the mouse. The following settings should be used for anesthesia during induction: 3-4% isoflurane, 100% oxygen, 1.5-2 liter per minute (LPM) flow rate. The following settings should be used for maintenance: 2-3.5% isoflurane, 100% oxygen, 1.5-2 LPM. Mouse should be continually monitored for regular breading and adequate anesthetic depth via toe pinch reflex. Anesthesia levels should be adjusted if breathing becomes to fast or slow, or if the mouse is responsive to toe pinch.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":87921,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"3","cases":[],"critical":null},{"id":1791290,"guid":"EC1698CF3F314D2DAE08CA104BACFF75","previous_id":1791289,"previous_guid":"4407DAC13BDC4A638BA8DA4B3DABFDCC","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"2jbua\",\"text\":\"Once the mouse is fully anesthetized, place the mouse under maintenance anesthesia at the surgical prep site.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":87921,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4","cases":[],"critical":null},{"id":1791291,"guid":"35F0723805614F69B5E7BF3A4F104C57","previous_id":1791290,"previous_guid":"EC1698CF3F314D2DAE08CA104BACFF75","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"a3q78\",\"text\":\"Administer 5 mg/kg Rimadyl via subcutaneous injection.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":87921,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"5","cases":[],"critical":null},{"id":1791292,"guid":"7510E54DA95441138979D6A80A65D905","previous_id":1791291,"previous_guid":"35F0723805614F69B5E7BF3A4F104C57","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"1p7pk\",\"text\":\"Shave the skin around the quadriceps region of both hind limbs.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":87921,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"6","cases":[],"critical":null},{"id":1791293,"guid":"99413FD544604501BC8BECE31457F11C","previous_id":1791292,"previous_guid":"7510E54DA95441138979D6A80A65D905","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"cmu4j\",\"text\":\"Spray the shaved regions with 70% ethanol and use sterile gauze to wipe off excess hair.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":87921,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"7","cases":[],"critical":null},{"id":1791294,"guid":"D1FA934BA9EE468FBB69E815EBA482BC","previous_id":1791293,"previous_guid":"99413FD544604501BC8BECE31457F11C","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"7q24n\",\"text\":\"Transfer mouse to sterile surgical area under maintenance anesthesia.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":87921,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"8","cases":[],"critical":null},{"id":1791295,"guid":"E3E87848B12E43E58DDF2D118EB7E3A3","previous_id":1791294,"previous_guid":"D1FA934BA9EE468FBB69E815EBA482BC","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"32iv8\",\"text\":\"Prior to making an incision, confirm anesthetic depth using toe pinch. If the animal is responsive, adjust the anesthetic dose and repeat under proper anesthetic depth is maintained.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":87921,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"9","cases":[],"critical":null},{"id":1791296,"guid":"2C2354B30CC3438BA65A704E784FABE2","previous_id":1791295,"previous_guid":"E3E87848B12E43E58DDF2D118EB7E3A3","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"flan0\",\"text\":\"Sterilize the incision area about the mouse quadriceps muscle by spraying 70% ethanol. Wipe off excess ethanol with a sterilie gauze and wait for skin to completely dry.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":87921,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"10","cases":[],"critical":null},{"id":1791297,"guid":"36A71039604C4E4BB955AB49B97B1F39","previous_id":1791296,"previous_guid":"2C2354B30CC3438BA65A704E784FABE2","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"8jaop\",\"text\":\"Move the tail underneath the leg to be operated on, and tape down the foot of the leg prior to making the incision.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":87921,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"11","cases":[],"critical":null},{"id":1791298,"guid":"F7199B8F03FE453DAF8E5A37DA87BE28","previous_id":1791297,"previous_guid":"36A71039604C4E4BB955AB49B97B1F39","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"b3gf\",\"text\":\"Make a 1 cm longitudinal skin incision just above the quadriceps muscle.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":87921,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"12","cases":[],"critical":null},{"id":1791299,"guid":"F9135F321A494CDEA55DD6FBC05E7D97","previous_id":1791298,"previous_guid":"F7199B8F03FE453DAF8E5A37DA87BE28","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"2gpgk\",\"text\":\"Remove the fascia overlaying the quadriceps muscle using forceps and scissors.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":87921,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"13","cases":[],"critical":null},{"id":1791300,"guid":"F51E498D610743E291DD69AF38E10F85","previous_id":1791299,"previous_guid":"F9135F321A494CDEA55DD6FBC05E7D97","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"fr7i2\",\"text\":\"At the muscle portion near the kneecap, grip the quadriceps muscle with tweezers and make an initial ~3 mm cut through the cross-section of the muscle. Then, cut ~3 mm of the muscle along the longitudinal plane of the muscles (i.e. parallel to the underlying tibia), and then pull the muscle up and perform a final cut to remove the muscle. The muscle defect should be approximately 3 mm x 3 mm x 4 mm in size.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":87921,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"14","cases":[],"critical":null},{"id":1791301,"guid":"54BC738B1E5C46738F1DEB1A8FEBE4A1","previous_id":1791300,"previous_guid":"F51E498D610743E291DD69AF38E10F85","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"fjhpi\",\"text\":\"Using the Abadie cuvette (Moria 1121B or similar), place on scoop of PCL particles into the muscle defect.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":87921,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"15","cases":[],"critical":null},{"id":1791302,"guid":"3F49BA01EDE84A19B1788DAB022E005C","previous_id":1791301,"previous_guid":"54BC738B1E5C46738F1DEB1A8FEBE4A1","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"605mm\",\"text\":\"Suture the muscle to close the wound.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":87921,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"16","cases":[],"critical":null},{"id":1791303,"guid":"7DB2EE9E8FC345B0B137F676793E89B0","previous_id":1791302,"previous_guid":"3F49BA01EDE84A19B1788DAB022E005C","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"eq3eb\",\"text\":\"Repeat process on the other leg, starting from Step 10.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":87921,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"17","cases":[],"critical":null},{"id":1791304,"guid":"32DC233C3B8B48C793DC2BA166172545","previous_id":1791303,"previous_guid":"7DB2EE9E8FC345B0B137F676793E89B0","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"6k1rk\",\"text\":\"Place mouse in clean cage on a heat pad to enable recovery. Remove the cage from the heat pad once all mice in the cage are alert and ambulatory, and do not appear to be in distress. Note: If mice are out for \\u003e1 hour, place a water gel pack or water bottle in cage to ensure they remain hydrated.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":87921,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"18","cases":[],"critical":null},{"id":1791305,"guid":"F68A884C0B744BAC95F3616B2AA9BAA7","previous_id":1791304,"previous_guid":"32DC233C3B8B48C793DC2BA166172545","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"944d4\",\"text\":\"Sterilize tools using the bead sterilizer for 30 sec at 225°C.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":87921,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"19","cases":[],"critical":null},{"id":1791306,"guid":"1A6466CCF34E462BA1CF4FEAE9E797DA","previous_id":1791305,"previous_guid":"F68A884C0B744BAC95F3616B2AA9BAA7","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"6k653\",\"text\":\"Monitor animals daily following surgery for the first week to ensure mice are not in pain or distress. If any mice are in pain or distress, follow necessary procedures to treat the pain or euthanize the mice according to the approved institutional animal protocol.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":87921,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"20","cases":[],"critical":null},{"id":1791307,"guid":"835445A07702428B835CDBFD68E34ADF","previous_id":1791306,"previous_guid":"1A6466CCF34E462BA1CF4FEAE9E797DA","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"ff61c\",\"text\":\"Sutures should be removed if still present after 2 weeks. Mice should be monitored regularly until the terminal harvest timepoint.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":87921,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"21","cases":[],"critical":null}],"template_id":1,"title":"JMN-MSMP Volumetric Muscle Loss Surgery","title_html":"JMN-MSMP Volumetric Muscle Loss Surgery","type_id":1,"units":[{"id":1,"type_id":3,"name":"µL","can_manage":0,"read_only":0},{"id":2,"type_id":3,"name":"mL","can_manage":0,"read_only":0},{"id":3,"type_id":3,"name":"L","can_manage":0,"read_only":0},{"id":4,"type_id":3,"name":"µg","can_manage":0,"read_only":0},{"id":5,"type_id":3,"name":"mg","can_manage":0,"read_only":0},{"id":6,"type_id":3,"name":"g","can_manage":0,"read_only":0},{"id":7,"type_id":3,"name":"kg","can_manage":0,"read_only":0},{"id":8,"type_id":3,"name":"ng","can_manage":0,"read_only":0},{"id":9,"type_id":3,"name":"Hz","can_manage":0,"read_only":0},{"id":10,"type_id":24,"name":"°C","can_manage":0,"read_only":0},{"i