protocols":[],"peer_reviewed":false,"public":true,"public_fork_note":"","published_on":1633960941,"references":[],"related_equipments":[],"retraction_reason":null,"shared_access_id":100,"show_comparison":false,"sign_info":null,"state_version_id":0,"stats":{"is_voted":false,"number_of_views":37,"number_of_steps":6,"number_of_bookmarks":0,"number_of_comments":0,"number_of_bookmarked_comments":0,"number_of_steps_comments":0,"number_of_protocol_comments":0,"number_of_exports":1,"number_of_runs":0,"number_of_votes":0,"number_of_reagents":2,"number_of_equipments":0,"number_of_collections":0,"number_of_forks":{"private":0,"public":0},"number_of_accessible_forks":0},"status":{"id":1,"info":"We use this protocol and it’s working"},"steps":[{"id":697570,"guid":"00048E40181111E994D0F9D311A2A63D","previous_id":0,"previous_guid":null,"section":"Potassium Chloride Solution","section_color":"#08b6f3","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"3f225\",\"text\":\"  of potassium chloride (KCl) solution is prepared at a concentration of 2 mEq/mL.  \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[{\"key\":0,\"offset\":0,\"length\":1}],\"data\":[]},{\"key\":\"f3skv\",\"text\":\"\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":{\"0\":{\"type\":\"amount\",\"mutability\":\"\",\"data\":{\"amount\":\"0.3\",\"guid\":\"10E8D290181E11E994D0F9D311A2A63D\",\"id\":0,\"label\":\"\",\"step_guid\":\"00048E40181111E994D0F9D311A2A63D\",\"unit\":2}}}}","data":null,"protocol_id":19295,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"1","cases":[],"critical":null},{"id":697701,"guid":"CE5700B0181C11E994D0F9D311A2A63D","previous_id":697570,"previous_guid":"00048E40181111E994D0F9D311A2A63D","section":"Anesthesia","section_color":"#1446e7","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"bk9rg\",\"text\":\"The animal (Sprague-Dawley female rat) is anesthetised with an intraperitoneal injection of ketamine (90 mg/kg) and xylazine (10 mg/kg). Anesthesia is maintained with a ketamine (30 mg/kg) injection approximately every hour.  \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":{}}","data":null,"protocol_id":19295,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"2","cases":[],"critical":null},{"id":697714,"guid":"FA8CCB00181D11E994D0F9D311A2A63D","previous_id":697701,"previous_guid":"CE5700B0181C11E994D0F9D311A2A63D","section":"Surgical Preparation","section_color":"#E57785","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"bj23a\",\"text\":\"A midline cervical incision is made to access the left cervical vagus nerve. Under a dissection microscope, the vagus nerve is isolated (5-7 mm) from the carotid artery and surrounding tissue and placed on a custom 3D-printed nerve holder. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":{}}","data":null,"protocol_id":19295,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"3","cases":[],"critical":null},{"id":697715,"guid":"BB4C0E00181E11E994D0F9D311A2A63D","previous_id":697714,"previous_guid":"FA8CCB00181D11E994D0F9D311A2A63D","section":"CFMA Insertion","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"7gacv\",\"text\":\"The Carbon Fiber Microelectrode Array (CFMA) is connected to a neural interface processor (Grapevine, Ripple) through a front-end headstage (Nano 2, Ripple). The headstage is controlled by a micromanipulator for accurate insertion of CFMA fibers into the vagus nerve. A small camera (MS100, Teslong) is positioned in the surgical opening to visulaize the alignment and insertion of CFMA fibers into the vagus nerve. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":{}}","data":null,"protocol_id":19295,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4","cases":[],"critical":null},{"id":698147,"guid":"800AFDE018FC11E9A100D7032F80C8E5","previous_id":697715,"previous_guid":"BB4C0E00181E11E994D0F9D311A2A63D","section":"Experiment","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"f5kvs\",\"text\":\"Once the CFMA fibers are inserted, a recording trial is intitiated. Forty seconds into the recording trial, 0.3 mL of KCl (2 mEq/mL) is applied on the vagus nerve.  \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":{}}","data":null,"protocol_id":19295,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"5","cases":[],"critical":null},{"id":698148,"guid":"98D0645018FC11E9A100D7032F80C8E5","previous_id":698147,"previous_guid":"800AFDE018FC11E9A100D7032F80C8E5","section":"Data Analysis","section_color":"#FFED92","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"d1vm\",\"text\":\"Recorded signals are sorted for spikes using Plexon Offline Sorter and analyzed using MATLAB to calculates peak-to-peak voltage of spikes, conduction velocity and noise floor.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":{}}","data":null,"protocol_id":19295,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"6","cases":[],"critical":null}],"template_id":1,"title":"Vagus Nerve Recordings Using Carbon Fiber Microelectrode Array (CFMA)","title_html":"Vagus Nerve Recordings Using Carbon Fiber Microelectrode Array (CFMA)","type_id":1,"units":[{"id":1,"type_id":3,"name":"µL","can_manage":0},{"id":2,"type_id":3,"name":"mL","can_manage":0},{"id":3,"type_id":3,"name":"L","can_manage":0},{"id":4,"type_id":3,"name":"µg","can_manage":0},{"id":5,"type_id":3,"name":"mg","can_manage":0},{"id":6,"type_id":3,"name":"g","can_manage":0},{"id":7,"ty