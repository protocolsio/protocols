:false,"public":true,"public_fork_note":"","published_on":1656414073,"references":[],"related_equipments":[],"retraction_reason":null,"shared_access_id":16,"show_comparison":false,"sign_info":null,"state_version_id":77,"stats":{"is_voted":false,"number_of_views":0,"number_of_steps":7,"number_of_bookmarks":0,"number_of_comments":0,"number_of_bookmarked_comments":0,"number_of_steps_comments":0,"number_of_protocol_comments":0,"number_of_exports":0,"number_of_runs":0,"number_of_votes":0,"number_of_reagents":0,"number_of_equipments":0,"number_of_collections":0,"number_of_forks":{"private":0,"public":0},"number_of_accessible_forks":0},"status":{"id":1,"info":"I use this protocol and it’s working"},"steps":[{"id":1431043,"guid":"763CA880F6D011EC83FAE3651B3F176A","previous_id":0,"previous_guid":null,"section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"2gcnr\",\"text\":\"Grow the cells 48-72 h before injection (around 70-80% confluent).\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":65540,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"1","cases":[],"critical":null},{"id":1431044,"guid":"770EC590F6D011EC83FAE3651B3F176A","previous_id":1431043,"previous_guid":"763CA880F6D011EC83FAE3651B3F176A","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"7m29t\",\"text\":\"Wash the cells with PBS and detach with 1 mL trypsin. Stop the reaction with 3-4 mL complete medium (+10% FBS). Aspirate up and down, and filter through a 40 um filter into sterile FACS tubes.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":65540,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"2","cases":[],"critical":null},{"id":1431045,"guid":"800B7DF0F6D011EC83FAE3651B3F176A","previous_id":1431044,"previous_guid":"770EC590F6D011EC83FAE3651B3F176A","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"7v723\",\"text\":\"Count the cells. Calculate the total required number of cells and aliquot into 1.5 mL eppendorf tubes (or 15 mL tubes if more cells needed)\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":65540,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"3","cases":[],"critical":null},{"id":1431046,"guid":"86F44A70F6D011EC83FAE3651B3F176A","previous_id":1431045,"previous_guid":"800B7DF0F6D011EC83FAE3651B3F176A","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"1ksp7\",\"text\":\"Spin down at RT in a benchtop centrifuge (3 min, 1200 rpm)\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":65540,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4","cases":[],"critical":null},{"id":1431047,"guid":"8AA173F0F6D011EC83FAE3651B3F176A","previous_id":1431046,"previous_guid":"86F44A70F6D011EC83FAE3651B3F176A","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"b77up\",\"text\":\"Remove the supernatant and resuspend the pelleted cells in the appropriate volume of PBS. Transfer to sterile Bijou tubes (for easier handling later)\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":65540,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"5","cases":[],"critical":null},{"id":1431048,"guid":"8E8739A0F6D011EC83FAE3651B3F176A","previous_id":1431047,"previous_guid":"8AA173F0F6D011EC83FAE3651B3F176A","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"ef5lo\",\"text\":\"Keep the cells on ice. Place the mice in the heating chamber.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":65540,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"6","cases":[],"critical":null},{"id":1431049,"guid":"94311D80F6D011EC83FAE3651B3F176A","previous_id":1431048,"previous_guid":"8E8739A0F6D011EC83FAE3651B3F176A","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"1t75v\",\"text\":\"Gently pipette the cell suspension up and down with a P1000 before each cage injection. Prepare the injections for all of the mice in the cage, aspirating around 50 uL extra. Just before the injection, mix the cells in the syringe by moving the plunger up and down. Adjust the volume, pick up the mouse, and inject. Repeat the process for each cage.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":65540,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"7","cases":[],"critical":null}],"template_id":4,"title":"Intravenous injections","title_html":"\u003cp\u003eIntravenous injections\u003c/p\u003e","type_id":1,"units":[{"id":1,"type_id":3,"name":"µL","can_manage":0},{"id":2,"type_id":3,"name":"mL","can_manage":0},{"id":3,"type_id":3,"name":"L","can_manage":0},{"id":4,"type_id":3,"name":"µg","can_manage":0},{"id":5,"type_id":3,"name":"mg","can_manage":0},{"id":6,"type_id":3,"name":"g","can_manage":0},{"id":7,"type_id":3,"name":"kg","can_manage":0},{"id":8,"type_id":3,"name":"ng","can_manage":0},{"id":9,"type_id":3,"name":"Hz","can_manage":0},{"id":10,"type_id":24,"name":"°C","can_manage":0},{"id":11,"type_id":24,"name":"°К","can_manage":0},{"id":12,"type_id":24,"name":"°F","can_manage":0},{"id":13,"type_id":25,"name":"Mass Percent","can_manage":0},{"id":14,"type_id":25,"name":"% volume","ca