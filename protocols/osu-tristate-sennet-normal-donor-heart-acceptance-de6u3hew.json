\":[],\"data\":{}},{\"key\":\"8kq60\",\"text\":\"To be considered a “normal” donor heart, the donor has no known history of heart failure, no current infection, and limited inflammation.\",\"type\":\"align-justify\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":101300,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"1","cases":[],"critical":null},{"id":2087385,"guid":"6079E116EFE74793B6090959E71DBED9","previous_id":2087384,"previous_guid":"A3757C5DE804416A93FBA985151479B1","section":"\u003cp\u003e\u003cstrong\u003eExclusion Criteria\u003c/strong\u003e\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"aajss\",\"text\":\"The donor is 17 years of age or younger, HBV, HCV, or HIV positive, or has an active COVID-19 infection.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":101300,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"2","cases":[],"critical":null}],"template_id":0,"title":"OSU TriState SenNet Normal Donor Heart Acceptance Criteria","title_html":"OSU TriState SenNet Normal Donor Heart Acceptance Criteria","type_id":1,"units":[{"id":1,"type_id":3,"name":"µL","can_manage":0,"read_only":0},{"id":2,"type_id":3,"name":"mL","can_manage":0,"read_only":0},{"id":3,"type_id":3,"name":"L","can_manage":0,"read_only":0},{"id":4,"type_id":3,"name":"µg","can_manage":0,"read_only":0},{"id":5,"type_id":3,"name":"mg","can_manage":0,"read_only":0},{"id":6,"type_id":3,"name":"g","can_manage":0,"read_only":0},{"id":7,"type_id":3,"name":"kg","can_manage":0,"read_only":0},{"id":8,"type_id":3,"name":"ng","can_manage":0,"read_only":0},{"id":9,"type_id":3,"name":"Hz","can_manage":0,"read_only":0},{"id":10,"type_id":24,"name":"°C","can_manage":0,"read_only":0},{"id":11,"type_id":24,"name":"°К","can_manage":0,"read_only":0},{"i