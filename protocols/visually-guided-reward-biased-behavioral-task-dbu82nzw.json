ll},{"id":2007240,"guid":"49F77BAC3B204B548C45B173405E91E7","previous_id":2007130,"previous_guid":"7EF90F55DB3348439CA9BA246EFB55CD","section":"\u003cp\u003eBehavioral Task\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"dl0oc\",\"text\":\"The central cue would then extinguish while a second target appeared on the screen's periphery, where the monkeys would have to saccade to and fixate for another 4 s before receiving a reward. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":97920,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"2","cases":[],"critical":null},{"id":2007241,"guid":"1422F6FEE95145228FBFF30E965D0997","previous_id":2007240,"previous_guid":"49F77BAC3B204B548C45B173405E91E7","section":"\u003cp\u003eBehavioral Task\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"4i054\",\"text\":\"Whether the second target appeared to the left or right of the center cue was associated with whether successful completion of a trial would result in either a big or small reward. The second target's location/reward-size association would swap after 15 to 45 trials or 50 to 100 trials.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":97920,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"3","cases":[],"critical":null},{"id":2007243,"guid":"D97CA4298CFC487088166E2B05EE2306","previous_id":2007241,"previous_guid":"1422F6FEE95145228FBFF30E965D0997","section":"\u003cp\u003eBehavioral Task\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"dfvn8\",\"text\":\"The reward was a liquid food mixture (355 ml of Plus Nutrition Shake Vanilla, 355 ml of water, blended with two large bananas, and eight biscuits) delivered via a mouthpiece located near the animal's mouth at small and large reward volumes of 1.5 and 2.8 mL or 0.1 and 0.3 mL, respectively.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":97920,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4","cases":[],"critical":null},{"id":2007244,"guid":"7DEA7B70A55448F4B57FED6BCB958F77","previous_id":2007243,"previous_guid":"D97CA4298CFC487088166E2B05EE2306","section":"\u003cp\u003eBehavioral Task\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"96ler\",\"text\":\"Monkeys had a large reward probability of either 50% or 25% and an intertrial interval length of a fixed 7.5 s or a fixed value of 5 to 15 s per session. Subjects would perform between 500 and 1200 individual trials per recording session. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":97920,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"5","cases":[],"critical":null}],"template_id":0,"title":"Visually-Guided Reward-Biased Behavioral Task","title_html":"Visually-Guided Reward-Biased Behavioral Task","type_id":1,"units":[{"id":1,"type_id":3,"name":"µL","can_manage":0,"read_only":0},{"id":2,"type_id":3,"name":"mL","can_manage":0,"read_only":0},{"id":3,"type_id":3,"name":"L","can_manage":0,"read_only":0},{"id":4,"type_id":3,"name":"µg","can_manage":0,"read_only":0},{"id":5,"type_id":3,"name":"mg","can_manage":0,"read_only":0},{"id":6,"type_id":3,"name":"g","can_manage":0,"read_only":0},{"id":7,"type_id":3,"name":"kg","can_manage":0,"read_only":0},{"id":8,"type_id":3,"name":"ng","can_manage":0,"read_only":0},{"id":9,"type_id":3,"name":"Hz","can_manage":0,"read_only":0},{"id":10,"type_id":24,"name":"°C","can_manage":0,"read_only":0},{"id":11,"type_id":2