tion":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"4eajl\",\"text\":\"Let isolation center know samples were received and in what condition. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":57865,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"2","cases":[],"critical":null},{"id":1340464,"guid":"F5C49DB0878B11EC906AEB63B7D8A22A","previous_id":1340463,"previous_guid":"D492B0F0878B11EC906AEB63B7D8A22A","section":"\u003cp\u003eRecieving Samples\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"8nv2o\",\"text\":\"Print QR barcodes for the acinar samples in this format and tape onto the tubes. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"ftp62\",\"text\":\"\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"8fp5u\",\"text\":\"HIGI#ACIN (# being the next sequential number)\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":57865,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"3","cases":[],"critical":null},{"id":1340465,"guid":"17EB23A0878C11EC906AEB63B7D8A22A","previous_id":1340464,"previous_guid":"F5C49DB0878B11EC906AEB63B7D8A22A","section":"\u003cp\u003eRecieving Samples\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"e4qvp\",\"text\":\"Store tubes in Yalow -80Â°C freezer in  \\\"IIDP - HIGI Acinar Samples\\\" box.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":57865,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4","cases":[],"critical":null},{"id":1340466,"guid":"346EA380878C11EC906AEB63B7D8A22A","previous_id":1340465,"previous_guid":"17EB23A0878C11EC906AEB63B7D8A22A","section":"\u003cp\u003eExtracting DNA\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"euelj\",\"text\":\"Thaw acinar tubes on ice. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":57865,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"5","cases":[],"critical":null},{"id":1340467,"guid":"3FE5AAB0878C11EC906AEB63B7D8A22A","previous_id":1340466,"previous_guid":"346EA380878C11EC906AEB63B7D8A22A","section":"\u003cp\u003eExtracting DNA\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"fs8e3\",\"text\":\"Use DNA extraction protocol for DNeasy Blood \\u0026 Tissue Kit (https://protocols.io/view/dna-extraction-qiagen-dneasy-blood-amp-tissue-kit-bjngkmbw). \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[{\"key\":0,\"offset\":59,\"length\":84}],\"data\":{}}],\"entityMap\":{\"0\":{\"type\":\"link\",\"mutability\":\"\",\"data\":{\"guid\":\"69BF618C878C11ECB1A00A58A9FEAC02\",\"url\":\"https://protocols.io/view/dna-extraction-qiagen-dneasy-blood-amp-tissue-kit-bjngkmbw\"}}}}","data":null,"protocol_id":57865,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"6","cases":[],"critical":null},{"id":1340468,"guid":"7900CEB0878C11EC906AEB63B7D8A22A","previous_id":1340467,"previous_guid":"3FE5AAB0878C11EC906AEB63B7D8A22A","section":"\u003cp\u003eQC\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"359c1\",\"text\":\"After extraction, measure the concentration by nanodrop, taking account of 260/280 and 260/230 values as well. 260/280 value should be close to 1.8 and 260/230 should be around 1.8-2.0. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":57865,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"7","cases":[],"critical":null},{"id":1340469,"guid":"B5D1F5D0878C11EC906AEB63B7D8A22A","previous_id":1340468,"previous_guid":"7900CEB0878C11EC906AEB63B7D8A22A","section":"\u003cp\u003eQC\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"664l5\",\"text\":\"Take a portion of the sample to measure concentration by Qubit. Protocol can be found here: https://protocols.io/view/qubit-dsdna-assays-bnstmeen. For genotyping, Qubit concentration should be above 50 ng/uL and total DNA should be above 750 ng. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[{\"key\":0,\"offset\":92,\"length\":53}],\"data\":{}}],\"entityMap\":{\"0\":{\"type\":\"link\",\"mutability\":\"\",\"data\":{\"guid\":\"E6FAC99F878C11ECB1A00A58A9FEAC02\",\"url\":\"https://protocols.io/view/qubit-dsdna-assays-bnstmeen\"}}}}","data":null,"protocol_id":57865,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"8","cases":[],"critical":null},{"id":1340472,"guid":"FFD3BB00878C11EC906AEB63B7D8A22A","previous_id":1340476,"previous_guid":"F10703B0878D11EC906AEB63B7D8A22A","section":"\u003cp\u003eGenotyping\u003c/p\u003e","section_color":"#FFED92","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"b7tlm\",\"text\":\"Record sample info, nanodrop QC and qubit information in genotyping QC spreadsheet, which will be sent to the SFGF core whenever the entire batch is ready (follow this protocol: https://www.protocols.io/edit/sop54v1-tgd-genotypingsubmissionforcollaborators-b8jaruie).\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[{\"key\":0,\"offset\":178,\"length\":87}],\"data\":{}}],\"entityMap\":{\"0\":{\"type\":\"link\",\"mutability\":\"MUTABLE\",\"data\":{\"guid\":\"6FC84E171CA611ED84CD0A58A9FEAC02\",\"url\":\"https://www.protocols.io/edit/sop54v1-tgd-genotypingsubmissionforcollaborators-b8jaruie\"}}}}","data":null,"protocol_id":57865,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"10","cases":[],"critical":null},{"id":1340476,"guid":"F10703B0878D11EC906AEB63B7D8A22A","previous_id":1340469,"previous_guid":"B5D1F5D0878C11EC906AEB63B7D8A22A","section":"\u003cp\u003eQC\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"bglb2\",\"text\":\"Create QR codes that include sample name in this format: HIGI#DNA. Label should also include nanodrop concentration and QC info. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":57865,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"9","cases":[],"critical":null},{"id":1867309,"guid":"3BCCFB55C44040208D142B274F8DB66B","previous_id":1340472,"previous_guid":"FFD3BB00878C11EC906AEB63B7D8A22A","section":"\u003cp\u003eGenotyping\u003c/p\u003e","section_color":"#FFED92","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"8pses\",\"text\":\"As of 2023-12-01, samples were sent to be genotyped on Illumina's Infinium Omni Exome 2.5 arrays.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":57865,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"11","cases":[],"critical":null}],"template_id":4,"title":"SOP52v1_TGD_IIDP-HIGISampleProcessing","title_html":"\u003cp\u003eSOP52v1_TGD_IIDP-HIGISampleProcessing\u003c/p\u003e","type_id":1,"units":[{"id":1,"type_id":3,"name":"ÂµL","can_manage":0,"read_only":0},{"id":2,"type_id":3,"name":"mL","can_manage":0,"read_only":0},{"id":3,"type_id":3,"name":"L","can_manage":0,"read_only":0},{"id":4,"type_id":3,"name":"Âµg","can_manage":0,"read_only":0},{"id":5,"type_id":3,"name":"mg","can_manage":0,"r