al_ids":"","duration":0,"original_id":0,"number":"1","cases":[],"critical":null},{"id":1912589,"guid":"2ECCE14C766641188BC5D3CD4E7C0630","previous_id":1912591,"previous_guid":"48EB49AB71D646D8BB9840F6AEE55E2A","section":"\u003cp\u003eAssessment\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"4fdv8\",\"text\":\"    \\nWith one hand use the tweezers to pick up one sticker, allowing it to sit on a single prong with the\\nadhesive side freely visible.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":93855,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"3","cases":[],"critical":null},{"id":1912590,"guid":"0E1488DAEAC74D0D8DBE17928DA9E07D","previous_id":1912589,"previous_guid":"2ECCE14C766641188BC5D3CD4E7C0630","section":"\u003cp\u003eAssessment\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"egl0k\",\"text\":\"    \\nWith the free hand grab one mouse and manually restrain with a “scruff technique” immobilizing the head and forepaws.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":93855,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4","cases":[],"critical":null},{"id":1912591,"guid":"48EB49AB71D646D8BB9840F6AEE55E2A","previous_id":1912588,"previous_guid":"2B699E2077C94C7697E4D09465B0A573","section":"\u003cp\u003eAcclimation\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"97r9o\",\"text\":\"Randomize the order of mice for assessment and record this order in your lab notebook\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":93855,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"2","cases":[],"critical":null},{"id":1912600,"guid":"D592CEE7BE094A54ABA529F8E1ED4B3E","previous_id":1912590,"previous_guid":"0E1488DAEAC74D0D8DBE17928DA9E07D","section":"\u003cp\u003eAssessment\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"b3hus\",\"text\":\"Using the tweezers gently press the adhesive side of the sticker onto the snout of the restrained mouse, gently putting down the tweezers and picking up a stopwatch.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":93855,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"5","cases":[],"critical":null},{"id":1912601,"guid":"6C1D5DAF5E024A258AC4FF4670BB94E2","previous_id":1912600,"previous_guid":"D592CEE7BE094A54ABA529F8E1ED4B3E","section":"\u003cp\u003eAssessment\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":true,"step":"{\"blocks\":[{\"key\":\"7gh9v\",\"text\":\"Avoid placing the tweezers onto a hard surface or dropping them, as the high pitch noise will disturb the mice.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":93855,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"5.1","cases":[],"critical":null},{"id":1912602,"guid":"8868399F28F444908A16A1D9CB67CF09","previous_id":1912601,"previous_guid":"6C1D5DAF5E024A258AC4FF4670BB94E2","section":"\u003cp\u003eAssessment\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"1dcrr\",\"text\":\"    \\nPlace the restrained mouse into a clean, empty, cage and allow the mouse to remove the sticker from the snout, using its forepaws. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":93855,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"6","cases":[],"critical":null},{"id":1912603,"guid":"EB38E119308B4EDA93D4D66434D56C4E","previous_id":1912602,"previous_guid":"8868399F28F444908A16A1D9CB67CF09","section":"\u003cp\u003eAssessment\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"1n3ff\",\"text\":\"Stop the timer once the sticker has been removed completely from the snout and remove the sticker (now on the forepaws) from the mouse. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":93855,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"7","cases":[],"critical":null},{"id":1912604,"guid":"410D41E147674DE9A2DF606056081D8D","previous_id":1912603,"previous_guid":"EB38E119308B4EDA93D4D66434D56C4E","section":"\u003cp\u003eAssessment\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"7ldgs\",\"text\":\"Allow the mouse 30 seconds to recover in the clean cage.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":93855,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"8","cases":[],"critical":null},{"id":1912605,"guid":"EEAC265FA18641EDA701C6245A920832","previous_id":1912604,"previous_guid":"410D41E147674DE9A2DF606056081D8D","section":"\u003cp\u003eAssessment\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"9ifhr\",\"text\":\"Repeat 2x times for a total of 3 replications.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":93855,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"9","cases":[],"critical":null},{"id":1912606,"guid":"393820E12AC34E62B966BD742BB7E8B0","previous_id":1912605,"previous_guid":"EEAC265FA18641EDA701C6245A920832","section":"\u003cp\u003eAssessment\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"bjtei\",\"text\":\"Move on to the next mouse.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":93855,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"10","cases":[],"critical":null},{"id":1912607,"guid":"C26EAE09079649E883C9EE4801566B95","previous_id":1912606,"previous_guid":"393820E12AC34E62B966BD742BB7E8B0","section":"\u003cp\u003eAssessment\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":true,"step":"{\"blocks\":[{\"key\":\"e509n\",\"text\":\"Clean the cage with 70% EtOH or use a new cage for the assessment when moving onto the next home cage.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":93855,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"10.1","cases":[],"critical":null},{"id":1912609,"guid":"743A7FB375D8479DB44367BE207E39FF","previous_id":1912607,"previous_guid":"C26EAE09079649E883C9EE4801566B95","section":"\u003cp\u003eAssessment\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"cgrc1\",\"text\":\"Record the adhesive removal time as the average of 3 replications.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":93855,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"11","cases":[],"critical":null}],"template_id":4,"title":"Sticker Removal","title_html":"Sticker Removal","type_id":1,"units":[{"id":1,"type_id":3,"name":"µL","can_manage":0,"read_only":0},{"id":2,"type_id":3,"name":"mL","can_manage":0