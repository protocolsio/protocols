ges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":107457,"case_id":0,"critical_ids":"","duration":1200,"original_id":0,"number":"2","cases":[],"critical":null},{"id":2226733,"guid":"62B9129108404567994CF98060B4D5BD","previous_id":2226670,"previous_guid":"95B2F647CCD54D60A9F79F9893619A2F","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"6d7jr\",\"text\":\"Dilute the pur20ified LRRK2RCKW to 22nM with the reaction buffer (50mM HEPES buffer (pH7.5), 10mM MgCl2, 1% glycerol, 1mM DTT, 0.2mg/mL BSA, 0.01% Tween20, 5µM AQT0615) and add 10µL to each well, except the 0% control wells,  with a multichannel pipette (Eppendorf). Add   10µL of pure reaction buffer to the 0% control wells.\",\"type\":\"align-justify\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"sup\",\"offset\":27,\"length\":4},{\"style\":\"sub\",\"offset\":102,\"length\":1}],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":107457,"case_id":0,"critical_ids":"","duration":1200,"original_id":0,"number":"3","cases":[],"critical":null},{"id":2226737,"guid":"668655F6F82248D393E257A02BD7CAC6","previous_id":0,"previous_guid":null,"section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"6gea9\",\"text\":\"Make 10mM, 100µM and 1.6µM stock solutions of the compounds in DMSO.\",\"type\":\"align-justify\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":107457,"case_id":0,"critical_ids":"","duration":3600,"original_id":0,"number":"1","cases":[],"critical":null},{"id":2226738,"guid":"E6E8440D8F73467CB494EA55C01741E1","previous_id":2226733,"previous_guid":"62B9129108404567994CF98060B4D5BD","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"d4vq9\",\"text\":\"Pipett 5nL 100mM ATP stock into each well with the ECHO acoustic dispenser (Labcyte).\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":107457,"case_id":0,"critical_ids":"","duration":600,"original_id":0,"number":"4","cases":[],"critical":null},{"id":2226741,"guid":"DF287A1242B949AFADF402D52D83A93B","previous_id":2226738,"previous_guid":"E6E8440D8F73467CB494EA55C01741E1","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"3suup\",\"text\":\"Centrifuge the plates at 1500gn for 2min, before incubating the reaction at room temperature for 4 h.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"sub\",\"offset\":30,\"length\":1}],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":107457,"case_id":0,"critical_ids":"","duration":14400,"original_id":0,"number":"5","cases":[],"critical":null},{"id":2226742,"guid":"67CA6DCC7A124D129071C5C050F88441","previous_id":2226741,"previous_guid":"DF287A1242B949AFADF402D52D83A93B","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"8hmch\",\"text\":\"Measure the fluorescence after 360nm excitation at 487nm emission with a PHERAstar plate reader (BMG Labtech).\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":107457,"case_id":0,"critical_ids":"","duration":600,"original_id":0,"number":"6","cases":[],"critical":null},{"id":2226743,"guid":"C035717327134998B7FA6F5192FF1558","previous_id":2226742,"previous_guid":"67CA6DCC7A124D129071C5C050F88441","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"drtes\",\"text\":\"Normalize the response to the average 0% and 100% controls. Calculate the IC50 values via non-linear regression of the log[inhibitor] vs. the normalized response with GraphPad Prism 8.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"style