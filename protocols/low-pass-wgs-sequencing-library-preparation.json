_remove_fork":1,"fork_id":null,"url":"https:\/\/www.protocols.io\/view\/low-pass-wgs-sequencing-library-preparation-bwqdpds6","forks_count":{"private":0,"public":0},"access":{"can_view":1,"can_remove":0,"can_add":0,"can_edit":0,"can_publish":0,"can_get_doi":0,"can_share":1,"can_move":1,"can_move_outside":0,"can_transfer":1,"can_download":1,"is_locked":0},"guid":"C3E04A80E8AB11EBA2F2259E603F9768","state_version_id":480,"steps":[{"id":1216178,"guid":"C79ED920E8AB11EBA2F2259E603F9768","previous_id":null,"previous_guid":null,"modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"6B922E6CEF6640CE85EA5D41723431D6","order_id":1,"type_id":6,"title":"Section","source":{"title":"DNA extraction "}},{"id":1054724,"guid":"FB42486D120A49B8B67FDCD1E92440B1","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Extract 10-50ng of tumor DNA from FFPE needle-core biopsy or resection using QIAamp DNA FFPE Tissue Kit<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":12600,"critical":null,"critical_id":null,"duration":7200},{"id":1216181,"guid":"616032A0E8AE11EBA2F2259E603F9768","previous_id":1216182,"previous_guid":"8AEF6370E8AE11EBA2F2259E603F9768","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"5D184AC884624F2D918358919D489AC0","order_id":1,"type_id":6,"title":"Section","source":{"title":"DNA extraction "}},{"id":1054724,"guid":"84398AA02B8B489CBFB223B25DA0FEA4","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Check sonicated DNA size distribution using Agilent Bioanalyzer 2100 system or similar electrophoresis<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":12600,"critical":null,"critical_id":null,"duration":3600},{"id":1216182,"guid":"8AEF6370E8AE11EBA2F2259E603F9768","previous_id":1216178,"previous_guid":"C79ED920E8AB11EBA2F2259E603F9768","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"B9259216E42740AEABCFB4B7BE68111A","order_id":1,"type_id":6,"title":"Section","source":{"title":"DNA extraction "}},{"id":1054724,"guid":"158F86D0176447ACB91967ED706C0CA5","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Quantify extracted DNA and then sonicate 10-50ng of DNA to fragment to ~150bp fragments.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":12600,"critical":null,"critical_id":null,"duration":1800},{"id":1216184,"guid":"4BFB18C0E8AF11EBA2F2259E603F9768","previous_id":1216181,"previous_guid":"616032A0E8AE11EBA2F2259E603F9768","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"9B84F8D6E10C455EBBC92390F9E56684","order_id":1,"type_id":6,"title":"Section","source":{"title":"Library Preparation"}},{"id":1054724,"guid":"CDE670DF291B46E6B12BD3A9C92D6D6E","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Using 10-50 ng of cfDNA from step 2 and KAPA HyperPrep Kit, prepare and carry out end-repair and A-tailing reaction per manufacturer\u2019s protocol (<\/div><div class = \"text-block\"><a href=\"https:\/\/rochesequencingstore.com\/wp-content\/uploads\/2017\/10\/KAPA-HyperPrep-Kit_KR0961-%E2%80%93-v7.19.pdf\" style = \"text-decoration:underline;color:blue;cursor:pointer;\"><span style = \":;\">https:\/\/rochesequencingstore.com\/wp-content\/uploads\/2017\/10\/KAPA-HyperPrep-Kit_KR0961-%E2%80%93-v7.19.pdf<\/span><\/a><\/div><div class = \"text-block\">)<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":10800,"critical":null,"critical_id":null,"duration":3600},{"id":1216186,"guid":"7F4BFFA0E8AF11EBA2F2259E603F9768","previous_id":1216184,"previous_guid":"4BFB18C0E8AF11EBA2F2259E603F9768","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"899D9F82A2F54CF4B7DD1F920C8DC42A","order_id":1,"type_id":6,"title":"Section","source":{"title":"Library Preparation"}},{"id":1054724,"guid":"AD954123154949C8B57B835A44D0D50E","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">During step 4 incubations, anneal TSHTDM adaptor oligos to an appropriate stock concentration (100uM) by combining, in equal parts, i5 (sense) and i7 (antisense) adaptor oligonucleotides being mindful of the index sequences used. Anneal the single-stranded oligos by heating the mixture to 95C for 5 min and letting cool to RT for 30-60 minutes.<\/div><br\/><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":10800,"critical":null,"critical_id":null,"duration":3600},{"id":1216189,"guid":"93D6A290E8AF11EBA2F2259E603F9768","previous_id":1216186,"previous_guid":"7F4BFFA0E8AF11EBA2F2259E603F9768","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"6EAF62F41E63418EAF1ED40850E16BCF","order_id":1,"type_id":6,"title":"Section","source":{"title":"Library Preparation"}},{"id":1054724,"guid":"DF63EA184ED440DFB4BA59772F601C84","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">On ice, dilute above annealed adaptors such that 5 uL gives a 200:1, [adaptor]:[cfDNA], molar excess and combine with above end-repair and A-tailing reaction, ligase, and ligation buffer per manufacturer\u2019s protocol. Be careful not to allow above reagents and\/or ligation reaction to exceed 4-16C prior to next incubation step.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":10800,"critical":null,"critical_id":null,"duration":300},{"id":1216191,"guid":"CFD8B9E0E8AF11EBA2F2259E603F9768","previous_id":1216192,"previous_guid":"D1F97700E8AF11EBA2F2259E603F9768","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"1D3F4BC8313642ECB7B700F8224B283A","order_id":1,"type_id":6,"title":"Section","source":{"title":"Library Preparation"}},{"id":1054724,"guid":"6D4BF18944E74A2F8CBBC864FA183517","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Bead-purify ligation reaction: Add 88uL (0.8X vol.) well-mixed, room-temperature SPRI Ampure XP Bead slurry (Beckman-Coulter) and mix thoroughly by pipetting or vortexing. Incubate at room temperature for 10 minutes, separate supernatant from beads using magnet and discard supernatant.  Immediately add 200 uL of 80% EtOH to wash, remove EtOH and repeat wash. After removing second EtOH wash, very briefly spin-down and quickly remove excess EtOH.* Immediately resuspend beads in 20 ul of nH2O.<\/div><br\/><br\/><\/div>"}},{"id":1054725,"guid":"ADC029D0942E4854BE07D5344EFEB8AD","order_id":2,"type_id":26,"title":"notes","source":{"id":0,"parent_id":0,"uri":"","title":"","body":"<div class = \"text-blocks\"><div class = \"text-block\"><span style = \"font-weight:bold;\">*Note:  <\/span><span>It\u2019s very important not to let beads dry after removal of second EtOH wash. <\/span><span style = \":UNDERLINE;\">Proceed immediately to spin step, quickly remove remaining EtOH by pipet and resuspend in 20 uL nH2O. <\/span><span>It is not critical that every last bit of EtOH is removed - the risk of reduced yield from over-drying greatly outweighs any issues associated with a small amount of EtOH carry-over.<\/span><\/div><\/div>","created_on":0,"changed_on":0,"creator":{"name":" ","affiliation":null,"affiliations":[],"username":"","note":null,"link":null,"image":{"source":"","placeholder":""},"badges":[],"verified":0,"is_verified_user":false,"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false},"comments":[]}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":10800,"critical":null,"critical_id":null,"duration":1800},{"id":1216192,"guid":"D1F97700E8AF11EBA2F2259E603F9768","previous_id":1216189,"previous_guid":"93D6A290E8AF11EBA2F2259E603F9768","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"5D02B149003C4F9DAA3DCCDCEAD2558E","order_id":1,"type_id":6,"title":"Section","source":{"title":"Library Preparation"}},{"id":1054724,"guid":"A162221DDE264A9E9E9E4A044914F1E4","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Ligate 15 minutes at 20C in a thermocycler (lid heat \"off\").<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":10800,"critical":null,"critical_id":null,"duration":1200},{"id":1216193,"guid":"9CC9EFA0E8B011EBA2F2259E603F9768","previous_id":1216191,"previous_guid":"CFD8B9E0E8AF11EBA2F2259E603F9768","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"FB2F8FD8462645128A989C2D2C3C8BB2","order_id":1,"type_id":6,"title":"Section","source":{"title":"Library Amplification"}},{"id":1054724,"guid":"E7A7F37190344A75A5DC850D49DD79B8","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Prepare 50 ul PCR reaction using 25ul KAPA HiFi Hotstart 2x MasterMix and Illumina Library Amplification Primers (10 uM [final]),  20 ul of above post-ligation reaction (from step 7), and 5 uL of nH2O + primers.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#84CE84","section_duration":8700,"critical":null,"critical_id":null,"duration":600},{"id":1216195,"guid":"A47AEE20E8B011EBA2F2259E603F9768","previous_id":1216193,"previous_guid":"9CC9EFA0E8B011EBA2F2259E603F9768","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"27D0C47B99BE492D9BA5EED2F14A8A6A","order_id":1,"type_id":6,"title":"Section","source":{"title":"Library Amplification"}},{"id":1054724,"guid":"4FAAD6D914B34ED097B7394F1236E41D","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Amplify library using KAPA HiFi Hotstart temperature protocol for 8-12 cycles (60C annealing temperature).<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#84CE84","section_duration":8700,"critical":null,"critical_id":null,"duration":2700},{"id":1216196,"guid":"D5570470E8B011EBA2F2259E603F9768","previous_id":1216195,"previous_guid":"A47AEE20E8B011EBA2F2259E603F9768","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"CF573E62575C46D0B75CCC20F89CE787","order_id":1,"type_id":6,"title":"Section","source":{"title":"Library Amplification"}},{"id":1054724,"guid":"85A504D0791248D4AFAD9E3BA3A17DD5","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Bead purify using 50ul bead solution (1X vol.) washing and eluting as above (step 8).<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#84CE84","section_duration":8700,"critical":null,"critical_id":null,"duration":1800},{"id":1216197,"guid":"D74518D0E8B011EBA2F2259E603F9768","previous_id":1216196,"previous_guid":"D5570470E8B011EBA2F2259E603F9768","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"A402B85FAEA145D7890941FD53D25400","order_id":1,"type_id":6,"title":"Section","source":{"title":"Library Amplification"}},{"id":1054724,"guid":"7108FD3C8DEA4EC78E04AB8C7FFF3903","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Quantify using BioAnalyzer 2100 (more accurate quantification can be done with qPCR via KAPA Library Quantification Kit but DNA electrophoresis is necessary to assess molecular size distribution).<\/div><br\/><br\/><br\/><br\/><\/div>"}},{"id":1054725,"guid":"4BDB6351C81641699A763CCABB5244D1","order_id":2,"type_id":17,"title":"result","source":{"body":"<div class = \"text-blocks\"><div style = \"text-align :; float : ;\"><img style = \"\" src = \"https:\/\/s3.amazonaws.com\/protocols-files\/files\/dz44bmcup.jpg\" \/><\/div><br\/><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#84CE84","section_duration":8700,"critical":null,"critical_id":null,"duration":3600},{"id":1216215,"guid":"0DC52A20E8B211EBA2F2259E603F9768","previous_id":1216197,"previous_guid":"D74518D0E8B011EBA2F2259E603F9768","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"D83578861665415899C884109801EB5F","order_id":1,"type_id":6,"title":"Section","source":{"title":"Library Amplification"}},{"id":1054724,"guid":"5DDCB81803AF44C7ADEFEDBC03476B48","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Amplified libraries are now ready for dilution and sequencing using an Illumina NGS platform. 40-80M PE reads is sufficient for 0.5-1X mean WGS coverage for analysis.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#84CE84","section_duration":8700,"critical":null,"critical_id":null,"duration":0}],"document":"","materials":[],"description":"<div class = \"text-blocks\"><div class = \"text-block\">Simple protocol for generating WGS libraries using KAPA HyperPrep library preparation reagents.<\/div><\/div>","changed_on":1626714290}