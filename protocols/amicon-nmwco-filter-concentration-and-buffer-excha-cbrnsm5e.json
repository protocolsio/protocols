e,"is_content_warning":false,"is_in_transfer":false,"is_owner":true,"is_research":true,"is_retracted":false,"is_shared_directly":false,"is_subprotocol":null,"is_unlisted":false,"item_id":950638,"journal":null,"journals":[],"keywords":null,"last_modified":1655825148,"link":"","location":null,"manuscript_citation":"","materials":[],"materials_text":"{\"blocks\":[{\"key\":\"22mkh\",\"text\":\"Amicon NMWCO Filter (MWCO dependent on the protein target)\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"cmn50\",\"text\":\"Centrifuge \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"dq9ne\",\"text\":\"0.5% TFA or 0.2% FA in mass spec grade H2O\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"sub\",\"offset\":40,\"length\":1}],\"entityRanges\":[],\"data\":{}},{\"key\":\"crmng\",\"text\":\"LoBind microcentrifuge tubes \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","ownership_history":null,"parent_collections":[],"parent_protocols":[],"peer_reviewed":false,"public":true,"public_fork_note":"","published_on":1655825148,"references":[],"related_equipments":[],"retraction_reason":null,"shared_access_id":100,"show_comparison":false,"sign_info":null,"state_version_id":97,"stats":{"is_voted":false,"number_of_views":1,"number_of_steps":3,"number_of_bookmarks":0,"number_of_comments":0,"number_of_bookmarked_comments":0,"number_of_steps_comments":0,"number_of_protocol_comments":0,"number_of_exports":0,"number_of_runs":0,"number_of_votes":0,"number_of_reagents":0,"number_of_equipments":0,"number_of_collections":0,"number_of_forks":{"private":0,"public":0},"number_of_accessible_forks":0},"status":{"id":1,"info":"We use this protocol and it’s working"},"steps":[{"id":1426140,"guid":"DF5DB210F17511EC92CD6F35F64E8BDF","previous_id":0,"previous_guid":null,"section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"2je66\",\"text\":\"Wash the filter three times with Optima grade water and centrifuging at 14,000 x g. Equilibrate the column by washing once with the same buffer as the elution buffer containing the target protein for ~10-30 minutes or until the volume has decreased.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":65038,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"1","cases":[],"critical":null},{"id":1426141,"guid":"FA3A38B0F17511EC92CD6F35F64E8BDF","previous_id":1426140,"previous_guid":"DF5DB210F17511EC92CD6F35F64E8BDF","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"7ugv3\",\"text\":\"Load the elution fraction containing the target protein into the filter and centrifuge at 14,000 x g for ~10-30 minutes or until the volume has decreased. Repeat this until the total elution volume has decreased ≤ 25 µL for LC-MS or ≤ 80 µL for I2MS. If the buffer is already acidic (TFA or FA), no additional buffer exchange is required. However, if the elution buffer is not acidic (ex: 100 mM Tris Base), buffer exchange should be performed. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"sup\",\"offset\":246,\"length\":1}],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":65038,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"2","cases":[],"critical":null},{"id":1426142,"guid":"22B47710F17611EC92CD6F35F64E8BDF","previous_id":1426141,"previous_guid":"FA3A38B0F17511EC92CD6F35F64E8BDF","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"2u6di\",\"text\":\"Gradually incorporate larger volumes of 0.2-0.5% TFA or FA to the sample volume after sequential centrifuge cycles until the majority of the final buffer is TFA or FA. Remove sample from filter by centrifuging upside down at 14,000 x g into a clean LoBind tube. Bring the final volume up to 25 µL for LC-MS or 80 µL for I2MS with 0.2-0.5% TFA or FA before analyzing by mass spectrometry. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"sup\",\"offset\":321,\"length\":1}],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":65038,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"3","cases":[],"critical":null}],"template_id":1,"title":"Amicon NMWCO Filter Concentration and Buffer Exchange","title_html":"\u003cp\u003eAmicon NMWCO Filter Concentration and Buffer Exchange\u003c/p\u003e","type_id":1,"units":[{"id":1,"type_id":3,"name":"µL","can_manage":0},{"id":2,"type_id":3,"name":"mL","can_manage":0},{"id":3,"type_id":3,"name":"L","can_manage":0},{"id":4,"type_id":3,"name":"µg","can_manage":0},{"id":5,"type_id":3,"name":"mg","can_manage":0},{"id":6,"type_id":3,"name":"g","can_manage":0},{"id":7,"type_id":3,"name":"kg","can_manage":0},{"id":8,"type_id":3,"name":"ng","can_manage":0},{"id":9,"type_id":3,"name":"Hz","can_manage":0},{"id":10,"type_id":24,"name":"°C","can_manage":0},{"id":11,"type_id":24,"name":"°К","can_manage":0},{"id":12,"type_id":24,"name":"°F","can_manage":0},{"id":13,"type_id":25,"name":"Mass Percent","can_manage":0},{"id":14,"type_id":25,"name":"% volume","can_manage":0},{"id":15,"type_id":25,"name":"Mass / % volume","can_manage":0},{"id":16,"type_id":25,"name":"Parts per Million (PPM)","can_manage":0},{"id":17,"type_id":25,"name":"Parts per Billion (PPB)","can_manage":0},{"id":18,"type_id":25,"name":"Parts per Trillion (PPT)","can_manage":0},{"id":19,"type_id":25,"name":"Mole Fraction","can_manage":0},{"id":20,"type_id":25,"name":"Mole Percent","can_manage":0},{"id":21,"type_id":25,"name":"Molarity (M)","can_manage":0},{"id":22,"type_id":25,"name":"Molarity (m)","can_manage":0},{"id":23,"type_id":25,"name":"Genome copies per ml","can_manage":0},{"id":24,"type_i