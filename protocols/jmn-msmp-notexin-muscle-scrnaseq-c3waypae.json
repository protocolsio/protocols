,"state_version_id":68,"stats":{"is_voted":false,"number_of_views":0,"number_of_steps":9,"number_of_bookmarks":0,"number_of_comments":0,"number_of_bookmarked_comments":0,"number_of_steps_comments":0,"number_of_protocol_comments":0,"number_of_exports":0,"number_of_runs":0,"number_of_votes":0,"number_of_reagents":0,"number_of_equipments":0,"number_of_collections":0,"number_of_forks":{"private":0,"public":0},"number_of_accessible_forks":0},"status":{"id":1,"info":"We use this protocol and it's working"},"steps":[{"id":1825884,"guid":"61C512FDB8FE4E3BB3A0FDB7FDD428EE","previous_id":0,"previous_guid":null,"section":"\u003cp\u003eSkeletal muscle injury and collection\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"aouj7\",\"text\":\"In a given mouse, both tibialis anterior (TA) muscles were injected with 10 µl of notexin (10 µg/ml; Latoxan, France).\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":89762,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"1","cases":[],"critical":null},{"id":1825885,"guid":"22C1DB9980D44E9DA3DBB0C2E27DADF6","previous_id":1825884,"previous_guid":"61C512FDB8FE4E3BB3A0FDB7FDD428EE","section":"\u003cp\u003eSkeletal muscle injury and collection\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"1n0ab\",\"text\":\"At various days post injury, the mouse was sacrificed and the TAs were collected.  Each TA was proccessed independently to generate single cell suspensions.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":89762,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"2","cases":[],"critical":null},{"id":1825888,"guid":"8D44CE661F7C45CCB5D4ADFB29D9A79F","previous_id":1825885,"previous_guid":"22C1DB9980D44E9DA3DBB0C2E27DADF6","section":"\u003cp\u003eGenerate single-cell suspensions\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"12ucj\",\"text\":\"Muscles were enzymatically digested with 8 mg/ml Collagenase D (Roche, Basel, Switzerland) and 10 U/ml Dispase II (Roche, Basel, Switzerland) and then manually dissociated to generate cell suspensions. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":89762,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"3","cases":[],"critical":null},{"id":1825889,"guid":"11851A581DD14F6D860CB295116E3886","previous_id":1825888,"previous_guid":"8D44CE661F7C45CCB5D4ADFB29D9A79F","section":"\u003cp\u003eGenerate single-cell suspensions\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"abnid\",\"text\":\"Myofiber debris was removed by filtering the cell suspensions through a 100 µm and then a 40 µm filter (Corning Cellgro # 431752 and # 431750). \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":89762,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4","cases":[],"critical":null},{"id":1825890,"guid":"024A1F53955248DCB0017D37B3D9EF53","previous_id":1825889,"previous_guid":"11851A581DD14F6D860CB295116E3886","section":"\u003cp\u003eGenerate single-cell suspensions\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"72qj4\",\"text\":\"After filtration, erythrocytes were removed by incubating the cell suspension in erythrocyte lysis buffer (IBI Scientific # 89135-030).\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":89762,"case_id":0,"critical_ids":"","