ation":0},{"id":886279,"guid":"169C58D054CF11EAA51F1DBE7E9CEB89","previous_id":886278,"previous_guid":"F9A863E054CE11EAA51F1DBE7E9CEB89","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"5691C416D15F4F72805991FA8EEF55AB","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"3761EA942B5249F6B917C1F7EB714B83","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Start slowly and increase the intensity of grinding as the liquid nitrogen completely evaporates. If necessary, add more liquid nitrogen and continue to grind until the sample is a fine powder.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":886280,"guid":"29C2D7E054CF11EAA51F1DBE7E9CEB89","previous_id":886279,"previous_guid":"169C58D054CF11EAA51F1DBE7E9CEB89","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"42DA4713E4B44275992CCA8A578D6A22","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"C72108245C1A4B0B92CD222862465F96","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">When grinding is complete, carefully pour the frozen sample powder into  2-ml polypropylene tubes. It may be necessary to use a spatula, precooled in liquid nitrogen, to guide the sample into the 2-ml tube to prevent thawing. Immediately place the microfuge tubes containing ground powder into liquid nitrogen.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":886281,"guid":"52CFE06054CF11EAA51F1DBE7E9CEB89","previous_id":886277,"previous_guid":"F7B0165054CE11EAA51F1DBE7E9CEB89","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"085454987F3E4AFB9F0A04AA97EFD7E4","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"ED0FAC2A52B64FB19426D1B73BC5994C","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Preheat extraction buffer in a 65 \u00b0C water bath before proceeding with tissue grinding.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":886282,"guid":"A490ADD054CF11EAA51F1DBE7E9CEB89","previous_id":886280,"previous_guid":"29C2D7E054CF11EAA51F1DBE7E9CEB89","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"A03249075B234017BC9647F3AB1C0489","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"8813E015CA6F4FB1AC70FEA255F3917A","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Add 1.2 ml of the preheated extraction buffer to the frozen tissue powder and invert the tube for 5\u201310 s to mix thoroughly.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":886284,"guid":"A336F4C054D011EAA51F1DBE7E9CEB89","previous_id":886282,"previous_guid":"A490ADD054CF11EAA51F1DBE7E9CEB89","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"B1A0A1B836034D30B45B71AB81311C61","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"99AE38AA1C1943C0B8B703A5078FAFAC","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Incubate at 65 \u00b0C for 30 min in an incubator or water bath and invert tubes every 5\u201310 min to allow mixing.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":886285,"guid":"B599397054D011EAA51F1DBE7E9CEB89","previous_id":886284,"previous_guid":"A336F4C054D011EAA51F1DBE7E9CEB89","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"295C5134DF5A42BD9684454C9DE81A74","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"DB58716ACE174388AAC15AD7C6244FC8","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span>Centrifuge at 13,500<\/span><span style = \"font-style:italic;\">g <\/span><span>for 10 min at RT to remove nonsoluble debris. Supernatant will be saved for step 10.<\/span><\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":886286,"guid":"C32209A054D011EAA51F1DBE7E9CEB89","previous_id":886285,"previous_guid":"B599397054D011EAA51F1DBE7E9CEB89","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"28392E1BCF0647AF809C050E671E94EF","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"A26D7A93AC704F2EB2E241428608CC44","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Dispense 800 \u03bcl of phenol:chloroform:isoamyl alcohol into 2-ml Phase Lock Gel tubes (PLG Heavy). Prepare one tube per sample.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":886287,"guid":"4214830054D111EAA51F1DBE7E9CEB89","previous_id":886286,"previous_guid":"C32209A054D011EAA51F1DBE7E9CEB89","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"8BA81AD1A8384BCA87B68C5CE2B737D0","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"C713AA7374D84CD3BF1B9492945CBCB0","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Transfer the supernatant from Step 8 to the 2-ml Phase Lock Gel tubes containing phenol:chloroform:isoamyl alcohol that you prepared in Step 9. <\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":886288,"guid":"5FC203F054D111EAA51F1DBE7E9CEB89","previous_id":886287,"previous_guid":"4214830054D111EAA51F1DBE7E9CEB89","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"A73F209CF2C7449E8DB37B8ECFFA81DE","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"47F2E36B4EB5482E9D3AAA4AF908848B","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Close the tubes tightly and mix gently by inverting tubes for 20 min at 20\u201322 \u00b0C (RT). A nutator mixer is recommended for this step.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":886289,"guid":"90777F7054D111EAA51F1DBE7E9CEB89","previous_id":886288,"previous_guid":"5FC203F054D111EAA51F1DBE7E9CEB89","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"CCB5ADE1175B4E5C88614665F7114457","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"C120E7AB940F423FBF27528BD20DC3A1","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span>Separate the phases by centrifuging the mixture at 13,500<\/span><span style = \"font-style:italic;\">g <\/span><span>for 10 min at RT in a microfuge. The Phase Lock Gel will eliminate interphase contamination of nucleic acid solution.<\/span><\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":886290,"guid":"E6C8836054D111EAA51F1DBE7E9CEB89","previous_id":886289,"previous_guid":"90777F7054D111EAA51F1DBE7E9CEB89","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"64421906FFAE4483965A18214EF5923A","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"E7E38C4F5A7147798D32A5D684A881ED","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Carefully transfer the aqueous (upper) layer to a new 2-ml microfuge tube containing 800 \u03bcl cold isopropanol (stored at \u221220 \u00b0C) and mix by inverting the tube and then incubate at RT for 10 min to precipitate the DNA.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":886291,"guid":"03E731D054D211EAA51F1DBE7E9CEB89","previous_id":886290,"previous_guid":"E6C8836054D111EAA51F1DBE7E9CEB89","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"54EB167F2F084745A9D21657B2DCDAB1","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"B8FCEA01E6C041E7A7CEF0260BD5A6AC","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span>Centrifuge the mixture at 13,500<\/span><span style = \"font-style:italic;\">g <\/span><span>for 10 min at RT.<\/span><\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":886292,"guid":"208601E054D211EAA51F1DBE7E9CEB89","previous_id":886291,"previous_guid":"03E731D054D211EAA51F1DBE7E9CEB89","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"8FC144A75F5846C0B84EB6CCBA622876","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"C45B14DBED3747D79951072C4F5A7E36","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Remove the supernatant using a micropipette and then gently resuspend the pellet in 250 \u03bcl TE at RT.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":886293,"guid":"2D748DE054D211EAA51F1DBE7E9CEB89","previous_id":886292,"previous_guid":"208601E054D211EAA51F1DBE7E9CEB89","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"BBC47B14262A408192B737E64F71CDD0","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"310BBA254B1E48E3B5E3175C3E4CD64A","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Pipette 2.5 \u03bcl of DNase-free RNase A (10mg\/mL) into sample mixture and incubate at 37 \u00b0C for 30 min.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":886294,"guid":"66DF0A6054D211EAA51F1DBE7E9CEB89","previous_id":886293,"previous_guid":"2D748DE054D211EAA51F1DBE7E9CEB89","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"ECDFFE3E429C455E83B9CFA04FEE8890","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"A34A884BDB874EFB86890EC1F5C2A0A8","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Pipette 25 \u03bcl of 3 M NaAc, pH 5 and mix. Add 600 \u03bcl of precooled 100%  ethanol (\u221220 \u00b0C), mix and incubate at \u221220 \u00b0C for 20 min to precipitate the DNA.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":886295,"guid":"E6C6922054D211EAA51F1DBE7E9CEB89","previous_id":886294,"previous_guid":"66DF0A6054D211EAA51F1DBE7E9CEB89","modified_on":0,"protocol_id":0,"compo