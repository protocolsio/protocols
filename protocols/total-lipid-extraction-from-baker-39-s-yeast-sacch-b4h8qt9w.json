ion":86400,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"1n5fs\",\"text\":\"Check OD600 at UV-Vis spectrophotometer\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"sub\",\"offset\":8,\"length\":3}],\"entityRanges\":[],\"data\":[]}],\"entityMap\":{}}","data":null,"protocol_id":57632,"case_id":0,"critical_ids":"","duration":0,"original_id":1175015,"number":"6","cases":[],"critical":null},{"id":1334458,"guid":"2D9B51FE25E744EAB892A3EBFAF570E9","previous_id":1334473,"previous_guid":"B460A330830511EC98B167E4F3738DF6","section":"\u003cp\u003eDay 5\u003c/p\u003e","section_color":"#84CE84","section_duration":86400,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"f3f4l\",\"text\":\"Vortex vigorously for 5 mins, and centrifuge at 150 x g for 5 mins at room temperature.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":{}}","data":null,"protocol_id":57632,"case_id":0,"critical_ids":"","duration":0,"original_id":1175032,"number":"12","cases":[],"critical":null},{"id":1334459,"guid":"77EF4FC96D764C2986755AE4ACDA2D45","previous_id":1334458,"previous_guid":"2D9B51FE25E744EAB892A3EBFAF570E9","section":"\u003cp\u003eDay 5\u003c/p\u003e","section_color":"#84CE84","section_duration":86400,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"59unj\",\"text\":\"After centrifugation, a two-phase system is obtained: aqueous top phase and organic bottom phase which contains the lipids.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":{}}","data":null,"protocol_id":57632,"case_id":0,"critical_ids":"","duration":0,"original_id":1175033,"number":"13","cases":[],"critical":null},{"id":1334460,"guid":"2617E2C785E146539F237D676A2206D7","previous_id":1334459,"previous_guid":"77EF4FC96D764C2986755AE4ACDA2D45","section":"\u003cp\u003eDay 5\u003c/p\u003e","section_color":"#84CE84","section_duration":86400,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"67vcf\",\"text\":\"Carefully remove the top aqueous layer and the middle insoluble layers (precipitated proteins).\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":{}}","data":null,"protocol_id":57632,"case_id":0,"critical_ids":"","duration":0,"original_id":1175034,"number":"14","cases":[],"critical":null},{"id":1334461,"guid":"6555762861764F29A02CD2F84D98D920","previous_id":1334460,"previous_guid":"2617E2C785E146539F237D676A2206D7","section":"\u003cp\u003eDay 5\u003c/p\u003e","section_color":"#84CE84","section_duration":86400,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"d7v5o\",\"text\":\"The organic bottom layer is dried using a speed vacuum or dried under a steady stream of nitrogen.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":{}}","data":null,"protocol_id":57632,"case_id":0,"critical_ids":"","duration":0,"original_id":1175035,"number":"15","cases":[],"critical":null},{"id":1334462,"guid":"6B1ED16167304874B7D70709CAAED217","previous_id":1334461,"previous_guid":"6555762861764F29A02CD2F84D98D920","section":"\u003cp\u003eDay 5\u003c/p\u003e","section_color":"#84CE84","section_duration":86400,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"bhno6\",\"text\":\"Record the weight of the dried sample by pre-weighing an Eppendorf tube or equivalent before or after drying the lipid.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":{}}","data":null,"protocol_id":57632,"case_id":0,"critical_ids":"","duration":0,"original_id":1175036,"number":"16","cases":[],"critical":null},{"id":1334463,"guid":"58875E3F3836469690A9DB35189A0F38","previous_id":1334463,"previous_guid":"58875E3F3836469690A9DB35189A0F38","section":null,"section_color":null,"section_duration":259200,"is_substep":true,"step":"{\"blocks\":[{\"key\":\"aiac0\",\"text\":\"Incubate plate at 30°C until colonies 1-2 mm in diamete