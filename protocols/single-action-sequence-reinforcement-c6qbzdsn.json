pments":0,"number_of_collections":0,"number_of_forks":{"private":0,"public":0},"number_of_accessible_forks":0},"status":{"id":1,"info":"We use this protocol and it's working"},"steps":[{"id":1887124,"guid":"C9BB00D192A24619B2C3CA0650E1AE74","previous_id":0,"previous_guid":null,"section":"\u003cp\u003eSession 1 + Baseline\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"35k72\",\"text\":\"Mice were placed in a white open field box for closed loop reinforcement protocol. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"6npjf\",\"text\":\"To acquire baseline behavior, individual mice were allowed to behave freely inside the box for 30 minutes on the first action A reinforcement session.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":92643,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"1","cases":[],"critical":null},{"id":1887138,"guid":"038B90419D074133942BDEE744ABA9C7","previous_id":1887124,"previous_guid":"C9BB00D192A24619B2C3CA0650E1AE74","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"22kt1\",\"text\":\"After initial behavior acquisition, closed loop reinforcement was initated by blue laser stimulation of VTA DA neurons were made available for 60 minutes.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":92643,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"2","cases":[],"critical":null},{"id":1887145,"guid":"53A5885F9E7547ABA5AE5A9F0B0159EE","previous_id":1887138,"previous_guid":"038B90419D074133942BDEE744ABA9C7","section":"\u003cp\u003eSessions 2 \u0026amp; 3 \u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\