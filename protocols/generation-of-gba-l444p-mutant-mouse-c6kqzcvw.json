ull,"doi_status":0,"ethics_statement":null,"fork_id":null,"fork_info":null,"forks":[],"funders":[{"funder_name":"ASAP","grant_id":" ASAP-000458"}],"groups":[],"guid":"F7AFA9C7ECC8406BAEEC9CE1E402321A","guidelines":"","has_references":false,"has_step_reagents":false,"has_versions":false,"id":92528,"image":{"source":"https://www.protocols.io/img/default_protocol.png","webp_source":null,"placeholder":"https://www.protocols.io/img/default_protocol.png","webp_placeholder":null},"image_attribution":"","in_trash":false,"is_bookmarked":false,"is_contact_suspended":false,"is_content_confidential":false,"is_content_warning":false,"is_doi_reserved":false,"is_in_pending_publishing":false,"is_in_transfer":false,"is_owner":true,"is_research":true,"is_retracted":false,"is_shared_directly":false,"is_subprotocol":null,"is_unlisted":false,"item_id":1199421,"journal":null,"journals":[],"keywords":"ASAPCRN","last_modified":1703020375,"link":"","location":null,"manuscript_citation":"","materials":[],"materials_text":"{\"blocks\":[{\"key\":\"28pbd\",\"text\":\"1. mouse gDNA\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"6dsuj\",\"text\":\"2. PCR kit\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"c7fe8\",\"text\":\"3. murine embryonic stem (ES) cells\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"8a192\",\"text\":\"4. Southern blot kit\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"etolj\",\"text\":\"5. Geneticin, G418 solution\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"672vj\",\"text\":\"6. C57BL/6 female mouse\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"8aih\",\"text\":\"\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","ownership_history":null,"parent_collections":[],"parent_protocols":[],"peer_reviewed":false,"protocol_references":"","public":true,"public_fork_note":"","published_on":1703020375,"references":[],"related_equipments":[],"related_materials":[],"reserved_doi":"","retraction_reason":null,"samples":{},"shared_access_id":119,"show_comparison":false,"sign_info":null,"space_access":{"can_view":false,"can_edit":false,"can_remove":false,"can_add":false,"can_publish":true,"can_get_doi":true,"can_share":false,"can_move":false,"can_move_outside":false,"can_transfer":false,"can_download":false,"limited_run":false,"limited_private_links":false,"limited_blind_links":false,"is_locked":false},"state_version_id":183,"stats":{"is_voted":false,"number_of_views":1,"number_of_steps":13,"number_of_bookmarks":0,"number_of_comments":0,"number_of_bookmarked_comments":0,"number_of_steps_comments":0,"number_of_protocol_comments":0,"number_of_exports":0,"number_of_runs":0,"number_of_votes":0,"number_of_reagents":0,"number_of_equipments":0,"number_of_collections":0,"number_of_forks":{"private":0,"public":0},"number_of_accessible_forks":0},"status":{"id":1,"info":"We use this protocol and it's working"},"steps":[{"id":1884481,"guid":"9A1A6B79A11A4BA9A003B00CBAB900BD","previous_id":0,"previous_guid":null,"section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"4441q\",\"text\":\"The replacement targeting vector enabling positive/negative selection contained a neomycin resistance (neoR) cassette flanked by loxP sequences inserted into the intergenic regions between murine metaxin (mtx) and glucocerebrosidase (gba).\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":92528,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"1","cases":[],"critical":null},{"id":1884483,"guid":"6D00CB9DFBE743EEAA3E7F3E88118BF0","previous_id":1884481,"previous_guid":"9A1A6B79A11A4BA9A003B00CBAB900BD","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"8bhbp\",\"text\":\"The L444P was introduced into a genomic clone of murine gba by PCR mutagenesis. For this, the normal murine Gba sequence in exon 9 was changed from TGACTTGGA to TGACCCGGA, resulting in the amino acid substitution of proline for leucine and introducing a Nci1 restriction site. Then, the change in sequence was confirmed by restriction digest and/or direct sequence analyses.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":92528,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"2","cases":[],"critical":null},{"id":1884488,"guid":"D56A4AD569E7425384FC19B77FF80B6B","previous_id":1884483,"previous_guid":"6D00CB9DFBE743EEAA3E7F3E88118BF0","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"5e8mm\",\"text\":\"The final constructs contained a 4.0 kb 5′ gba homologous arm and a 1.4 kb 3′ mtx homologous arm.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":92528,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"3","cases":[],"critical":null},{"id":1884489,"guid":"25B42879E58D4639AAB3F49B03B76005","previous_id":1884488,"previous_guid":"D56A4AD569E7425384FC19B77FF80B6B","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"7de7g\",\"text\":\"A diphtheria toxin A (DTA) cassette was placed downstream as a negative selectable marker.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":92528,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4","cases":[],"critical":null},{"id":1884490,"guid":"E120F7C947BC47CD8501041663091C0D","previous_id":1884489,"previous_guid":"25B42879E58D4639AAB3F49B03B76005","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"6o2qi\",\"text\":\"After linearization, the constructs were introduced into murine embryonic stem (ES) cells by electroporation, and the ES cells were subjected to drug selection in culture with G418 solution.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":92528,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"5","cases":[],"critical":null},{"id":1884491,"guid":"1656FDA79528458FA410D0B886B4D28E","previous_id":1884490,"previous_guid":"E120F7C947BC47CD8501041663091C0D","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"fatjd\",\"text\":\"The correct gene targeting event in G418 resistant individual clones was identified by Southern blot and PCR analysis.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":92528,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"6","cases":[],"critical":null},{"id":1884492,"guid":"58D674CCDF134D22B94AF4D4E85510C5","previous_id":1884491,"previous_guid":"1656FDA79528458FA410D0B886B4D28E","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"bd9vb\",\"text\":\"Cells from ES clones containing the correctly targeted mutation in one allele of the gba gene were injected into blastocysts from C57BL/6 mice and then transferred to foster mice.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":92528,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"7","cases":[],"critical":null},{"id":1884493,"guid":"9853047A854D42B6ACFC948F1AE168CB","previous_id":1884492,"previous_guid":"58D674CCDF134D22B94AF4D4E85510C5","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"7mkj6\",\"text\":\"Male offspring from these injections having more than 30% coat color chimerism were test-bred against C57BL/6 females, and progeny were screened by PCR and Southern analyses for transmission of the mutant gba allele.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":92528,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"8","cases":[],"critical":null},{"id":1884494,"guid":"B54121D1B5D2432AA820FD8205D9E013","previous_id":1884493,"previous_guid":"9853047A854D42B6ACFC948F1AE168CB","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"2rom9\",\"text\":\"Lines of mice containing the mutant gba allele were identified, and the DNA sequence confirmed by direct sequencing of PCR amplified DNA containing the mutation.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":92528,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"9","cases":[],"critical":null},{"id":1884495,"guid":"0B998E948E3541E5B5990AF12FB3612B","previous_id":1884494,"previous_guid":"B54121D1B5D2432AA820FD8205D9E013","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"asn2b\",\"text\":\"Mice heterozygous for the mutant gba gene were mated and homozygous mutant progeny were identified by Southern blot and PCR analysis.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":92528,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"10","cases":[],"critical":null},{"id":1884498,"guid":"BE10350ED4274D90A96B9D75EF837C68","previous_id":1884495,"previous_guid":"0B998E948E3541E5B5990AF12FB3612B","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"7kau6\",\"text\":\"In addition, heterozygous mice were mated to mice carrying a transgene for CRE DNA recombinase, resulting in the excision of the neoR marker, leaving only a 34 bp loxP sequence.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":92528,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"11","cases":[],"critical":null},{"id":1884499,"guid":"BF963B6C72DE4433A83556051F1ACEEA","previous_id":1884498,"previous_guid":"BE10350ED4274D90A96B9D75EF837C68","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"158tp\",\"text\":\"As expected, the targeted mutations were transmitted in a Mendelian fashion.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":92528,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"12","cases":[],"critical":null},{"id":1884501,"guid":"3920C74ECBDC4624840B0FD86F40BD44","previous_id":1884499,"previous_guid":"BF963B6C72DE4433A83556051F1ACEEA","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"3akk3\",\"text\":\"Mice homozygous for the gba mutations without the neoR cassette were used to expand the colonies.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":92528,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"13","cases":[],"critical":null}],"template_id":4,"title":"Generation of Gba L444P mutant mouse","title_html":"Generation of Gba L444P mutant mouse","type_id":1,"units":[{"id":1,"type