":null,"samples":{},"shared_access_id":100,"show_comparison":false,"sign_info":null,"space_access":{"can_view":false,"can_edit":false,"can_remove":false,"can_add":false,"can_publish":true,"can_get_doi":true,"can_share":false,"can_move":false,"can_move_outside":false,"can_transfer":false,"can_download":false,"limited_run":false,"limited_private_links":false,"limited_blind_links":false,"is_locked":false},"state_version_id":1042,"stats":{"is_voted":false,"number_of_views":6,"number_of_steps":9,"number_of_bookmarks":0,"number_of_comments":0,"number_of_bookmarked_comments":0,"number_of_steps_comments":0,"number_of_protocol_comments":0,"number_of_exports":0,"number_of_runs":0,"number_of_votes":0,"number_of_reagents":0,"number_of_equipments":0,"number_of_collections":0,"number_of_forks":{"private":0,"public":0},"number_of_accessible_forks":0},"status":{"id":1,"info":"We use this protocol and it's working"},"steps":[{"id":1811190,"guid":"B38DE5FE742649A89922F368C49BE2F4","previous_id":0,"previous_guid":null,"section":"\u003cp\u003eTissue Embedding\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"bchpb\",\"text\":\"Embed tissue according to this protocol: dx.doi.org/10.17504/protocols.io.kqdg3x8keg25/v1\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[{\"key\":0,\"offset\":41,\"length\":48}],\"data\":{}}],\"entityMap\":{\"0\":{\"type\":\"link\",\"mutability\":\"MUTABLE\",\"data\":{\"guid\":\"642159C1648311EEBE9E0A58A9FEAC02\",\"url\":\"https://dx.doi.org/10.17504/protocols.io.kqdg3x8keg25/v1\"}}}}","data":null,"protocol_id":88925,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"1","cases":[],"critical":null},{"id":1811191,"guid":"6C2653EB3B004B8AAB21020BFCC42E20","previous_id":1811190,"previous_guid":"B38DE5FE742649A89922F368C49BE2F4","section":"\u003cp\u003eCryosectioning\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"9bhnn\",\"text\":\"Spray the workplace and tools (blades, tweezers, brushes, etc) with RNAseZap to decontaminate the environment.  Spray 100% ethanol onto the cryostat to remove any residue of RNAseZap or reagent residue.  Set the cryostat to -20°C.  Prepare dry ice in a foam box.  Place the empty clean slide box in the dry ice.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":88925,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"2","cases":[],"critical":null},{"id":1811201,"guid":"D1AC9A0546CF4676917283EDD74B5E22","previous_id":1811191,"previous_guid":"6C2653EB3B004B8AAB21020BFCC42E20","section":"\u003cp\u003eCryosectioning\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"fhga2\",\"text\":\"Remove tissue blocks from the -80°C freezer/dry ice and allow them to warm in cyrostat to sectioning temperature (-20°C) for 20 minutes.  Locate the region of interest in the tissue block.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":88925,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"3","cases":[],"critical":null},{"id":1811202,"guid":"D89E5BE202B94193B37F4E1B9F99F18B","previous_id":1811201,"previous_guid":"D1AC9A0546CF4676917283EDD74B5E22","section":"\u003cp\u003eCryosectioning\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"4a8ne\",\"text\":\"Trim the block if needed.  Collect the defect tissue slides on a regular glass slide to test for RIN number.  Usually, 3-4 sections of a 1 cm x 1 cm tissue on a glass slide is good for a RIN test.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":88925,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4","cases":[],"critical":null},{"id":1811209,"guid":"8DBE86DE83044076B80A4FAA4F6ED38F","previous_id":1811300,"previous_guid":"94CFC48C9D12492FBAAEBD8E17754795","section":"\u003cp\u003eCryosectioning\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"5eh0l\",\"text\":\"One section per slide needs to be put onto the center of the Ultra clean poly-L-lysine glass slide (see picture below).  The glass slides are at room temperature.  Label the bottom side of the slide for sample name with a marker.  Make sure the top side of the slide is free of dust or any labeling.  Use Dustoff gas to blow off the surface.  \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"6vo5j\",\"text\":\"   \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[{\"key\":0,\"offset\":0,\"length\":1},{\"key\":1,\"offset\":2,\"length\":1}],\"data\":{}}],\"entityMap\":{\"0\":{\"type\":\"image\",\"mutability\":\"MUTABLE\",\"data\":{\"height\":164,\"id\":411133,\"is_video\":false,\"legend\":\"\",\"mime\":\"image/png\",\"original_name\":\"image.png\",\"placeholder\":\"https://content.protocols.io/files/ntr9b86tp.jpg\",\"source\":\"https://content.protocols.io/files/ntr7b86tp.jpg\",\"type\":1,\"webp_placeholder\":\"https://content.protocols.io/files/ntr8b86tp.webp\",\"webp_source\":\"https://content.protocols.io/files/ntr6b86tp.webp\",\"width\":502}},\"1\":{\"type\":\"image\",\"mutability\":\"MUTABLE\",\"data\":{\"height\":362,\"id\":411129,\"is_video\":false,\"legend\":\"\",\"mime\":\"image/png\",\"original_name\":\"image.png\",\"placeholder\":\"https://content.protocols.io/files/ntr5b86tp.jpg\",\"source\":\"https://content.protocols.io/files/ntr3b86tp.jpg\",\"type\":1,\"webp_placeholder\":\"https://content.protocols.io/files/ntr4b86tp.webp\",\"webp_source\":\"https://content.protocols.io/files/ntr2b86tp.webp\",\"width\":484}}}}","data":null,"protocol_id":88925,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"6","cases":[],"critical":null},{"id":1811210,"guid":"1D6CB0C5C90C4D62A4AEDD911956745E","previous_id":1811209,"previous_guid":"8DBE86DE83044076B80A4FAA4F6ED38F","section":"\u003cp\u003eCryosectioning\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"3jet3\",\"text\":\"Once the tissue slice is picked up onto the glass slide, allow 30 seconds to 1 min at room temperature before transferring to a slide box in dry ice. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":88925,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"7","cases":[],"critical":null},{"id":1811211,"guid":"5562F20450D142039B83C653D16C2134","previous_id":1811210,"previous_guid":"1D6CB0C5C90C4D62A4AEDD911956745E","section":"\u003cp\u003eCryosectioning\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"74m12\",\"text\":\"Save the slide box in a sealed plastic bag at -80° C before use.  The bag helps prevent water condensation on the slides.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":88925,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"8","cases":[],"critical":null},{"id":1811212,"guid":"3496B84AEA2A4978BDAE1B9E89E30760","previous_id":1811211,"previous_guid":"5562F20450D142039B83C653D16C2134","section":"\u003cp\u003eCryosectioning\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"28sp\",\"text\":\"For slides used for H\\u0026E, thaw them at room temperature for 10 min, then dry them with compressed air for at least 5 min.  Make sure no moisture is observed on the slide.  Dip the slides in 4% paraformaldehyde at room temperature for 15 min (alternative: 30 min in pre-chilled methanol at -20° C).  Wash with deionized water 3 times.  Air blow dry the slides or leave them at room temperature overnight before doing H\\u0026E staining.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":88925,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"9","cases":[],"critical":null},{"id":1811300,"guid":"94CFC48C9D12492FBAAEBD8E17754795","previous_id":1811202,"previous_guid":"D89E5BE202B94193B37F4E1B9F99F18B","section":"\u003cp\u003eCryosectioning\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"abf5k\",\"text\":\"Adjust cyrostat thickness to 7-10 μm.  For spatial transcriptomes, 7-10 μm is the best thickness, but generally 8-13 μm works as well. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":88925,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"5","cases":[],"critical":null}],"template_id":1,"title":"Yale Murine TMC -  Frozen Tissue Sectioning Protocol for Spatial Transcriptomics (DBiTSeq) and Immunofluorescence  ","title_html":"Yale Murine TMC -  Frozen Tissue Sectioning Protocol for Spatial Transcriptomics (DBiTSeq) and Immunofluorescence  ","type_id":1,"units":[{"id":1,"type_id":3,"name":"µL","can_manage":0,"read_only":0},{"id":2,"type_id":3,"name":"mL","can_manage":0,"read_only":0},{"id":3,"type_id":3,"name":"L","can_manage":0,"read_only":0},{"id":4,"type_id":3,"name":"µg","can_manage":0,"read_only":0},{"id":5,"type_id":3,"name":"mg","can_manage":0,"read_only":0},{"id":6,"type_id":3,"name":"g","can_manage":0,"read_only":0},{"id":7,"type_id":3,"name":"kg","can_manage":0,"read_only":0},{"id":8,"type_id":3,"name":"ng","can_manage":0,"read_only":0},{"id":9,"type_id":3,"name":"Hz","ca