n_share":false,"can_move":false,"can_move_outside":false,"can_transfer":false,"can_download":false,"limited_run":false,"limited_private_links":false,"limited_blind_links":false,"is_locked":false},"state_version_id":402,"stats":{"is_voted":false,"number_of_views":12,"number_of_steps":4,"number_of_bookmarks":0,"number_of_comments":0,"number_of_bookmarked_comments":0,"number_of_steps_comments":0,"number_of_protocol_comments":0,"number_of_exports":2,"number_of_runs":0,"number_of_votes":0,"number_of_reagents":0,"number_of_equipments":0,"number_of_collections":0,"number_of_forks":{"private":0,"public":0},"number_of_accessible_forks":0},"status":{"id":1,"info":"We use this protocol and it's working"},"steps":[{"id":1553103,"guid":"2E9363A0818811ED8D9749BC474796A2","previous_id":0,"previous_guid":null,"section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"2e33d\",\"text\":\"Incubator Settings: 37 degrees Celsius, 5% CO2\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"bold\",\"offset\":0,\"length\":20}],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":74350,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"1","cases":[],"critical":null},{"id":1553104,"guid":"2E93FFE1818811ED8D9749BC474796A2","previous_id":1553103,"previous_guid":"2E9363A0818811ED8D9749BC474796A2","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"3l2tu\",\"text\":\"Medium: 88% RPMI 1640, 10% FBS, 1% Pen/Strep/Glutamine, 1% (Final: 10 mM) of 1M HEPES\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"bold\",\"offset\":0,\"length\":7}],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":74350,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"2","cases":[],"critical":null},{"id":1553105,"guid":"2E949C21818811ED8D9749BC474796A2","previous_id":1553104,"previous_guid":"2E93FFE1818811ED8D9749BC474796A2","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"6gn0k\",\"text\":\"Cryopreservation: Freeze medium: 90% FBS, 10% DMSO. 5 million cells are resuspended in 500uL of freeze media, transferred to cryotubes, and frozen slowly in Mr. Frosty™ Freezing Container at -80 degrees Celsius.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"bold\",\"offset\":0,\"length\":17}],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":74350,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"3","cases":[],"critical":null},{"id":1553106,"guid":"4B6C7D90818811ED8D9749BC474796A2","previous_id":1553105,"previous_guid":"2E949C21818811ED8D9749BC474796A2","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"h6df\",\"text\":\"Guidelines for Experimental Use:\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"bold\",\"offset\":0,\"length\":32}],\"entityRanges\":[],\"data\":{}},{\"key\":\"6sgrr\",\"text\":\"Passages are not intuitive for suspension cells and passage number should instead be used to indicate number of times the original stock has been grown out and frozen for future experiments.\",\"type\":\"ordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"3hng9\",\"text\":\"New cell lines should be grown to at least 35 million cells and a minimum of 6 vials should be frozen for experimental use throughout the year and keeping at least one for long term storage in liquid nitrogen. \",\"type\":\"ordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"8sct9\",\"text\":\"Cell lines should be authenticated through STR profiling annually or whenever a new set of stocks are cryopreserved for future experiments to confirm lack of cross contamination.\",\"type\":\"ordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"25g5c\",\"text\":\"Cell lines should not be cultured for longer than 2 months when used for experiments.\",\"type\":\"ordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"1j4p7\",\"text\":\"Cell lines should be maintained at concentrations between 0.5 million to 2 million per mL throughout culture as to not exhaust media.\",\"type\":\"ordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"ali5f\",\"text\":\"Experiments requiring incubation times less than 24 hours should have cells plated at 1 million per mL media.\",\"type\":\"ordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"bkjf\",\"text\":\"Experiments requiring incubation times greater than 24 hours should have cells plated at 0.25-0.5 million per mL media.\",\"type\":\"ordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"3c50s\",\"text\":\"When ordering new lots of FBS, a sample should be requested to test if the new lot will be deleterious to cell survival and cell proliferation.\",\"type\":\"ordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":74350,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4","cases":[],"critical":null}],"template_id":1,"title":"General Guidelines for Culture of Multiple Myeloma Cell Lines","title_html":"General Guidelines for Culture of Multiple Myeloma Cell Lines","type_id":1,"units":[{"id":1,"type_id":3,"name":"µL","can_manage":0,"read_only":0},{"id":2,"type_id":3,"name":"mL","can_manage":0,"read_only":0},{"id":3,"type_id":3,"name":"L","can_manage":0,"read_only":0},{"id":4,"type_id":3,"name":"µg","can_manage":0,"read_only":0},{"id":5,"type_id":3,"name":"mg","can_manage":0,"read_only":0},{"id":6,"type_id":3,"name":"g","ca