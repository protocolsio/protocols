"id":120478,"mol_weight":0,"name":"Protease from Bacillus Licheniformis ","linfor":"","url":"https:\/\/www.sigmaaldrich.com\/catalog\/product\/sigma\/p5380?lang=en&region=US","sku":"P5380","cas_number":"","rrid":"","public":0,"vendor":{"name":"Sigma","affiliation":null,"affiliations":[],"username":null,"note":null,"link":"http:\/\/www.sigma.com","image":{"source":"https:\/\/www.protocols.iohttps:\/\/www.protocols.io\/img\/vendors\/placeholder.png","placeholder":"https:\/\/www.protocols.iohttps:\/\/www.protocols.io\/img\/vendors\/placeholder.png"},"badges":[],"verified":0,"is_verified_user":false,"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false,"id":114},"can_edit":0,"stats":{"total_protocols":0}}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":[],"critical_id":null,"duration":0},{"id":884044,"guid":"D2BACB2051BC11EA8B3BF740E8D91DCC","previous_id":884040,"previous_guid":"7506FAA051BA11EA8B3BF740E8D91DCC","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"38AF2A2EB7DF417D9B2CBDE691D241F2","order_id":1,"type_id":6,"title":"Section","source":{"title":"Dissociation"}},{"id":1054724,"guid":"A1C9BA16BA8D4939BFFAF5158AC92529","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Place on a tube rotator with continuous rotation.<\/div><br\/><\/div>"}},{"id":1054725,"guid":"17A6D62051BD11EA8B3BF740E8D91DCC","order_id":2,"type_id":28,"title":"equipment","source":{"brand":"Thermo Scientific","sku":"TS-4002110Q","specifications":null,"can_edit":false,"name":"Labquake Rotisserie","type":"Tube Rotator","link":null,"file":null,"image":null}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#FFED92","section_duration":1500,"critical":[],"critical_id":null,"duration":1500},{"id":884046,"guid":"4A3C18B051BE11EA8B3BF740E8D91DCC","previous_id":884044,"previous_guid":"D2BACB2051BC11EA8B3BF740E8D91DCC","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"704AC1DAEB764AAB95761E1AC08C185B","order_id":1,"type_id":6,"title":"Section","source":{"title":"Dissociation"}},{"id":1054724,"guid":"08D28B93C6D448E384179F4CEEDC9A0A","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">At 25 minutes (end of dissociation) triturate using a normal p-1000 tip 5-10 times, such that tissue is deformed as it enters the tip to yield material into the supernatant, and clusters are broken apart. The supernatant should become somewhat cloudy with cells. Inspect supernatant on a hemocytometer. Cells shoud be mostly single or in clusters of 10 or less. There should be at least 100K cells total. If tissue does not adequately yield cells, continue incubation for another 5 minutes before triturating again.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#FFED92","section_duration":1500,"critical":[],"critical_id":null,"duration":0},{"id":884047,"guid":"0E23246051C611EA8B3BF740E8D91DCC","previous_id":884008,"previous_guid":"0E61CF5051AB11EABCCD758B2F458A2E","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"76B72539DC9F48B191F8D4AEDC00B63F","order_id":1,"type_id":6,"title":"Section","source":{"title":"Final washes"}},{"id":1054724,"guid":"2064D6AF6F3B4400B002244735318D59","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Spin cells at 700xg for 5 minutes to pellet.<\/div><br\/><br\/><\/div>"}},{"id":1054725,"guid":"4A24F7A0F48011EBAC8001A886DABFF5","order_id":2,"type_id":30,"title":"centrifuge","source":{"centrifuge":"700","duration":300,"error_centrifuge":false,"error_temperature":false,"guid":"4A24F7A0F48011EBAC8001A886DABFF5","id":0,"label":"","pause":null,"preset":[],"scientificNotation":{"centrifuge":false,"temperature":false},"start":null,"te