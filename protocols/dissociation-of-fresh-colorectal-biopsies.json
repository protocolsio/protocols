4006,"guid":"01FDA90051AB11EABCCD758B2F458A2E","previous_id":884028,"previous_guid":"3D4FD7A051B311EABCCD758B2F458A2E","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"BC7B867CE8A248178F3B86B134E31C64","order_id":1,"type_id":6,"title":"Section","source":{"title":"Chelation"}},{"id":1054724,"guid":"5B4B7D0FF7F94051936911E953D60E48","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Move tissue to a 2ml tube containing ~1.8ml of chelation buffer and place on a tube rotator with continuous rotation.<\/div><br\/><\/div>"}},{"id":1054725,"guid":"F0FB6BB051B911EA8B3BF740E8D91DCC","order_id":2,"type_id":28,"title":"equipment","source":{"brand":"Thermo Scientific","sku":"TS-4002110Q","specifications":null,"can_edit":false,"name":"Labquake Rotisserie","type":"Tube Rotator","link":null,"file":null,"image":null}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#84CE84","section_duration":4500,"critical":[],"critical_id":null,"duration":4500},{"id":884007,"guid":"0A91184051AB11EABCCD758B2F458A2E","previous_id":884035,"previous_guid":"F50434A051B711EABCCD758B2F458A2E","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"D3D36A9370154EFC84EE696757954B30","order_id":1,"type_id":6,"title":"Section","source":{"title":"Dissociation"}},{"id":1054724,"guid":"89BE462339F645649F7DD5DD57C36F36","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Gently remove chelation buffer and re-suspend tissue in DPBS.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#FFED92","section_duration":1500,"critical":[],"critical_id":null,"duration":0},{"id":884008,"guid":"0E61CF5051AB11EABCCD758B2F458A2E","previous_id":884046,"previous_guid":"4A3C18B051BE11EA8B3BF740E8D91DCC","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"928AE802FF4F40A889C39BCD370947BD","order_id":1,"type_id":6,"title":"Section","source":{"title":"Final washes"}},{"id":1054724,"guid":"FE60385DAAD9454CB838E93EB3658372","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Pipette cells to mix and re-suspend before transfering onto a 70\u00b5m filter on a clean tube in a cold rack. Allow cells to filter through by gravity and check filtrate for cells before proceeding.  <\/div><br\/><\/div>"}},{"id":1054725,"guid":"3216D380526611EA8B3BF740E8D91DCC","order_id":2,"type_id":20,"title":"reagents","source":{"id":349709,"mol_weight":0,"name":"mini-strainer 70\u00b5m","linfor":"","url":"https:\/\/www.pluriselect.com\/us\/pluristrainer-mini-70-m-cell-strainer.html#","sku":"43-10070-50","cas_number":"","rrid":"","public":0,"vendor":{"name":"pluriSelect","affiliation":null,"affiliations":[],"username":null,"note":null,"link":"https:\/\/www.pluriselect.com\/","image":{"source":"https:\/\/www.protocols.iohttps:\/\/www.protocols.io\/img\/vendors\/placeholder.png","placeholder":"https:\/\/www.protocols.iohttps:\/\/www.protocols.io\/img\/vendors\/placeholder.png"},"badges":[],"verified":0,"is_verified_user":false,"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false,"id":154},"can_edit":0,"stats":{"total_protocols":0}}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#EA9F6C","section_duration":900,"critical":[],"critical_id":null,"duration":0},{"id":884028,"guid":"3D4FD7A051B311EABCCD758B2F458A2E","previous_id":884031,"previous_guid":"89901B2051B311EABCCD758B2F458A2E","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"C166BD70BEA64CD09E8497577BD086E1","order_id":1,"type_id":6,"title":"Section","source":{"title":"Reciept \/ Grossing"}},{"id":1054724,"guid":"C1E835EF45FF416E88A9BC6484451EA0","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Wash tissue gently in DPBS to remove any debris or excess transport media.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":300,"critical":[],"critical_id":null,"duration":0},{"id":884031,"guid":"89901B2051B311EABCCD758B2F458A2E","previous_id":884004,"previous_guid":"EB6F1FC051AA11EABCCD758B2F458A2E","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"FF080DE6AE374C2F973E51F08DBABDF4","order_id":1,"type_id":6,"title":"Section","source":{"title":"Reciept \/ Grossing"}},{"id":1054724,"guid":"4FB935B7C98A4FEFB679256F32240966","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span>If tissue is solid\/pedunculated, it may need to be bisected or trisected to increase surface area, do not cut smaller than 2mm<\/span><span style = \"vertical-align:super;\">2.<\/span><\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":300,"critical":[],"critical_id":null,"duration":0},{"id":884032,"guid":"F1D9CC8051B311EABCCD758B2F458A2E","previous_id":884006,"previous_guid":"01FDA90051AB11EABCCD758B2F458A2E","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"AFB1201E47F34503958A3E5B13895813","order_id":1,"type_id":6,"title":"Section","source":{"title":"Chelation"}},{"id":1054724,"guid":"51E0F34938FE430DA264DBA88D085B92","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Gently replace chelation buffer on sample at 30 minutes and 1 hour into chelation, ensuring that enough air remains in tube to displace (agitate) tissue during rotation<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#84CE84","section_duration":4500,"critical":[],"critical_id":null,"duration":0},{"id":884035,"guid":"F50434A051B711EABCCD758B2F458A2E","previous_id":884032,"previous_guid":"F1D9CC8051B311EABCCD758B2F458A2E","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"8C6B80F7614E4F7DB2623A3CC1B49712","order_id":1,"type_id":6,"title":"Section","source":{"title":"Chelation"}},{"id":1054724,"guid":"7C099716FB8348719FF7D468D6333410","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Prepare DNAse for use before chelation is over by adding 1ml of DPBS to a frozen 1ml aliqout of 2x DNAse and leaving on bench to thaw. Move DNAse to Ice after it has thawed.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#84CE84","section_duration":4500,"critical":[],"critical_id":null,"duration":0},{"id":884040,"guid":"7506FAA051BA11EA8B3BF740E8D91DCC","previous_id":884041,"previous_guid":"9740BA7051BA11EA8B3BF740E8D91DCC","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"6570CE8A708147ED8599CFE42EAF8C47","order_id":1,"type_id":6,"title":"Section","source":{"title":"Dissociation"}},{"id":1054724,"guid":"9BAB6A57E03B4AA592509D5F678B843E","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Gently remove DPBS from tissue and resuspend in 1.8ml of Dissociation solution. Make sure there is enough air in the tube to displace the tissue on inversion.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#FFED92","section_duration":1500,"critical":[],"critical_id":null,"duration":0},{"id":884041,"guid":"9740BA7051BA11EA8B3BF740E8D91DCC","previous_id":884007,"previous_guid":"0A91184051AB11EABCCD758B2F458A2E","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"FFCA554309734388B32DCE8C62BE50BE","order_id":1,"type_id":6,"title":"Section","source":{"title":"Dissociation"}},{"id":1054724,"guid":"380BFF4B9FEC4B12B825E9A67DD93E94","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Quickly prepare Dissociation solution by adding 1.9ml of 1x DNAse to a frozen 100\u00b5l aliquot of 20x cold Protease. Mix by pipetting.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#FFED92","section_duration":1500,"critical":[],"critical_id":null,"duration":0},{"id":884042,"guid":"7E8549A051BB11EA8B3BF740E8D91DCC","previous_id":null,"previous_guid":null,"modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"767243B4088840D9A7FAF08A6EC8A404","order_id":1,"type_id":6,"title":"Section","source":{"title":"Prep work"}},{"id":1054724,"guid":"2CD6A85332A0416FA0A9BB11D311ED7B","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Hydrate, aliquot, and freeze 2x DNAse<\/div><br\/><div class = \"text-block\">Hydrate to 5mg\/ml in DPBS; keep cold; aliquot 1ml each into 2ml tubes and freeze immediately.<\/div><div class = \"text-block\">Store at -20\u02dac and use within 6 months.<\/div><\/div>"}},{"id":1054725,"guid":"BF0F812051BB11EA8B3BF740E8D91DCC","order_id":2,"type_id":20,"title":"reagents","source":{"id":349706,"mol_weight":0,"name":"DNAse","linfor":"","url":"https:\/\/www.sigmaaldrich.com\/catalog\/product\/sigma\/dn25","sku":"DN25-100MG","cas_number":"9003-98-9","rrid":"","public":0,"vendor":{"name":"Sigma Aldrich","affiliation":null,"affiliations":[],"username":null,"note":null,"link":"http:\/\/sigmaaldrich.com","image":{"source":"https:\/\/www.protocols.iohttps:\/\/www.protocols.io\/img\/vendors\/placeholder.png","placeholder":"https:\/\/www.protocols.iohttps:\/\/www.protocols.io\/img\/vendors\/placeholder.png"},"badges":[],"verified":0,"is_verified_user":false,"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false,"id":17},"can_edit":0,"stats":{"total_protocols":0}}},{"id":1054726,"guid":"7ED7E10051BC11EA8B3BF740E8D91DCC","order_id":3,"type_