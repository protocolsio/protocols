4,"guid":"64DE588D9E304B08AC702E6381D0A46A","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Remove all EtOH and allow to pellet to dry for 5 minutes<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#FFED92","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1156296,"guid":"E0FF4170963511EB9D87F1AF982731CD","previous_id":1156295,"previous_guid":"D6A34730963511EB9D87F1AF982731CD","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"F150E7E454424F478D096CC7AC7A1CF0","order_id":1,"type_id":6,"title":"Section","source":{"title":"Library Quantification & Pooling"}},{"id":1054724,"guid":"55E7EE1505FC430F98EE9D620125B0C5","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Remove tube from magnet and add 20 \u00b5L Tris-HCl pH 8.0 to pellet. Slowly pipette mix<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#FFED92","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1156297,"guid":"E6232C70963511EB9D87F1AF982731CD","previous_id":1156296,"previous_guid":"E0FF4170963511EB9D87F1AF982731CD","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"4C61ED1292004289ABC7C30E8BD3719D","order_id":1,"type_id":6,"title":"Section","source":{"title":"Library Quantification & Pooling"}},{"id":1054724,"guid":"5570D19894C5436EAEE234F6C3DA9AA1","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Incubate at room temperature for at least 5 minutes before clearing with magnet<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#FFED92","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1156300,"guid":"07BB5510963611EB9D87F1AF982731CD","previous_id":1156297,"pr