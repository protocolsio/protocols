:0,"number_of_steps":7,"number_of_bookmarks":0,"number_of_comments":0,"number_of_bookmarked_comments":0,"number_of_steps_comments":0,"number_of_protocol_comments":0,"number_of_exports":0,"number_of_runs":0,"number_of_votes":0,"number_of_reagents":0,"number_of_equipments":0,"number_of_collections":0,"number_of_forks":{"private":0,"public":0},"number_of_accessible_forks":0},"status":{"id":1,"info":"We use this protocol and it's working"},"steps":[{"id":1846216,"guid":"CCEC64C351644E5BAAFDDB27DB611CFC","previous_id":0,"previous_guid":null,"section":"\u003cp\u003eElectron microscopy (EM)\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"85bsq\",\"text\":\"\\nHippocampal neurons were plated at a density of\\n60,000 cells/cm2. DIV-3 neurons were infected with lentiviruses\\ncarrying α-syn tagged at the C-terminus to mScarlet, or mScarlet alone\\n(multiplicity of infection or MOI=2.5). The transduced neurons were cultured to\\nmaturity (DIV17-DIV21) before imaging. The ~100 % infection efficiency of the\\nmScarlet-tagged constructs was confirmed before using the coverslip for EM\\nexperiments. \\n\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"sup\",\"offset\":64,\"length\":1}],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90812,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"1","cases":[],"critical":null},{"id":1846217,"guid":"8CDC1E3FA45E48BE9FEB6DF1A8DC8B02","previous_id":1846216,"previous_guid":"CCEC64C351644E5BAAFDDB27DB611CFC","section":"\u003cp\u003eElectron microscopy (EM)\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"aun1i\",\"text\":\"EM embedding protocol for monolayer cultured cells was performed\\non ice. The coverslips were fixed in 2 % glutaraldehyde and in 0.1 M sodium\\ncacodylate (SC buffer) buffer for at least 60min at 4 °C, washed 3 min with 0.1\\nM SC buffer (5 times), and postfixed in 1% osmium tetroxide (OsO4) diluted in\\n0.1 M SC buffer for 30 min to 1hr. \\n\\n\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90812,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"2","cases":[],"critical":null},{"id":1846218,"guid":"41C9AEEDA98F47E6A27A35A19AA170A6","previous_id":1846217,"previous_guid":"8CDC1E3FA45E48BE9FEB6DF1A8DC8B02","section":"\u003cp\u003eElectron microscopy (EM)\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"3ov5o\",\"text\":\"The coverslips were then washed for 3 min (5\\ntimes) with 0.1 M SC buffer, followed by a water rinse, sequential ethanol\\ndehydration (20%, 50%, 70%, 90%, 2x100%) 1min each, and subsequent incubation\\nwith dry acetone for 1 min at room temperature (2 times). \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90812,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"3","cases":[],"critical":null},{"id":1846219,"guid":"17990BED849C4B67A37468AD84E74885","previous_id":1846218,"previous_guid":"41C9AEEDA98F47E6A27A35A19AA170A6","section":"\u003cp\u003eElectron microscopy (EM)\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"47brf\",\"text\":\"The samples were then incubated with a Durcupan:Acetone solution (50:50%) for 1hr in rotation,\\nfollowed by incubation with 100% Durcupan for 1 hr (2 times). The cells were\\nthen embedded using foil plates along with wood sticks in the embedding oven\\n(overnight incubation). \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90812,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4","cases":[],"critical":null},{"id":1846220,"guid":"34EE8555AFF04E3D9147D92A029C362C","previous_id":1846219,"previous_guid":"17990BED849C4B67A37468AD84E74885","section":"\u003cp\u003eElectron microscopy (EM)\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"brcs7\",\"text\":\"60 nm ultrathin sections were cut with a diamond knife\\nand mounted on 300 mesh grids. Grids were post-stained with 2% of uranyl\\nacetate for 5 min and lead for 1 min. JEOL 1400 Electron Microscopy and Gatan\\ndigital Camera were used to obtain images. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90812,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"5","cases":[],"critical":null},{"id":1846221,"guid":"E0090FF41159409B9CE434504BCF1FF4","previous_id":1846220,"previous_guid":"34EE8555AFF04E3D9147D92A029C362C","section":"\u003cp\u003equantification\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"b86s7\",\"text\":\"\\n\\n\\n\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"abovr\",\"text\":\"For quantification of inter-vesicular (IV) distances between vesicles, we\\nconsidered all the vesicles in the synapse encompassing 1µmx1µm. To mark each\\nvesicle, a line was drawn across the center of the vesicle, recording its\\ndiameter and its position.                                                        \\n\",\"type\":\"align-justify\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"7klog\",\"text\":\"\\n\\n\\n\\n\\n\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90812,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"6","cases":[],"critical":null},{"id":1846222,"guid":"60B22D6EE4264E83B9210671BA996199","previous_id":1846221,"previous_guid":"E0090FF41159409B9CE434504BCF1FF4","section":"\u003cp\u003equantification\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"n5s5\",\"text\":\"These parameters were then fed into our algorithm on MATLAB (RRID:SCR_001622)( http://www.mathworks.com/products/matlab/), which gave the output of IV distances\\nfor a particular image. The IV distance for all images per condition was pooled\\ntogether. This algorithm is available upon request.         \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90812,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"7","cases":[],"critical":null}],"template_id":1,"title":"Electron microscopy (EM) and quantification","title_html":"Electron microscopy (EM) and quantification","type_id":1,"units":[{"id":1,"type_id":3,"name":"µL","can_manage":0,"read_only":0},{"id":2,"type_id":3,"name":"mL","can_manage":0,"read_only":0},{"id":3,"type_id":3,"name":"L","can_manage":0,"read_only":0},{"id":4,"type_id":3,"name":"µg","can_manage":0,"read_only":0},{"id":5,"type_id":3,"name":"mg","can_manage":0,"read_only":0},{"id":6,"type_id":3,"name":"g","can_manage":0,"read_only":0},{"id":7,"type_id":3,"name":"kg","can_manage":0,"read_only":0},{"id":8,"type_id":3,"name":"ng","can_manage":0,"read_only":0},{"id":9,"type_id":3,"name":"Hz","can_manage":0,"read_only":0},{"id":10,"type_id":24,"name":"°C","can_manage":0,"read_only":0},{"id":11,"type_id":24,"name":"°К","can_manage":0,"read_only":0},{"id":12,"type_id":24,"name":"°F","can_manage":0,"read_only":0},{"id":13,"type_id":25,"name":"Mass Percent","can_manage":0,"read_only":0},{"id":14,"type_id":25,"name":"% volume","can_manage":0,"read_only":0},{"id":15,"type_id":25,"name":"Mass / % volume","can_manage":0,"read_only":0},{"id":16,"type_id":25,"name":"Parts per Million (PP