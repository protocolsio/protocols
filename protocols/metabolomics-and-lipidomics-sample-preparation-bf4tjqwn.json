false,"limited_run":false,"limited_private_links":false,"limited_blind_links":false,"is_locked":false},"space_id":1381,"state_version_id":104,"stats":{"is_voted":false,"number_of_views":71,"number_of_steps":9,"number_of_bookmarks":0,"number_of_comments":0,"number_of_bookmarked_comments":0,"number_of_steps_comments":0,"number_of_protocol_comments":0,"number_of_exports":31,"number_of_runs":0,"number_of_votes":0,"number_of_reagents":0,"number_of_equipments":0,"number_of_collections":0,"number_of_forks":{"private":0,"public":0},"number_of_accessible_forks":0},"status":{"id":1,"info":""},"steps":[{"id":934775,"guid":"B65C6550900B11EABA7EDD5E3C034762","previous_id":0,"previous_guid":null,"section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"40u3h\",\"text\":\"Roughly 30 mg of frozen tissues were homogenized in 500 µl ice-cold methanol by bead beating (MP bioscience cat# 6913-100, Solon, OH) at 4°C (2 x 45 s). \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":{}}","data":null,"protocol_id":36723,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"1","cases":[],"critical":null},{"id":934776,"guid":"EE35E190900B11EABA7EDD5E3C034762","previous_id":934775,"previous_guid":"B65C6550900B11EABA7EDD5E3C034762","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"430v2\",\"text\":\"1 ml of MTBE was added to 300 μl of the homogenate spiked-in with 40 µL deuterated lipid internal standards (Sciex, cat#: 5040156, lot#: LPISTDKIT-101). \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":{}}","data":null,"protocol_id":36723,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"2","cases":[],"critical":null},{"id":934777,"guid":"147C28F0900C11EABA7EDD5E3C034762","previous_id":934776,"previous_guid":"EE35E190900B11EABA7EDD5E3C034762","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"4edn\",\"text\":\"The samples were then sonicated (3 x 30 s) and agitated at 4°C for 30 min.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":{}}","data":null,"protocol_id":36723,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"3","cases":[],"critical":null},{"id":934778,"guid":"1BCA1FE0900C11EABA7EDD5E3C034762","previous_id":934777,"previous_guid":"147C28F0900C11EABA7EDD5E3C034762","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"c9vs7\",\"text\":\"After addition of 250 μl of ice-cold water, the samples were vortexed for 1 min and centrifuged at 14,000 g for 5 min at 20°C. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":{}}","data":null,"protocol_id":36723,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4","cases":[],"critical":null},{"id":934779,"guid":"235DD260900C11EABA7EDD5E3C034762","previous_id":934778,"previous_guid":"1BCA1FE0900C11EABA7EDD5E3C034762","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"8f57v\",\"text\":\"The upper organic phase contains the lipids, the lower aqueous phase contains the metabolites and the proteins are precipitated at the bottom of the tube. For quality controls, 3 reference plasma samples (40 µL plasma) and 1 preparation blank were prepared in parallel.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":{}}","data":null,"protocol_id":36723,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"5","cases":[],"critical":null},{"id":934780,"guid":"3397B8D0900C11EABA7EDD5E3C034762","previous_id":934779,"previous_guid":"235DD260900C11EABA7EDD5E3C034762","section":"\u003cp\u003eMetabolites\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"61jq9\",\"text\":\"Proteins were further precipitated by adding 700 μl of 33/33/33 acetone/acetonitrile/methanol spiked-in with 15 labeled metabolite internal standards to 300 μl of the aqueous phase and 200 μl of the lipid phase and incubating the samples overnight at -20°C\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":{}}","data":null,"protocol_id":36723,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"6","cases":[],"critical":null},{"id":934781,"guid":"482FF050900C11EABA7EDD5E3C034762","previous_id":934780,"previous_guid":"3397B8D0900C11EABA7EDD5E3C034762","section":"\u003cp\u003eMetabolites\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"5ittq\",\"text\":\"After centrifugation at 17,000 g for 10 min at 4°C, the metabolic extracts were dried down to completion under a stream of nitrogen and resuspended in 100 μl 50/50 methanol/water.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":{}}","data":null,"protocol_id":36723,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"7","cases":[],"critical":null},{"id":934782,"guid":"4EECF1E0900C11EABA7EDD5E3C034762","previous_id":934781,"previous_guid":"482FF050900C11EABA7EDD5E3C034762","section":"\u003cp\u003eComplex lipids\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"1f2ii\",\"text\":\"700 µl of the organic upper phase was dried down and resolubilized in 200 μl of methanol for storage at -20°C until analysis. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":{}}","data":null,"protocol_id":36723,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"8","cases":[],"cr