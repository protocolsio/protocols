:"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1003624,"guid":"CD65CBE0DDD311EA97D5ABEA9173794E","previous_id":1003623,"previous_guid":"CD62E5B0DDD311EA97D5ABEA9173794E","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"93DAF1604AF1498AA2949BA4636940EE","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"7F3444351462438B8E6BC0FD4E0AC795","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">The nitrocellulose membranes are blocked overnight in 10% non-fat skim milk in PBS with 0.05% Tween-20 pH 7.4 and then washed 4x, 10 minutes with PBS-Tween 20.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1003625,"guid":"CD694E50DDD311EA97D5ABEA9173794E","previous_id":1003624,"previous_guid":"CD65CBE0DDD311EA97D5ABEA9173794E","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"8453852740A14CEF8DFE584EB37C0FDE","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"64017762BD14460FB2A22E79DC2C3E26","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Recombinant protein LA (Sigma-Aldrich Co, St Louis, Missouri) at a concentration of 5 \u00b5g\/ml is added to the membranes.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1003626,"guid":"CD6D9410DDD311EA97D5ABEA9173794E","previous_id":1003625,"previous_guid":"CD694E50DDD311EA97D5ABEA9173794E","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"393A746920614C269B86EDCD9157BD8B","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"574B228657014A6D95B7DCF7A0AF1E9F","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">After that there is an incubation period at 4\u00b0C overnight.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1003627,"guid":"CD729D20DDD311EA97D5ABEA9173794E","previous_id":1003626,"previous_guid":"CD6D9410DDD311EA97D5ABEA9173794E","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"A57D72F19222450C819EF4F1D714FA22","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"FB4EF96D448742AAA242B3108BDEDD2F","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">The nitrocellulose membranes were washed as above.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1003628,"guid":"CD7709F0DDD311EA97D5ABEA9173794E","previous_id":1003627,"previous_guid":"CD729D20DDD311EA97D5ABEA9173794E","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"B2834946D08F447AB4EF5852B886C30E","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"6E8E7D7ABDC44815B242A6E2A7D342BE","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">A secondary antibody (rabbit anti-chicken IgY horseradish peroxidase, Sigma Aldrich) is added at a 1:15 000 dilution.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1003629,"guid":"CD7B9DD0DDD311EA97D5ABEA9173794E","previous_id":1003628,"previous_guid":"CD7709F0DDD311EA97D5ABEA9173794E","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"DEF07F41DAFF4F34A185473F9712D166","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"A85D769DB54D43ABB09E7566258D6EC0","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">It is incubated for one hour at room temperature and washed as above.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1003630,"guid":"CD80A6E0DDD311EA97D5ABEA9173794E","previous_id":1003629,"previous_guid":"CD7B9DD0DDD311EA97D5ABEA9173794E","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"8417FA9BB20E46A6BEBB424BE4200A66","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"F9C7BEE8C9DB487C8BF94E8BC3675EB1","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Tetramethyl-benzidine (TMB) solution is added to the nitrocellulose membranes, which are then incubated in the dark for a few minutes.  Then, the membranes are shaken gently and rinsed thoroughly in de-ionized water to stop the blotting process and are left to dry.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1003631,"guid":"CD867340DDD311EA97D5ABEA9173794E","previous_id":1003630,"previous_guid":"CD80A6E0DDD311EA97D5ABEA9173794E","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"76B794AEA8CE43D5A907FF84A3F1D344","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"4C95BCC7D99A474E91750AC5039809A4","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Alternatively, Ig samples are transferred to nitrocellulose membranes and directly probed using SpLA-HRP (diluted 1:5000) and TMB (this system was mainly used for detecting avian Igs).<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0}],"document":null,"materials":[],"description":null,"changed_on":1597371809}