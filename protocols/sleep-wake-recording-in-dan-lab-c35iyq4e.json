f_equipments":0,"number_of_collections":0,"number_of_forks":{"private":0,"public":0},"number_of_accessible_forks":0},"status":{"id":1,"info":"We use this protocol and it's working"},"steps":[{"id":1831305,"guid":"91048100B0B6450B9FB237D72326C8BD","previous_id":0,"previous_guid":null,"section":"\u003cp\u003eSurgery\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"5uppm\",\"text\":\"Mice were anesthetized with 1.5% isoflurane and placed in a stereotaxic frame. \\n\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":89994,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"1","cases":[],"critical":null},{"id":1831306,"guid":"DBC09C8B32CE422186FCA6C4E1AABD41","previous_id":1831305,"previous_guid":"91048100B0B6450B9FB237D72326C8BD","section":"\u003cp\u003eSurgery\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"9in0t\",\"text\":\"Body temperature was maintained using a heating pad. \\n\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":89994,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"2","cases":[],"critical":null},{"id":1831309,"guid":"EF7C6AFE45DD44DD8BE2BF3B1DA29E91","previous_id":1831306,"previous_guid":"DBC09C8B32CE422186FCA6C4E1AABD41","section":"\u003cp\u003eSurgery\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"2g16k\",\"text\":\"The skin was incised to expose the skull after asepsis and connective tissue was\\nremoved.\\n\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":89994,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"3","cases":[],"critical":null},{"id":1831315,"guid":"2FC8B33E4429420C9B91854D382ACA7A","previous_id":1831309,"previous_guid":"EF7C6AFE45DD44DD8BE2BF3B1DA29E91","section":"\u003cp\u003eSurgery\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"3qirr\",\"text\":\"To implant electroencephalogram (EEG) and electromyogram (EMG) recording\\nelectrodes, two stainless steel screws were inserted into the skull 2.5 mm from\\nthe midline and 3 mm posterior to the bregma, and two EMG electrodes were also\\ninserted into the neck muscles. \\n\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":89994,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4","cases":[],"critical":null},{"id":1831318,"guid":"4E8E2A67187441C6BFEE643B5B124C87","previous_id":1831315,"previous_guid":"2FC8B33E4429420C9B91854D382ACA7A","section":"\u003cp\u003eSurgery\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"f10s7\",\"text\":\"A reference screw for grounding was placed on top of the left cerebellum. \\n\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":89994,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"5","cases":[],"critical":null},{"id":1831319,"guid":"AE5E6932A8ED4A4EB0D9E0C5790C196B","previous_id":1831318,"previous_guid":"4E8E2A67187441C6BFEE643B5B124C87","section":"\u003cp\u003eSurgery\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"bb4e8\",\"text\":\"Insulated leads from the EEG and EMG electrodes were soldered to a pin header, which was\\nsecured to the skull using dental cement.\\n\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":89994,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"6","cases":[],"critical":null},{"id":1831321,"guid":"A55DFA3E281447E2B17025227B0AC029","previous_id":1831319,"previous_guid":"AE5E6932A8ED4A4EB0D9E0C5790C196B","section":"\u003cp\u003eRecording\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"26hqv\",\"text\":\"After a minimum of 7 days of undisturbed post-surgery recovery, behavioral\\nexperiments were carried out in home cages placed in sound-attenuating boxes. \\n\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":89994,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"7","cases":[],"critical":null},{"id":1831322,"guid":"5CE6F13739A247839CA8559E1C341781","previous_id":1831321,"previous_guid":"A55DFA3E281447E2B17025227B0AC029","section":"\u003cp\u003eRecording\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"4eiiq\",\"text\":\"EEG and EMG electrodes were connected to flexible recording cables via a\\nmini-connector. \\n\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":89994,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"8","cases":[],"critical":null},{"id":1831323,"guid":"4CE4BAA2936A460B844168910EFC6820","previous_id":1831322,"previous_guid":"5CE6F13739A247839CA8559E1C341781","section":"\u003cp\u003eRecording\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"6n5f1\",\"text\":\"Recordings started after 1-2 days of habituation and continued for 3-4 days on a 12-h\\ndark/12-h light cycle. \\n\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":89994,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"9","cases":[],"critical":null},{"id":1831324,"guid":"62C730B0747C487FB8973C3359252C08","previous_id":1831323,"previous_guid":"4CE4BAA2936A460B844168910EFC6820","section":"\u003cp\u003eRecording\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"65fuh\",\"text\":\"Mouse behavior was recorded using a video camera at 10 frames per second (fps), along\\nwith EEG and EMG recordings. \\n\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":89994,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"10","cases":[],"critical":null},{"id":1831325,"guid":"F56193E594824D708C0909289A9AE5F1","previous_id":1831324,"previous_guid":"62C730B0747C487FB8973C3359252C08","section":"\u003cp\u003eRecording\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"cj6q7\",\"text\":\"The signals were recorded with a TDT RZ10x/PZ5-32 or LR10-SK1 system. \\n\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":89994,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"11","cases":[],"critical":null}],"template_id":1,"title":"Sleep-Wake Recording in Dan lab","title_html":"Sleep-Wake Recording in Dan lab","type_id":1,"units":[{"id":1,"type_id":3,"name":"µL","can_manage":0,"read_only":0},{"id":2,"type_id":3,"name":"mL","can_manage":0,"read_only":0},{"id":3,"type_id":3,"name":"L","can_manage":0,"read_only":0},{"id":4,"type_id":3,"name":"µg","can_manage":0,"read_only":0},{"id":5,"type_id":3,"name":"mg","can_manage":0,"read_only":0},{"id":6,"type_id":3,"name":"g","can_manage":0,"read_only":0},{"id":7,"type_id":3,"name":"kg","can_manage":0,"read_only":0},{"id":8,"type_id":3,"name":"ng","can_manage":0,"read_only"