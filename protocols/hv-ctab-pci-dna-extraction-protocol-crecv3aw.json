rds":true,"can_remove_fork":false,"can_sign":false,"child_steps":{},"cited_protocols":[],"collection_items":[],"created_on":1679094181,"creator":{"name":"Vicky Ooi","affiliation":"","affiliation_url":null,"username":"vicky-ooi","link":"","image":{"source":"/img/avatars/004.png","placeholder":"/img/avatars/004.png","webp_source":""},"badges":[{"id":2,"name":"Author","image":{"source":"/img/badges/bronze.svg","placeholder":"/img/badges/bronze.svg"}}],"affiliations":[]},"description":"{\"blocks\":[{\"key\":\"5a8gq\",\"text\":\"A ‘High Volume­­- Cetyltrimethyl Ammonium Bromide- Phenol-Chloroform-Isoamyl Alcohol ’ (HV-CTAB-PCI) method was developed.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"sub\",\"offset\":14,\"length\":1}],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","disclaimer":"","document":"","documents":[],"doi":null,"doi_status":0,"ethics_statement":null,"fork_id":null,"fork_info":null,"forks":[],"funders":[{"funder_name":"Sea World Research and Rescue Foundation Inc.","grant_id":"SWR/1/2015; SWR/6/2016"}],"groups":[],"guid":"C0CBD690C51611EDA6E6B3D6AC385465","guidelines":"","has_references":false,"has_step_reagents":false,"has_versions":false,"id":79012,"image":{"source":"https://www.protocols.io/img/default_protocol.png","webp_source":null,"placeholder":"https://www.protocols.io/img/default_protocol.png","webp_placeholder":null},"image_attribution":"","in_trash":false,"is_bookmarked":false,"is_contact_suspended":false,"is_content_confidential":false,"is_content_warning":false,"is_doi_reserved":false,"is_in_pending_publishing":false,"is_in_transfer":false,"is_owner":true,"is_research":true,"is_retracted":false,"is_shared_directly":false,"is_subprotocol":null,"is_unlisted":false,"item_id":1095642,"journal":null,"journals":[],"keywords":"","last_modified":1679095950,"link":"","location":null,"manuscript_citation":"","materials":[],"materials_text":"","ownership_history":null,"parent_collections":[],"parent_protocols":[],"peer_reviewed":false,"protocol_references":"","public":true,"public_fork_note":"","published_on":1679095950,"references":[],"related_equipments":[],"related_materials":[],"reserved_doi":"","retraction_reason":null,"samples":{},"shared_access_id":107,"show_comparison":false,"sign_info":null,"state_version_id":56,"stats":{"is_voted":false,"number_of_views":12,"number_of_steps":17,"number_of_bookmarks":0,"number_of_comments":0,"number_of_bookmarked_comments":0,"number_of_steps_comments":0,"number_of_protocol_comments":0,"number_of_exports":0,"number_of_runs":0,"number_of_votes":1,"number_of_reagents":0,"number_of_equipments":0,"number_of_collections":0,"number_of_forks":{"private":0,"public":0},"number_of_accessible_forks":0},"status":{"id":1,"info":"We use this protocol and it's working"},"steps":[{"id":1630346,"guid":"DD461230C51711EDA6E6B3D6AC385465","previous_id":0,"previous_guid":null,"section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"6atq4\",\"text\":\"Clean the working bench with TriGene.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":79012,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"1","cases":[],"critical":null},{"id":1630347,"guid":"DD46AE70C51711EDA6E6B3D6AC385465","previous_id":1630346,"previous_guid":"DD461230C51711EDA6E6B3D6AC385465","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"82pqf\",\"text\":\"Scrape 1 g of faecal material from the outer surface of a faeces and put it into a 15 mL centrifuge tube. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":79012,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"2","cases":[],"critical":null},{"id":1630348,"guid":"DD474AB1C51711EDA6E6B3D6AC385465","previous_id":1630347,"previous_guid":"DD46AE70C51711EDA6E6B3D6AC385465","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"9irht\",\"text\":\"Transfer the faecal material into a mortar and grind the faeces into powder with liquid nitrogen.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":79012,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"3","cases":[],"critical":null},{"id":1630349,"guid":"DD47E6F1C51711EDA6E6B3D6AC385465","previous_id":1630348,"previous_guid":"DD474AB1C51711EDA6E6B3D6AC385465","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"cliu0\",\"text\":\"Add 1 mL of Lysis Buffer 1 (LB1: CTAB 2%, Tris– HCL 100 mM, EDTA 20 mM, NaCl 1.4 M, pH 7.5) to the mortar containing the faecal material to further grind and mix in the buffer with the ground faeces. Transfer the liquid back into the 15 mL tube.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":79012,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4","cases":[],"critical":null},{"id":1630350,"guid":"DD48D150C51711EDA6E6B3D6AC385465","previous_id":1630349,"previous_guid":"DD47E6F1C51711EDA6E6B3D6AC385465","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"9ka6d\",\"text\":\"Add another 1 mL of LB1 to the mortar to mix in any leftover faecal material on the mortar and transfer the liquid back into the 15 mL tube. Repeat this step once more, and then add 2 mL of LB1 to the 15 mL tube containing the faecal homogenate. Thus, a total of 5 mL of LB1 would be added to the ground faeces.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":79012,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"5","cases":[],"critical":null},{"id":1630351,"guid":"DD4994A0C51711EDA6E6B3D6AC385465","previous_id":1630350,"previous_guid":"DD48D150C51711EDA6E6B3D6AC385465","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"buqfk\",\"text\":\"Vortex the faecal homogenate and incubate in a thermomixer for 3 h, with occasional mixing, at 60°C for cell lysis.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":79012,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"6","cases":[],"critical":null},{"id":1630352,"guid":"DD4A57F1C51711EDA6E6B3D6AC385465","previous_id":1630351,"previous_guid":"DD4994A0C51711EDA6E6B3D6AC385465","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"3r2q6\",\"text\":\"Centrifuge the sample at 3,150 g (4,000 rpm) for 12 min and pipette 4 mL of the supernatant into a new 15 mL tube.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"italic\",\"offset\":31,\"length\":2}],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":79012,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"7","cases":[],"critical":null},{"id":1630353,"guid":"DD4B6960C51711EDA6E6B3D6AC385465","previous_id":1630352,"previous_guid":"DD4A57F1C51711EDA6E6B3D6AC385465","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"5d8so\",\"text\":\"Add 4 mL of phenol: chloroform: isoamyl alcohol (21:20:1) to the supernatant, then gently mix the tube. Centrifuge the sample for 3 min at 3,150 g (4,000 rpm) and pipette 3 mL of the aqueous phase into a new 15 mL tube.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"italic\",\"offset\":145,\"length\":1}],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":79012,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"8","cases":[],"critical":null},{"id":1630354,"guid":"DD4C7AD1C51711EDA6E6B3D6AC385465","previous_id":1630353,"previous_guid":"DD4B6960C51711EDA6E6B3D6AC385465","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"912po\",\"text\":\"Add 330 µL of Lysis Buffer 2 (LB2: CTAB 10%, NaCl 0.5 M, pH 5.5) to the aqueous phase, and leave it to lyse further at 60°C for 4 h.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":79012,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"9","cases":[],"critical":null},{"id":1630355,"guid":"DD4DB351C51711EDA6E6B3D6AC385465","previous_id":1630354,"previous_guid":"DD4C7AD1C51711EDA6E6B3D6AC385465","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"20h9g\",\"text\":\"Add 104 uL of protease to the sample and leave it to digest proteins for another 1 h.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":79012,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"10","cases":[],"critical":null},{"id":1630356,"guid":"DD4F12E0C51711EDA6E6B3D6AC385465","previous_id":1630355,"previous_guid":"DD4DB351C51711EDA6E6B3D6AC385465","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"d0ugo\",\"text\":\"Add 3434 µL of phenol: chloroform: isoamyl alcohol (21:20:1) to the mixture, and then gently mix the tube well, and centrifuge the sample for 12 min at 3,150 g (4,000 rpm). Then, pipette 3 mL of the aqueous phase into a new 15 mL tube.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"italic\",\"offset\":158,\"length\":1}],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":79012,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"11","cases":[],"critical":null},{"id":1630357,"guid":"DD507270C51711EDA6E6B3D6AC385465","previous_id":1630356,"previous_guid":"DD4F12E0C51711EDA6E6B3D6AC385465","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"799da\",\"text\":\"Add one volume of isopropanol (i.e., 3 mL) to precipitate the DNA overnight at -20°C.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":79012,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"12","cases":[],"critical":null},{"id":1630358,"guid":"DD522020C51711EDA6E6B3D6AC385465","previous_id":1630357,"previous_guid":"DD507270C51711EDA6E6B3D6AC385465","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"9oudh\",\"text\":\"Centrifuge the sample for 20 min at 8000 g (5,200 rpm), and then get rid of all the supernatant.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"italic\",\"offset\":41,\"length\":1}],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":79012,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"13","cases":[],"critical":null},{"id":1630359,"guid":"DD546A10C51711EDA6E6B3D6AC385465","previous_id":1630358,"previous_guid":"DD522020C51711EDA6E6B3D6AC385465","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"bht96\",\"text\":\"Add 400 µL of 70% ethanol to the pellet to wash it. Vortex and then centrifuge the sample at 3,150 g for 12 min and get rid of the supernatant.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"italic\",\"offset\":99,\"length\":2}],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":79012,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"14","cases":[],"critical":null},{"id":1630360,"guid":"DD56B400C51711EDA6E6B3D6AC385465","previous_id":1630359,"previous_guid":"DD546A10C51711EDA6E6B3D6AC385465","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"anj5n\",\"text\":\"Dry the pellet at room temperature for 15 min.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":79012,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"15","cases":[],"critical":null},{"id":1630361,"guid":"DD594C11C51711EDA6E6B3D6AC385465","previous_id":1630360,"previous_guid":"DD56B400C51711EDA6E6B3D6AC385465","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"dqaor\",\"text\":\"Resuspend the pellet in 250 µL of TE buffer (10 mM Tris–HCl, 1 mM EDTA, pH 8).\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":79012,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"16","cases":[],"critical":null},{"id":1630362,"guid":"DD5B9600C51711EDA6E6B3D6AC385465","previous_id":1630361,"previous_guid":"DD594C11C51711EDA6E6B3D6AC385465","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"2aclj\",\"text\":\"Store the DNA isolate at -20°C for use within a week or at -80°C for longer-time storage.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":79012,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"17","cases":[],"critical":null}],"template_id":1,"title":"HV-CTAB-PCI DNA Extraction Protocol","title_html":"HV-CTAB-PCI DNA Extraction Protocol","type_id":1,"units":[{"id":1,"type_id":3,"name":"µL","can_manage":0,"read_only":0},{"id":2,"type_id":3,"name":"mL","can_manage":0,"read_only":0},{"id