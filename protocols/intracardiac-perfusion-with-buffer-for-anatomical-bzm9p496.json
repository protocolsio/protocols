ource":"/img/vendors/placeholder.png","placeholder":"/img/vendors/placeholder.png"}}},{"guid":"41A5D236341F11EE9D4302C0B41BC903","mol_weight":0,"name":"Heparin sodium ","linfor":"","url":"http://www.ellar.com.au","sku":"","cas_number":"","rrid":"","public":1,"can_edit":0,"entries":[{"section":"","step":"","field":"materials_text"}],"vendor":{"id":483,"name":"Ellar Laboratories","link":"http://www.ellar.com.au","is_requested":false,"image":{"source":"/img/vendors/placeholder.png","placeholder":"/img/vendors/placeholder.png"}}}],"reserved_doi":"","retraction_reason":null,"samples":{},"shared_access_id":109,"show_comparison":true,"sign_info":null,"space_access":{"can_view":false,"can_edit":false,"can_remove":false,"can_add":false,"can_publish":false,"can_get_doi":false,"can_share":false,"can_move":false,"can_move_outside":false,"can_transfer":false,"can_download":false,"limited_run":false,"limited_private_links":false,"limited_blind_links":false,"is_locked":false},"space_id":1100,"state_version_id":42,"stats":{"is_voted":false,"number_of_views":4,"number_of_steps":8,"number_of_bookmarks":0,"number_of_comments":0,"number_of_bookmarked_comments":0,"number_of_steps_comments":0,"number_of_protocol_comments":0,"number_of_exports":0,"number_of_runs":0,"number_of_votes":0,"number_of_reagents":7,"number_of_equipments":0,"number_of_collections":0,"number_of_forks":{"private":0,"public":0},"number_of_accessible_forks":0},"status":{"id":1,"info":""},"steps":[{"id":1279342,"guid":"8740D5253ACD11ECA7920A58A9FEAC02","previous_id":null,"previous_guid":null,"section":"Preparation for perfusion","section_color":"#f8e71c","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"ca5ot\",\"text\":\"Make up the following solutions:\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"50tgv\",\"text\":\"\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"dc7do\",\"text\":\"Perfusion prewash solution: To 300 ml 0.9% sodium chloride (w/v) add 3.75 ml 1% sodium nitrite (w/v) and 0.11 ml heparin (5000 IU/ml). This is made up immediately prior to use.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"bold\",\"offset\":0,\"length\":28}],\"entityRanges\":[],\"data\":{}},{\"key\":\"7cmac\",\"text\":\"\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"74obp\",\"text\":\"Fixative: 4% paraformaldehyde in 0.1M phosphate buffer, pH 7.4. This is made up no longer than 48h prior to use, stored at 4C and brought to room temperature on the day of perfusion.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"bold\",\"offset\":0,\"length\":10}],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":54689,"case_id":0,"critical_ids":"","duration":0,"original_id":860766,"number":"1","cases":[],"critical":null},{"id":1279343,"guid":"874113FF3ACD11ECA7920A58A9FEAC02","previous_id":1279342,"previous_guid":"8740D5253ACD11ECA7920A58A9FEAC02","section":"Perfusion","section_color":"#b8e986","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"e9q2o\",\"text\":\"Induce anesthesia by an intraperitoneal injection of ketamine (100 mg/kg) and xylazine (10 mg/kg).\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":{}}","data":null,"protocol_id":54689,"case_id":0,"critical_ids":"","duration":0,"original_id":860767,"number":"2","cases":[],"critical":null},{"id":1279344,"guid":"87414D5C3ACD11ECA7920A58A9FEAC02","previous_id":1279343,"previous_guid":"874113FF3ACD11ECA7920A58A9FEAC02","section":"Perfusion","section_color":"#b8e986","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"8022a\",\"text\":\"After opening the chest cavity, inject the left ventricle with mixture of 0.25 ml heparin (5000 IU/ml) and 0.5 ml 1% sodium nitrite.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":{}}","data":null,"protocol_id":54689,"case_id":0,"critical_ids":"","duration":0,"original_id":860768,"number":"3","cases":[],"critical":null},{"id":1279345,"guid":"874187003ACD11ECA7920A58A9FEAC02","previous_id":1279344,"previous_guid":"87414D5C3ACD11ECA7920A58A9FEAC02","section":"Perfusion","section_color":"#b8e986","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"3km1e\",\"text\":\" A needle connected to tubing (T-connector to prewash and perfusion solutions), and a peristaltic pump (setting: 50 ml/min) is then inserted into the left ventricle and clamped into place using hemostats. Make a small incision in the right atrium to drain blood and perfusate during the procedure.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":{}}","data":null,"protocol_id":54689,"case_id":0,"critical_ids":"","duration":0,"original_id":860769,"number":"4","cases":[],"critical":null},{"id":1279346,"guid":"8741BF893ACD11ECA7920A58A9FEAC02","previous_id":1279345,"previous_guid":"874187003ACD11ECA7920A58A9FEAC02","section":"Perfusion","section_color":"#b8e986","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"cmehf\",\"text\":\" Perfuse with pre-wash solution until the fluid flowing from the right atrium is clear, the liver and extremities are pale (typically 2-3 min). \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":{}}","data":null,"protocol_id":54689,"case_id":0,"critical_ids":"","duration":0,"original_id":860770,"number":"5","cases":[],"critical":null},{"id":1279348,"guid":"87422D853ACD11ECA7920A58A9FEAC02","previous_id":1279346,"previous_guid":"8741BF893ACD11ECA7920A58A9FEAC02","section":"Perfusion","section_color":"#b8e986","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"3ip26\",\"text\":\"Dissect tissues required 