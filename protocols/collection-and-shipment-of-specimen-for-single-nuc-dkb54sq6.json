of_protocol_comments":0,"number_of_exports":0,"number_of_runs":0,"number_of_votes":0,"number_of_reagents":0,"number_of_equipments":0,"number_of_collections":0,"number_of_forks":{"private":0,"public":0},"number_of_accessible_forks":0},"status":{"id":1,"info":"We use this protocol and it's working"},"steps":[{"id":2206867,"guid":"008FCB383EDD4CCE860E01B258E4804D","previous_id":0,"previous_guid":null,"section":"\u003cp\u003eReagents and Materials\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"9j9co\",\"text\":\"2mL Cryovials/ screw-cap tubes\",\"type\":\"unordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"6l134\",\"text\":\"ice cold 1X PBS\",\"type\":\"unordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"b69sl\",\"text\":\"Kimwipes\",\"type\":\"unordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"3nhrj\",\"text\":\"Petri dishes\",\"type\":\"unordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"972nu\",\"text\":\"Liquid Nitrogen\",\"type\":\"unordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"89i6c\",\"text\":\"Dry ice\",\"type\":\"unordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"5tjbb\",\"text\":\"Tweezers (clean, sterile)\",\"type\":\"unordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":106589,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"1","cases":[],"critical":null},{"id":2206868,"guid":"A133D02E34E04DFA8EE1F4B785E3D413","previous_id":2206867,"previous_guid":"008FCB383EDD4CCE860E01B258E4804D","section":"\u003cp\u003eQuality Key Points:\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"1dq36\",\"text\":\"The tissue specimen should be kept at 4 degrees Celsius and RNase-free until snap-freezing and maintained at -80 degrees C or on dry ice thereafter, until it is processed at JAX. \",\"type\":\"unordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"828ua\",\"text\":\"It is crucial to not store the tissue specimen at RT to avoid freeze-thaw cycles, any cell death, and tissue and/or RNA degradation. \",\"type\":\"unordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":106589,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"2","cases":[],"critical":null},{"id":2206870,"guid":"45FF8909FE1C498EA4DC437E4C31FD72","previous_id":2206868,"previous_guid":"A133D02E34E04DFA8EE1F4B785E3D413","section":"\u003cp\u003eProcedure:\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"debcj\",\"text\":\"Timeline: The daily shipping deadline at Jax BH is 12:00 noon. So, harvest the tissues before and closer to noon.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"bold\",\"offset\":0,\"length\":9}],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":106589,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"3","cases":[],"critical":null},{"id":2206875,"guid":"F680C500708143319695BC09D8E64321","previous_id":2206870,"previous_guid":"45FF8909FE1C498EA4DC437E4C31FD72","section":"\u003cp\u003eProcedure:\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"9kc3c\",\"text\":\"Collection/Harvest:\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"bold\",\"offset\":0,\"length\":19}],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":106589,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4","cases":[],"critical":null},{"id":2206877,"guid":"9DD80DADC742445080D3589A0B1F0306","previous_id":2206875,"previous_guid":"F680C500708143319695BC09D8E64321","section":"\u003cp\u003eProcedure:\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":true,"step":"{\"blocks\":[{\"key\":\"chl4\",\"text\":\"Animal was euthanized via cervical dislocation. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":106589,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4.1","cases":[],"critical":null},{"id":2206878,"guid":"203825662C594C0A889D22B050CCC678","previous_id":2206877,"previous_guid":"9DD80DADC742445080D3589A0B1F0306","section":"\u003cp\u003eProcedure:\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":true,"step":"{\"blocks\":[{\"key\":\"34utv\",\"text\":\"Animal was pinned to a necropsy tray which remained on wet ice throughout the harvest.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":106589,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4.2","cases":[],"critical":null},{"id":2206879,"guid":"EED929FCFE9D426FBA52D2CC10842DAE","previous_id":2206878,"previous_guid":"203825662C594C0A889D22B050CCC678","section":"\u003cp\u003eProcedure:\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":true,"step":"{\"blocks\":[{\"key\":\"7pmsc\",\"text\":\" Animal was perfused with 20ml cold PBS.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":106589,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4.3","cases":[],"critical":null},{"id":2206880,"guid":"B701831E9BF14DFE9DF8D13A77F74CAE","previous_id":2206879,"previous_guid":"EED929FCFE9D426FBA52D2CC10842DAE","section":"\u003cp\u003eProcedure:\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":true,"step":"{\"blocks\":[{\"key\":\"esugb\",\"text\":\"Heart, pancreas, and kidney was collected and the specimen was collected and the specimen was rinsed with ice-cold 1x PBS in a Petri dish to remove blood and other debris.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":106589,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4.4","cases":[],"critical":null},{"id":2206883,"guid":"6D4406A5D15D46E4AB50E7769C395A41","previous_id":2206880,"previous_guid":"B701831E9BF14DFE9DF8D13A77F74CAE","section":"\u003cp\u003eProcedure:\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":true,"step":"{\"blocks\":[{\"key\":\"1urgt\",\"text\":\"Pat the specimen dry using clean Kimwipes and freeze as quickly as possible after harvest. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":106589,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4.5","cases":[],"critical":null},{"id":2206884,"guid":"3EDB7958AAB04083AE8A8B3000ACB00B","previous_id":2206883,"previous_guid":"6D4406A5D15D46E4AB50E7769C395A41","section":"\u003cp\u003eProcedure:\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":true,"step":"{\"blocks\":[{\"key\":\"8mhc5\",\"text\":\"To flash freeze (snap freeze), place the tissue in a cryo-tube and submerge the tube in liquid nitrogen or immerse deeply in dry ice.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":106589,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4.6","cases":[],"critical":null},{"id":2206885,"guid":"D21CE3BA57F647FDB511DC61959395DE","previous_id":2206884,"previous_guid":"3EDB7958AAB04083AE8A8B3000ACB00B","section":"\u003cp\u003eProcedure:\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":true,"step":"{\"blocks\":[{\"key\":\"dcnet\",\"text\":\"Wait at least 2-3 minutes for the tissue to completely freeze, and transfer the tube containing the tissue to -80 degrees C.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":106589,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4.7","cases":[],"critical":null},{"id":2206886,"guid":"466CE5E5EC654532AFCF9E4FC00D589B","previous_id":2206885,"previous_guid":"D21CE3BA57F647FDB511DC61959395DE","section":"\u003cp\u003eProcedure:\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":true,"step":"{\"blocks\":[{\"key\":\"afkg8\",\"text\":\"Keep at -80 degrees Celsius thereafter, until shipping.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":106589,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4.8","cases":[],"critical":null},{"id":2206888,"guid":"CD2BAA99FBB9420CB7007CC4A23A9721","previous_id":2206886,"previous_guid":"466CE5E5EC654532AFCF9E4FC00D589B","section":"\u003cp\u003eProcedure:\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"9amct\",\"text\":\"Shipment:\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"bold\",\"offset\":0,\"length\":9}],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":106589,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"5","cases":[],"critical":null},{"id":2206889,"guid":"3647C72523CC4A0295B56783FC7D73E4","previous_id":2206888,"previous_guid":"CD2BAA99FBB9420CB7007CC4A23A9721","section":"\u003cp\u003eProcedure:\u003c/p\u003e","section_color":"#84CE8