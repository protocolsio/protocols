:0,"critical":null,"critical_id":null,"duration":0},{"id":1091335,"guid":"B618A6E03BF411EBA3F97D5010A9EA8E","previous_id":1091334,"previous_guid":"96D0AFD03BF411EBA3F97D5010A9EA8E","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"C9E33A4E3276476DAC53AA2F13B317C0","order_id":1,"type_id":6,"title":"Section","source":{"title":"General SEM Fixation Procedure"}},{"id":1054724,"guid":"8BE6CCC828F04DD4B4A585FC15BB1EF6","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Rinse the sample in a buffer (1X PBS) at least 3 times for 10 minutes each.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1091336,"guid":"C38CB6903BF411EBA3F97D5010A9EA8E","previous_id":1091335,"previous_guid":"B618A6E03BF411EBA3F97D5010A9EA8E","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"26E45DE3C61048D187115BC9BC59FE55","order_id":1,"type_id":6,"title":"Section","source":{"title":"General SEM Fixation Procedure"}},{"id":1054724,"guid":"70A074372ECC48108EF0C6FE60D17010","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Secondary Fixation: (Not usually required)<\/div><div class = \"text-block\"><ul style = \"list-style-type:disc;\"><li style = \"counter-reset:ol0;list-style-type:disc;\">2% Osmium tetroxide in 0.1M cacodylate (1X PBS) = add 200uL<\/li><li style = \"counter-reset:ol0;list-style-type:disc;\">Fix for 1 hour at room temperature.<\/li><\/ul><\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1091338,"guid":"D0EF3A103BF411EBA3F97D5010A9EA8E","previous_id":1091336,"previous_guid":"C38CB6903BF411EBA3F97D5010A9EA8E","modified_on":0,"protocol_id":0,"components":[{"id"