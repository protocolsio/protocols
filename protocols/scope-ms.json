:0,"critical":null,"critical_id":null,"duration":0},{"id":948978,"guid":"EA42BECC7B2C476DAA1CA6BB0E2214A6","previous_id":948977,"previous_guid":"5D342A74E9CD41418D705457B4C820E5","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"5E6CD3F354CA482EB1BF30CEDD48EF8F","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"DB9973A339954BD4B0943353E4E8E6B7","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Once the thermocycler has hit 90C and at least 5 minutes in the -80 have passed, put the\u00a0plate\u00a0into the thermocycler and hit resume (thermocycler will hold 90C for 10 minutes and then go to 4C)<\/div><\/div>"}},{"id":1054725,"guid":"A2AFECA7FC4D41C1AB7EC92D763C743E","order_id":2,"type_id":4,"title":"duration","source":{"duration":600,"title":""}}],"cases":[],"data":null,"html":null,"section":null,"section_color":null,"section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":948979,"guid":"8FDB2F4E20F04834B0DE9CF57570005A","previous_id":948978,"previous_guid":"EA42BECC7B2C476DAA1CA6BB0E2214A6","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"64645A84925545889B8ADFFCCBA406FE","order_id":1,"type_id":6,"title":"Section","source":{"title":"Digestion and Labeling"}},{"id":1054724,"guid":"BCE434875E6244979419280EAD800728","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Once the thermocycler has gone down to 4C remove the plate and spin it down.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":null,"section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":948980,"guid":"541FCFDEF6D74086A0238E4DEA691571","previous_id":948979,"previous_guid":"8FDB2F4E20F04834B0DE9CF57570005A","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"BD65CE7DF9C240EBBBAA165418647FF8",