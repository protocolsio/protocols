ffset\":0,\"length\":163}],\"data\":{}}],\"entityMap\":{\"0\":{\"type\":\"link\",\"mutability\":\"MUTABLE\",\"data\":{\"guid\":\"37396EB0D81311EE98040A58A9FEAC02\",\"url\":\"https://www.promega.com/-/media/files/resources/protocols/technical-manuals/101/celltiterglo-2-0-assay-protocol.pdf?rev=2a68347929f049afa3ff8a3b68ae7f6d\\u0026sc_lang=en\"}}}}","public":true,"public_fork_note":"","published_on":1709400222,"references":[],"related_equipments":[],"related_materials":[],"reserved_doi":"","retraction_reason":null,"samples":{},"shared_access_id":0,"show_comparison":false,"sign_info":null,"space_access":{"can_view":false,"can_edit":false,"can_remove":false,"can_add":false,"can_publish":true,"can_get_doi":true,"can_share":false,"can_move":false,"can_move_outside":false,"can_transfer":false,"can_download":false,"limited_run":false,"limited_private_links":false,"limited_blind_links":false,"is_locked":false},"space_id":108707,"state_version_id":432,"stats":{"is_voted":false,"number_of_views":3,"number_of_steps":14,"number_of_bookmarks":0,"number_of_comments":0,"number_of_bookmarked_comments":0,"number_of_steps_comments":0,"number_of_protocol_comments":0,"number_of_exports":0,"number_of_runs":0,"number_of_votes":0,"number_of_reagents":0,"number_of_equipments":0,"number_of_collections":0,"number_of_forks":{"private":0,"public":0},"number_of_accessible_forks":0},"status":{"id":1,"info":"We use this protocol and it's working"},"steps":[{"id":1962756,"guid":"63793ED501A34168BC1D86E05DCD67ED","previous_id":0,"previous_guid":null,"section":"\u003cp\u003eTo perform one CellTitre-Glo Viability Assay:\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"fqlj9\",\"text\":\"Aseptically culture immortalized rat RT4-D6P2T Schwann cells (ATCC, Cat #CRL-2768, Manassas, VA) or S16 Schwann cells (ATCC, Cat #CRL-2941, Manassas, VA) in Dulbecco’s Modified Eagle Medium (DMEM) (ATCC, Cat #30-2002, Manassas, VA) supplemented with 10% fetal bovine serum (FBS) (Thermo Fisher, Cat #16000044, Waltham, VA) and 1% penicillin/streptomycin (Pen-strep) (GIBCO, Cat #15140-015, Gaithersburg, MD)/amphotericin B (R\\u0026D Systems, Cat #B23192, Minneapolis, MN) at 37°C and 5% CO2 in poly-L-lysine (PLL)-coated dishes.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":96036,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"1","cases":[],"critical":null},{"id":1962757,"guid":"AD3D8BE7919C46438C0F5067EE2499C2","previous_id":1962756,"previous_guid":"63793ED501A34168BC1D86E05DCD67ED","section":"\u003cp\u003eTo perform one CellTitre-Glo Viability Assay:\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"2ug2c\",\"text\":\"At 80% confluency, split and seed cells into DMEM (200 μL DMEM/well) in a PLL-coated 96-well plate at a density of ~20,000 cells/well.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":96036,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"2","cases":[],"critical":null},{"id":1962758,"guid":"ACB2F1D5E9D74664B783641829B95C37","previous_id":1962757,"previous_guid":"AD3D8BE7919C46438C0F5067EE2499C2","section":"\u003cp\u003eTo perform one CellTitre-Glo Viability Assay:\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"2snmi\",\"text\":\"Incubate cells in DMEM for 24 hours.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":96036,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"3","cases":[],"critical":null},{"id":1962759,"guid":"F8079DA5B90B47078B2B39AE861237FF","previous_id":1962758,"previous_guid":"ACB2F1D5E9D74664B783641829B95C37","section":"\u003cp\u003eTo perform one CellTitre-Glo Viability Assay:\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"4fp2h\",\"text\":\"After 24 hours, aspirate the DMEM and wash each well 2-3x with 200 μL HBSS. After the last wash, add 200μL N2 media (DMEM/F12, no phenol red [Thermo Fisher, Cat #21041025, Waltham, MA] supplemented with5 μg/mL insulin [Sigma, Cat #91077C, St. Louis, MO] and 100 μg/mL apo-transferrin [Sigma, Cat #T1147, St. Louis, MO]) to each well\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":96036,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4","cases":[],"critical":null},{"id":1962760,"guid":"E397F12CB7F44A8C931C397CA450704C","previous_id":1962759,"previous_guid":"F8079DA5B90B47078B2B39AE861237FF","section":"\u003cp\u003eTo perform one CellTitre-Glo Viability Assay:\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"dmm8d\",\"text\":\"Incubate cells in N2 media for 24 hours.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":96036,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"5","cases":[],"critical":null},{"id":1962761,"guid":"A83F2FFB50FC4ABBA553605D45CC1082","previous_id":1962760,"previous_guid":"E397F12CB7F44A8C931C397CA450704C","section":"\u003cp\u003eTo perform one CellTitre-Glo Viability Assay:\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"f9cke\",\"text\":\"After 24 hours, prepare the forskolin-supplemented media by adding 5 μL of a 2 mM forskolin stock to 10\\nmL of N2 media.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":96036,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"6","cases":[],"critical":null},{"id":1962762,"guid":"317471695D9047FE9E94BA63DDDC036D","previous_id":1962761,"previous_guid":"A83F2FFB50FC4ABBA553605D45CC1082","section":"\u003cp\u003eTo perform one CellTitre-Glo Viability Assay:\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"afll9\",\"text\":\"After adding the media, add the appropriate LPS dose to each well following the plate layout. For a 0.1 μg/mL dose of LPS, add 2 μL of a 10 μg/mL LPS stock OR 20 μL of a 1 μg/mL LPS stock. For a 1 μg/mL dose of LPS, add 2 μL of a 100 μg/mL LPS stock OR 20 μL of a 10 μg/mL LPS stock. For a 10 μg/mL dose of LPS, add 2 μL of a 1 mg/mL LPS stock OR 20 μL of a 100 μg/mL LPS stock. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":96036,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"7","cases":[],"critical":null},{"id":1962763,"guid":"64F8DD0C2A074754831AB457E849B9AF","previous_id":1962762,"previous_guid":"317471695D9047FE9E94BA63DDDC036D","section":"\u003cp\u003eTo perform one CellTitre-Glo Viability Assay:\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"4248e\",\"text\":\"Allow cells to incubate in the different treatment combinations for the required incubation time (1, 3, 12, or 24 hours).\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":96036,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"8","cases":[],"critical":null},{"id":1962764,"guid":"27F05D91CDA34603B08A8E880B5D6DB8","previous_id":1962787,"previous_guid":"19165EB388CD4769BD814D63C3399389","section":"\u003cp\u003eTo perform one CellTitre-Glo Viability Assay:\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"2tki2\",\"text\":\"Allow cells to equilibrate to room temperature for approximately 30 minutes.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":96036,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"10","cases":[],"critical":null},{"id":1962781,"guid":"99D6C1C05F024E6AB09834B597C52692","previous_id":1962764,"previous_guid":"27F05D91CDA34603B08A8E880B5D6DB8","section":"\u003cp\u003eTo perform one CellTitre-Glo Viability Assay:\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"ahprn\",\"text\":\"Add an equivalent volume of CellTitre-Glo 2.0 Reagent to the volume of cell culture medium in each well. Be sure this step is done in minimal light. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":96036,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"11","cases":[],"critical":null},{"id":1962782,"guid":"6D3654F60F3F431ABE55E90A6B95DF3F","previous_id":1962781,"previous_guid":"99D6C1C05F024E6AB09834B597C52692","section":"\u003cp\u003eTo perform one CellTitre-Glo Viability Assay:\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"cndj3\",\"text\":\"Mix the contents for 2 minutes to induce cell lysis.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":96036,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"12","cases":[],"critical":null},{"id":1962786,"guid":"DD5800B518A04C5CB1C5DAF0D7E2FAD6","previous_id":1962782,"previous_guid":"6D3654F60F3F431ABE55E90A6B95DF3F","section":"\u003cp\u003eTo perform one CellTitre-Glo Viability Assay:\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"c106b\",\"text\":\"Once the contents have been mixed, read the plate in the SpectraMax M4 Multi-Mode Microplate Reader (Molecular Devices, San Jose, CA)\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":96036,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"13","cases":[],"critical":null},{"id":1962787,"guid":"19165EB388CD4769BD814D63C3399389","previous_id":1962763,"previous_guid":"64F8DD0C2A074754831AB457E849B9AF","section":"\u003cp\u003eTo perform one CellTitre-Glo Viability Assay:\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"60nin\",\"text\":\"Prior to the end of the incubation period, remove the CellTitre-Glo 2.0 Reagent and allow to equilibrate to room temperature. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":96036,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"9","cases":[],"critical":null},{"id":1962789,"guid":"01558255E93447929869B4642590FB02","previous_id":1962786,"previous_guid":"DD5800B518A04C5CB1C5DAF0D7E2FAD6","section":"\u003cp\u003eTo perform one CellTitre-Glo Viability Assay:\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"6lums\",\"text\":\"Luminescence is measured as an indicator of cell viability, with a higher luminescence indicating more viable cells, and a lower luminescence indicating less viable cells.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":96036,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"14","cases":[],"critical":null}],"template_id":0,"title":"CellTitre Glo 2.0 Viability Assay","title_html":"CellTitre Glo 2.0 Viability Assay","type_id":1,"units":[{"id":1,"type_id":3,"name":"µL","can_manage":0,"read_only":0},{"id":2,"type_id":3,"name":"mL","can_manage":0,"read_only":0},{"id":3,"type_id":3,"name":"L","can_manage":0,"read_only":0},{"id":4,"type_id":3,"name":"µg","can_manage":0,"read_only":0},{"id":5,"type_id":3,"name":"mg","can_manage":0,"read_only":0},{"id":6,"type_id":3,"name":"g","can_manage":0,"read_only":0},{"id":7,"type_id":3,"name":"kg","can_manage":0,"read_only":0},{"id":8,"type_id":3,"name":"ng","can_manage":0,"read_only":0},{"id":9,"type_id":3,"name":"Hz","can_manage":0,"read_only":0},{"id":10,"type_id":24,"name":"°C","can_manage":0,"read_only":0},{"id":11,"type_id":24,"name":"°К","can_manage":0,"read_only":0},{"id":12,"type_id":24,"name":"°F","can_manage":0,"read_only":0},{"id":13,"type_id":25,"name":"Mass Percent","can_manage":0,"read_only":0},{"id":14,"type_id":25,"name":"% volume","can_manage":0,"read_only":0},{"id":15,"type_id":25,"name":"Mass / % volume","can_manage":0,"read_only":0},{"id":16,"type_id":25,"name":"Parts per Million (PPM)","can_manage":0,"read_only":0},{"id":17,"type_id":25,"name":"Parts per Billion (PPB)","can_manage":0,"read_only":0},{"id":18,"type_id":25,"name":"Parts per Trillion (PPT)","can_manage":0,"read_only":0},{"id":19,"type_id":25,"name":"Mole Fraction","can_manage":0,"read_only":0},{"id":20,"type_id":25,"name":"Mole Percent","can_manage":0,"read_only":0},{"id":21,"type_id":25,"name":"Molarity (M)","can_manage":0,"read_only":1},{"id":22,"type_id":25,"name":"Molarity (M)","can_manage":0,"read_only":0},{"id":23,"type_id":25,"name":"Genome copies per ml","can_manage":0,"read_only":0},{"id":24,"type_id":3,"name":"μV","can_manage":0,"read_only":0},{"id":25,"type_id":3,"name":"ms","can_manage":0,"read_only":0},{"id":26,"type_id":3,"name":"pg","can_manage":0,"read_only":0},{"id":27,"type_id":25,"name":"Molarity dilutions","can_manage":0,"read_only":0},{"id":28,"type_id":25,"name":"millimolar (mM)","can_manage":0,"read_only":0},{"id":29,