get_doi":0,"can_share":1,"can_move":1,"can_move_outside":1,"can_transfer":1,"can_download":1,"is_locked":0},"guid":"A2D0EBC039A511EB9C86BD4C214E0EF3","state_version_id":206,"steps":[{"id":1090026,"guid":"752B8F1039A811EB9C86BD4C214E0EF3","previous_id":null,"previous_guid":null,"modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"869D80752646400EA0DD3896AD48F531","order_id":1,"type_id":6,"title":"Section","source":{"title":"DONOR SELECTION"}},{"id":1054724,"guid":"1C069C2631F24A2B86C0514D2D9518ED","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Specimens used will be residual skin from clinical excision procedures. A search in the UPMC laboratory information system for excision specimens from adults in sun-exposed sites and non- sun exposed sites will be performed. <\/div><div class = \"text-block\"><ul style = \"list-style-type:disc;\"><li style = \"counter-reset:ol0;list-style-type:disc;\">Blocks and unstained slides will be created from archived formalin fixed paraffin embedded tissue from patients that have had prior skin excisions.<\/li><li style = \"counter-reset:ol0;list-style-type:disc;\">Tissue selected will be waste tissue that is not required for patient care. <\/li><li style = \"counter-reset:ol0;list-style-type:disc;\">Slides and blocks will be de-identified by the investigator who has normal access to the identifiable specimens\/records.<\/li><\/ul><\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1090036,"guid":"24EC150039A911EB9C86BD4C214E0EF3","previous_id":1090026,"previous_guid":"752B8F1039A811EB9C86BD4C214E0EF3","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"29243C0A4CBE45C89B40193E426B3020","order_id":1,"type_id":6,"title":"Section","source":{"title":"DONOR SELECTION"}},{"id":1054724,"guid":"0D954EF1E2E842E2A7EF696414780CC5","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Blocks and slides will be examined for adequate tissue. <\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1090037,"guid":"4866B94039A911EB9C86BD4C214E0EF3","previous_id":1090036,"previous_guid":"24EC150039A911EB9C86BD4C214E0EF3","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"BDF00F3C20FF4592A2533B5B0012F199","order_id":1,"type_id":6,"title":"Section","source":{"title":"DONOR SELECTION"}},{"id":1054724,"guid":"55AAB1CAAE2F4C4CAC39C4E2288A83D3","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Once deemed adequate, de-identified blank slides and blocks will be created.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1090038,"guid":"588109C039A911EB9C86BD4C214E0EF3","previous_id":1090037,"previous_guid":"4866B94039A911EB9C86BD4C214E0EF3","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"5F5C43B47D0E4BAE975D41FBBC92D3C6","order_id":1,"type_id":6,"title":"Section","source":{"title":"INCLUSION AND EXCLUSION CRITERIA"}},{"id":1054724,"guid":"D68B57ED36A04270A5A0AFD4D5D85E56","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Inclusion Criteria:  <\/div><div class = \"text-block\"><ul style = \"list-style-type:disc;\"><li style = \"counter-reset:ol0;list-style-type:disc;\">Age: Adulthood -Young through middle age and early senior years (approx. 18 to 70 years)<\/li><li style = \"counter-reset:ol0;list-style-type:disc;\">All sex, gender, race, ethnicity are eligible<\/li><li style = \"counter-reset:ol0;list-style-type:disc;\">Known state of health and diagnoses<\/li><\/ul><\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1090039,"guid":"A05BA93039A911EB9C86BD4C214E0EF3","previous_id":1090038,"previous_guid":"588109C039A911EB9C86BD4C214E0EF3","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"C4481B26078E43A99F4D83994305A124","order_id":1,"type_id":6,"title":"Section","source":{"title":"INCLUSION AND EXCLUSION CRITERIA"}},{"id":1054724,"guid":"360D3195F49B4C0294519639F579CF1B","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Exclusion Criteria: <\/div><div class = \"text-block\"><ul style = \"list-style-type:disc;\"><li style = \"counter-reset:ol0;list-style-type:disc;\">Unknown time of death<\/li><li style = \"counter-reset:ol0;list-style-type:disc;\">Prolonged Warm Ischemic Time \u2013preferred to be less than 5-10 minutes as per transplant organ recovery protocols but may accept up to 6 hours after cardiac arrest, with knowledge and discretion of the investigator.<\/li><li style = \"counter-reset:ol0;list-style-type:disc;\">Known disease including congenital anomaly or inborn error unsuitable for HuBMAP research<\/li><li style = \"counter-reset:ol0;list-style-type:disc;\">Infection with high-level organism representing risk in processing and shipment<\/li><\/ul><\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0}],"document":"","materials":[],"description":"<div class = \"text-blocks\"><div class = \"text-block\">This document outlines the required criteria for donor inclusion of skin specimens in the Human BioMolecular Atlas Program (HuBMAP). The study intends to be as inclusive as possible of race and ethnicity, dependent only on the consent for donor tissue for research. Disease known at the time of donation or later identified during the research will not necessarily exclude the sample from the research program.<\/div><\/div>","changed_on":1607556716}