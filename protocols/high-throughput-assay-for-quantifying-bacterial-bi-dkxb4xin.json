{"access":{"can_view":true,"can_edit":false,"can_remove":false,"can_add":false,"can_publish":false,"can_get_doi":false,"can_share":true,"can_move":true,"can_move_outside":true,"can_transfer":true,"can_download":true,"limited_run":false,"limited_private_links":false,"limited_blind_links":false,"is_locked":false},"authors":[{"name":"Lisa Bowers","affiliation":"St. Olaf College","affiliation_url":"St. Olaf College","username":"lisa-bowers","link":"","user_image_file":{"guid":"00000000000000000000000000000001","file_name":"avatar.png","url":"https://www.protocols.io/img/avatars/001.png","mime":"image/png","size":0,"width":100,"height":100,"avg_color":"","scan_status":0,"created_at":0},"note":"","is_verified_user":true}],"before_start":"{\"blocks\":[{\"key\":\"da6ng\",\"text\":\"\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","book_chapter":null,"can_accept_authorship":false,"can_be_copied":true,"can_claim_authorship":false,"can_manage_keywords":true,"can_remove_fork":false,"can_sign":false,"child_steps":{},"cited_protocols":[],"collection_items":[],"created_on":1725903046,"creator":{"name":"Lisa Bowers","affiliation":"St. Olaf College","affiliation_url":"St. Olaf College","username":"lisa-bowers","link":"","user_image_file":{"guid":"00000000000000000000000000000001","file_name":"avatar.png","url":"https://www.protocols.io/img/avatars/001.png","mime":"image/png","size":0,"width":100,"height":100,"avg_color":"","scan_status":0,"created_at":0},"badges":[],"affiliations":[{"affiliation":"St. Olaf College","url":"St. Olaf College","job_title":"Associate Professor","is_default":true}]},"cross_cloud_origin":null,"description":"{\"blocks\":[{\"key\":\"fepo5\",\"text\":\"Biofilms are assemblages of single-celled organisms attached to a surface and encased in a sticky extracellular polymeric substance that the cells produce. Difficult to eradicate, they can have profound effects on human health and industrial processes. Studying biofilm formation and testing potential biofilm treatments or mitigation strategies requires a robust protocol for quantifying bacterial biofilms with high-throughput and reproducibility. We have developed such a protocol using readily-available staining materials and a plate reader. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","disclaimer":"","document":"","documents":null,"doi":"dx.doi.org/10.17504/protocols.io.14egn6o8ql5d/v1","doi_status":2,"ethics_statement":"","fork_id":null,"fork_info":null,"fork_info_status":"not_fork","forks":[],"funders":[],"groups":[],"guid":"278A2162678145B0B365780B97C82A86","guidelines":"","has_references":false,"has_step_reagents":false,"has_versions":false,"id":107203,"image":{"source":"https://content.protocols.io/files/rgt54eew.jpg","webp_source":"https://content.protocols.io/files/rgt44eew.webp","placeholder":"https://content.protocols.io/files/rgsy4eew.png","webp_placeholder":""},"image_attribution":"","in_trash":false,"is_bookmarked":false,"is_contact_suspended":false,"is_content_confidential":false,"is_content_warning":false,"is_doi_reserved":false,"is_in_pending_publishing":false,"is_in_transfer":false,"is_owner":true,"is_research":true,"is_retracted":false,"is_shared_directly":false,"is_subprotocol":null,"is_unlisted":false,"item_id":1295349,"journal":null,"journals":[],"keywords":"Biofilm quantification, biofilm inhibition","last_modified":1725918298,"link":"","location":null,"manuscript_citation":"","materials":[],"materials_text":"{\"blocks\":[{\"key\":\"8ulpb\",\"text\":\"96-well plates\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"4eu2e\",\"text\":\"Incubator\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"8npmc\",\"text\":\"Multichannel pipette (P200)\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"emo87\",\"text\":\"Crystal violet (0.04%)\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"dd6cr\",\"text\":\"Ethanol (95%)\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"edtgu\",\"text\":\"Plate reader\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","ownership_history":null,"parent_collections":[],"parent_protocols":[],"peer_reviewed":false,"protocol_references":"","public":true,"public_fork_note":"","published_on":1725918298,"references":[],"related_equipments":[],"related_materials":[],"reserved_doi":"","retraction_reason":null,"samples":{},"shared_access_id":0,"show_comparison":false,"sign_info":null,"space_access":{"can_view":false,"can_edit":false,"can_remove":false,"can_add":false,"can_publish":true,"can_get_doi":true,"can_share":false,"can_move":false,"can_move_outside":false,"can_transfer":false,"can_download":false,"limited_run":false,"limited_private_links":false,"limited_blind_links":false,"is_locked":false},"space_id":131711,"state_version_id":1097,"stats":{"is_voted":false,"number_of_views":6,"number_of_steps":24,"number_of_bookmarks":0,"number_of_comments":0,"number_of_bookmarked_comments":0,"number_of_steps_comments":0,"number_of_protocol_comments":0,"number_of_exports":0,"number_of_runs":0,"number_of_votes":0,"number_of_reagents":0,"number_of_equipments":0,"number_of_collections":0,"number_of_forks":{"private":0,"public":0},"number_of_accessible_forks":0},"status":{"id":1,"info":"We use this protocol and it's working"},"steps":[{"id":2222256,"guid":"4CD9E119D7A447B8B3F83273546B0CF8","previous_id":0,"previous_guid":null,"section":"\u003cp\u003eSet-up biofilms\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"bmeu2\",\"text\":\"Dilute overnight bacterial culture to an optical density (OD600) of 0.01 with fresh media. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":107203,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"1","cases":[],"critical":null},{"id":2222262,"guid":"B002E9579E9D48FFA854FF51E7C70582","previous_id":2222256,"previous_guid":"4CD9E119D7A447B8B3F83273546B0CF8","section":"\u003cp\u003eSet-up biofilms\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"f9kmd\",\"text\":\"For the blank samples, pipette 100 uL of sterile media into at least 8 wells of a 96 well plate. These wells have media but no cells and no treatment. Purpose: account for the effect of media on optical density.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":107203,"case_id":0,"critical_ids":"3","duration":0,"original_id":0,"number":"2","cases":[],"critical":[{"id":3,"title":"Pipetting","icon":"PipettingIcon"}]},{"id":2222263,"guid":"1E13566B3B01410A97DE89F53F6E5585","previous_id":2222268,"previous_guid":"88A556EC34134FDEAEE8E5CB9738EC5D","section":"\u003cp\u003eSet-up biofilms\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"3bks4\",\"text\":\"For the positive control samples, pipette 100 uL of diluted bacterial culture into at least 8 wells of a 96 well plate. These wells have cells but no treatment. Purpose: assess maximum biofilm formation.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":107203,"case_id":0,"critical_ids":"3","duration":0,"original_id":0,"number":"4","cases":[],"critical":[{"id":3,"title":"Pipetting","icon":"PipettingIcon"}]},{"id":2222265,"guid":"0B0B6AF595654F5698C872A944CBAB52","previous_id":2222263,"previous_guid":"1E13566B3B01410A97DE89F53F6E5585","section":"\u003cp\u003eSet-up biofilms\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"1h0hj\",\"text\":\"For the experimental samples, pipette 100 uL of diluted bacterial culture plus 1-10 uL of treatment into at least 8 wells. Purpose: assess effect of treatment on biofilm formation.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":107203,"case_id":0,"critical_ids":"3","duration":0,"original_id":0,"number":"5","cases":[],"critical":[{"id":3,"title":"Pipetting","icon":"PipettingIcon"}]},{"id":2222268,"guid":"88A556EC34134FDEAEE8E5CB9738EC5D","previous_id":2222262,"previous_guid":"B002E9579E9D48FFA854FF51E7C70582","section":"\u003cp\u003eSet-up biofilms\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"a24tq\",\"text\":\"For the blank+treatment samples, pipette 100 uL of sterile media + treatment into at least 8 wells of the 96 well plate. Do this for each treatment. These wells have media + treatment but no cells. Purpose: account for the effect of treatment on optical density.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":107203,"case_id":0,"critical_ids":"3","duration":0,"original_id":0,"number":"3","cases":[],"critical":[{"id":3,"title":"Pipetting","icon":"PipettingIcon"}]},{"id":2222273,"guid":"3E281EC32E16488A80208B65F3444C11","previous_id":2222265,"previous_guid":"0B0B6AF595654F5698C872A944CBAB52","section":"\u003cp\u003eSet-up biofilms\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"c6f70\",\"text\":\"Incubate 96 well plate with lid for 24 hours without shaking at optimal temperature for bacterial growth. To reduce evaporation in the wells, keep the lidded plate inside a bag along with a moist paper towel.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":107203,"case_id":0,"critical_ids":"1,40","duration":86400,"original_id":0,"number":"6","cases":[],"critical":[{"id":1,"title":"Incubation","icon":"IncubationIcon"},{"id":40,"title":"Overnight Step","icon":"Overnight"}]},{"id":2222275,"guid":"E934C9A18A5C46F0AD1303A9B172103C","previous_id":2222273,"previous_guid":"3E281EC32E16488A80208B65F3444C11","section":"\u003cp\u003eRemove planktonic cells\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"9loo7\",\"text\":\"After 24 hours incubation, measure OD600 of the wells with a plate reader. This will be a measure of overall growth, not biofilm formation. Check for contamination in the blank wells. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":107203,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"7","cases":[],"critical":null},{"id":2222284,"guid":"E4DC7C06D3D04A2FB6DA04AD035932E6","previous_id":2222275,"previous_guid":"E934C9A18A5C46F0AD1303A9B172103C","section":"\u003cp\u003eRemove planktonic cells\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"6ohfj\",\"text\":\"The biofilms should be adhered to the bottom and sides of each well. To remove media and planktonic cells, gently pour off supernatant into sink with running water or container with bleach-water.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":107203,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"8","cases":[],"critical":null},{"id":2222287,"guid":"4D78841AC3164BB7A86198F578CB66F2","previous_id":2222284,"previous_guid":"E4DC7C06D3D04A2FB6DA04AD035932E6","section":"\u003cp\u003eRemove planktonic cells\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"2mrq5\",\"text\":\"Wash the biofilms - with a multichannel pipette, gently transfer 200 uL sterile water into each well. Swirl without disturbing biofilms. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":107203,"case_id":0,"critical_ids":"3,5","duration":0,"original_id":0,"number":"9","cases":[],"critical":[{"id":3,"title":"Pipetting","icon":"PipettingIcon"},{"id":5,"title":"Wash","icon":"WashIcon"}]},{"id":2222289,"guid":"FA757C034A0B49539A6F1666A90616CE","previous_id":2222287,"previous_guid":"4D78841AC3164BB7A86198F578CB66F2","section":"\u003cp\u003eRemove planktonic cells\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"1t7p0\",\"text\":\"Pour off the sterile water by inverting the plate and shaking it gently over the drain/container. You want to remove the planktonic cells without disturbing the biofilms.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":107203,"case_id":0,"critical_ids":"5","duration":0,"original_id":0,"number":"10","cases":[],"critical":[{"id":5,"title":"Wash","icon":"WashIcon"}]},{"id":2222291,"guid":"8B65356F477840A6A6841FF281447883","previous_id":2222289,"previous_guid":"FA757C034A0B49539A6F1666A90616CE","section":"\u003cp\u003eRemove planktonic cells\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"6ggso\",\"text\":\"Repeat step 9: Gently transfer another 200 uL sterile water into each well. Swirl without disturbing biofilms.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":107203,"case_id":0,"critical_ids":"3,5","duration":0,"original_id":0,"number":"11","cases":[],"critical":[{"id":3,"title":"Pipetting","icon":"PipettingIcon"},{"id":5,"title":"Wash","icon":"WashIcon"}]},{"id":2222292,"guid":"69193D6A765448789E49324E09879CAE","previous_id":2222291,"previous_guid":"8B65356F477840A6A6841FF281447883","section":"\u003cp\u003eRemove planktonic cells\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"9e4m2\",\"text\":\"Repeat step 10: Pour off the sterile water by inverting the plate and shaking it gently over the drain/container. You want to remove the planktonic cells without disturbing the biofilms. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":107203,"case_id":0,"critical_ids":"5","duration":0,"original_id":0,"number":"12","cases":[],"critical":[{"id":5,"title":"Wash","icon":"WashIcon"}]},{"id":2222293,"guid":"054D8821B360456D99CD4C6AAD0F7310","previous_id":2222292,"previous_guid":"69193D6A765448789E49324E09879CAE","section":"\u003cp\u003eStain biofilms\u003c/p\u003e","section_color":"#84CE84","section_duration":300,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"6io8n\",\"text\":\"Using a multichannel pipette, add 150 uL 0.04% crystal violet per well. Make sure to coat all of the cells that are stuck in the biofilm. Incubate for 5 minutes. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":107203,"case_id":0,"critical_ids":"3","duration":300,"original_id":0,"number":"13","cases":[],"critical":[{"id":3,"title":"Pipetting","icon":"PipettingIcon"}]},{"id":2222295,"guid":"C7B1358E81BE43C9B9CCF9B70A5D005E","previous_id":2222293,"previous_guid":"054D8821B360456D99CD4C6AAD0F7310","section":"\u003cp\u003eStain biofilms\u003c/p\u003e","section_color":"#84CE84","section_duration":300,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"ft1do\",\"text\":\"Pour off the crystal violet by inverting the plate and shaking gently over the drain/container with water running. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":107203,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"14","cases":[],"critical":null},{"id":2222296,"guid":"836BFE59911D48E5B1F9C6240A9190C0","previous_id":2222295,"previous_guid":"C7B1358E81BE43C9B9CCF9B70A5D005E","section":"\u003cp\u003eStain biofilms\u003c/p\u003e","section_color":"#84CE84","section_duration":300,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"a55bd\",\"text\":\"To wash away excess crystal violet, gently transfer 200 uL sterile water into each well. Swirl without disturbing biofilms.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":107203,"case_id":0,"critical_ids":"3,5","duration":0,"original_id":0,"number":"15","cases":[],"critical":[{"id":3,"title":"Pipetting","icon":"PipettingIcon"},{"id":5,"title":"Wash","icon":"WashIcon"}]},{"id":2222297,"guid":"D68455EEDE6E4E3AA8A3397F01A43A50","previous_id":2222296,"previous_guid":"836BFE59911D48E5B1F9C6240A9190C0","section":"\u003cp\u003eStain biofilms\u003c/p\u003e","section_color":"#84CE84","section_duration":300,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"enpds\",\"text\":\"Pour off the sterile water by inverting the plate and shaking it gently over the drain/container. Do not disturb the biofilms. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":107203,"case_id":0,"critical_ids":"5","duration":0,"original_id":0,"number":"16","cases":[],"critical":[{"id":5,"title":"Wash","icon":"WashIcon"}]},{"id":2222298,"guid":"B5AA0FA59D4F4F77B3EA4C51480290C3","previous_id":2222297,"previous_guid":"D68455EEDE6E4E3AA8A3397F01A43A50","section":"\u003cp\u003eStain biofilms\u003c/p\u003e","section_color":"#84CE84","section_duration":300,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"e0570\",\"text\":\"Repeat step 15: Gently transfer another 200 uL sterile water into each well. Swirl without disturbing biofilms.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":107203,"case_id":0,"critical_ids":"3,5","duration":0,"original_id":0,"number":"17","cases":[],"critical":[{"id":3,"title":"Pipetting","icon":"PipettingIcon"},{"id":5,"title":"Wash","icon":"WashIcon"}]},{"id":2222299,"guid":"7FA0F45FF1744E3DA2652C27695758F1","previous_id":2222298,"previous_guid":"B5AA0FA59D4F4F77B3EA4C51480290C3","section":"\u003cp\u003eStain biofilms\u003c/p\u003e","section_color":"#84CE84","section_duration":300,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"20sb3\",\"text\":\"Repeat step 16: Pour off the sterile water by inverting the plate and shaking it gently over the drain/container. Do not disturb the biofilms. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":107203,"case_id":0,"critical_ids":"5","duration":0,"original_id":0,"number":"18","cases":[],"critical":[{"id":5,"title":"Wash","icon":"WashIcon"}]},{"id":2222300,"guid":"D2E0DDA6717D4CFC8F559F4B2B6221DC","previous_id":2222299,"previous_guid":"7FA0F45FF1744E3DA2652C27695758F1","section":"\u003cp\u003eSolubilize the cells\u003c/p\u003e","section_color":"#FFED92","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"ah21f\",\"text\":\"Using a multichannel pipette, add 100 uL of 95% ethanol to each sample well. Make sure to coat all cells in the biofilm. Pipette up and down to release the cells from the biofilm and mix with the ethanol. DO NOT POUR OFF THE ETHANOL.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":107203,"case_id":0,"critical_ids":"3,6","duration":0,"original_id":0,"number":"19","cases":[],"critical":[{"id":3,"title":"Pipetting","icon":"PipettingIcon"},{"id":6,"title":"Mix","icon":"MixIcon"}]},{"id":2222311,"guid":"F9FB45793F6F471ABCE33CBC33DADF28","previous_id":2222315,"previous_guid":"CAD325AD08A945FA986FCC274C6174B8","section":"\u003cp\u003eQuantify biofilms\u003c/p\u003e","section_color":"#EA9F6C","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"flceo\",\"text\":\"Use a plate reader to measure the OD492 of each well. The amount of biofilm in each well is proportional to the amount of crystal violet released from the ethanol-solubilized biofilm cells. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":107203,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"21","cases":[],"critical":null},{"id":2222312,"guid":"0AD6F31C50A3429F97AABFFCDE8F47D6","previous_id":2222311,"previous_guid":"F9FB45793F6F471ABCE33CBC33DADF28","section":"\u003cp\u003eQuantify biofilms\u003c/p\u003e","section_color":"#EA9F6C","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"682h1\",\"text\":\"Determine if there are outliers in each set. A common rule for outliers is any data point that is 1.5x the IQR above the upper quartile or below the lower quartile of a set of data. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":107203,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"22","cases":[],"critical":null},{"id":2222313,"guid":"110CD238AB134367A9D3840321069B02","previous_id":2222312,"previous_guid":"0AD6F31C50A3429F97AABFFCDE8F47D6","section":"\u003cp\u003eQuantify biofilms\u003c/p\u003e","section_color":"#EA9F6C","section_duration":0,