ents":0,"number_of_exports":0,"number_of_runs":0,"number_of_votes":0,"is_voted":0},"parent_protocols":[],"parent_collections":[],"cited_protocols":[]},"created_on":1623165795,"resolve_on":0,"resolved_user":{"name":" ","affiliation":null,"affiliations":[],"username":null,"note":null,"link":null,"image":{"source":null,"placeholder":null},"badges":[],"verified":0,"is_verified_user":false,"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false},"shared":false}}],"is_owner":1,"has_subprotocols":0,"is_subprotocol":0,"is_bookmarked":0,"can_claim_authorship":0,"can_accept_authorship":0,"can_be_copied":1,"can_remove_fork":1,"fork_id":null,"url":"https:\/\/www.protocols.io\/view\/encrypting-your-data-bvmsn46e","forks_count":{"private":0,"public":0},"access":{"can_view":1,"can_remove":1,"can_add":0,"can_edit":0,"can_publish":0,"can_get_doi":1,"can_share":1,"can_move":1,"can_move_outside":1,"can_transfer":1,"can_download":1,"is_locked":0},"guid":"F9F4FBD0C86B11EBA388CF292F6D667E","state_version_id":191,"steps":[{"id":1193400,"guid":"082C3470C86C11EBA388CF292F6D667E","previous_id":null,"previous_guid":null,"modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"8E00888EE811473E84AE4967AE9CE91A","order_id":1,"type_id":6,"title":"Section","source":{"title":"encrypt"}},{"id":1054724,"guid":"CB819D4232174E58BFB91CE60A176F51","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">In order to encrypt your data, you'll need to zip your folder or file, creating a password protected archive.<\/div><div class = \"text-block\">People without the password will still be able to look at the names of the files in the archive, but won't be able to read the file.<\/div><div class = \"text-block\">Using the command line, navigate to the folder you want to encrypt and run:<\/div><div class = \"text-block\"><pre><code><div class = \"text-blocks\"><div class = \"text-block\">zip -re output_archive.zip my_folder<\/div><\/div><\/code><\/pre><\/div><div class = \"text-block\">with \"output_archive\" the name of the encrypted archive you want to create and \"my_folder\" the name of the folder (or file) you want to encrypt.<\/div><div class = \"text-block\">In the prompt, a password will be aseked twice. Make sure to use a strong password (8-10 characters with special characters)<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1193402,"guid":"998956F0C86C11EBA388CF292F6D667E","previous_id":1193400,"previous_guid":"082C3470C86C11EBA388CF292F6D667E","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"C8E678D5D2104671B3F943EFC7C0698D","order_id":1,"type_id":6,"title":"Section","source":{"title":"Decrypt"}},{"id":1054724,"guid":"5499B0141C8141D4B457396172BB3F07","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">To decrypt the archive, use the unzip command as usual. A prompt will ask you the password before unzipping the files.<\/div><div class = \"text-block\"><pre><code><div class = \"text-blocks\"><div class = \"text-block\">unzip output_archive.zip<\/div><\/div><\/code><\/pre><\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0}],"document":"","materials":[],"description":"<div class = \"text-blocks\"><div class = \"text-block\">How to encrypt a folder or a file using the command line on a linux\/mac system !<\/div><\/div>","changed_on":1623165795}