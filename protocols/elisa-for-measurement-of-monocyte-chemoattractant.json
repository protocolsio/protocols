ection_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1011182,"guid":"AF47CB0680094E658D8F3DD093BE8899","previous_id":1011181,"previous_guid":"A340E106605348DCA733E4E19C6B399B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"6369D13D8FC640F09DFE4813521F546D","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"5FDD6F9752FE4DBBA29AB99986B816D2","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Incubate the microwells in the dark for 20 min.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1011183,"guid":"35D9E5A07C1E4028AF67E960EE401D71","previous_id":1011182,"previous_guid":"AF47CB0680094E658D8F3DD093BE8899","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"BB46FE496AEE4E598031A9C67BBA764F","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"6F90D51AD2CA42DA991A2557BA7D5D55","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">A colored product is formed in proportion to the quantity of MCP-1\/CCL2 present in the sample or standard.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1011184,"guid":"18F0505AAB5947A5B9514875F5731A5F","previous_id":1011183,"previous_guid":"35D9E5A07C1E4028AF67E960EE401D71","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"31675EEDDF424187827661BAC51E5209","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"F92953FF8C254C42ADBB6413801F9A7A","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">The reaction is terminated by addition of 100 \u00b5l 3M H2SO4  and the absorbance is measured at 450 nm.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1011185,"guid":"3EC195506CA749D2AAC333C1CD219652","previous_id":1011184,"previous_guid":"18F0505AAB5947A5B9514875F5731A5F","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"599244C518EF4EDB9C0A3E59EA0F3C33","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"B2E4B929196D446BA527243A5818C0FF","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">A standard curve is made from 7 human MCP-1\/CCL2 standard dilutions and the human MCP-1\/CCL2 sample concentration is determined.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1011186,"guid":"C9B9D1B699414DA79E53163F558518E5","previous_id":1011185,"previous_guid":"3EC195506CA749D2AAC333C1CD219652","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"42AD7860C94D457BA9C4612357EFE2F7","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"D96BC8D7D8BB43DF84533D6B83094781","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">For better results place the microplate on a microplate shaker in every incubation.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0}],"document":null,"materials":[],"description":"<div class = \"text-blocks\"><div class = \"text-block\">MCP-1 is believed to play an important role in monocyte infiltration into tumor tissues. [1] MCP-1 plays an important role in the pathogenesis of atherosclerosis. There is considerable evidence that supports that monocytes containing MCPs and macrophages influence the growth of other cell types within the atherosclerotic lesion. [2]<\/div><div class = \"text-block\">References<\/div><div class = \"text-block\"><span>1. Deshmane SL, Kremlev S, Amini S, Sawaya BE. Monocyte chemoattractant protein-1 (MCP-1): an overview. <\/span><span style = \"font-style:italic;\">J Interferon Cytokine Res<\/span><span>. 2009;29(6):313-326. doi:10.1089\/jir.2008.0027<\/span><\/div><div class = \"text-block\">2. Lin J, Kakkar V, Lu X. Impact of MCP-1 in atherosclerosis. Curr Pharm Des. 2014;20(28):4580-4588. doi:10.2174\/1381612820666140522115801<\/div><\/div>","changed_on":1598163326}