ection_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"4cp7m\",\"text\":\"Resuspend cells in 10 mL of Resuspension Buffer (50 mM Tris pH 8.0, 10% sucrose).\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":97344,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"3","cases":[],"critical":null},{"id":1993640,"guid":"9A91904CF8F84475BE0E00028DBA9D42","previous_id":1993639,"previous_guid":"2828F5FDCDA54C7CBD0D1DDB7F017B4E","section":"\u003cp\u003eDNA extraction\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"e2jid\",\"text\":\"Add 2.5 mL Lysis Buffer (Tris pH 8.0, 10 mg/mL lysozyme), 1.5 mL 10% SDS, 5 µL 100 mg/mL RNase A, and 25 µL 50 mg/mL proteinase K to lyse cells and stabilize DNA.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":97344,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4","cases":[],"critical":null},{"id":1993641,"guid":"3B9AE7CD86EB46F187585AC33B11E511","previous_id":1993640,"previous_guid":"9A91904CF8F84475BE0E00028DBA9D42","section":"\u003cp\u003eDNA extraction\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"bqbt7\",\"text\":\"Incubate for 1 h at 37°C.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":97344,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"5","cases":[],"critical":null},{"id":1993642,"guid":"80183064465A42E7BE3BC9B884B88324","previous_id":1993641,"previous_guid":"3B9AE7CD86EB46F187585AC33B11E511","section":"\u003cp\u003eDNA extraction\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"7b9cn\",\"text\":\"Add 1.3 mL of 3.0 M sodium acetate (pH 5.5) and 30 mL of 100% ethanol to separate DNA from other biomacromolecules by precipitation.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":97344,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"6","cases":[],"critical":null},{"id":1993643,"guid":"A0C9F03F72974B56B34E0CAF692DA4C7","previous_id":1993642,"previous_guid":"80183064465A42E7BE3BC9B884B88324","section":"\u003cp\u003eDNA extraction\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"10e9n\",\"text\":\"Spool DNA onto a glass hook and transfer to 20 mL of TE buffer.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":97344,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"7","cases":[],"critical":null},{"id":1993644,"guid":"D65E6F2772E448E29CDDCAD0A7AAEE71","previous_id":1993643,"previous_guid":"A0C9F03F72974B56B34E0CAF692DA4C7","section":"\u003cp\u003eDNA extraction\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"8aceh\",\"text\":\"Dissolve DNA in TE buffer for 1 h at 37°C.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":97344,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"8","cases":[],"critical":null},{"id":1993645,"guid":"0A47E0BBD0264BA1903FCFB82E29A608","previous_id":1993644,"previous_guid":"D65E6F2772E448E29CDDCAD0A7AAEE71","section":"\u003cp\u003eDNA extraction\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"bnn7g\",\"text\":\"Add a 5 mL aliquot of DNA to 7 mL of phenol:chloroform:isoamyl alcohol (25:24:1, Saturated with 10mM Tris, pH 8.0, 1mM EDTA) and mix by inversion.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":97344,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"9","cases":[],"critical":null},{"id":1993646,"guid":"DD7C51E53C5D4B5BA1963BEA8B210F8A","previous_id":1993645,"previous_guid":"0A47E0BBD0264BA1903FCFB82E29A608","section":"\u003cp\u003eDNA extraction\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"cvsvp\",\"text\":\"Separate phases by centrifugation at 10,000 x g for 10 min.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":97344,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"10","cases":[],"critical":null},{"id":1993647,"guid":"0647EE91DE1943909B2D2A25345B12CD","previous_id":1993646,"previous_guid":"DD7C51E53C5D4B5BA1963BEA8B210F8A","section":"\u003cp\u003eDNA extraction\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"ardqa\",\"text\":\"Pipette off the aqueous phase and add to 5 mL of chloroform. Mix by vortexing.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":97344,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"11","cases":[],"critical":null},{"id":1993648,"guid":"7E182E8567294C8D8AA47A05E0F3CD0D","previous_id":1993647,"previous_guid":"0647EE91DE1943909B2D2A25345B12CD","section":"\u003cp\u003eDNA extraction\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"8stc9\",\"text\":\"Separate phases by centrifugation at 10,000 x g for 10 min.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}