er_of_views":5,"number_of_steps":9,"number_of_bookmarks":0,"number_of_comments":0,"number_of_bookmarked_comments":0,"number_of_steps_comments":0,"number_of_protocol_comments":0,"number_of_exports":0,"number_of_runs":0,"number_of_votes":0,"number_of_reagents":0,"number_of_equipments":0,"number_of_collections":0,"number_of_forks":{"private":0,"public":0},"number_of_accessible_forks":0},"status":{"id":1,"info":"We use this protocol and it's working"},"steps":[{"id":1650351,"guid":"3013DBC0D4CF11ED95AA4F9E31B40D26","previous_id":0,"previous_guid":null,"section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"83hl6\",\"text\":\"As precursor(s) of melanin, 1) l-dopa, 2) DHI, 3) a mixture of DHI and DHICA, 4) DHICA, 5) a mixture of l-dopa and l-cysteine, and 6) 5SCD in the presence of l-dopa is used.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":80143,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"1","cases":[],"critical":null},{"id":1650352,"guid":"5B5B1CD0D4CF11ED95AA4F9E31B40D26","previous_id":1650351,"previous_guid":"3013DBC0D4CF11ED95AA4F9E31B40D26","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"aug7e\",\"text\":\"To a solution of the precursor(s) (1 mmol) in 0.1 M sodium phosphate buffer, pH 6.8 (98 ml), add a solution of mushroom tyrosinase in the same buffer (2 ml) at 25˚C.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":80143,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"2","cases":[],"critical":null},{"id":1650353,"guid":"6173A100D4CF11ED95AA4F9E31B40D26","previous_id":1650354,"previous_guid":"782EB880D4CF11ED95AA4F9E31B40D26","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"do84k\",\"text\":\"Carry out oxidation with vigorous (maximal, but avoiding splashing) stirring in a 500 ml Erlenmeyer flask.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":80143,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"3","cases":[],"critical":null},{"id":1650354,"guid":"782EB880D4CF11ED95AA4F9E31B40D26","previous_id":1650352,"previous_guid":"5B5B1CD0D4CF11ED95AA4F9E31B40D26","section":"","section_color":"","section_duration":0,"is_substep":true,"step":"{\"blocks\":[{\"key\":\"2ohmr\",\"text\":\" The amount of mushroom tyrosinase (Sigma-Aldrich, 4270 U/mg) is either 50,000 U or 100,000 U.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":80143,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"2.1","cases":[],"critical":null},{"id":1650355,"guid":"834995F0D4CF11ED95AA4F9E31B40D26","previous_id":1650353,"previous_guid":"6173A100D4CF11ED95AA4F9E31B40D26","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"4u21t\",\"text\":\"Stop oxidation (1-4) at 4 h by adding 6 M HCl (2 ml) to make pH 1 and keep the mixture at 4˚C overnight to ensure complete precipitation of the pigment.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":80143,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4","cases":[],"critical":null},{"id":1650356,"guid":"8933DE30D4CF11ED95AA4F9E31B40D26","previous_id":1650355,"previous_guid":"834995F0D4CF11ED95AA4F9E31B40D26","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"2qa13\",\"text\":\"Collect the precipitate by centrifugation at 3,000 rpm for 10 min and wash with 0.1 M HCl (40 ml x 3).\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":80143,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"5","cases":[],"critical":null},{"id":1650357,"guid":"CA77A1B0D4CF11ED95AA4F9E31B40D26","previous_id":1650356,"previous_guid":"8933DE30D4CF11ED95AA4F9E31B40D26","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"d2ee2\",\"text\":\"For the preparation of pheomelanins (5 and 6), acidify the oxidation mixture with acetic acid (3 ml) to make pH 3, and wash the precipitate with 1% acetic acid (40 ml x 3). \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":80143,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"6","cases":[],"critical":null},{"id":1650358,"guid":"D6655F80D4CF11ED95AA4F9E31B40D26","previous_id":1650357,"previous_guid":"CA77A1B0D4CF11ED95AA4F9E31B40D26","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"b4vc0\",\"text\":\"Dry the melanin powder by lyophilization and equilibrate with moisture in a desiccator with a saturated CaCl2 solution. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"sub\",\"offset\":108,\"length\":1}],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":80143,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"7","cases":[],"critical":null},{"id":1650359,"guid":"E083B480D4CF11ED95AA4F9E31B40D26","previous_id":1650358,"previous_guid":"D6655F80D4CF11ED95AA4F9E31B40D26","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"3n0uu\",\"text\":\"Prior to treatment, suspend the synthetic DOPA pheomelanin (DOPA:Cys = 1:1) and DOPA eumelanin (DOPA:Cys = 1:0) in sterile PBS and sonicate overnight.\",\"type\":\"unstyled\",\"depth\":0,\"inlineSty