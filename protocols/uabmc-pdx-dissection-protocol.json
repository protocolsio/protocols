rked":0,"can_claim_authorship":0,"can_accept_authorship":0,"can_be_copied":1,"can_remove_fork":1,"fork_id":null,"url":"https:\/\/www.protocols.io\/view\/uabmc-pdx-dissection-protocol-bg9yjz7w","forks_count":{"private":0,"public":0},"access":{"can_view":1,"can_remove":1,"can_add":0,"can_edit":0,"can_publish":0,"can_get_doi":1,"can_share":1,"can_move":1,"can_move_outside":1,"can_transfer":1,"can_download":1,"is_locked":0},"guid":"DC86B8D0A93C11EABF5601D5F45BACF3","state_version_id":520,"steps":[{"id":956658,"guid":"320C2F60A93D11EABF5601D5F45BACF3","previous_id":null,"previous_guid":null,"modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"28A007CDD655408F9C23D08D58109D5B","order_id":1,"type_id":6,"title":"Section","source":{"title":"TUMOR HARVEST"}},{"id":1054724,"guid":"BCCBB87D69D647C49448EC2CC18D912F","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Select 1 to 2 mice that have tumors that measure no more than 8-10mm in greatest diameter, are not ulcerated and appear to have grown in a consistent fashion.<\/div><\/div>"}},{"id":1054725,"guid":"7A99AB1FF3DE42B8A04AFB3EAC452A16","order_id":2,"type_id":26,"title":"notes","source":{"id":0,"parent_id":0,"uri":"","title":"","body":"<div class = \"text-blocks\"><div class = \"text-block\"><span style = \"font-weight:bold;\">This must be done aseptically using sterile surgical techniques<\/span><\/div><\/div>","created_on":0,"changed_on":0,"creator":{"name":" ","affiliation":null,"affiliations":[],"username":"","link":null,"image":{"source":"","placeholder":""},"badges":[],"verified":0,"is_verified_user":false,"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false},"comments":[]}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":956659,"guid":"320D8EF0A93D11EABF5601D5F45BACF3","previous_id":956658,"previous_guid":"320C2F60A93D11EABF5601D5F45BACF3","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"89A84BA7BCB648FA95D9852E3AE6CBC6","order_id":1,"type_id":6,"title":"Section","source":{"title":"TUMOR HARVEST"}},{"id":1054724,"guid":"EE4F5815530B401DAEB424B67809BAE2","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Euthanize one mouse at a time with CO2 asphyxiation in designated inhalation chamber attached via a two-stage regulator to a CO2 tank.\u00a0 Perform cervical dislocation or bilateral thoracotomy to ensure the mouse is dead.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":956660,"guid":"320EC770A93D11EABF5601D5F45BACF3","previous_id":956659,"previous_guid":"320D8EF0A93D11EABF5601D5F45BACF3","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"EBB3238B0C9B4184B5C62D7896174EB1","order_id":1,"type_id":6,"title":"Section","source":{"title":"TUMOR HARVEST"}},{"id":1054724,"guid":"4400232F99444B248A4E4C884E0A0358","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Spray the mouse with 3% chlorhexidine sanitizing solution (over the sink) and transfer mouse to a sterile convertor cloth. Sanitize the skin around the tumor with betadine scrub solution.\u00a0 Wait at least <\/div><div class = \"text-block\"> before proceeding.<\/div><\/div>"}},{"id":1054725,"guid":"B1AFDAE0A93E11EABF5601D5F45BACF3","order_id":2,"type_id":4,"title":"duration","source":{"duration":120,"title":""}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":956661,"guid":"3210C340A93D11EABF5601D5F45BACF3","previous_id":956660,"previous_guid":"320EC770A93D11EABF5601D5F45BACF3","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"4013A9A80D8145289D86D551151140A2","order_id":1,"type_id":6,"title":"Section","source":{"title":"TUMOR HARVEST"}},{"id":1054724,"guid":"0C8B894E9B04415B8C4170DE3DDDF5C2","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Use sterile forceps and one scalpel with a #11 blade to produce an incision in the skin forming a half-circle about 5mm away from the tumor on the ventral (lower) side of the tumor.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":956662,"guid":"321270F0A93D11EABF5601D5F45BACF3","previous_id":956661,"previous_guid":"3210C340A93D11EABF5601D5F45BACF3","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"F6CF42E7EB9846978260F556EEEDAEE4","order_id":1,"type_id":6,"title":"Section","source":{"title":"TUMOR HARVEST"}},{"id":1054724,"guid":"0816BA0096DD413AA16C083D28D08155","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Use the forceps to lift the edge of the skin and use a new sterile scalpel with #11 blade to carefully dissect under the tumor freeing it from the underlying muscle tissues. Blunt dissection works best, where possible.\u00a0 Use your ring finger of the hand holding the forceps to push the tumor forward during this dissection.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":956663,"guid":"32146CC0A93D11EABF5601D5F45BACF3","previous_id":956662,"previous_guid":"321270F0A93D11EABF5601D5F45BACF3","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"B8C5A63E57BF4D04AED798BC239EDBC0","order_id":1,"type_id":6,"title":"Section","source":{"title":"TUMOR HARVEST"}},{"id":1054724,"guid":"C62ED3900325442593AFEB56D32D1710","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">After you have dissected the tumor free from the body of the mouse, continue to dissect the tumor from the overlying skin.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":956664,"guid":"32168FA0A93D11EABF5601D5F45BACF3","previous_id":956663,"previous_guid":"32146CC0A93D11EABF5601D5F45BACF3","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"080CF0A15ED24B54B6E7DE67D26BF6E0","order_id":1,"type_id":6,"title":"Section","source":{"title":"TUMOR HARVEST"}},{"id":1054724,"guid":"78E6CA0187754E3EA2875BD937483494","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Open one of the glass Petri dishes and set the cover aside.\u00a0 Continue the dissection of the tumor such that it \u201cdrops\u201d into the open dish. Cover the Dish and transfer to a laminar flow biological safety cabinet.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":956665,"guid":"3218D990A93D11EABF5601D5F45BACF3","previous_id":956664,"previous_guid":"32168FA0A93D11EABF5601D5F45BACF3","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"2E51A00C4A814A9BBFBAAFED549C0DBC","order_id":1,"type_id":6,"title":"Section","source":{"title":"TUMOR HARVEST"}},{"id":1054724,"guid":"23A7DDB3C3FE4DA8807AF2E56BBDD46D","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Dice tumor into large pieces (chunks) and dissect out any necrotic (dead) tissue. Necrosis can be white, yellowish or dark red and bloody.\u00a0 Transfer healthy-looking tumor tissue chunks to a new glass Petri dish, if necessary. Place Petri dish on an ice bath until all tumor chunks have been harvested and processed to this point.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":956666,"guid":"321B98B0A93D11EABF5601D5F45BACF3","previous_id":956665,"previous_guid":"3218D990A93D11EABF5601D5F45BACF3","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"C1EDE4D8D2FF49D3AEBA4ABA5B482764","order_id":1,"type_id":6,"title":"Section","source":{"title":"TUMOR HARVEST"}},{"id":1054724,"guid":"A02AB3BA8EAE4CB0A957B32A5D349B1F","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Place the carcass in the black plastic sack and proceed to euthanize the next mouse and repeat the process.\u00a0 Ensure that the carcass bag is well tied and disposed in the Morgue in Animal Resource Program space.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":956668,"guid":"3221DA40A93D11EABF5601D5F45BACF3","previous_id":956666,"previous_guid":"321B98B0A93D11EABF5601D5F45BACF3","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"6F13F810BB54411C8DF3377A1CD4B339","order_id":1,"type_id":6,"title":"Section","source":{"title":"PREPARATION OF ENZYME SOLUTION"}},{"id":1054724,"guid":"9F9470FD14B94DED8ACFF1CF3F4467A5","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Add 35ml of serum-free RPMI 1640 to a 50ml centrifuge tube. Add one aliquot of Enzyme D, Enzyme R and Enzyme A to the tube.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":956669,"guid":"32250E90A93D11EABF5601D5F45BACF3","previous_id":956668,"previous_guid":"3221DA40A93D11EABF5601D5F45BACF3","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"0A9B9894631F4C4C861BBCF48EA45B32","order_id":1,"type_id":6,"title":"Section","source":{"title":"PREPARATION OF ENZYME SOLUTION"}},{"id":1054724,"guid":"334730C185564A089E0CFF24756D3890","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Sterile filter the enzyme solution using a 5ml syringe equipped with a 0.22um syringe filter into a \u201cC\u201d tube.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":956672,"guid":"322FE401A93D11EABF5601D5F45BACF3","previous_id":956669,"previous_guid":"32250E90A93D11EABF5601D5F45BACF3","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"213E892B3E0F4365986DEC03D9AF10E7","order_id":1,"type_id":6,"title":"Section","source":{"title":"DISSOCIATION OF PDX TUMOR TISSUE"}},{"id":1054724,"guid":"5341D1A8EF3A4B6FA5F7F605A7A8D9E4","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><\/div>"}},{"id":1054725,"guid":"D1D12DBCE24E4E0CBC74C2892B1D30D2","order_id":2,"type_id":26,"title":"notes","source":{"id":0,"parent_id":0,"uri":"","title":"","body":"<div class = \"text-blocks\"><div class = \"text-block\">This should be performed aseptically in a laminar flow biological safety cabinet \u2013 Be sure to use gloves, sanitize your gloves with 3% chlorhexidine spray and observe strict aseptic, no-touch techniques <\/div><\/div>","created_on":0,"changed_on":0,"creator":{"name":" ","affiliation":null,"affiliations":[],"username":"","link":null,"image":{"source":"","placeholder":""},"badges":[],"verified":0,"is_verified_user":false,"research_interests":null,"blocked_by_you":fals