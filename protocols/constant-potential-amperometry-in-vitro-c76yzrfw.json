ges\":[{\"style\":\"italic\",\"offset\":13,\"length\":6},{\"style\":\"italic\",\"offset\":112,\"length\":11},{\"style\":\"bold\",\"offset\":124,\"length\":3}],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","public":true,"public_fork_note":"","published_on":1706631772,"references":[],"related_equipments":[],"related_materials":[],"reserved_doi":"10.17504/protocols.io.yxmvm34p9l3p/v1","retraction_reason":null,"samples":{},"shared_access_id":265,"show_comparison":false,"sign_info":null,"space_access":{"can_view":false,"can_edit":false,"can_remove":false,"can_add":false,"can_publish":true,"can_get_doi":true,"can_share":false,"can_move":false,"can_move_outside":false,"can_transfer":false,"can_download":false,"limited_run":false,"limited_private_links":false,"limited_blind_links":false,"is_locked":false},"space_id":115074,"state_version_id":867,"stats":{"is_voted":false,"number_of_views":1,"number_of_steps":13,"number_of_bookmarks":0,"number_of_comments":0,"number_of_bookmarked_comments":0,"number_of_steps_comments":0,"number_of_protocol_comments":0,"number_of_exports":0,"number_of_runs":0,"number_of_votes":0,"number_of_reagents":0,"number_of_equipments":0,"number_of_collections":1,"number_of_forks":{"private":0,"public":0},"number_of_accessible_forks":0},"status":{"id":1,"info":""},"steps":[{"id":1917175,"guid":"0EB08B4073FA4F538D7A0CD113AC8D36","previous_id":0,"previous_guid":null,"section":"\u003cp\u003eSlice preparation\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"c4req\",\"text\":\"Anesthetize the animal by isofluorane inhalation and decapitate the animal. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94136,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"1","cases":[],"critical":null},{"id":1917177,"guid":"67AD2A839A424E8B8914E3CBFB1D667A","previous_id":1917175,"previous_guid":"0EB08B4073FA4F538D7A0CD113AC8D36","section":"\u003cp\u003eSlice preparation\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"1g39\",\"text\":\"Revome quickly and carefully the brain from the skull, and slice the brain in 250–300 µm thick coronal slices containing the striatum by a vibratome immersed in cooled bubbled (95% O2, 5% CO2) ‘sucrose-based’ artificial CSF (aCSF) solution containing (in mM): KCl3, NaH2PO4 (1.25), NaHCO3 (26), MgSO4 (10), CaCl2 (0.5), glucose (25), sucrose (185); ~300 mOsm, pH 7.4 \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94136,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"2","cases":[],"critical":null},{"id":1917178,"guid":"7068C7C9A1E1483C8DC0FC68F3108C94","previous_id":1917177,"previous_guid":"67AD2A839A424E8B8914E3CBFB1D667A","section":"\u003cp\u003eSlice preparation\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"bpd4i\",\"text\":\"Transfer slices in standard aCSF solution containing (in mM): NaCl (126), KCl (2.5), NaH2PO4 (1.2), NaHCO3 (24), MgCl2 (1.3), CaCl2 (2.4), glucose 10; ~290 mOsm, pH 7.4, bubbling (95% O2, 5% CO2); at 32 °C, and let them recovery for at least 40 min\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94136,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"3","cases":[],"critical":null},{"id":1917179,"guid":"64912660698D449D95716409B1FE042B","previous_id":1919000,"previous_guid":"68638DA8BFAD46C2AE7BDA3A4444505E","section":"\u003cp\u003eAmperometric recordings \u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"9e2er\",\"text\":\"Place a bipolar Ni/Cr insulated stimulating electrode into the striatal slice. To monitor the\\nelectrically evoked dopamine overflow, gently place a carbon fiber electrode (active surface 30 μm in diameter and 100 μm long; World Precision Instruments, CF10) into the striatal slice to a depth of 100–150 μm near the stimulating electrode, connected to a potentiostat (MicroC, World Precision Instruments) to apply voltage and measure current.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94136,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"5","cases":[],"critical":null},{"id":1917192,"guid":"106C8788993842EBA2F0AF59F444FC1B","previous_id":1919004,"previous_guid":"CFC1CCE51F554F58A52A099B52CA755A","section":"\u003cp\u003eAmperometric recordings \u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"lctj\",\"text\":\"Apply every 5 min a single rectangular electrical pulse through a bipolar stimulating electrode placed on the striatal slice by using a Digitimer DS3 stimulator (80–500 μA, 20–40 μs duration)\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94136,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"6","cases":[],"critical":null},{"id":1919000,"guid":"68638DA8BFAD46C2AE7BDA3A4444505E","previous_id":1917178,"previous_guid":"7068C7C9A1E1483C8DC0FC68F3108C94","section":"\u003cp\u003eSlice preparation\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"atn5h\",\"text\":\"Transfer one slice for amperometric recordings into the recording chamber under an optical microscope, and superfuse (2-3 ml/min) the slice with standard aCSF solution kept at 32°C\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94136,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4","cases":[],"critical":null},{"id":1919004,"guid":"CFC1CCE51F554F58A52A099B52CA755A","previous_id":1917179,"previous_guid":"64912660698D449D95716409B1FE042B","section":"\u003cp\u003eAmperometric recordings \u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":true,"step":"{\"blocks\":[{\"key\":\"fiaep\",\"text\":\" Impose 0.55 V between the carbon fiber electrode and the Ag/AgCl pellet \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94136,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"5.1","cases":[],"critical":null},{"id":1919006,"guid":"AB887B3854A24382A0E43CE949A03930","previous_id":1917192,"previous_guid":"106C8788993842EBA2F0AF59F444FC1B","section":"\u003cp\u003eAmperometric recordings \u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":true,"step":"{\"blocks\":[{\"key\":\"66ilc\",\"text\":\"Apply increasing electrical pulse step until maximum evoked dopamine outflow \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94136,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"6.1","cases":[],"critical":null},{"id":1919007,"guid":"9C17078C757D4E8A9434AC0746315572","previous_id":1919009,"previous_guid":"BB1DADA25726494EBE2D2818BB5624B3","section":"\u003cp\u003eDrug application \u003c/p\u003e","section_color":"#84CE84","section_duration":3600,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"48g5q\",\"text\":\"Monitor the extracellular dopamine response to electrical stimulation is stable for four or five successive stimulations\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94136,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"8","cases":[],"critical":null},{"id":1919009,"guid":"BB1DADA25726494EBE2D2818BB5624B3","previous_id":1919006,"previous_guid":"AB887B3854A24382A0E43CE949A03930","section":"\u003cp\u003eAmperometric recordings \u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"aqhh0\",\"text\":\"At the end of each experiment, perform electrode calibration in aCSF containing known dopamine concentration (300 nmol/L–3 μmol/L)\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94136,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"7","cases":[],"critical":null},{"id":1919010,"guid":"51F8D0CE8A40435498EA8B54C5879CFB","previous_id":1919007,"previous_guid":"9C17078C757D4E8A9434AC0746315572","section":"\u003cp\u003eDrug application \u003c/p\u003e","section_color":"#84CE84","section_duration":3600,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"eldbv\",\"text\":\"Superfuse the striatal slice with drugs (0.3 µM cocaine, 1 µM cocaine, or 0.03 µM quinpirole, dissolved in standard aCSF) for 10 min (cocaine) and 5 min (quinpirole) \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94136,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"9","cases":[],"critical":null},{"id":1919011,"guid":"7E4B8D51CFD447ADA17D4CC65C78F4F4","previous_id":1919010,"previous_guid":"51F8D0CE8A40435498EA8B54C5879CFB","section":"\u003cp\u003eDrug application \u003c/p\u003e","section_color":"#84CE84","section_duration":3600,"is_substep":true,"step":"{\"blocks\":[{\"key\":\"278ua\",\"text\":\"Wash out with standard  aCSF for 1h (cocaine) or 30 min (quinpirole) \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94136,"case_id":0,"critical_ids":"","duration"