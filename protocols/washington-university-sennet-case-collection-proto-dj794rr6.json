substep":false,"step":"{\"blocks\":[{\"key\":\"4mgak\",\"text\":\"Study coordinator will evaluate patient for eligibility using criteria detailed in following sections. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":106465,"case_id":0,"critical_ids":"","duration":0,"original_id":1518974,"number":"4","cases":[],"critical":null},{"id":2203754,"guid":"A38576FE640911EF8E380A58A9FEAC02","previous_id":2203752,"previous_guid":"A3846AC3640911EF8E380A58A9FEAC02","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"cu5i0\",\"text\":\"Study coordinator will call potential study participants over phone using IRB approved phone script to see if participants are interested. Other option is to approach potential participants in Clinic when they are meeting with surgeon before the surgery date. Discuss with surgeon team which is best option as to not disrupt clinic flow.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":106465,"case_id":0,"critical_ids":"","duration":0,"original_id":1518996,"number":"2","cases":[],"critical":null},{"id":2203755,"guid":"A385A8EB640911EF8E380A58A9FEAC02","previous_id":2203754,"previous_guid":"A38576FE640911EF8E380A58A9FEAC02","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"9lovn\",\"text\":\"Once patient is identified and interested, go over IRB approved consent form with patient. Answer all questions and allow for time if they need to think the process over. Must give patient a signed copy of consent form for their own personal records. Enter subjects into Oncore within 24 hours of consent process.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":106465,"case_id":0,"critical_ids":"","duration":0,"original_id":1518997,"number":"3","cases":[],"critical":null},{"id":2203756,"guid":"A385F792640911EF8E380A58A9FEAC02","previous_id":2203753,"previous_guid":"A3852E0E640911EF8E380A58A9FEAC02","section":"\u003cp\u003eInclusion Criteria\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"bd5bc\",\"text\":\"Able to understand and willing to sign an IRB-approved written informed consent document\",\"type\":\"unordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"cfj1h\",\"text\":\"Undergoing routine surgery under general anesthesia for non-cancer diagnosis, including: \",\"type\":\"unordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"97il1\",\"text\":\"Laparoscopic upper gastrointestinal surgery (for benign diagnosis, e.g. cholecystectomy, hernia repair) or Open abdominal surgery for similar indications. (need access to liver and omental fat)\",\"type\":\"unordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"buc7l\",\"text\":\"Orthopedic knee and hip replacement surgeries\",\"type\":\"unordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"1j6g7\",\"text\":\"18 years of age or older\",\"type\":\"unordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"ea6bp\",\"text\":\"All races and ethnicities eligible\",\"type\":\"unordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"6fvpf\",\"text\":\"All BMIs eligible\",\"type\":\"unordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"2puuc\",\"text\":\"Equal number of female and male donors will be accepted\",\"type\":\"unordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"bhcpk\",\"text\":\"Past history of cancer and chemo are eligible\",\"type\":\"unordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":106465,"case_id":0,"critical_ids":"","duration":0,"original_id":1518999,"number":"5","cases":[],"critical":null},{"id":2203757,"guid":"A3862C5E640911EF8E380A58A9FEAC02","previous_id":2203756,"previous_guid":"A385F792640911EF8E380A58A9FEAC02","section":"\u003cp\u003eExclusion Criteria\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"eqii0\",\"text\":\"Undergoing a Cancer surgery\",\"type\":\"unordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"5d6h6\",\"text\":\"Currently receiving Chemo treatment\",\"type\":\"unordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"1vmvv\",\"text\":\"Positive for HIV\",\"type\":\"unordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"atjjm\",\"text\":\"Under 18 years of age\",\"type\":\"unordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"da5ej\",\"text\":\"Unable to fully understand/sign consent form. (e.g. language barrier, compromised mental status)\",\"type\":\"unordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":106465,"case_id":0,"critical_ids":"","duration":0,"original_id":1519002,"number":"6","cases":[],"critical":null},{"id":2203758,"guid":"A3866074640911EF8E380A58A9FEAC02","previous_id":2203757,"previous_guid":"A3862C5E640911EF8E380A58A9FEAC02","section":"\u003cp\u003eDocuments\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"lhjp\",\"text\":\"Signed consent document saved and stored in secure location. Copy 