rnal":null,"journals":[],"keywords":"nuclei,isolation,FACS","last_modified":1706292661,"link":"","location":null,"manuscript_citation":"","materials":[],"materials_text":"{\"blocks\":[{\"key\":\"4tghv\",\"text\":\"\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","ownership_history":null,"parent_collections":[],"parent_protocols":[],"peer_reviewed":false,"protocol_references":"{\"blocks\":[{\"key\":\"fqmvb\",\"text\":\"Adapted from the Allen Institute's protocol.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","public":true,"public_fork_note":"","published_on":1706292661,"references":[],"related_equipments":[],"related_materials":[],"reserved_doi":"","retraction_reason":null,"samples":{"5FDEBEF0F10411ED8CED412DF83D3073":{"name":"Blocking Buffer","description":""},"AEE36000F4DD11ED99CA5F4847BDD42C":{"name":"10% BSA","description":""}},"shared_access_id":265,"show_comparison":true,"sign_info":null,"space_access":{"can_view":false,"can_edit":false,"can_remove":false,"can_add":false,"can_publish":true,"can_get_doi":true,"can_share":false,"can_move":false,"can_move_outside":false,"can_transfer":false,"can_download":false,"limited_run":false,"limited_private_links":false,"limited_blind_links":false,"is_locked":false},"space_id":96348,"state_version_id":153,"stats":{"is_voted":false,"number_of_views":7,"number_of_steps":22,"number_of_bookmarks":0,"number_of_comments":0,"number_of_bookmarked_comments":0,"number_of_steps_comments":0,"number_of_protocol_comments":0,"number_of_exports":0,"number_of_runs":0,"number_of_votes":0,"number_of_reagents":0,"number_of_equipments":0,"number_of_collections":0,"number_of_forks":{"private":1,"public":0},"number_of_accessible_forks":0},"status":{"id":1,"info":"We use this protocol and it's working"},"steps":[{"id":1908618,"guid":"4E819368B61811EEA5120A58A9FEAC02","previous_id":1908639,"previous_guid":"4E8F34E1B61811EEA5120A58A9FEAC02","section":"\u003cp\u003ePrepare Fresh Solutions\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"8tqo3\",\"text\":\"Make 3 mL Homogenization Buffer by adding 2.892 mL Nuclear Isolation Media to a 5 mL eppendorf. Then add 60 uL protease inhibitor, 30 μL 10% Triton X-100, 15 uL RNase inhibitor, and 3 μL 100 mM DTT.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"bold\",\"offset\":10,\"length\":22}],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":93752,"case_id":0,"critical_ids":"","duration":0,"original_id":1820190,"number":"4","cases":[],"critical":null},{"id":1908619,"guid":"4E821F13B61811EEA5120A58A9FEAC02","previous_id":null,"previous_guid":null,"section":"\u003cp\u003ePrepare Stock Solutions\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"1qajs\",\"text\":\"Make 50 mL Nuclear Isolation Media by filling a 100 mL bottle with 25 mL of MilliQ water and then adding 500 uL 1M Tris, 1.25 mL 1M KCl, 250 uL 1M MgCl2, and 4.28 g Sucrose. Shake until sucrose is dissolved then fill to 50 mL with MilliQ water. Filter it using a stericup into another clean bottle. (4°C - 2 weeks)\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"bold\",\"offset\":11,\"length\":24}],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":93752,"case_id":0,"critical_ids":"","duration":0,"original_id":1820192,"number":"1","cases":[],"critical":null},{"id":1908620,"guid":"4E82C494B61811EEA5120A58A9FEAC02","previous_id":1908618,"previous_guid":"4E819368B61811EEA5120A58A9FEAC02","section":"\u003cp\u003ePrepare Fresh Solutions\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"9tnoq\",\"text\":\"Make 15 mL Blocking Buffer by adding 13.425 mL 1X PBS, 1.5 mL 10% BSA, and 75 uL RNase inhibitor (0.2 U/uL) to a 15 mL falcon tube.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"bold\",\"offset\":11,\"length\":15}],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":93752,"case_id":0,"critical_ids":"","duration":0,"original_id":1820193,"number":"5","cases":[],"critical":null},{"id":1908621,"guid":"4E836EAAB61811EEA5120A58A9FEAC02","previous_id":1908619,"previous_guid":"4E821F13B61811EEA5120A58A9FEAC02","section":"\u003cp\u003ePrepare Stock Solutions\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"lhai\",\"text\":\"Make 20 mL 10% Triton X-100 by combining 18 mL MilliQ water with 2 mL Triton X-100 in a 50 mL tube. Vortex and then incubate at room temperature for 20 minutes. Filter it through a 0.22 μm filter with a syringe into a clean 50 mL tube. (4°C - 1 month)\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"bold\",\"offset\":11,\"length\":17}],\"entityRanges\":[],\"data\":{}},{\"key\":\"flbp3\",\"text\":\"\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"apgq3\",\"text\":\"For a smaller stock, make 1 mL by combining 900 uL MilliQ water with 100 uL Triton-X 100. Filter with a FlowMi after vortex and incubation. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":93752,"case_id":0,"critical_ids":"","duration":0,"original_id":1820194,"number":"2","cases":[],"critical":null},{"id":1908622,"guid":"4E8415B5B61811EEA5120A58A9FEAC02","previous_id":1908620,"previous_guid":"4E82C494B61811EEA5120A58A9FEAC02","section":"\u003cp\u003eHomogenization\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"5vhno\",\"text\":\"Clean dounce, scalpel, and forceps using MilliQ water, ethanol, RNase Zap, then MilliQ again. The red-tape forceps are for unfixed tissue. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":93752,"case_id":0,"critical_ids":"","duration":0,"original_id":1820195,"number":"6","cases":[],"critical":null},{"id":1908623,"guid":"4E84B9D0B61811EEA5120A58A9FEAC02","previous_id":1908622,"previous_guid":"4E8415B5B61811EEA5120A58A9FEAC02","section":"\u003cp\u003eHomogenization\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"256dn\",\"text\":\"Place your sample on dry ice. Tare a weigh boat or tube on the scale and weigh your sample.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":93752,"case_id":0,"critical_ids":"","duration":0,"original_id":1820196,"number":"7","cases":[],"critical":null},{"id":1908624,"guid":"4E8560B9B61811EEA5120A58A9FEAC02","previous_id":1908623,"previous_guid":"4E84B9D0B61811EEA5120A58A9FEAC02","section":"\u003cp\u003eHomogenization\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"bb864\",\"text\":\"Add tissue to dounce and push it to the bottom using 1 mL of Homegenization Buffer and the pestle. Homogenize the tissue without creating bubbles. Then add the remaining 2 mL of the Homogenization Buffer and continue to dounce until homogenized.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":93752,"case_id":0,"critical_ids":"","duration":0,"original_id":1820197,"number":"8","cases":[],"critical":null},{"id":1908625,"guid":"4E860775B61811EEA5120A58A9FEAC02","previous_id":1908624,"previous_guid":"4E8560B9B61811EEA5120A58A9FEAC02","section":"\u003cp\u003eHomogenization\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"fivhr\",\"text\":\"Pass all of the nuclei suspension through a 40 um filter.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":93752,"case_id":0,"critical_ids":"","duration":0,"original_id":1820198,"number":"9","cases":[],"critical":null},{"id":1908626,"guid":"4E86AD4FB61811EEA5120A58A9FEAC02","previous_id":1908625,"previous_guid":"4E860775B61811EEA5120A58A9FEAC02","section":"\u003cp\u003eHomogenization\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"cqhc0\",\"text\":\"Centrifuge at 900 g/rcf for 10 minutes at 4°C.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":93752,"case_id":0,"critical_ids":"","duration":0,"original_id":1820199,"number":"10","cases":[],"critical":null},{"id":1908627,"guid":"4E875B17B61811EEA5120A58A9FEAC02","previous_id":1908628,"previous_guid":"4E880130B61811EEA5120A58A9FEAC02","section":"\u003cp\u003eBlocking Buffer \u0026amp; Antibodies\u003c/p\u003e","section_color":"#FFED92","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"23dr3\",\"text\":\"The amount of Blocking Buffer we will need to resuspend our nuclei pellet with after centrifuging depends on the number of controls we will run. Each control tube will require 50 μL of nuclei suspension, and our sample needs to have 1,000 μL of nuclei suspension left over. In this case we are running two controls.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"ac4tt\",\"text\":\"\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":93752,"case_id":0,"critical_ids":"","duration":0,"original_id":1820200,"number":"12","cases":[],"critical":null},{"id":1908628,"guid":"4E880130B61811EEA5120A58A9FEAC02","previous_id":1908626,"previous_guid":"4E86AD4FB61811EEA5120A58A9FEAC02","section":"\u003cp\u003eBlocking Buffer \u0026amp; Antibodies\u003c/p\u003e","section_color":"#FFED92","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"721cg\",\"text\":\"Add 950 μL Blocking Buffer to each of your control tubes.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":93752,"case_id":0,"critical_ids":"","duration":0,"original_id":1820201,"number":"11","cases":[],"critical":null},{"id":1908629,"guid":"4E88A4AAB61811EEA5120A58A9FEAC02","previous_id":1908627,"previous_guid":"4E875B17B61811EEA5120A58A9FEAC02","section":"\u003cp\u003eBlocking Buffer \u0026amp; Antibodies\u003c/p\u003e","section_color":"#FFED92","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"804gs\",\"text\":\"After the cell homogenate is done centrifuging, discard the supernatant and resuspend the pellet in 1,100 μL of Blocking Buffer. Incubate for 15 minutes on ice. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":93752,"case_id":0,"critical_ids":"","duration":0,"original_id":1820202,"number":"13","cases":[],"critical":null},{"id":1908630,"guid":"4E8948CEB61811EEA5120A58A9FEAC02","previous_id":1908629,"previous_guid":"4E88A4AAB61811EEA5120A58A9FEAC02","section":"\u003cp\u003eBlocking Buffer \u0026amp; Antibodies\u003c/p\u003e","section_color":"#FFED92","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"9tpb0\",\"text\":\"Add 50 μL of nuclei suspension to each control tubes.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":93752,"case_id":0,"critical_ids":"","duration":0,"original_id":1820203,"number":"14","cases":[],"critical":null},{"id":1908631,"guid":"4E89F403B61811EEA5120A58A9FEAC02","previous_id":1908630,"previous_guid":"4E8948CEB61811EEA5120A58A9FEAC02","section":"\u003cp\u003eBlocking Buffer \u0026amp; Antibodies\u003c/p\u003e","section_color":"#FFED92","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"2i0af\",\"text\":\"Add the corresponding antibodies to the control tubes:\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"34j6e\",\"text\":\"2 μL NeuN-PE (1/500) to the PE-only control\",\"type\":\"unordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"9gesl\",\"text\":\"0.5 μL Olig2-Alexa 488 (1/2000) to the A488-only control\",\"type\":\"unordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":93752,"case_id":0,"critical_ids":"","duration":0,"original_id":1820204,"number":"15","cases":[],"critical":null},{"id":1908632,"guid":"4E8A9955B61811EEA5120A58A9FEAC02","previous_id":1908631,"previous_guid":"4E89F403B61811EEA5120A58A9FEAC02","section":"\u003cp\u003eBlocking Buffer \u0026amp; Antibodies\u003c/p\u003e","section_color":"#FFED92","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"bqe5h\",\"text\":\"Add both 2 μL NeuN-PE and 0.5 μL Olig2-Alexa 488 to the sample tube.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":93752,"case_id":0,"critical_ids":"","duration":0,"original_id":1820205,"number":"16","cases":[],"critical":null},{"id":1908633,"guid":"4E8B5523B61811EEA5120A58A9FEAC02","previous_id":1908632,"previous_guid":"4E8A9955B61811EEA5120A58A9FEAC02","section":"\u003cp\u003eBlocking Buffer \u0026amp; Antibodies\u003c/p\u003e","section_color":"#FFED92","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"c1iqo\",\"text\":\"Place all tubes in a rotator placed in a dark 4°C fridge and incubate for 30 minutes.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":93752,"case_id":0,"critical_ids":"","duration":0,"original_id":1820206,"number":"17","cases":[],"critical":null},{"id":1908634,"guid":"4E8BFABDB61811EEA5120A58A9FEAC02","previous_id":1908633,"previous_guid":"4E8B5523B61811EEA5120A58A9FEAC02","section":"\u003cp\u003eWash\u003c/p\u003e","section_color":"#EA9F6C","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"8vj3k\",\"text\":\"After incubation, spin down all of the tubes at 500 g for 5 minutes at 4°C. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":93752,"case_id":0,"critical_ids":"","duration":0,"original_id":1820207,"number":"18","cases":[],"critical":null},{"id":1908635,"guid":"4E8C9F1AB61811EEA5120A58A9FEAC02","previous_id":1908634,"previous_guid":"4E8BFABDB61811EEA5120A58A9FEAC02","section":"\u003cp\u003eWash\u003c/p\u003e","section_color":"#EA9F6C","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"8l28u\",\"text\":\"Decant and resuspend in 999 uL blocking buffer. Consider adding an additional wash step here.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":93752,"case_id":0,"critical_ids":"","duration":0,"original_id":1820208,"number":"19","cases":[],"critical":null},{"id":1908636,"guid":"4E8D4379B61811EEA5120A58A9FEAC02","previous_id":1908638,"previous_guid":"4E8E8ED5B61811EEA5120A58A9FEAC02","section":"\u003cp\u003eWash\u003c/p\u003e","section_color":"#EA9F6C","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"c7tl9\",\"text\":\"Store tubes on ice until FACS. Bring DAPI stock and Blocking Buffer with you to FACS in case we have to dilute the sample to lower the event count.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":93752,"case_id":0,"critical_ids":"","duration":0,"original_id":1820209,"number":"22","cases":[],"critical":null},{"id":1908637,"guid":"4E8DE773B61811EEA5120A58A9FEAC02","previous_id":1908635,"previous_guid":"4E8C9F1AB61811EEA5120A58A9FEAC02","section":"\u003cp\u003eWash\u003c/p\u003e","section_color":"#EA9F6C","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"6bt5r\",\"text\":\"Add 1 uL of 0.1 mg/mL DAPI, a final concentration of 0.1 ug/mL, to each sample and control tube and invert to mix.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":93752,"case_id":0,"critical_ids":"","duration":0,"original_id":1820210,"number":"20","cases":[],"critical":null},{"id":1908638,"guid":"4E8E8ED5B61811EEA5120A58A9FEAC02","previous_id":1908637,"previous_guid":"4E8DE773B61811EEA5120A58A9FEAC02","section":"\u003cp\u003eWash\u003c/p\u003e","section_color":"#EA9F6C","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"67o57\",\"text\":\"Add 300 uL Blocking Buffer to falcon FACS tubes and label them A488+, PE+, and Double Negative. We will sort into these.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":93752,"case_id":0,"critical_ids":"","duration":0,"original_id":1820213,"number":"21","cases":[],"critical":null},{"id":1908639,"guid":"4E8F34E1B61811EEA5120A58A9FEAC02","previous_id":1908621,"previous_guid":"4E836EAAB61811EEA5120A58A9FEAC02","section":"\u003cp\u003ePrepare Stock Solutions\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"51rf8\",\"text\":\"Make 1 mL of 0.1 mg/mL DAPI by adding 100 uL of 1 mg/mL DAPI stock to 900 uL of milliQ water. (4C - 1 month)\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges