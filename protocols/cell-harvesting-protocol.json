cess":{"can_view":1,"can_remove":1,"can_add":0,"can_edit":0,"can_publish":0,"can_get_doi":1,"can_share":1,"can_move":1,"can_move_outside":1,"can_transfer":1,"can_download":1,"is_locked":0},"guid":"15DD7A204C6B11EBA525F16D1E02653F","state_version_id":160,"steps":[{"id":1098769,"guid":"DED00B004C6B11EBA525F16D1E02653F","previous_id":null,"previous_guid":null,"modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"36FDE1185CB44C3CB0CD4093DC001FC6","order_id":1,"type_id":6,"title":"Section","source":{"title":"Procedure"}},{"id":1054724,"guid":"7C1DA12D855D4868BA731248545E193D","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span>Dilute the overnight culture to OD<\/span><span style = \"vertical-align:sub;\">600<\/span><span> 0.01 and grow cells in an appropriate medium to the desired time point\/OD<\/span><span style = \"vertical-align:sub;\">600<\/span><span> in at least 10mL of liquid culture.<\/span><\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1098770,"guid":"926E67604C6C11EBA525F16D1E02653F","previous_id":1098769,"previous_guid":"DED00B004C6B11EBA525F16D1E02653F","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"0E1530291CE148C4917A837FF10D56F3","order_id":1,"type_id":6,"title":"Section","source":{"title":"Procedure"}},{"id":1054724,"guid":"F41D64C0EA2048FA9E9CCFA18B746A96","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Harvest cells directly into pre-labeled falcon tubes (if running flow same-day) or Eppendorf tubes. Once cells have been harvested, minimize exposure to direct light if looking for fluorescence.\u00a0<\/div><div class = \"text-block\"><ol style = \"list-style-type: decimal;\"><li style = \"counter-reset:ol0;\"><span>For <\/span><span style = \"font-style:italic;\">E. coli <\/span><span>from overnight LB, at least 25uL<\/span><\/li><li style = \"counter-reset:ol0;\"><span>For<\/span><span style = \"font-style:italic;\"> E. coli<\/span><span> from exponential phase (OD~0.3) LB, at least 50uL<\/span><\/li><li style = \"counter-reset:ol0;\"><span>For <\/span><span style = \"font-style:italic;\">E. coli<\/span><span> from the overnight or exponential phase in minimal media, at least 500uL<\/span><\/li><\/ol><\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1098771,"guid":"43601A504C6D11EBA525F16D1E02653F","previous_id":1098770,"previous_guid":"926E67604C6C11EBA525F16D1E02653F","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"6C3659EA045F4657BC5B1D39BEB846D6","order_id":1,"type_id":6,"title":"Section","source":{"title":"Procedure"}},{"id":1054724,"guid":"3FA0ABC2FFA240D58F0A948844CE6874","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">For membrane depolarization assay:<\/div><div class = \"text-block\"><ol style = \"list-style-type: decimal;\"><li style = \"counter-reset:ol0;\">Wash the cells twice with 1x PBS and resuspend in 1 ml PBS<\/li><li style = \"counter-reset:ol0;\"><span>Add DiBAC<\/span><span style = \"vertical-align:sub;\">4<\/span><span>-3 reagent- final concentration 10mg\/mL (4ml of 2.5 mg\/mL working stock in 1 mL)<\/span><\/li><li style = \"counter-reset:ol0;\">Mix and incubate in dark for 20 minutes. Wash twice in PBS.<\/li><li style = \"counter-reset:ol0;\">Fix using paraformaldehyde and glutaraldehyde (see below)<\/li><\/ol><\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1098772,"guid":"7B6058704C6D11EBA525F16D1E02653F","previous_id":1098771,"previous_guid":"43601A504C6D11EBA525F16D1E02653F","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"69BB98451B5045A9BEE6F67581F6DFFD","order_id":1,"type_id":6,"title":"Section","source":{"title":"Procedure"}},{"id":1054724,"guid":"EB648EBFCF1544DF8E173C403633AA6A","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Optional: Fix with desired fixation method in the chemical fume hood<\/div><div class = \"text-block\"><ol style = \"list-style-type: decimal;\"><li style = \"counter-reset:ol0;\">Required if using the LSR-II in Mossman. Special permission needed to run live-cells through a flow cytometer.\u00a0<\/li><li style = \"counter-reset:ol0;\">Potential fixation methods:<\/li><\/ol><\/div><div class = \"text-block\"><ul style = \"list-style-type:disc;\"><li style = \"counter-reset:ol0;list-style-type:disc;\">0.5% paraformaldehyde for 10 min in a cold room in dark (T. Hancock)<\/li><li style = \"counter-reset:ol0;list-style-type:disc;\">3% glutaraldehyde for 1hr at room temp in dark (R. Johnston)\u00a0<\/li><li style = \"counter-reset:ol0;list-style-type:disc;\">paraformaldehyde to final concentration 2.8% (vol\/vol) and glutaraldehyde to 0.04% (vol\/vol) for 15 min at room temp in dark (Dr. J.Mannik)\u00a0<\/li><\/ul><\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1098773,"guid":"03343A004C6E11EBA525F16D1E02653F","previous_id":1098772,"previous_guid":"7B6058704C6D11EBA525F16D1E02653F","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"A0E59E71946041E18CF36A5453A977DB","order_id":1,"type_id":6,"title":"Section","source":{"title":"Procedure"}},{"id":1054724,"guid":"3321D1251B8144029EA9A49B913D57B8","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Wash cells 3 times<\/div><div class = \"text-block\"><ol style = \"list-style-type: decimal;\"><li style = \"counter-reset:ol0;\">Centrifuge at 4 degrees until pellets form (tabletop for 3 min, if in falcon tubes, 10 min in a large centrifuge)<\/li><li style = \"counter-reset:ol0;\">Decant supernatant in an appropriate chemical waste container in the hood<\/li><li style = \"counter-reset:ol0;\">Flood sample with PBS and vortex<\/li><li style = \"counter-reset:ol0;\">Repeat steps 1-3 2 additional times<\/li><\/ol><\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1098774,"guid":"5EA5CD904C6E11EBA525F16D1E02653F","previous_id":1098773,"previous_guid":"03343A004C6E11EBA525F16D1E02653F","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"E580E96FB0E1451DBC97E22B9DB0B6B3","order_id":1,"type_id":6,"title":"Section","source":{"title":"Procedure"}},{"id":1054724,"guid":"C6AB4D6A2F764123BA1E571BEA8D374D","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Decant liquid and add PBS to the desired volume.<\/div><div class = \"text-block\"><ol style = \"list-style-type: decimal;\"><li style = \"counter-reset:ol0;\">For flow and E. coli, 1 mL for minimal media samples, 0.5 mL for LB samples if the followed recommendation in step 2.<\/li><li style = \"counter-reset:ol0;\">If mounting cells on slides for microscopy, do not add PBS during the last round. Aspirate remaining liquid out instead.<\/li><\/ol><\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1098775,"guid":"7E0719004C6E11EBA525F16D1E02653F","previous_id":1098774,"previous_guid":"5EA5CD904C6E11EBA525F16D1E02653F","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"C5D9641DA83B4462841AECE96126EBF7","order_id":1,"type_id":6,"title":"Section","source":{"title":"Procedure"}},{"id":1054724,"guid":"D8C39E2F949C4CB8BCE848124F2886B8","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">If applicable, store cells in the fridge (4 degrees).\u00a0<\/div><div class = \"text-block\"><ol style = \"list-style-type: decimal;\"><\/ol><\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0}],"document":"","materials":[],"description":"<div class = \"text-blocks\"><div class = \"text-block\">A general procedure for harvesting bacterial cells for flow cytometry or fluorescence microscopy<\/div><div class = \"text-block\">Write up by S. Shore 11\/17\/2020 based on B. Bogati communications, proofread by Bikash Bogati<\/div><\/div>","changed_on":1609542512}