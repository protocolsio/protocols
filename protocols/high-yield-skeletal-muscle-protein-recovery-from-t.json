n_duration":0,"critical":[{"id":12,"title":"Critical step","icon":"CriticalIcon"}],"critical_id":null,"duration":0},{"id":983602,"guid":"586BEEE0CA7E11EAB7769B77BB948B0A","previous_id":983601,"previous_guid":"36737DD0CA7E11EAB7769B77BB948B0A","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"57211C6E6FA74747BDFAB18268508043","order_id":1,"type_id":6,"title":"Section","source":{"title":"RNA Extraction"}},{"id":1054724,"guid":"18E52D0C58924972A2F4372D30848736","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Isolate RNA using TRIzol\u00ae reagent, following manufacturer\u2019s instructions.<\/div><div class = \"text-block\"><ul style = \"list-style-type:disc;\"><li style = \"counter-reset:ol0;list-style-type:disc;\"><span style = \"font-weight:bold;\">Important point: <\/span><span>Use 0.5 ml of TRIzol\u00ae reagent per piece (<15 mg) of tissue.<\/span><\/li><li style = \"counter-reset:ol0;list-style-type:disc;\"><span style = \"font-weight:bold;\">Important point: <\/span><span>For better phase separation, use 100 \u00b5l of BCP instead of Chloroform.<\/span><\/li><\/ul><\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":983604,"guid":"73B05F10CA7E11EAB7769B77BB948B0A","previous_id":983602,"previous_guid":"586BEEE0CA7E11EAB7769B77BB948B0A","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"A30319C12A3B43A08B8C73C002A643AF","order_id":1,"type_id":6,"title":"Section","source":{"title":"RNA Extraction"}},{"id":1054724,"guid":"95D38787BCA94BDFBA34766A0C5A164A","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Remove top RNA fraction (aqueous phase) according to manufacturer\u2019s recommendation.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":983605,"guid":"798E2430CA7E11EAB7769B77BB948B0A","previous_id":983604,"previous_guid":"73B05F10CA7E11EAB7769B77BB948B0A","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"13D849904E054A668B9BA99C98FB06C8","order_id":1,"type_id":6,"title":"Section","source":{"title":"RNA Extraction"}},{"id":1054724,"guid":"ECF71B25E97F453EA8CB355229DE16E2","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Follow manufacturer\u2019s instructions for RNA precipitation, wash, and re-suspension or choice of RNA column purification kit.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":983606,"guid":"7CFBACA0CA7E11EAB7769B77BB948B0A","previous_id":983605,"previous_guid":"798E2430CA7E11EAB7769B77BB948B0A","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"5C1F040F8C074A76B2F214579A7733F2","order_id":1,"type_id":6,"title":"Section","source":{"title":"RNA Extraction"}},{"id":1054724,"guid":"18E472E237E64229BD3710F36AF106EE","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Proceed to DNA precipitation.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":983607,"guid":"83B83900CA7E11EAB7769B77BB948B0A","previous_id":983606,"previous_guid":"7CFBACA0CA7E11EAB7769B77BB948B0A","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"44F2394E9E9D451A97529451C804ECC6","order_id":1,"type_id":6,"title":"Section","source":{"title":"DNA Precipitation"}},{"id":1054724,"guid":"998988E883A34BDAA1E24C0614257A91","order_id":1,"type_id":1,"title":"description","source":{"des