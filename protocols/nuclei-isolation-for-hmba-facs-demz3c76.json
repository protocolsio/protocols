","disclaimer":"","document":"","documents":null,"doi":"dx.doi.org/10.17504/protocols.io.kxygx35ywg8j/v3","doi_status":1,"ethics_statement":"","fork_id":null,"fork_info":null,"forks":[],"funders":[],"groups":[{"id":96673,"uri":"krienen-lab","title":"Krienen Lab","is_public":false,"image":{"source":"https://content.protocols.io/files/A2DE5592F0FB11ED98AB0A58A9FEAC02-placeholder.png","placeholder":"https://content.protocols.io/files/A2DE5592F0FB11ED98AB0A58A9FEAC02-placeholder.png"},"tech_support":{"email":null,"phone":null,"use_email":false,"hide_contact":false,"url":null}}],"guid":"9EB3E4271D1411EFB24D0A58A9FEAC02","guidelines":"{\"blocks\":[{\"key\":\"4p1hr\",\"text\":\"Keep tissue/nuclei on ice as much as possible.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","has_references":true,"has_step_reagents":false,"has_versions":true,"id":100761,"image":{"source":"https://content.protocols.io/files/qpbsb6cvf.jpg","webp_source":"https://content.protocols.io/files/qpbrb6cvf.webp","placeholder":"https://content.protocols.io/files/mqfxb6cvf.png","webp_placeholder":""},"image_attribution":"","in_trash":false,"is_bookmarked":false,"is_contact_suspended":false,"is_content_confidential":false,"is_content_warning":false,"is_doi_reserved":false,"is_in_pending_publishing":false,"is_in_transfer":false,"is_owner":true,"is_research":true,"is_retracted":false,"is_shared_directly":false,"is_subprotocol":null,"is_unlisted":false,"item_id":1254915,"journal":null,"journals":[],"keywords":"nuclei,isolation,FACS","last_modified":1716918220,"link":"","location":null,"manuscript_citation":"","materials":[],"materials_text":"{\"blocks\":[{\"key\":\"4tghv\",\"text\":\"\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","ownership_history":null,"parent_collections":[],"parent_protocols":[],"peer_reviewed":false,"protocol_references":"{\"blocks\":[{\"key\":\"fqmvb\",\"text\":\"Adapted from the Allen Institute's protocol.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","public":true,"public_fork_note":"","published_on":1716918220,"references":[],"related_equipments":[],"related_materials":[],"reserved_doi":"","retraction_reason":null,"samples":{"5FDEBEF0F10411ED8CED412DF83D3073":{"name":"Blocking Buffer","description":""},"AEE36000F4DD11ED99CA5F4847BDD42C":{"name":"10% BSA","description":""}},"shared_access_id":265,"show_comparison":true,"sign_info":null,"space_access":{"can_view":false,"can_edit":false,"can_remove":false,"can_add":false,"can_publish":true,"can_get_doi":true,"can_share":false,"can_move":false,"can_move_outside":false,"can_transfer":false,"can_download":false,"limited_run":false,"limited_private_links":false,"limited_blind_links":false,"is_locked":false},"space_id":96348,"state_version_id":306,"stats":{"is_voted":false,"number_of_views":33,"number_of_steps":17,"number_of_bookmarks":0,"number_of_comments":0,"number_of_bookmarked_comments":0,"number_of_steps_comments":0,"number_of_protocol_comments":0,"number_of_exports":0,"number_of_runs":0,"number_of_votes":0,"number_of_reagents":0,"number_of_equipments":0,"number_of_collections":0,"number_of_forks":{"private":0,"public":0},"number_of_accessible_forks":0},"status":{"id":1,"info":"We use this protocol and it's working"},"steps":[{"id":2076495,"guid":"9ECDA7D61D1411EFB24D0A58A9FEAC02","previous_id":2076516,"previous_guid":"9ED0CC1D1D1411EFB24D0A58A9FEAC02","section":"\u003cp\u003ePrepare Fresh Solutions\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"8tqo3\",\"text\":\"Make 3 mL Homogenization Buffer by adding 2.892 mL Nuclear Isolation Media to a 5 mL eppendorf tube. Then add 60 uL protease inhibitor, 30 μL 10% Triton X-100, 15 uL RNase inhibitor, and 3 μL 100 mM DTT.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"bold\",\"offset\":10,\"length\":22}],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":100761,"case_id":0,"critical_ids":"","duration":0,"original_id":1908618,"number":"4","cases":[],"critical":null},{"id":2076496,"guid":"9ECDF8971D1411EFB24D0A58A9FEAC02","previous_id":null,"previous_guid":null,"section":"\u003cp\u003ePrepare Stock Solutions\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"1qajs\",\"text\":\"Make 50 mL Nuclear Isolation Media by filling a 50 mL steriflip with 43 mL of MilliQ water, 5 mL of 2.5 M sucrose, 1.25 mL 1M KCl, 500 uL 1M Tris, 250 uL 1M MgCl2. Vortex and filter with the vacuum system.(4°C - 2 weeks)\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"bold\",\"offset\":11,\"length\":24}],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":100761,"case_id":0,"critical_ids":"","duration":0,"original_id":1908619,"number":"1","cases":[],"critical":null},{"id":2076497,"guid":"9ECE26811D1411EFB24D0A58A9FEAC02","previous_id":2076495,"previous_guid":"9ECDA7D61D1411EFB24D0A58A9FEAC02","section":"\u003cp\u003ePrepare Fresh Solutions\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"9tnoq\",\"text\":\"Make 5 mL Blocking Buffer by adding 4.475 mL 1X PBS, 500 uL 10% BSA, and 25 uL RNase inhibitor to a 5 mL eppendorf tube.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"bold\",\"offset\":10,\"length\":15}],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":100761,"case_id":0,"critical_ids":"","duration":0,"original_id":1908620,"number":"5","cases":[],"critical":null},{"id":2076498,"guid":"9ECE4F061D1411EFB24D0A58A9FEAC02","previous_id":2076496,"previous_guid":"9ECDF8971D1411EFB24D0A58A9FEAC02","section":"\u003cp\u003ePrepare Stock Solutions\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"lhai\",\"text\":\"Make 20 mL 10% Triton X-100 by combining 18 mL MilliQ water with 2 mL Triton X-100 in a 50 mL tube. Vortex and then incubate at room temperature for 20 minutes. Filter it through a 0.22 μm filter with a syringe into a clean 50 mL tube. (4°C - 1 month)\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"bold\",\"offset\":11,\"length\":17}],\"entityRanges\":[],\"data\":{}},{\"key\":\"flbp3\",\"text\":\"\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"apgq3\",\"text\":\"For a smaller stock, make 1 mL by combining 900 uL MilliQ water with 100 uL Triton-X 100. Filter with a FlowMi after vortex and incubation. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":100761,"case_id":0,"critical_ids":"","duration":0,"original_id":1908621,"number":"2","cases":[],"critical":null},{"id":2076499,"guid":"9ECE75701D1411EFB24D0A58A9FEAC02","previous_id":2076497,"previous_guid":"9ECE26811D1411EFB24D0A58A9FEAC02","section":"\u003cp\u003eHomogenization\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"5vhno\",\"text\":\"Clean dounce, scalpel, and forceps using MilliQ water, ethanol, RNase Zap, then MilliQ again. You can soak the dounce in the RNase Zap while preparing the buffers. The red-tape forceps are for unfixed tissue. Chill the dounce on ice before adding the tissue.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":100761,"case_id":0,"critical_ids":"","duration":0,"original_id":1908622,"number":"6","cases":[],"critical":null},{"id":2076500,"guid":"9ECE9A021D1411EFB24D0A58A9FEAC02","previous_id":2076499,"previous_guid":"9ECE75701D1411EFB24D0A58A9FEAC02","section":"\u003cp\u003eHomogenization\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"256dn\",\"text\":\"Place your sample on dry ice. Tare a weigh boat or tube on the scale and weigh your sample.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":100761,"case_id":0,"critical_ids":"","duration":0,"original_id":1908623,"number":"7","cases":[],"critical":null},{"id":2076501,"guid":"9ECEBB991D1411EFB24D0A58A9FEAC02","previous_id":2076500,"previous_guid":"9ECE9A021D1411EFB24D0A58A9FEAC02","section":"\u003cp\u003eHomogenization\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"bb864\",\"text\":\"Add 1 mL of Homogenization Buffer to the dounce on ice and then drop in the frozen tissue. Homogenize the tissue without creating bubbles. Then add the remaining 2 mL of the Homogenization Buffer and continue to dounce until homogenized.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":100761,"case_id":0,"critical_ids":"","duration":0,"original_id":1908624,"number":"8","cases":[],"critical":null},{"id":2076502,"guid":"9ECEDED51D1411EFB24D0A58A9FEAC02","previous_id":2076501,"previous_guid":"9ECEBB991D1411EFB24D0A58A9FEAC02","section":"\u003cp\u003eHomogenization\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"fivhr\",\"text\":\"Pass all of the nuclei suspension through a 30 um filter.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":100761,"case_id":0,"critical_ids":"","duration":0,"original_id":1908625,"number":"9","cases":[],"critical":null},{"id":2076503,"guid":"9ECF01601D1411EFB24D0A58A9FEAC02","previous_id":2076502,"previous_guid":"9ECEDED51D1411EFB24D0A58A9FEAC02","section":"\u003cp\u003eHomogenization\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"cqhc0\",\"text\":\"Centrifuge at 900 g/rcf for 10 minutes at 4°C. Go to step 11 if you are running controls at FACS, if not skip to step 12.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":100761,"case_id":0,"critical_ids":"","duration":0,"original_id":1908626,"number":"10","cases":[],"critical":null},{"id":2076511,"guid":"9ED01C0D1D1411EFB24D0A58A9FEAC02","previous_id":2076539,"previous_guid":"C3D80AF64F234FC4BF12525AABB34B1D","section":"\u003cp\u003eResuspension \u0026amp; DAPI\u003c/p\u003e","section_color":"#EA9F6C","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"8vj3k\",\"text\":\"After incubation, spin down all of the tubes at 500 g for 5 minutes at 4°C. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":100761,"case_id":0,"critical_ids":"","duration":0,"original_id":1908634,"number":"13","cases":[],"critical":null},{"id":2076512,"guid":"9ED03E331D1411EFB24D0A58A9FEAC02","previous_id":2076511,"previous_guid":"9ED01C0D1D1411EFB24D0A58A9FEAC02","section":"\u003cp\u003eResuspension \u0026amp; DAPI\u003c/p\u003e","section_color":"#EA9F6C","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"8l28u\",\"text\":\"Decant and resuspend in 1 mL blocking buffer. You can add another filtration step here; I use the FlowMis.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":100761,"case_id":0,"critical_ids":"","duration":0,"original_id":1908635,"number":"14","cases":[],"critical":null},{"id":2076513,"guid":"9ED060241D1411EFB24D0A58A9FEAC02","previous_id":2076515,"previous_guid":"9ED0A4481D1411EFB24D0A58A9FEAC02","section":"\u003cp\u003eResuspension \u0026amp; DAPI\u003c/p\u003e","section_color":"#EA9F6C","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"c7tl9\",\"text\":\"Store tubes on ice until FACS. Bring DAPI stock and Blocking Buffer with you to FACS in case we have to dilute the sample to lower the event count.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":100761,"case_id":0,"critical_ids":"","duration":0,"original_id":1908636,"number":"17","cases":[],"critical":null},{"id":2076514,"guid":"9ED0823F1D1411EFB24D0A58A9FEAC02","previous_id":2076512,"previous_guid":"9ED03E331D1411EFB24D0A58A9FEAC02","section":"\u003cp\u003eResuspension \u0026amp; DAPI\u003c/p\u003e","section_color":"#EA9F6C","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"6bt5r\",\"text\":\"Add 1 uL of 0.1 mg/mL DAPI, a final concentration of 0.1 ug/mL, to each sample and control tube and invert to mix. (Adding more DAPI than necessary is okay.)\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":100761,"case_id":0,"critical_ids":"","duration":0,"original_id":1908637,"number":"15","cases":[],"critical":null},{"id":2076515,"guid":"9ED0A4481D1411EFB24D0A58A9FEAC02","previous_id":2076514,"previous_guid":"9ED0823F1D1411EFB24D0A58A9FEAC02","section":"\u003cp\u003eResuspension \u0026amp; DAPI\u003c/p\u003e","section_color":"#EA9F6C","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"67o57\",\"text\":\"Add 300 uL Blocking Buffer to three FACS tubes and label them A488+, PE+, and Double Negative. We will sort into these.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":100761,"case_id":0,"critical_ids":"","duration":0,"original_id":1908638,"number":"16","cases":[],"critical":null},{"id":2076516,"guid":"9ED0CC1D1D1411EFB24D0A58A9FEAC02","previous_id":2076498,"previous_guid":"9ECE4F061D1411EFB24D0A58A9FEAC02","section":"\u003cp\u003ePrepare Stock Solutions\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"51rf8\",\"text\":\"Make 1 mL of 0.1 mg/mL DAPI by adding 100 uL of 1 mg/mL DAPI stock to 900 uL of milliQ water. (4C - 1 month)\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"bold\",\"offset\":13,\"length\":15}],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":100761,"case_id":0,"critical_ids":"","duration":0,"original_id":1908639,"number":"3","cases":[],"critical":null},{"id":2076534,"guid":"6932BB4AEE684CD68E05F676131FCC7D","previous_id":2076503,"previous_guid":"9ECF01601D1411EFB24D0A58A9FEAC02","section":"\u003cp\u003eBlocking Buffer \u0026amp; Antibodies\u003c/p\u003e","section_color":"#FFED92","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"cu5m7\",\"text\":\"If you are running controls at FACS:\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"775bl\",\"text\":\"\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"1301s\",\"text\":\"The amount of Blocking Buffer we will need to resuspend our nuclei pellet with after centrifuging depends on the number of controls we will run. Each control tube will require 50 μL of nuclei suspension, and our sample needs to have 1,000 μL of nuclei suspension left over. In this case we are running two controls.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"8aem7\",\"text\":\"\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"cvegd\",\"text\":\"After the homogenate is done centrifuging, discard the supernatant and resuspend the pellet in 1,100 μL of Blocking Buffer. Incubate for 10 minutes on ice. Add 950 μL Blocking Buffer to each of your control tubes. Add 50 μL of nuclei suspension to each control tube. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"feb86\",\"text\":\"\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"ernb7\",\"text\":\"Add the corresponding antibodies to the control tubes:\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"andgu\",\"text\":\"2 μL NeuN-PE (1/500) to the PE-only control\",\"type\":\"unordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"4etqs\",\"text\":\"0.4 μL Olig2-Alexa 488 (1/2500) to the A488-only control\",\"type\":\"unordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"cib6m\",\"text\":\"\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"8tk61\",\"text\":\"Place sample and controls in a rotator placed in the dark at 4°C and incubate for 30 minutes.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":100761,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"11","cases":[],"critical":null},{"id":2076539,"guid":"C3D80AF64F234FC4BF12525AABB34B1D","previous_id":2076534,"previous_guid":"6932BB4AEE684CD68E05F676131FCC7D","section":"\u003cp\u003eBlocking Buffer \u0026amp; Antibodies\u003c/p\u003e","section_color":"#FFED92","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"dp858\",\"text\":\"If you are not running controls at FACS:\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"4mt8m\",\"text\":\"\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"fu501\",\"text\":\"After the homogenate is done centrifuging, discard the supernatant and resuspend the pellet in 1 mL of Blocking Buffer. Transfer the nuclei to a 1 mL tube and incubate for 10 minutes on ice.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"964vb\",\"text\":\"\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"95cns\",\"text\":\"Add both 2 μL NeuN-PE and 0.4 μL Olig2-Alexa 488 to the sample tube. Place sample in a rotator placed in the dark at 4°C and incubate for 30 minutes.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":100761,"case_id":0,"critical_ids":"12","duration":0,"original_id":0,"number":"12","cases":[],"critical":[{"id":12,"title":"Critical step","icon":"CriticalIcon"}]}],"template_id":4,"title":"Nuclei Isolation for HMBA FACS","title_html":"Nuclei Isolation for HMBA FACS","type_id":1,"units":[{"id":1,"type_id":3,"name":"µL","can_manage":0,"read_only":0},{"id":2,"type_id":3,"name":"mL","can_manage":0,"read_only":0},{"id":3,"type_id":3,"name":"L","can_manage":0,"read_only":0},{"id":4,"type_id":3,"name":"µg","can_manage":0,"read_only":0},{"id":5,"type_id":3,"name":"mg","can_manage":0,"read_only":0},{"id":6,"type_id":3,"name":"g","can_manage":0,"read_only":0},{"id":7,"type_id":3,"name":"kg","can_manage":0,"read_only":0},{"id":8,"type_id":3,"name":"ng","can_manage":0,"read_only":0},{"id":9,"type_id":3,"name":"Hz","can_manage":0,"read_only":0},{"id":10,"type_id":24,"name":"°C","can_manage":0,"read_only":0},{"id":11,"type_id":24,"name":"°К","can_manage":0,"read_only":0},{"id":12,"type_id":24,"name":"°F","can_manage":0,"read_only":0},{"id":13,"type_id":25,"name":"Mass Percent","can_manage":0,"read_only":0},{"id":14,"type_id":25,"name":"% volume","can_manage":0,"read_only":0},{"id":15,"type_id":25,"name":"Mass / % volume","can_ma