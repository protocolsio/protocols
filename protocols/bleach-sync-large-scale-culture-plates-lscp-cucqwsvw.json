:false,"step":"{\"blocks\":[{\"key\":\"evc81\",\"text\":\"Wash LSCP with 50mL M9, Centrifuge and aspirate the supernatant from the wash 15 mL at a time (it should take about 3 centrifuges since some of the buffer will soak into the agar or be irrecoverable).\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":82032,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"1","cases":[],"critical":null},{"id":1688275,"guid":"377506C0F4F311EDB689A9415B4A2928","previous_id":1688274,"previous_guid":"37741C60F4F311EDB689A9415B4A2928","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"cgock\",\"text\":\"Repeat the 50 mL washes 2 more times for a total of 3 washes. (Shaver et al., 2021)\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":82032,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"2","cases":[],"critical":null},{"id":1688276,"guid":"37757BF1F4F311EDB689A9415B4A2928","previous_id":1688275,"previous_guid":"377506C0F4F311EDB689A9415B4A2928","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"3iuci\",\"text\":\"Centrifuge the flip top tubes at 515 RCF for 1 minute. Aspirate out supernatant.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":82032,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"3","cases":[],"critical":null},{"id":1688277,"guid":"37761831F4F311EDB689A9415B4A2928","previous_id":1688276,"previous_guid":"37757BF1F4F311EDB689A9415B4A2928","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"6tpb5\",\"text\":\"Add 2.5 mL of bleach solution (7:1:2 ratio of ddH2O, 5M NaOH, and bleach) to the flip top tube and swirl around the solution. Flip the tubes to ensure the pellet completely dissociates from the bottom and mix vigorously. Vortex if needed.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":82032,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4","cases":[],"critical":null},{"id":1688278,"guid":"3776B471F4F311EDB689A9415B4A2928","previous_id":1688277,"previous_guid":"37761831F4F311EDB689A9415B4A2928","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"2t7dl\",\"text\":\"Perform bleaching for only up to 3 tubes at a time to ensure quality.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":82032,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"5","cases":[],"critical":null},{"id":1688279,"guid":"377777C1F4F311EDB689A9415B4A2928","previous_id":1688278,"previous_guid":"3776B471F4F311EDB689A9415B4A2928","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"frjaj\",\"text\":\"Check the effect of bleaching every 30 seconds by aliquoting a 1 uL sample onto a glass slide to see how the worms are responding.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":82032,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"6","cases":[],"critical":null},{"id":1688280,"guid":"37783B11F4F311EDB689A9415B4A2928","previous_id":1688279,"previous_guid":"377777C1F4F311EDB689A9415B4A2928","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"cf5n1\",\"text\":\"Once most of the adult worms are splitting apart and releasing their eggs, stop the bleach by diluting the tube all the way to 15 mL with M9.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":82032,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"7","cases":[],"critical":null},{"id":1688281,"guid":"3778FE61F4F311EDB689A9415B4A2928","previous_id":1688280,"previous_guid":"37783B11F4F311EDB689A9415B4A2928","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"2sqft\",\"text\":\"Centrifuge immediately at 525 RCF for 1 minute. Aspirate out the bleach solution quickly.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":82032,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"8","cases":[],"critical":null},{"id":1688282,"guid":"3779C1B1F4F311EDB689A9415B4A2928","previous_id":1688281,"previous_guid":"3778FE61F4F311EDB689A9415B4A2928","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"alsng\",\"text\":\"Wash egg pellet with 10 mL of M9 once, centrifuging at 309 RCF for 3 minutes. Aspirate out the buffer after each centrifuge. Shake the tubes vigorously after adding the M9 to ensure the pellet dissociates.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":82032,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"9","cases":[],"critical":null},{"id":1688283,"guid":"377AAC11F4F311EDB689A9415B4A2928","previous_id":1688282,"previous_guid":"3779C1B1F4F311EDB689A9415B4A2928","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"euv4b\",\"text\":\"Add a final volume of 3 mL M9 to the tube and shake just enough to free the pellet from the bottom but without letting the eggs collect on the upper sides of the tube.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":82032,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"10","cases":[],"critical":null},{"id":1688284,"guid":"377BBD80F4F311EDB689A9415B4A2928","previous_id":1688283,"previous_guid":"377AAC11F4F311EDB689A9415B4A2928","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"d636s\",\"text\":\"Transfer to a 50 mL autoclaved flask covered in aluminum foil to minimize cross contamination, and put in a shaker at 1 RCF and 20 C.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":82032,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"11","cases":[],"critical":null},{"id":1688285,"guid":"377CA7E1F4F311EDB689A9415B4A2928","previous_id":1688284,"previous_guid":"377BBD80F4F311EDB689A9415B4A2928","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"5v1mn\",\"text\":\"Allow to grow to L1 arrest overnight.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":82032,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"12","cases":[],"critical":null}],"template_id":4,"title":"Bleach/Sync Large Scale Culture Plates (LSCP)","title_html":"Bleach/Sync Large Scale Culture Plates (LSCP)","type_id":1,"units":[{"id":1,"type_id":3,"name":"µL","can_manage":0,"read_only":0},{"id":2,"type_id":3,"name":"mL","can_manage":0,"read_only":0},{"id":3,"type_id":3,"name":"L","can_manage":0,"read_only":0},{"id":4,"type_id":3,"name":"µg","can_manage":0,"read_only":0},{"id":5,"type_id":3,"name":"mg","can_manage":0,"read_only":0},{"id":6,"type_id":3,"name":"g","can_manage":0,"read_only":0},{"id":7,"type_id":3,"name":"kg","can_manage":0,"read_only":0},{"id":8,"type_id":3,"name":"ng","can_manage"