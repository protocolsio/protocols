tatus":{"id":1,"info":"We use this protocol and it's working"},"steps":[{"id":1716344,"guid":"934E67F0103E11EEA8D9EF295AC049A2","previous_id":0,"previous_guid":null,"section":"\u003cp\u003eDay 1 - Sample collection\u003c/p\u003e","section_color":"#A492FF","section_duration":900,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"aurtr\",\"text\":\"Take 6mm punch biopsy of skin and place in a vial of Hypothermosol that has been pre-chilled at 4oC. Record time of biopsy.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":83796,"case_id":0,"critical_ids":"","duration":300,"original_id":0,"number":"1","cases":[],"critical":null},{"id":1716345,"guid":"934F7960103E11EEA8D9EF295AC049A2","previous_id":1716344,"previous_guid":"934E67F0103E11EEA8D9EF295AC049A2","section":"\u003cp\u003eDay 1 - Sample collection\u003c/p\u003e","section_color":"#A492FF","section_duration":900,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"1ale8\",\"text\":\"Proceed to embed and freeze the tissue as soon as possible, no longer than 24 hours later, ensuring that the sample is kept in Hypothermosol at 4oC in the mean time. Record time of freezing.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":83796,"case_id":0,"critical_ids":"","duration":600,"original_id":0,"number":"2","cases":[],"critical":null},{"id":1716346,"guid":"934FEE90103E11EEA8D9EF295AC049A2","previous_id":1716345,"previous_guid":"934F7960103E11EEA8D9EF295AC049A2","section":"\u003cp\u003eDay 1 - Embedding\u003c/p\u003e","section_color":"#94EBFF","section_duration":1560,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"e3beh\",\"text\":\"Prepare an isopentane-dry ice slurry. Place a small metal box onto dry ice, and fill with isopentane to a depth of several centimetres. Slowly add dry ice to the isopentane. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":83796,"case_id":0,"critical_ids":"","duration":120,"original_id":0,"number":"3","cases":[],"critical":null},{"id":1716347,"guid":"93508AD1103E11EEA8D9EF295AC049A2","previous_id":1716346,"previous_guid":"934FEE90103E11EEA8D9EF295AC049A2","section":"\u003cp\u003eDay 1 - Embedding\u003c/p\u003e","section_color":"#94EBFF","section_duration":1560,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"ailsf\",\"text\":\"Label cryomoulds with both sample and orientation information; it is recommended that at least the cutting face is noted on the cryomould. Half-fill cryomoulds with OCT then leave for at least several minutes; this will minimise bubbles.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":83796,"case_id":0,"critical_ids":"","duration":120,"original_id":0,"number":"4","cases":[],"critical":null},{"id":1716348,"guid":"93512710103E11EEA8D9EF295AC049A2","previous_id":1716347,"previous_guid":"93508AD1103E11EEA8D9EF295AC049A2","section":"\u003cp\u003eDay 1 - Embedding\u003c/p\u003e","section_color":"#94EBFF","section_duration":1560,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"dh8n9\",\"text\":\"Process tissues one at a time.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":83796,"case_id":0,"critical_ids":"","duration":600,"original_id":0,"number":"5","cases":[],"critical":null},{"id":1716349,"guid":"9351C351103E11EEA8D9EF295AC049A2","previous_id":1716348,"previous_guid":"93512710103E11EEA8D9EF295AC049A2","section":"\u003cp\u003eDay 1 - Embedding\u003c/p\u003e","section_color":"#94EBFF","section_duration":1560,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"a2ni3\",\"text\":\"Prior to embedding and freezing, it is recommended that tissues are examined using a dissection microscope. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":83796,"case_id":0,"critical_ids":"","duration":600,"original_id":0,"number":"6","cases":[],"critical":null},{"id":1716350,"guid":"93525F91103E11EEA8D9EF295AC049A2","previous_id":1716349,"previous_guid":"9351C351103E11EEA8D9EF295AC049A2","section":"\u003cp\u003eDay 1 - Embedding\u003c/p\u003e","section_color":"#94EBFF","section_duration":1560,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"faq9g\",\"text\":\"Following all necessary examination and dissection, use blotting filter paper to quickly and gently remove any excess Hypothermosol.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":83796,"case_id":0,"critical_ids":"","duration":120,"original_id":0,"number":"7","cases":[],"critical":null},{"id":1716351,"guid":"9352FBD1103E11EEA8D9EF295AC049A2","previous_id":1716350,"previous_guid":"93525F91103E11EEA8D9EF295AC049A2","section":"\u003cp\u003eDay 1 - Freezing\u003c/p\u003e","section_color":"#84CE84","section_duration":1740,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"c8q3h\",\"text\":\"Using spatula, transfer the tissue into a cryomould pre-filled with OCT. For skin tissue, place the sample with the epidermis facing down, and make a note of the surface needed to cut to ensure that all skin layers are sectioned\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":83796,"case_id":0,"critical_ids":"","duration":300,"original_id":0,"number":"8","cases":[],"critical":null},{"id":1716352,"guid":"9353BF21103E11EEA8D9EF295AC049A2","previous_id":1716351,"previous_guid":"9352FBD1103E11EEA8D9EF295AC049A2","section":"\u003cp\u003eDay 1 - Freezing\u003c/p\u003e","section_color":"#84CE84","section_duration":1740,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"chcrh\",\"text\":\"Carefully manipulate the orientation of the material to make sure the skin surface is at the bottom of the mould, but not touching the bottom if possible. Fill the rest of the mould with OCT so the sample is covered.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":83796,"case_id":0,"critical_ids":"","duration":120,"original_id":0,"number":"9","cases":[],"critical":null},{"id":1716353,"guid":"93545B61103E11EEA8D9EF295AC049A2","previous_id":1716352,"previous_guid":"9353BF21103E11EEA8D9EF295AC049A2","section":"\u003cp\u003eDay 1 - Freezing\u003c/p\u003e","section_color":"#84CE84","section_duration":1740,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"dl60g\",\"text\":\"Use a pair of forceps to place the cryomould into the isopentane-dry ice slurry, keeping it level during movement to prevent loss of OCT and sample movement.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":83796,"case_id":0,"critical_ids":"","duration":300,"original_id":0,"number":"10","cases":[],"critical":null},{"id":1716354,"guid":"93551EB1103E11EEA8D9EF295AC049A2","previous_id":1716353,"previous_guid":"93545B61103E11EEA8D9EF295AC049A2","section":"\u003cp\u003eDay 1 - Freezing\u003c/p\u003e","section_color":"#84CE84","section_duration":1740,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"c1d9h\",\"text\":\"Rest cryomould immersed in the top of the isopentane liquid until the OCT has started to freeze\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":83796,"case_id":0,"critical_ids":"","duration":300,"original_id":0,"number":"11","cases":[],"critical":null},{"id":1716355,"guid":"9355E200103E11EEA8D9EF295AC049A2","previous_id":1716354,"previous_guid":"93551EB1103E11EEA8D9EF295AC049A2","section":"\u003cp\u003eDay 1 - Freezing\u003c/p\u003e","section_color":"#84CE84","section_duration":1740,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"5c2ds\",\"text\":\"Once completely frozen, lift the cryomould out of the isopentane-dry ice slurry, and onto fresh dry ice, to allow excess isopentane to drain and evaporate. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":83796,"case_id":0,"critical_ids":"","duration":300,"original_id":0,"number":"12","cases":[],"critical":null},{"id":1716356,"guid":"93567E41103E11EEA8D9EF295AC049A2","previous_id":1716355,"previous_guid":"9355E200103E11EEA8D9EF295AC049A2","section":"\u003cp\u003eDay 1 - Freezing\u003c/p\u003e","section_color":"#84CE84","section_duration":1740,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"ff6q9\",\"text\":\"Wrap each sample, frozen in individual cryomoulds, in aluminium foil. Carefully label the outside of the foil prior to wrapping around each sample.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":83796,"case_id":0,"critical_ids":"","duration":300,"original_id":0,"number":"13","cases":[],"critical":null},{"id":1716357,"guid":"93574191103E11EEA8D9EF295AC049A2","previous_id":1716356,"previous_guid":"93567E41103E11EEA8D9EF295AC049A2","section":"\u003cp\u003eDay 1 - Freezing\u003c/p\u003e","section_color":"#84CE84","section_duration":1740,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"cs5ti\",\"text\":\"Transfer samples to a pre-cooled cryobox and store at -80Â°C.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":83796,"case_id":0,"critical_ids":"","duration":120,"original_id":0,"number":"14","cases":[],"critical":null}],"template_id":4,"title":"Sample collection, embedding and freezing tissue","title_html":"Sample collection, embedding and freez