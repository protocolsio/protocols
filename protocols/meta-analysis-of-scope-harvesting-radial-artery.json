ml":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":60},{"id":966012,"guid":"CED1E660B44F11EAB51C73177ACA74C3","previous_id":966011,"previous_guid":"6C359B50B44F11EAB51C73177ACA74C3","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"15FD514662B54ADAA4AA753E6A6D8879","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"784BD0F93221483DB5B3F6EF3AD450B3","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Articles with comparisons of open and endoscope artery harvesting were included as long as there were adult patients with coronary artery disease undergoing CABG. <\/div><div class = \"text-block\">Systemic-reviews, case series and case reports were excluded. Studies comparing open arery hatvesting and minimal invasive radial artery harvesting were also excluded. Randomized controlled trials (RCTs) and non-randomized observational articles including prospective or retrospective studies were included.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":180},{"id":966013,"guid":"8418E9B0B45011EAB51C73177ACA74C3","previous_id":966012,"previous_guid":"CED1E660B44F11EAB51C73177ACA74C3","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"65A95B5C87A6467499FC4AE304405616","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"6F08473E2BBD45A6B626BE5F72B59C7F","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Primary outcomes included the wound infection rate, the wound complication rate, harvesting site neurological complications of the forearm during hospitalization, in-hospital mortality, long-term survival (over one year), and the patency rate (duration may be defined differently by research, from in-hospital to 3\u20135 years).<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":60},{"id":966014,"guid":"B8AF36F0B45211EAB51C73177ACA74C3","previous_id":966013,"previous_guid":"8418E9B0B45011EAB51C73177ACA74C3","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"F9C23724FD4B424FB7660C7B6F4881DC","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"63A9589143654644B44126539CB7D2FC","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">The risk of bias of randomized controlled trials was evaluated by the Cochrane Collaboration risk-of-bias tool\u2014Revised Cochrane risk-of-bias tool for randomized trials (RoB 2).<\/div><div class = \"text-block\">The methodological quality of non-randomized controlled trials (NRCTs) was assessed by the Newcastle\u2013Ottawa Quality Assessment Scale (NOS) .<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":60},{"id":966015,"guid":"0A1EB010B45311EAB51C73177ACA74C3","previous_id":966014,"previous_guid":"B8AF36F0B45211EAB51C73177ACA74C3","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"6A31D681A04241EBAC0FB337040979D9","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"EFE7BE54C3824FEABF20A4074C4FA2D9","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Data synthesis and statistical analysis were conducted using Review Manager<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":60},{"id":966016,"guid":"28BFDFD0B45311EAB51C73177ACA74C3","pre