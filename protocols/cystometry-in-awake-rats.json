s":[],"total_members":0,"total_followers":0,"total_child_groups":0,"total_parent_groups":0,"has_collaborations":0},"user_status":{"is_member":true,"is_confirmed":true,"is_invited":false,"is_owner":false,"is_admin":false,"is_following":false},"join_link":null,"token":null,"owner":{"name":" ","affiliation":null,"affiliations":[],"username":null,"link":null,"image":{"source":null,"placeholder":null},"badges":[],"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false},"is_protocol_requested":0,"is_group_requested":0,"is_my":false,"is_request":false,"is_confirmed":1,"is_declined":0,"requester":{"name":" ","affiliation":null,"affiliation_url":null,"username":null,"link":null},"protocol":{"id":0,"title":"Cystometry in awake rats","title_html":"Cystometry in awake rats","image":{"source":null,"placeholder":null},"doi":null,"doi_status":0,"uri":"cystometry-in-awake-rats-bakjicun","type_id":1,"published_on":null,"stats":{"number_of_views":0,"number_of_steps":0,"number_of_bookmarks":0,"number_of_comments":0,"number_of_exports":0,"number_of_runs":0,"number_of_votes":0,"is_voted":0},"parent_protocols":[],"parent_collections":[],"cited_protocols":[]},"created_on":1587617078,"resolve_on":0,"resolved_user":{"name":" ","affiliation":null,"affiliations":[],"username":null,"link":null,"image":{"source":null,"placeholder":null},"badges":[],"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false},"shared":false}}],"is_owner":1,"has_subprotocols":0,"is_subprotocol":0,"is_bookmarked":0,"can_be_copied":1,"can_remove_fork":1,"fork_id":null,"url":"https:\/\/www.protocols.io\/view\/cystometry-in-awake-rats-bakjicun","forks_count":{"private":0,"public":0},"access":{"can_view":1,"can_remove":0,"can_add":1,"can_edit":0,"can_publish":0,"can_get_doi":0,"can_share":1,"can_move":1,"can_move_outside":1,"can_transfer":1,"can_download":1,"is_locked":0},"guid":"D517ED08B0744FB5B6A2C1C37A11F379","state_version_id":794,"steps":[{"id":861981,"guid":"6F6DE7DFD6254B5FA8DB2D1C0508E7E2","previous_id":null,"previous_guid":null,"modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"5C05B659CC194D6EA1ED5BD09FCF773E","order_id":1,"type_id":6,"title":"Section","source":{"title":"Preparation for surgery"}},{"id":1054724,"guid":"F9CACEF123584264B0D278DB061BF4F1","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Prepare bladder cannula at least 24 - 48 h prior to surgery. This is comprised of a short length of PE tubing that connects directly with the bladder lumen; this tubing  inserts into a long piece of PVC tubing that is exteriorised at the base of the neck. <\/div><div class = \"text-block\"><ul style = \"list-style-type:disc;\"><li style = \"counter-reset:ol0;list-style-type:disc;\">By carefully holding PE tubing over an open flame, flare the end to form a bell shape. Ensure that the lumen of the tubing does not seal during this process. <\/li><li style = \"counter-reset:ol0;list-style-type:disc;\">Cut the flared end to a length of 0.5-1 cm. <\/li><li style = \"counter-reset:ol0;list-style-type:disc;\">Insert the non-flared end into the end of the PVC tubing as far east it will go (usually 2-3 mm)<\/li><li style = \"counter-reset:ol0;list-style-type:disc;\">Secure by applying superglue to the joint.\u00a0<\/li><li style = \"counter-reset:ol0;list-style-type:disc;\">To make the collar (securing point used for attaching to the neck skin): cut 0.5-1 cm of silastic tubing and glue it to the exteriorised end of the cannula tubing (leaving 5 cm overhang). <\/li><\/ul><\/div><\/div>"}},{"id":1054725,"guid":"7ADCD5EBCBA040D1866F04CF01D3C043","order_id":2,"type_id":26,"title":"notes","source":{"id":0,"parent_id":0,"uri":"","title":"","body":"<div class = \"text-blocks\"><div class = \"text-block\">The length of PVC tubing required is ~25 cm (male rat) and ~20 cm (female rat). This length includes room for the animal to grow and to provide sufficient amount exposed at the neck for daily flushing and experimental cystometry. The length of PE tubing required is ~ 2 cm.<\/div><\/div>","created_on":0,"changed_on":0,"creator":{"name":" ","affiliation":"","affiliations":[],"username":"","link":null,"image":{"source":"","placeholder":""},"badges":[],"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false},"comments":[]}}],"cases":[],"data":null,"section":null,"section_color":"#f8e71c","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":861982,"guid":"33BF849D3B594BB087670DEBDBA7F4BA","previous_id":861981,"previous_guid":"6F6DE7DFD6254B5FA8DB2D1C0508E7E2","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"ED2FB0DC60574CCEAA6D8F5E8EFB1620","order_id":1,"type_id":6,"title":"Section","source":{"title":"Preparation for surgery"}},{"id":1054724,"guid":"2FB5E4D216CF4419AFE63528F51553C8","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Anesthetise animal (2.5% isoflurane in oxygen, or as required for maintenance)<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#f8e71c","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":861983,"guid":"BDAAC111ACD3423FA469558D8BA8ED6C","previous_id":861984,"previous_guid":"9B7C4374237F4DA1B5918AEC9388CB69","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"D468F46B61734C8F98624E4EF426964A","order_id":1,"type_id":6,"title":"Section","source":{"title":"Preparation for surgery"}},{"id":1054724,"guid":"0CD02612D9734429AE9C30BAAA5AFB95","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Apply eye lubricant and place animal on heated pad.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#f8e71c","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":861984,"guid":"9B7C4374237F4DA1B5918AEC9388CB69","previous_id":861982,"previous_guid":"33BF849D3B594BB087670DEBDBA7F4BA","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"E7782CECE7804875A8476ECFEE6D2BDD","order_id":1,"type_id":6,"title":"Section","source":{"title":"Preparation for surgery"}},{"id":1054724,"guid":"9FED38D07B2849B497BCF70BA4A37D12","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Shave and clean the ventral abdomen and the dorsal neck.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#f8e71c","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":861985,"guid":"0FE8B480AE57475886A69DFC783C504E","previous_id":861983,"previous_guid":"BDAAC111ACD3423FA469558D8BA8ED6C","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"B96E1A5B9395488F9CE617C72073A7DA","order_id":1,"type_id":6,"title":"Section","source":{"title":"Surgery"}},{"id":1054724,"guid":"C1E3FC14BEAB4DAF99CCCE7131D1CE3F","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Perform a ventral midline incision in the skin overlying the region of the bladder. Place gauze dampened with sterile 0.9% saline over the abdominal incision site and turn the animal over. <\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#7ed321","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":861987,"guid":"A9F5A78EFB0B45499075FB2AB2E01CE3","previous_id":861985,"previous_guid":"0FE8B480AE57475886A69DFC783C504E","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"94A3B0FD07994004BE81F544345F9F82","order_id":1,"type_id":6,"title":"Section","source":{"title":"Surgery"}},{"id":1054724,"guid":"721509CD44B84B9F8172C955CD8F9A72","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Perform a dorsal midline incision in the skin at the base of the neck. This will be the exit site for the bladder cannula.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#7ed321","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":861988,"guid":"AE17CAF1C1394656B6247D7C3C3BBD84","previous_id":861987,"previous_guid":"A9F5A78EFB0B45499075FB2AB2E01CE3","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"8786C66E52B245E4BEBE2C3900EFFDFB","order_id":1,"type_id":6,"title":"Section","source":{"title":"Surgery"}},{"id":1054724,"guid":"D0D69104FCE047A88A76EE38AF3A9A1D","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span>Blunt-dissect under the skin from this neck incision site to the ventral abdominal incision side using hemostats. Gently pull the bladder cannula tubing through.<\/span><span style = \"font-weight:bold;\"> <\/span><\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#7ed321","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":861990,"guid":"F8749C69565D4A1BB95A84E55E424861","previous_id":861988,"previous_guid":"AE17CAF1C1394656B6247D7C3C3BBD84","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"EE99EC91E73149749CA058078B9B49CF","order_id":1,"type_id":6,"title":"Section","source":{"title":"Surgery"}},{"id":1054724,"guid":"C50B4449EE624895878DC0C4E1B73CDC","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Perform a ventral midline incision in the muscle. Make a smaller secondary incision in the abdominal muscle on the same side as the cannula tubing being tunnelled. Feed the cannula through the hole to enter the abdominal region.\u00a0<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#7ed321","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":861992,"guid":"2656441A02834E858E298EE6AFB3DC65","previous_id":861990,"previous_guid":"F8749C69565D4A1BB95A84E55E424861","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"DDD37FACE7794F06A2096B7FA201EC14","order_id":1,"type_id":6,"title":"Section","source":{"title":"Surgery"}},{"id":1054724,"guid":"E8D858A563874E6AADF12F2AACA437CE","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Using cotton-tipped applicators dampened with sterile 0.9% saline, gently expose the bladder. Using 4\u20190 PDS suture, place a very loose purse string knot around the tip of the bladder dome. This should be located between a quarter and a third of the way down the apex of the dome.\u00a0DO NOT TIGHTEN.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#7ed321","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":861994,"guid":"45E9EDB7D9CA4440BE8E21A7AFDD5BF5","previous_id":861992,"previous_guid":"2656441A02834E858E298EE6AFB3DC65","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"67337C8FCBB74B8BBB2C56D9BEDE7172","order_id":1,"type_id":6,"title":"Section","source":{"title":"Surgery"}},{"id":1054724,"guid":"423C42F7D9CB47CA95E24280553B5987","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Pierce the tip of the bladder dome with a 19G needle, carefully absorbing any leaked urine with gauze. Use fine forceps to widen the hole into the bladder.\u00a0<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#7ed321","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":861996,"guid":"DE205D9BA38A42C4B4F20776B036D04A","previous_id":861994,"previous_guid":"45E9EDB7D9CA4440BE8E21A7AFDD5BF5","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"FC16219AF0F74815BCD7B272450C5068","order_id":1,"type_id":6,"title":"Section","source":{"title":"Surgery"}},{"id":1054724,"guid":"2EB7E3F7C1E141C68AB78E347F6C135B","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Fill the cannula with 200-300 \u00b5l sterile saline and insert flared end of the cannula into the bladder, ensuring that the end terminates below the location of the purse string suture. Close the purse string suture and tie off.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#7ed321","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":861998,"guid":"F4330D3EDB6A4A798F6717C2DFF2C720","previous_id":861996,"previous_guid":"DE205D9BA38A42C4B4F20776B036D04A","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"0CBF1FBD9DC64F77985A4F01657BCCF9","order_id":1,"type_id":6,"title":"Section","source":{"title":"Surgery"}},{"id":1054724,"guid":"4CE3251CA8F94A748ECD2DB9BC91E2B5","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Once secured, infuse 0.2 ml 0.9% sterile saline into the bladder to check for leaks. If there are leaks, re-tie slightly lower than the original suture.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#7ed321","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":861999,"guid":"85B5B79E6C1D43F192A802C34F96D888","previous_id":861998,"previous_guid":"F4330D3EDB6A4A798F6717C2DFF2C720","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"DBD27446B1464DF5B08F0A9FDF48E100","order_id":1,"type_id":6,"title":"Section","source":{"title":"Surgery"}},{"id":1054724,"guid":"B9E70A8EC2554C25961CFB3A311450A2","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Close the muscle wall and abdominal skin using approved procedures.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#7ed321","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":862000,"guid":"0299F03F2B7C45F5B3A2EC07BCD0A981","previous_id":861999,"previous_guid":"85B5B79E6C1D43F192A802C34F96D888","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"EDF7C2857630484E92DBCBFFD563B848","order_id":1,"type_id":6,"title":"Section","source":{"title":"Surgery"}},{"id":1054724,"guid":"E7A5FA88C26C43E181105B6A4939FDF2","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">At the dorsal neck, orientate the collar and cannula tubing so they face caudally. Place a suture through the neck skin, then collar, and then opposite side of the neck skin, then tighten firmly. Repeat this step two more times to secure the collar to the skin.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#7ed321","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":862002,"guid":"844E0618433D465C854D47AC82C0ED66","previous_id":862546,"previous_guid":"CAC2CB60215211EAAD30D7EDBA2612DA","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"C5EE11D9FD0B42848857853F597E4BB2","order_id":1,"type_id":6,"title":"Section","source":{"title":"Surgery"}},{"id":1054724,"guid":"433FA2A833D642E8B9E3F6C0D5675142","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Administer analgesics and monitor animal during postoperative period as per local approved procedures.  To maintain patency of the cannula, infuse with gentamicin (0.2 ml, 40 mg\/ml in sterile 0.9% saline) for 3 d and then daily with sterile 0.9% saline (0.5 ml).<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#7ed321","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":862003,"guid":"A29C0B4E68404A9C9A490219FA17304F","previous_id":862002,"previous_guid":"844E0618433D465C854D47AC82C0ED66","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"1370CD5FC21340F088586C67A6E9835F","order_id":1,"type_id":6,"title":"Section","source":{"title":"Habituation "}},{"id":1054724,"guid":"5DFAD2CC8F4442C1AA7299E60AA38734","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">After post-surgical recovery, habituate the animal to the recording chamber (30 min per day) at least 3 days before cystometry recording<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#50e3c2","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":862004,"guid":"38A0CF55584744BCB5B53AB16C139C5B","previous_id":862003,"previous_guid":"A29C0B4E68404A9C9A490219FA17304F","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"B1B3D572E4F346A88E1F0D7B8CD92229","order_id":1,"type_id":6,"title":"Section","source":{"title":"Cystometry recording"}},{"id":1054724,"guid":"22460DDEDDAC4919A88B78F4FB4A5383","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Place the animal into the experiment chamber. Fill up a 25 ml syringe with sterile 0.9% saline. Fill the transducer and connection to the bladder cannula with sterile 0.9% saline.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#92bdf0","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":862006,"guid":"6F65045D86254D04A08D837B266C9D69","previous_id":862004,"previous_guid":"38A0CF55584744BCB5B53AB16C139C5B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"349ABF3AE6D64B9D9F346D560AFBA652","order_id":1,"type_id":6,"title":"Section","source":{"title":"Cystometry recording"}},{"id":1054724,"guid":"65F421AE389945C389F12B506A334E75","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Set the baseline pressure (Labchart) by holding a flowing tube of saline at the transducer level prior to inserting it into the cannula line of the rat. Allow this to run for a minute before connecting the line to the animal. <\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#92bdf0","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":862007,"guid":"D13963D763824639A8D7547FDC715DEA","previous_id":862006,"previous_guid":"6F65045D86254D04A08D837B266C9D69","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"7713D9F2D4B34FD683F6736FF0EA246B","order_id":1,"type_id":6,"title":"Section","source":{"title":"Cystometry recording"}},{"id":1054724,"guid":"1AD7FCCFF4794A6D8017954913D757A0","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Slowly infuse sterile 0.9% saline into the bladder at rate 0.1 ml\/min or the rate required for your experiment<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#92bdf0","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":862008,"guid":"D638961B9F1044249931AF5412B1CCF9","previous_id":862007,"previous_guid":"D13963D763824639A8D7547FDC715DEA","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"E7402BBA3044430AACF39AE9034E089C","order_id":1,"type_id":6,"title":"Section","source":{"title":"Cystometry recording"}},{"id":1054724,"guid":"74050403376C4CCABA4F2B77572D5B55","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">After inserting the line into the cannula, record bladder pressure for 30 min while the rat settles. Continue cystometry for approximately another 90 min or the duration required for the experiment.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#9