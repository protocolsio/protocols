_of_bookmarked_comments":0,"number_of_steps_comments":0,"number_of_protocol_comments":0,"number_of_exports":0,"number_of_runs":0,"number_of_votes":0,"number_of_reagents":0,"number_of_equipments":0,"number_of_collections":0,"number_of_forks":{"private":0,"public":0},"number_of_accessible_forks":0},"status":{"id":1,"info":"We use this protocol and it's working"},"steps":[{"id":1979337,"guid":"5ACD72715B2448D1823EED8BE7EA311A","previous_id":0,"previous_guid":null,"section":"\u003cp\u003eKidney tissue collection\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"of79\",\"text\":\"C57BL/6J male mice from the National Institute of Aging Rodent Resource were handled according to the guidelines of the Institutional Animal Care Committee at the University of Washington.  Mice aged 5 and 26 months were selected for the study. Kidney tissue extraction was conducted immediately following euthanasia.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":96734,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"1","cases":[],"critical":null},{"id":1979338,"guid":"DAA8F4D281B5402B871DF095F56B8387","previous_id":1979337,"previous_guid":"5ACD72715B2448D1823EED8BE7EA311A","section":"\u003cp\u003eKidney tissue collection\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"81df4\",\"text\":\"Make an incision then cut the skin from the bottom to the top of the mice followed the abdominal line with the round side of the scissors.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":96734,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"2","cases":[],"critical":null},{"id":1979340,"guid":"FEAA33B557924E4894AFD728BB18EBA2","previous_id":1979338,"previous_guid":"DAA8F4D281B5402B871DF095F56B8387","section":"\u003cp\u003eKidney tissue collection\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"7nlcm\",\"text\":\"Proceed to cut the lateral skin to get access to the kidney. Carefully extract the kidney, rinse it with HBSS (Hank's Balanced Salt Solution, Thermo, 14025092), remove the capsule, and section the kidney into pieces measuring 3 to 5 mm in thickness.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":96734,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"3","cases":[],"critical":null},{"id":1979341,"guid":"B14684E2068B44D29657B8EFDFEB9CA3","previous_id":1979340,"previous_guid":"FEAA33B557924E4894AFD728BB18EBA2","section":"\u003cp\u003eOCT Embedding \u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"82tr8\",\"text\":\" Apply a thin layer of OCT compound (Sakura Tissue-Tek O.C.T. Compound, Sigma, 4583) into a disposable mold (Epredia, 18985-1).\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":96734,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4","cases":[],"critical":null},{"id":1979343,"guid":"B4339956B629452FBD585C76C5D4387B","previous_id":1979341,"previous_guid":"B14684E2068B44D29657B8EFDFEB9CA3","section":"\u003cp\u003eOCT Embedding \u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"b4o7t\",\"text\":\"Position the sample tissue at the center of the mold, ensuring that the side intended for cutting faces inward.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":96734,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"5","cases":[],"critical":null},{"id":1979344,"guid":"7CA169C26D844632A61AE713E237F7DA","previous_id":1979343,"previous_guid":"B4339956B629452FBD585C76C5D4387B","section":"\u003cp\u003eOCT Embedding \u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"7fr39\",\"text\":\"Cover the tissue completely with additional OCT compound.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":96734,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"6","cases":[],"critical":null},{"id":1979345,"guid":"6FFE9D6A29AA4832B78FBDCD09FB3695","previous_id":1979344,"previous_guid":"7CA169C26D844632A61AE713E237F7DA","section":"\u003cp\u003eOCT Embedding \u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"895oj\",\"text\":\"Lower the mold into liquid nitrogen, being careful to not fully submerge the mold. Hold in liquid nitrogen for a few seconds until fully frozen.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":96734,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"7","cases":[],"critical":null},{"id":1979346,"guid":"B38417AEC48F460DB2823DB71C14B253","previous_id":1979345,"previous_guid":"6FFE9D6A29AA4832B78FBDCD09FB3695","section":"\u003cp\u003eOCT Embedding \u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"9f7dl\",\"text\":\"Transfer the frozen mold to dry ice for storage at -80ºC.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":96734,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"8","cases":[],"critical":null}],"template_id":0,"title":"Preparation for OCT embedding Mou