mber_of_forks":{"private":0,"public":0},"number_of_accessible_forks":0},"status":{"id":1,"info":"We use this protocol and it's working"},"steps":[{"id":1882873,"guid":"6347414F9D9711EE9FCB0A58A9FEAC02","previous_id":1882887,"previous_guid":"7EC365F0EB5740FB8C6624218876D054","section":"\u003cp\u003eTissue Collection and Storage \u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"9uv69\",\"text\":\"Dissect and extract the brain.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":92448,"case_id":0,"critical_ids":"","duration":0,"original_id":1473953,"number":"4","cases":[],"critical":null},{"id":1882874,"guid":"634779699D9711EE9FCB0A58A9FEAC02","previous_id":1882873,"previous_guid":"6347414F9D9711EE9FCB0A58A9FEAC02","section":"\u003cp\u003eTissue Collection and Storage \u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"3h57u\",\"text\":\"Store the brain in 4 % PFA at 4 °C overnight.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":92448,"case_id":0,"critical_ids":"","duration":0,"original_id":1473954,"number":"5","cases":[],"critical":null},{"id":1882876,"guid":"6347BE449D9711EE9FCB0A58A9FEAC02","previous_id":1882874,"previous_guid":"634779699D9711EE9FCB0A58A9FEAC02","section":"\u003cp\u003eDehydration\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"ekm26\",\"text\":\"Slice each brain into 3 mm coronal sections (approximately).\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":92448,"case_id":0,"critical_ids":"","duration":0,"original_id":1473956,"number":"6","cases":[],"critical":null},{"id":1882877,"guid":"6347DE419D9711EE9FCB0A58A9FEAC02","previous_id":1882876,"previous_guid":"6347BE449D9711EE9FCB0A58A9FEAC02","section":"\u003cp\u003eDehydration\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"89cga\",\"text\":\"Pass each section through graded alcohol solutions (50, 70, 80, 95, 100, 100 and 100 % ethanol), holding for 10 minutes in each.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":92448,"case_id":0,"critical_ids":"","duration":0,"original_id":1473958,"number":"7","cases":[],"critical":null},{"id":1882878,"guid":"6347FFDC9D9711EE9FCB0A58A9FEAC02","previous_id":1882877,"previous_guid":"6347DE419D9711EE9FCB0A58A9FEAC02","section":"\u003cp\u003eDehydration\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"bkr40\",\"text\":\"Transfer each section into xylene three times, for 20 minutes each.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":92448,"case_id":0,"critical_ids":"","duration":0,"original_id":1473959,"number":"8","cases":[],"critical":null},{"id":1882879,"guid":"634821D29D9711EE9FCB0A58A9FEAC02","previous_id":1882878,"previous_guid":"6347FFDC9D9711EE9FCB0A58A9FEAC02","section":"\u003cp\u003eDehydration\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"emsi7\",\"text\":\"Transfer each section into paraffin two times, for two hours each.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":92448,"case_id":0,"critical_ids":"","duration":0,"original_id":1503861,"number":"9","cases":[],"critical":null},{"id":1882880,"guid":"63484D2F9D9711EE9FCB0A58A9FEAC02","previous_id":null,"previous_guid":null,"section":"\u003cp\u003eBrain Preparation\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"b19ge\",\"text\":\"Euthanize the mouse by lethal intraperitoneal injection of pentobarbital.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":92448,"case_id":0,"critical_ids":"","duration":0,"original_id":1503862,"number":"1","cases":[],"critical":null},{"id":1882881,"guid":"634875A69D9711EE9FCB0A58A9FEAC02","previous_id":1882888,"previous_guid":"194FB0A36C564B188FB84F7064BC3CC5","section":"\u003cp\u003eSectioning\u003c/p\u003e","section_color":"#FFED92","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"ckc4k\",\"text\":\"Take coronal sections at 7 µm using a Leica microtome (Leica Microsystems, Wetzlar, Germany) and mounted on glass slides (VWR, Superfrost Plus)\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":92448,"case_id":0,"critical_ids":"","duration":0,"original_id":1503873,"number":"11","cases":[],"critical":null},{"id":1882886,"guid":"679786B8484A4490A2A7293E315936E5","previous_id":1882880,"previous_guid":"63484D2F9D9711EE9FCB0A58A9FEAC02","section":"\u003cp\u003eBrain Preparation\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"3danm\",\"text\":\"Upon loss of the pedal withdrawal reflex, dissect the thoracic cavity open.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":92448,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"2","cases":[],"critical":null},{"id":1882887,"guid":"7EC365F0EB5740FB8C6624218876D054","previous_id":1882886,"previous_guid":"679786B8484A4490A2A7293E315936E5","section":"\u003cp\u003eBrain Preparation\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"d8h1m\",\"text\":\"Perfuse 25 mL phosphate-buffered saline (PBS) transcardially, followed by 25 mL 4 % paraformaldehyde (PFA), diluted in PBS.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":92448,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"3","cases":[],"critical":null},{"id":1882888,"guid":"194FB0A36C564B188FB84F7064BC3CC5","previous_id":1882879,"previous_guid":"634821D29D9711EE9FCB0A58A9FEAC02","section":"\u003cp\u003eDehydration\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"d48g5\",\"text\":\"Embed each section in fresh paraffin, orienting as desired.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":92448,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"10","cases":[],"critical":null},{"id":1882889,"guid":"16DD795A211D4C7DB8355CF294025C1E","previous_id":1882881,"previous_guid":"634875A69D9711EE9FCB0A58A9FEAC02","section":"\u003cp\u003eDeparaffinisation, Antigen Retrieval and Blocking\u003c/p\u003e","section_color":"#EA9F6C","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"aljo0\",\"text\":\"Transfer each slide-mounted set of sections through xylene (twice) and reversed graded alcohols (100, 95, 70, 50 %) for 10 minutes each.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":92448,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"12","cases":[],"critical":null},{"id":1882890,"guid":"23590AC702924051B9A282C604C80512","previous_id":1882889,"previous_guid":"16DD795A211D4C7DB8355CF294025C1E","section":"\u003cp\u003eDeparaffinisation, Antigen Retrieval and Blocking\u003c/p\u003e","section_color":"#EA9F6C","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"4u4vu\",\"text\":\"Transfer to 1X citrate buffer (ab96678, Abcam) and perform antigen retrieval by microwave heat (20 minutes).\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":92448,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"13","cases":[],"critical":null},{"id":1882891,"guid":"D8109511B39D4A4C989CB57C2EEF399C","previous_id":1882890,"previous_guid":"23590AC702924051B9A282C604C80512","section":"\u003cp\u003eDeparaffinisation, Antigen Retrieval and Blocking\u003c/p\u003e","section_color":"#EA9F6C","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"e8pf0\",\"text\":\"Block for one hour at room temperature using 10 % donkey serum in tris-buffered saline (TBS).\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":92448,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"14","cases":[],"critical":null},{"id":1882892,"guid":"E0E3E23E851F4DACBE3926068C887832","previous_id":1882891,"previous_guid":"D8109511B39D4A4C989CB57C2EEF399C","section":"\u003cp\u003ePrimary Staining\u003c/p\u003e","section_color":"#E57785","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"7dsgm\",\"text\":\"Apply primary antibodies to TH (Abcam Cat# ab76442, RRID:AB_1524535, 1:500), GFP (Thermo Fisher Scientific Cat# A-11122, RRID:AB_221569, Invitrogen, 1:1000), CASR (Abcam Cat# ab79038, RRID:AB_2071489, 1:100) or TYROBP/DAP12 (Abcam Cat# ab283679, 1:100), prepared in 1 % donkey serum in TBS and incubate overnight at 4 °C\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":92448,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"15","cases":[],"critical":null},{"id":1882893,"guid":"CCDA480437CD4E54BBA076A5A1314CBF","previous_id":1882892,"previous_guid":"E0E3E23E851F4DACBE3926068C887832","section":"\u003cp\u003eSecondary Staining\u003c/p\u003e","section_color":"#EA94FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"foat4\",\"text\":\"Wash slides in TBS three times, for five minutes each.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":92448,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"16","cases":[],"critical":null},{"id":1882894,"guid":"11923CE9596B4544884FD862A0B6D190","previous_id":1882893,"previous_guid":"CCDA480437CD4E54BBA076A5A1314CBF","section":"\u003cp\u003eSecondary Staining\u003c/p\u003e","section_color":"#EA94FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"f5rvb\",\"text\":\"Incubate in secondary antibodies (Alexa Fluor-conjugated anti-rabbit/chicken A-31573,  RRID:AB_2536183 and A78952, RRID:AB_2921074 1:1000) and DAPI (Thermo Fisher Scientific Cat# D3571, RRID:AB_2307445, 1:5000), prepared in TBS for 1 hour at room temperature.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":92448,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"17","cases":[],"critical":null},{"id":1882895,"guid":"B509393A0D7A48EDB477F07668C68323","previous_id":1882894,"previous_guid":"11923CE9596B4544884FD862A0B6D190","section":"\u003cp\u003eSecondary Staining\u003c/p\u003e","section_color":"#EA94FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"db80r\",\"text\":\"Wash slides in TBS three times, for five minutes each.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":92448,"case_id":0,"critical_ids":"","duration":0,"origina