reserved_doi":"","retraction_reason":null,"samples":{},"shared_access_id":265,"should_acknowledge":false,"show_comparison":false,"sign_info":null,"space_access":{"can_preview":false,"can_view":false,"can_edit":false,"can_remove":false,"can_add":false,"can_publish":true,"can_get_doi":true,"can_share":false,"can_share_organization":false,"can_move":false,"can_move_outside":false,"can_transfer":false,"can_download":false,"limited_run":false,"limited_private_links":false,"limited_blind_links":false,"is_locked":false},"space_id":1381,"state_version_id":62,"stats":{"is_voted":false,"number_of_views":35,"number_of_steps":8,"number_of_bookmarks":0,"number_of_comments":0,"number_of_bookmarked_comments":0,"number_of_steps_comments":0,"number_of_protocol_comments":0,"number_of_exports":19,"number_of_runs":0,"number_of_votes":0,"number_of_reagents":0,"number_of_equipments":0,"number_of_collections":0,"number_of_forks":{"private":0,"public":0},"number_of_accessible_forks":0},"status":{"id":1,"info":""},"steps":[{"id":934797,"guid":"17A6B820901011EA833D41C79FFDF1FE","previous_id":0,"previous_guid":null,"section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"1m08c\",\"text\":\"Data from each mode were independently analyzed using Progenesis QI software (v2.3) (Nonlinear Dynamics, Durham, NC). \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":{}}","data":null,"protocol_id":36726,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"1","cases":[],"critical":null},{"id":934798,"guid":"1DEBF1A0901011EA833D41C79FFDF1FE","previous_id":934797,"previous_guid":"17A6B820901011EA833D41C79FFDF1FE","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"6smf2\",\"text\":\"Metabolic features from blanks and those that didn’t show sufficient linearity upon dilution in QC samples (r \\u003c 0.6) were discarded. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":{}}","data":null,"protocol_id":36726,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"2","cases":[],"critical":null},{"id":934799,"guid":"23AA9600901011EA833D41C79FFDF1FE","previous_id":934798,"previous_guid":"1DEBF1A0901011EA833D41C79FFDF1FE","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"2gkus\",\"text\":\"Only metabolic features present in \\u003e2/3 of the samples were kept for further analysis. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":{}}","data":null,"protocol_id":36726,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"3","cases":[],"critical":null},{"id":934800,"guid":"2BF192F0901011EA833D41C79FFDF1FE","previous_id":934799,"previous_guid":"23AA9600901011EA833D41C79FFDF1FE","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"e1asp\",\"text\":\"Median normalization was applied to correct for differential starting material quantity. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":{}}","data":null,"protocol_id":36726,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4","cases":[],"critical":null},{"id":934801,"guid":"30E54630901011EA833D41C79FFDF1FE","previous_id":934800,"previous_guid":"2BF192F0901011EA833D41C79FFDF1FE","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"diku4\",\"text\":\"Missing values were imputed by drawing from a random distribution of low values in the corresponding sample. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":{}}","data":null,"protocol_id":36726,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"5","cases":[],"critical":null},{"id":934802,"guid":"369DF720901011EA833D41C79FFDF1FE","previous_id":934801,"previous_guid":"30E54630901011EA833D41C79FFDF1FE","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"diaoo\",\"text\":\"Data from each mode were merged and metabolites were formally identified by matching fragmentation spectra and retention time to analytical-grade standards when possible or matching experimental MS/MS to fragmentation spectra in publicly available databases. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":{}}","data":null,"protocol_id":36726,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"6","cases":[],"critical":null},{"id":934803,"guid":"3D7DEA00901011EA833D41C79FFDF1FE","previous_id":934802,"previous_guid":"369DF720901011EA833D41C79FFDF1FE","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"6n6s0\",\"text\":\"Metabolite abundances were reported as spectral counts.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":{}}","data":null,"protocol_id":36726,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"7","cases":[],"critical":null},{"id":940996,"guid":"CE371900956E11EA833D41C79FFDF1FE","previous_id":934803,"previous_guid":"3D7DEA00901011EA833D41C79FFDF1FE","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"6vhl7\",\"text\":\"Peak annotation was first performed by matching experimental m/z, retention time and MS/MS spectra to an in-house library of analytical-grade standards. Remaining peaks were identified by matching experimental m/z and fragmentation spectra to publicly available databases including HMDB (http://www.hmdb.ca/), MoNA (http://mona.fiehnlab.ucdavis.edu/) and MassBank (http://www.massbank.jp/) using the R package ‘MetID’ (v0.2.0) (PMID: 30944337). Briefly, metabolic feature tables from Progenesis QI were matched to fragmentation spectra with a m/z and a retention time window of  ±15 ppm and ±30 s (HILIC) and ± 20 s (RPLC), respectively. When multiple MS/MS spectra match a single metabolic feature, all matched MS/MS spectra were used for the identification. Next, MS1 and MS2 pairs were searched against public databases and a similarity score was calculated using the forward dot–product algorithm which takes into account both fragments and intensities (PMID: 24222034). Metabolites were reported if the similarity score was above 0.4. Spectra from metabolites of interest were further manually investigated to confirm identification.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[{\"key\":0,\"offset\":288,\"length\":19},{\"key\":1,\"offset\":316,\"length\":33},{\"key\":2,\"offset\":365,\"length\":23}],\"data\":[]}],\"entityMap\":{\"0\":{\"type\":\"link\",\"mutability\":\"\",\"data\":{\"guid\":\"CEDF8B7C956E11EAAB120A58A9FEAC2A\",\"url\":\"http://www.hmdb.ca\"}},\"1\":{\"type\":\"link\",\"mutability\":\"\",\"data\":{\"guid\":\"CEDF8C10956E11EAAB120A58A9FEAC2A\",\"url\":\"http://mona.fiehnlab.ucdavis.edu\"}},\"2\":{\"type\":\"link\",\"mutability\":\"\",\"data\":{\"guid\":\"CEDF8C71956E11EAAB120A58A9FEAC2A\",\"url\":\"http://www.massbank.jp\"}}}}","data":null,"protocol_id":36726,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"8","cases":[],"critical":null}],"table_of_contents":[],"template_id":1,"title":"LC-MS/MS Untargeted Metabolomics Data Processing","title_html":"\u003cp\u003eLC-MS/MS Untargeted Metabolomics Data Processing\u003c/p\u003e","translations":[],"troubleshooting_items":[],"type_id":1,"units":[{"id":1,"type_id":3,"name":"µL","can_manage":0,"read_only":0},{"id":2,"type_id":3,"name":"mL","can_manage":0,"read_only":0},{"id":3,"type_id":3,"name":"L","can_manage":0,"read_only":0},{"id":4,"type_id":3,"name":"µg","can_manage":0,"read_only":0},{