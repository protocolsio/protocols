,"guid":"70CDD8F0002511EA91E4B9FCAF61BD0B","previous_id":840992,"previous_guid":"70CCC780002511EA91E4B9FCAF61BD0B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"4B00AFF12F9A4AF09F2A066D0FB1E0E2","order_id":1,"type_id":6,"title":"Section","source":{"title":"Transfection"}},{"id":1054724,"guid":"94D85965D67D472AB3533E1B1920ABA2","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Incubate for 15 min at RT.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":[],"critical_id":null,"duration":0},{"id":840994,"guid":"70CEEA60002511EA91E4B9FCAF61BD0B","previous_id":840993,"previous_guid":"70CDD8F0002511EA91E4B9FCAF61BD0B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"3733E0E177E54B0DA7045FC049F457A0","order_id":1,"type_id":6,"title":"Section","source":{"title":"Transfection"}},{"id":1054724,"guid":"BD8CBD8ADCFC44AA8E737996AE2CE05E","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Volumize to 100ml with complete DMEM<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":[],"critical_id":null,"duration":0},{"id":840995,"guid":"70D022E0002511EA91E4B9FCAF61BD0B","previous_id":840994,"previous_guid":"70CEEA60002511EA91E4B9FCAF61BD0B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"6F56578B77124FDEB78079083AB759A0","order_id":1,"type_id":6,"title":"Section","source":{"title":"Transfection"}},{"id":1054724,"guid":"56A45F1ABFD54FC3846DE4F81D33654E","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">One by one, remove culture medium from each dishes and gently add the fresh transfection solution (20ml for each dishes).<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":[],"critical_id":null,"duration":0},{"id":840996,"guid":"70D15B60002511EA91E4B9FCAF61BD0B","previous_id":840995,"previous_guid":"70D022E0002511EA91E4B9FCAF61BD0B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"598BD33D4F394AC68C06010F7D617779","order_id":1,"type_id":6,"title":"Section","source":{"title":"Lysing of cells and harvesting of AAVs"}},{"id":1054724,"guid":"DF0A4E0D69464361A7D20C514CCE8E4B","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">48 to 72 hours after transfection (depending on health of the HEK cells), remove media from cell culture plates and discard. The cells will still exhibit nice monolayer formation - of cells start to peel away from the bottom of the flask it is too late. Watch for media turning from pink to yellow. If media starts to look yellow, it is time to wash. <\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":[],"critical_id":null,"duration":0},{"id":840997,"guid":"70D293E0002511EA91E4B9FCAF61BD0B","previous_id":840996,"previous_guid":"70D15B60002511EA91E4B9FCAF61BD0B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"1A9B34A6CFA14B09991D74A1EC0CEC75","order_id":1,"type_id":6,"title":"Section","source":{"title":"Lysing of cells and harvesting of AAVs"}},{"id":1054724,"guid":"3FD9B61A6C1641DE85C2E1B55DB87B90","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Wash cells with warm (37C) 1x PBS 10ml. <\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":[],"critical_id":null,"duration":0},{"id":840998,"guid":"70D3CC60002511EA91E4B9FCAF61BD0B","previous_id":840997,"previous_guid":"70D293E0002511EA91E4B9FCAF61BD0B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"8F72C67FF22541FD8683F6412FE30961","order_id":1,"type_id":6,"title":"Section","source":{"title":"Lysing of cells and harvesting of AAVs"}},{"id":1054724,"guid":"13BF4273FF0841B797C9520A88E1789B","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Add 20 mL warm PBS to each plate and gently remove cells with cell scraper.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":[],"critical_id":null,"duration":0},{"id":840999,"guid":"70D55300002511EA91E4B9FCAF61BD0B","previous_id":840998,"previous_guid":"70D3CC60002511EA91E4B9FCAF61BD0B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"E93399F0AB9C40638E75BCED986796F5","order_id":1,"type_id":6,"title":"Section","source":{"title":"Lysing of cells and harvesting of AAVs"}},{"id":1054724,"guid":"2E1AB97FF7C74E14B0F9D7F20916CDE6","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Collect suspension in 50 mL falcon tubes.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":[],"critical_id":null,"duration":0},{"id":841000,"guid":"70D6D9A0002511EA91E4B9FCAF61BD0B","previous_id":840999,"previous_guid":"70D55300002511EA91E4B9FCAF61BD0B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"0A5DFE42FAF44D0A80BBF3601AE1C11D","order_id":1,"type_id":6,"title":"Section","source":{"title":"Lysing of cells and harvesting of AAVs"}},{"id":1054724,"guid":"7B2773BE06224D68BEE049B1E0A67095","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Pellet cells at 800 x g for 10 min<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_col