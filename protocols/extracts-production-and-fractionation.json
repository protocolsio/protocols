:null,"section_color":"#FFED92","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1117733,"guid":"EE3BD420656611EBBE951F37A4C1D62D","previous_id":1117732,"previous_guid":"E4692E20656611EBBE951F37A4C1D62D","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"985ED849CC734B5EA04B12496A943DD6","order_id":1,"type_id":6,"title":"Section","source":{"title":"Fractionation"}},{"id":1054724,"guid":"8C1DAF2CBEB04FC29073CEE90857B4CE","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Dry these fractions in a rotary evaporator at 45 \u00b0C under vacuum 200 mbar, weight it and use it for further activities<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#FFED92","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1117734,"guid":"70F64770656611EBBE951F37A4C1D62D","previous_id":1117715,"previous_guid":"451D74C0656611EBBE951F37A4C1D62D","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"7293400E5EDA4FDEA88619DF6EE4CF3D","order_id":1,"type_id":6,"title":"Section","source":{"title":"pre-inoculum"}},{"id":1054724,"guid":"2DED10984FFB43B39A8B107212A78525","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">and incubated at 28 \u00b0C for 5 to 7 days<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1117738,"guid":"6ACE5290656911EBA7E753FFF90AF207","previous_id":1117727,"previous_guid":"AE6FBC80656611EBBE951F37A4C1D62D","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"E669133391FA470BB549A96270E2F0A9","order_id":1,"type_id":6,"title":"Section","source":{"title":"Extraction"}},{"id":1054724,"guid":"179CB63313EF4DA8956BBED8D7B0C4B5","order_id":1,"type_id":1,"title":"description","so