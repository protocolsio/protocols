f_comments":0,"number_of_exports":0,"number_of_runs":0,"number_of_votes":0,"is_voted":0},"parent_protocols":[],"parent_collections":[],"cited_protocols":[]},"created_on":1589908328,"resolve_on":0,"resolved_user":{"name":" ","affiliation":null,"affiliations":[],"username":null,"note":null,"link":null,"image":{"source":null,"placeholder":null},"badges":[],"verified":0,"is_verified_user":false,"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false},"shared":false}}],"is_owner":1,"has_subprotocols":0,"is_subprotocol":0,"is_bookmarked":0,"can_claim_authorship":0,"can_accept_authorship":0,"can_be_copied":1,"can_remove_fork":1,"fork_id":null,"url":"https:\/\/www.protocols.io\/view\/bubble-strip-aqueous-gas-sampling-bkb9ksr6","forks_count":{"private":0,"public":0},"access":{"can_view":1,"can_remove":0,"can_add":0,"can_edit":0,"can_publish":0,"can_get_doi":0,"can_share":1,"can_move":1,"can_move_outside":1,"can_transfer":1,"can_download":1,"is_locked":0},"guid":"070BB058EC4B4ED38B9CDC2EB03B3D5E","state_version_id":329,"steps":[{"id":1014032,"guid":"7F319CABE8D649A9922C0E6F0063A26E","previous_id":null,"previous_guid":null,"modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"56B4EFCD733D46EDBA4D142EB4BCE6F9","order_id":1,"type_id":6,"title":"Section","source":{"title":"Prepping sample vials"}},{"id":1054724,"guid":"D327758BF3BD472DBF3CD0D819FB33F3","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Pre-treat blue chlorobutyl stoppers to reduce potential contaminants by boiling in 0.1 N NaOH for 45 min followed by immersion in distilled water for ~8 hours), according to procedure of Oremland et al. (1987).<\/div><div class = \"text-block\">Oremland R. S., Miller L. G. and Whiticar M. J. (1987) Sources and flux of natural gasses from Mono Lake, California. Geochim. Cosmochim. Acta 51, 2915\u20132929.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":null,"section_duration":86400,"critical":null,"critical_id":null,"duration":0},{"id":1014033,"guid":"E80E3D0DB7D0440783FF3E64ABC2319F","previous_id":1014034,"previous_guid":"43F43EA99F06469F90B3BC4AFFF3B3F3","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"96D62A4D6DCA4C238AE2AB4702782E3E","order_id":1,"type_id":6,"title":"Section","source":{"title":"Prepping sample vials"}},{"id":1054724,"guid":"71EC1E4C5F92425F807D2ECC4946F77D","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">While still warm, insert stoppers into 37 mL borosilicate glass vials, and secure stoppers with aluminum crimp tops.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":null,"section_duration":86400,"critical":null,"critical_id":null,"duration":0},{"id":1014034,"guid":"43F43EA99F06469F90B3BC4AFFF3B3F3","previous_id":1014035,"previous_guid":"002FFDA5B1B1470BB543A37FCCEB16B3","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"118DB82BE7BF440484BBA251D472B6EF","order_id":1,"type_id":6,"title":"Section","source":{"title":"Prepping sample vials"}},{"id":1054724,"guid":"568F8F035FBC47859BE6243254145FF3","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Autoclave sterilize vials, stoppers, and NaCl solution (90 minutes at 125\u02daC).<\/div><\/div>"}},{"id":1054725,"guid":"B5CF4680E7AD11EA9CE921FFD994EAA0","order_id":2,"type_id":4,"title":"duration","source":{"duration":7200,"title":"including cooldown time"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":null,"section_duration":86400,"critical":null,"critical_id":null,"duration":0},{"id":1014035,"guid":"002FFDA5B1B1470BB543A37FCCEB16B3","previous_id":1014032,"previous_guid":"7F319CABE8D649A9922C0E6F0063A26E","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"B84EC7F2B538473088FECE7384CC40DB","order_id":1,"type_id":6,"title":"Section","source":{"title":"Prepping sample vials"}},{"id":1054724,"guid":"9624794806884BEDBDE9499825D73FBE","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Sampling vials will be filled with a near-saturation NaCl solution which will be displaced by the sample gas. Calculate the volume needed to fill all your vials. In a large beaker, prepare a near-saturation NaCl solution that is a bit larger than the volume of your vials.<\/div><\/div>"}},{"id":1054725,"guid":"D707E32D236B498597FA6398A3F9BA3A","order_id":2,"type_id":26,"title":"notes","source":{"id":0,"parent_id":0,"uri":"","title":"","body":"<div class = \"text-blocks\"><div class = \"text-block\">Leave plenty of extra room in the beakers with NaCl because when you autoclave in the next step, the NaCl brine can expand and overflow the beakers.<\/div><\/div>","created_on":0,"changed_on":0,"creator":{"name":" ","affiliation":null,"affiliations":[],"username":"","note":null,"link":null,"image":{"source":"","placeholder":""},"badges":[],"verified":0,"is_verified_user":false,"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false},"comments":[]}}],"cases":[],"data":null,"html":null,"section":null,"section_color":null,"section_duration":86400,"critical":null,"critical_id":null,"duration":0},{"id":1014036,"guid":"7EAADDC9C77148B6A8799D3E4229535C","previous_id":1014050,"previous_guid":"F22FF890E7AD11EA9CE921FFD994EAA0","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"A4BD7C180B8F4BC9A19A6C286AB14B62","order_id":1,"type_id":6,"title":"Section","source":{"title":"Field Sampling"}},{"id":1054724,"guid":"DCE87CD300CB4A0BB1864A9B14505021","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Attach a 100 mL borosilicate glass bottle (with appropriate fittings installed - see materials) to a ring stand, cap facing up. Connect inlet tubing of bubble strip to pumped water source (i.e. manifold).<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":3600,"critical":null,"critical_id":null,"duration":0},{"id":1014037,"guid":"C29AE40B1FCB498FB2BDC72EAD135F6D","previous_id":1014036,"previous_guid":"7EAADDC9C77148B6A8799D3E4229535C","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"0F3CF33871294CE389CF73C3D0BF9D3B","order_id":1,"type_id":6,"title":"Section","source":{"title":"Field Sampling"}},{"id":1054724,"guid":"38C911A558FD4B79819D0D01DD9EADF5","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Slowly open water flow valve on manifold. Let bottle fill up with water.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":3600,"critical":null,"critical_id":null,"duration":0},{"id":1014038,"guid":"F000DCC6FCC74856833692345E91D9EA","previous_id":1014037,"previous_guid":"C29AE40B1FCB498FB2BDC72EAD135F6D","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"BC55ABC1059D43A6A3130E64C6D2928D","order_id":1,"type_id":6,"title":"Section","source":{"title":"Field Sampling"}},{"id":1054724,"guid":"BB13F9322AB647EDBD4CA06C12153139","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Flip bottle 180\u02da such that the cap faces downwards.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":3600,"critical":null,"critical_id":null,"duration":0},{"id":1014039,"guid":"7176ACD29B9E4203AD0BB5587D5ED9C6","previous_id":1014038,"previous_guid":"F000DCC6FCC74856833692345E91D9EA","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"793501B22CF3497496A93530D189317B","order_id":1,"type_id":6,"title":"Section","source":{"title":"Field Sampling"}},{"id":1054724,"guid":"2FE5D7E9B7EF4DC8955A00D4459C9062","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Attach a mininert stopcock to a 60 mL plastic syringe.  Fill syringe with 40 mL of air.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":3600,"critical":null,"critical_id":null,"duration":0},{"id":1014040,"guid":"9B9E1E1C7D18470CB582BC5A94A28687","previous_id":1014039,"previous_guid":"7176ACD29B9E4203AD0BB5587D5ED9C6","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"0056D6E298DB40DD8525AB3E3CB975D2","order_id":1,"type_id":6,"title":"Section","source":{"title":"Field Sampling"}},{"id":1054724,"guid":"6F338F8BC2294CD4900BDA96AAB1DB46","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Attach syringe to sampling port on bottle cap.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":3600,"critical":null,"critical_id":null,"duration":0},{"id":1014041,"guid":"4F156965F7794AF49D6288C8E350CD7B","previous_id":1014040,"previous_guid":"9B9E1E1C7D18470CB582BC5A94A28687","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"11BB6BA3612047258AF16B98578960E7","order_id":1,"type_id":6,"title":"Section","source":{"title":"Field Sampling"}},{"id":1054724,"guid":"B4A997EF0D394D11A26E9FAF611506B4","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Inject 40 mL air through the sampling port into the glass bottle. Close mininert valve. The setup should look like this picture. In this example (from 2018 Oman sampling), 2 bubble strip gas samples are equilibrating simultaneously. Other tubes connected to biomass filters and\/or water sampling ports are unrelated to the bubble strip.<\/div><div style = \"text-align :; float : ;\"><img style = \"\" src = \"https:\/\/s3.amazonaws.com\/pr-journal\/bhdc5r8w.jpg\" \/><\/div><\/div>"}},{"id":1054725,"guid":"DD3A625B8C9244F6A281848114F56183","order_id":2,"type_id":1,"title":"description","source":{"description":"<div style = \"text-align :; float : ;\"><img style = \"\" src = \"https:\/\/s3.amazonaws.com\/pr-journal\/bhdc5r8w.jpg\" \/><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":3600,"critical":null,"critical_id":null,"duration":0},{"id":1014042,"guid":"F602044E558547AF85C43D37DB7A576A","previous_id":1014041,"previous_guid":"4F156965F7794AF49D6288C8E350CD7B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"C85F7BD078BE4C77B2FD99C008A26A30","order_id":1,"type_id":6,"title":"Section","source":{"title":"Field Sampling"}},{"id":1054724,"guid":"05BDA768DBE74BFE8CD82484F608355A","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Measure flow rate by timing the filling of a 500 mL graduated cylinder from outlet tube. Flow rate should be at least 300 mL\/min.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":3600,"critical":null,"critical_id":null,"duration":0},{"id":1014043,"guid":"6965AA9155C54ECA84CA6327BB387C46","previous_id":1014042,"previous_guid":"F602044E558547AF85C43D37DB7A576A","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"3944B3963CF64BD190FCD8A71C9D0C3A","order_id":1,"type_id":6,"title":"Section","source":{"title":"Field Sampling"}},{"id":1054724,"guid":"020688048A804E739C87A74F21CE1401","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Periodically monitor flow rate and water temperature. If flow remains at least 300 mL\/min, the headspace gas should equilibrate within 30 minutes of flow, and then can be sampled. The water temperature is good to note so that Henry's constants can be adjusted for temperature.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":3600,"critical":null,"critical_id":null,"duration":0},{"id":1014044,"guid":"520D3966EF214FB8A22C66E1C590ADB1","previous_id":1014043,"previous_guid":"6965AA9155C54ECA84CA6327BB387C46","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"F97B49FA96F440BA822708E10EB0D0EF","order_id":1,"type_id":6,"title":"Section","source":{"title":"Field Sampling"}},{"id":1054724,"guid":"2B32AC186A5645EE91ACD679792BE5F1","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">After sufficient flow has been allowed for gas equilibration, re-attach syringe to bubble strip apparatus if it has been removed (although typically it is best to just leave syringe connected). Then, open mininert valve, and withdraw 35-40 mL of headspace gas. Close mininert valve.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":3600,"critical":null,"critical_id":null,"duration":0},{"id":1014045,"guid":"A2D0E5EE0DCD423FBC163EBA243811ED","previous_id":1014044,"previous_guid":"520D3966EF214FB8A22C66E1C590ADB1","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"461E6F92A24D48FE9F384B7EBEE5D80A","order_id":1,"type_id":6,"title":"Section","source":{"title":"Field Sampling"}},{"id":1054724,"guid":"F5D94DABFEB74EC0A32E2EF9E2BDA9F6","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Remove syringe and mininert valve from sampling apparatus. Connect a sterile 0.2 \u00b5m syringe filter and sterile 18G needle (BD precisionglide) to the mininert valve.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":3600,"critical":null,"critical_id":null,"duration":0},{"id":1014046,"guid":"ACE2A74A95174F2FBA943AA999E3CC4D","previous_id":1014045,"previous_guid":"A2D0E5EE0DCD423FBC163EBA243811ED","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"804D808490534CB68BB8530A591C4DC6","order_id":1,"type_id":6,"title":"Section","source":{"title":"Field Sampling"}},{"id":1054724,"guid":"7B445BA2753442C3816403209C395D66","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Open mininert valve on syringe. Inject gas through the filter and needle until the syringe reads 30 mL. Immediately stick the syringe and needle into the sampling vial through the rubber stopper. Using another sterile needle, puncture the sample vial stopper to provide and outflow for the NaCl solution. Displace 30 mL of NaCl solution with the gas in the syringe. Holding needle securely, remove syringe from vial. Store vials stopper-side down until analysis.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":3600,"critical":null,"critical_id":null,"duration":0},{"id":1014049,"guid":"58CADDF0E7AD11EA9CE921FFD994EAA0","previous_id":1014033,"previous_guid":"E80E3D0DB7D0440783FF3E64ABC2319F","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"453ABAE80A5A40A99DF89CA15C4F9A3A","order_id":1,"type_id":6,"title":"Section","source":{"title":"Prepping sample vials"}},{"id":1054724,"guid":"AE66037418D44B90A893639580EA6FA2","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Wait for vials, stoppers, and NaCl solution to cool down to near room temperature.<\/div><\/div>"}},{"id":1054725,"guid":"A39D60A0E7AD11EA9CE921FFD994EAA0","order_id":2,"type_id":4,"title":"duration","source":{"duration":7200,"title":""}},{"id":1054726,"guid":"B1E0EE498E44467FA80A6C5BDE1EC6DA","order_id":3,"type_id":26,"title":"notes","source":{"id":0,"parent_id":0,"uri":"","title":"","body":"<div class = \"text-blocks\"><div class = \"text-block\">hot stoppers don't reseal well upon puncturing with needles<\/div><\/div>","created_on":0,"changed_on":0,"creator":{"name":" ","affiliation":null,"affiliations":[],"username":"","note":null,"link":null,"image":{"source":"","placeholder":""},"badges":[],"verified":0,"is_verified_user":false,"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false},"comments":[]}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":86400,"critical":null,"critical_id":null,"duration":0},{"id":1014050,"guid":"F22FF890E7AD11EA9CE921FFD994EAA0","previous_id":1014049,"previous_guid":"58CADDF0E7AD11EA9CE921FFD994EAA0","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"E2D3E655B3224867804948A439368131","order_id":1,"type_id":6,"title":"Section","source":{"title":"Prepping sample vials"}},{"id":1054724,"guid":"05A481C68CA44CE78B2F1D19C8E23DB1","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Using  2 18G BD precisionglide needle per vial and a 60 mL syringe, fill the vial as completely as possible with NaCl solution.<\/div><\/div>"}},{"id":1054725,"guid":"1C8ECC2E3A514DBE891227F591AD1596","order_id":2,"type_id":26,"title":"notes","source":{"id":0,"parent_id":0,"uri":"","title":"","body":"<div class = \"text-blocks\"><div class = \"text-block\">It is not essential to use 100% sterile technique for the NaCl solution because it is so salty that microbial life is inhibited in it.<\/div><\/div>","created_on":0,"changed_on":0,"creator":{"name":" ","affiliation":null,"affiliations":[],"username":"","note":null,"link":null,"image":{"source":"","placeholder":""},"badges":[],"verified":0,"is_verified_user":fa