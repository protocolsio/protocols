,"components":[{"id":1054723,"guid":"0EF1037C302E4BDCA783962E9F7F27EE","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"4C32247AE06E4CF7964784A80EBDD4EA","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Double click the \u2018Scatter Parameter\u2019 field to change which parameter is being used for light scatter calibration.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":968353,"guid":"55D55C60B67011EA8ED78FE3C7563A70","previous_id":968352,"previous_guid":"55D4C020B67011EA8ED78FE3C7563A70","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"E85B915462624AF49F8120BE59F54422","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"DFA224A362E54189A96550532F67D9ED","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Alter the \u2018Scatter Wavelength (nm)\u2019 to the relevant wavelength for the parameter being used to calibrate light scatter. <\/div><\/div>"}},{"id":1054725,"guid":"26446457522648A496D1EDA62E29DE3F","order_id":2,"type_id":26,"title":"notes","source":{"id":0,"parent_id":0,"uri":"","title":"","body":"<div class = \"text-blocks\"><div class = \"text-block\">You will see that the \u2018Sheath RI\u2019 field will automatically update when this is altered. In the background reference bead, core-shell model, and homogenous sphere model refractive indices will all also be updated. <\/div><\/div>","created_on":0,"changed_on":0,"creator":{"name":" ","affiliation":null,"affiliations":[],"username":"","note":null,"link":null,"image":{"source":"","placeholder":""},"badges":[],"verified":0,"is_verified_user":false,"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false},"comments":[]}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":968354,"guid":"72008310B67011EA8ED78FE3C7563A70","previous_id":968353,"previous_guid":"55D55C60B67011EA8ED78FE3C7563A70","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"A2F88A7D4C364F5993931438E9ADC874","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"A4A86FE87E514299927F328F5C31C152","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">If the selected \u2018Scatter Parameter\u2019 was used as a triggering threshold then the \u2018Scatter Threshold\u2019 field will automatically update to show the values used as thresholds in the .fcs files loaded. Select a \u2018Scatter Threshold\u2019 by double clicking the field and selecting and option from the dropdown menu. A custom entry can also be inputted.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":968355,"guid":"72011F51B67011EA8ED78FE3C7563A70","previous_id":968354,"previous_guid":"72008310B67011EA8ED78FE3C7563A70","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"22D6ECF0A4884BB9A4A496149D9001F4","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"F17D09179F2647E5BFE2E8A4634BA660","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Load the light scatter reference beads used by double clicking the \u2018Bead Set\u2019 field. Once loaded the beads within the set will populate the bottom table. <\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":968356,"guid":"7201BB91B67011EA8ED78FE3C7563A70","previous_id":968355,"previous_guid":"72011F51B67011EA8ED78FE3C7563A70","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"1E875C941DE04F95A34E4E1017429401","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"207466044A9943F1AC5FAA59CA20CF82","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">The \u2018Sheath RI\u2019 field automatically accounts for \u2018Scatter Wavelength\u2019 but can be updated manually by double clicking the field. <\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":968357,"guid":"72034230B67011EA8ED78FE3C7563A70","previous_id":968356,"previous_guid":"7201BB91B67011EA8ED78FE3C7563A70","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"D58286E5989F4018B88CA8AE1D9611CC","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"7F2F3B63C886444EB3B323B2987C7BE3","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">In the bottom table enter the median scatter parameter statistic for each population. The acquired CV can optionally also be completed, its use will, however, only be used for plotting purposes and not alter the model calculations. <\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":968358,"guid":"72040581B67011EA8ED78FE3C7563A70","previous_id":968357,"previous_guid":"72034230B67011EA8ED78FE3C7563A70","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"DA9287A7931F4574AE3D39FC0BA0F636","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"0122D46E73C549D0AF90D799D8C12930","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Once complete click \u2018Next\u2019.<\/div><\/div>"}},{"id":1054725,"guid":"29E8DEC0E3CC40CABE629FD1FF0AD1A5","order_id":2,"type_id":26,"title":"notes","source":{"id":0,"parent_id":0,"uri":"","title":"","body":"<div class = \"text-blocks\"><div class = \"text-block\"><span style = \"font-style:italic;\">Custom core-shell models, solid sphere models, plot data points, modelling parameters, and output settings can be entered or altered by clicking the \u2018Advanced Settings\u2019 button. By default, three EV core-shell models relating to high, medium, and low EV refractive indices are calculated. All core-shell models assume a 5 nm shell thickness. <\/span><span><\/span><\/div><\/div>","created_on":0,"changed_on":0,"creator":{"name":" ","affiliation":null,"affiliations":[],"username":"","note":null,"link":null,"image":{"source":"","placeholder":""},"badges":[],"verified":0,"is_verified_user":false,"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false},"comments":[]}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0}],"document":null,"materials":[],"description":"<div class = \"text-blocks\"><div class = \"text-block\">This protocol outlines the steps required to input light scatter calibration parameters using the FCMPASS software. This is one of a number of protocols in the pipeline for performing small particle calibration using the fcmpass software package.<\/div><\/div>","changed_on":1596442270}