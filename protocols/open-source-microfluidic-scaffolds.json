:986732,"guid":"F3606040CCE811EAAE4015FA587B06DC","previous_id":986731,"previous_guid":"AFF1CD80CCE811EAAE4015FA587B06DC","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"1A4790D34F0246299DE76881439819C8","order_id":1,"type_id":6,"title":"Section","source":{"title":"Printing"}},{"id":1054724,"guid":"9DA086D302DB46E5AB38F78E25E7C094","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Once the print is completed, remove the models from the build plate.<\/div><\/div>"}},{"id":1054725,"guid":"4D600CE8BC54426EBBB13D6A7337E44A","order_id":2,"type_id":26,"title":"notes","source":{"id":0,"parent_id":0,"uri":"","title":"","body":"<div class = \"text-blocks\"><div class = \"text-block\"><span style = \"font-style:italic;\">We found the best way to do this was using a stanley knife (or similar) and prising the connectors of the build plate. The channels are very delicate so this takes some practice and patience.<\/span><\/div><\/div>","created_on":0,"changed_on":0,"creator":{"name":" ","affiliation":null,"affiliations":[],"username":"","note":null,"link":null,"image":{"source":"","placeholder":""},"badges":[],"verified":0,"is_verified_user":false,"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false},"comments":[]}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#FFED92","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":986733,"guid":"3EB9ED40CCE911EAAE4015FA587B06DC","previous_id":986732,"previous_guid":"F3606040CCE811EAAE4015FA587B06DC","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"0D46AD3BA5B947AB9E8360F85B469260","order_id":1,"type_id":6,"title":"Section","source":{"title":"Thermal Bonding to Glass"}},{"id":1054724,"guid":"A44847491AD24DB3854ACD43FAEA780B","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Mount the 3D-printed microfluid channel modules onto 1 mm thick glass microscope slides in the desired configuration using the ball-and-socket-connectors.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#EA9F6C","section_duration":480,"critical":null,"critical_id":null,"duration":60},{"id":986734,"guid":"9F834D10CCE911EAAE4015FA587B06DC","previous_id":986733,"previous_guid":"3EB9ED40CCE911EAAE4015FA587B06DC","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"DFF1A9A234D949798A117CD4240F6EA8","order_id":1,"type_id":6,"title":"Section","source":{"title":"Thermal Bonding to Glass"}},{"id":1054724,"guid":"CDDF410B9B604C68B16A02B26015EA3E","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Place a second 1mm thick glass slide on top of the channel configuration to act as a weighting slide<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#EA9F6C","section_duration":480,"critical":null,"critical_id":null,"duration":30},{"id":986735,"guid":"A95556D0CCE911EAAE4015FA587B06DC","previous_id":986734,"previous_guid":"9F834D10CCE911EAAE4015FA587B06DC","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"5CA94936C2CD4D229717CD3D8C428B35","order_id":1,"type_id":6,"title":"Section","source":{"title":"Thermal Bonding to Glass"}},{"id":1054724,"guid":"B214579EB8854168B964330228617B22","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Heat the slide-channel-slide assembly at 200<\/div><div style = \"text-align :; float : ;\"><span style = \"\">\\degree<\/span><\/div><div class = \"text-block\">C for 45-60s to bond the PLA thermoplastic to the glass substrate and smooth out inconsistencies.  This is best performed on a temprature controlled hot plate, but heating could also be achieved in an oven or over a flame, however timings and cosistency will vary.  <\/div><\/div>"}},{"id":1054725,"guid":"C89B517E3C3F471ABE9515EC8E3F7FD4","order_id":2,"type_id":19,"title":"safety","source":{"body":"<div class = \"text-blocks\"><div class = \"text-block\">CAUTION: Hot surfaces and heated assemblies can cause severse burns. <\/div><\/div>","link":""}},{"id":1054726,"guid":"976AEADA18434BCDBCA709048BB76B54","order_id":3,"type_id":26,"title":"notes","source":{"id":0,"parent_id":0,"uri":"","title":"","body":"<div class = \"text-blocks\"><div class = \"text-block\">NOTE: Heating times will vary depending on the height of the 3D-printed scaffold.<\/div><\/div>","created_on":0,"changed_on":0,"creator":{"name":" ","affiliation":null,"affiliations":[],"username":"","note":null,"link":null,"image":{"source":"","placeholder":""},"badges":[],"verified":0,"is_verified_user":false,"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false},"comments":[]}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#EA9F6C","section_duration":480,"critical":null,"critical_id":null,"duration":60},{"id":986736,"guid":"F38705A0CCE911EAAE4015FA587B06DC","previous_id":986735,"previous_guid":"A95556D0CCE911EAAE4015FA587B06DC","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"B28AB5E31B714BA488E96D54B9D53172","order_id":1,"type_id":6,"title":"Section","source":{"title":"Thermal Bonding to Glass"}},{"id":1054724,"guid":"B1DD368450FB4839A8F80D7F42FF496A","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">After heating, use tweerzers, tongs or oven gloves to transfer the thermally bonded slides onto a metal cooling plate (weighting slide down), and leave for 5 minutes.  <\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#EA9F6C","section_duration":480,"critical":null,"critical_id":null,"duration":300},{"id":986737,"guid":"2705B340CCEA11EAAE4015FA587B06DC","previous_id":987570,"previous_guid":"5A9E5140CD8911EA8021EB6A2599C9B9","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"2A2E533681944381875C0CFDAD694FA2","order_id":1,"type_id":6,"title":"Section","source":{"title":"PDMS Fabrication"}},{"id":1054724,"guid":"DFB66753722844E890CCA4CCBA9688B1","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span>Use the resulting thermally bonded channel design on the substrate slide as the master mould for PDMS casting. See work by Lake <\/span><span style = \"font-style:italic;\">et al.<\/span><span> for a more detailed explanation of this step (<\/span><\/div><div class = \"text-block\"><a href=\"https:\/\/doi.org\/10.1038\/protex.2015.069\" style = \"text-decoration:underline;color:blue;cursor:pointer;\"><span style = \":;\">https:\/\/doi.org\/10.1038\/protex.2015.069<\/span><\/a><\/div><div class = \"text-block\">).<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#E57785","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":987570,"guid":"5A9E5140CD8911EA8021EB6A2599C9B9","previous_id":986736,"previous_guid":"F38705A0CCE911EAAE4015FA587B06DC","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"CB5CDBF7D14C4AF4B55C347BFF8C0375","order_id":1,"type_id":6,"title":"Section","source":{"title":"Thermal Bonding to Glass"}},{"id":1054724,"guid":"1625F4ABEC484F849E19273790A07D75","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">After cooling, slide a blade between the substrate and weighting slides and gently twist to remove the weighting slide.<\/div><\/div>"}},{"id":1054725,"guid":"0B9D9B573BEC4B82973E3B96C02ACAFC","order_id":2,"type_id":26,"title":"notes","source":{"id":0,"parent_id":0,"uri":"","title":"","body":"<div class = \"text-blocks\"><div class = \"text-block\">NOTE: If the channel scaffolds disbond slightly from the subrate during this step, they can be easily rebonded by breifly reheating the substrate slide at 200 deg C without the weighting slide.<\/div><\/div>","created_on":0,"changed_on":0,"creator":{"name":" ","affiliation":null,"affiliations":[],"username":"","note":null,"link":null,"image":{"source":"","placeholder":""},"badges":[],"verified":0,"is_verified_user":false,"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false},"comments":[]}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#EA9F6C","section_duration":480,"critical":null,"critical_id":null,"duration":30}],"document":"","materials":[],"description":"","changed_on":1612381259}