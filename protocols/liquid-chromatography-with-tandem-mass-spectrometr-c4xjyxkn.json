ases":[],"critical":null},{"id":1846291,"guid":"69741265A5E74C14A38099F4794DBE66","previous_id":1846290,"previous_guid":"134C4BE0471A45A8B0FE7060FCC5F53B","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"2an1v\",\"text\":\"The mass spectrometer cycle was programmed to collect one full MS scan followed by 20\\ndata-dependent MS/MS scans. The MS scans (400-1600 m/z range, 3 x 106\\nAGC target, 100 ms maximum ion time) were collected at a resolution of 70,000\\nat m/z 200 in profile mode. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"sup\",\"offset\":154,\"length\":1}],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90827,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4","cases":[],"critical":null},{"id":1846292,"guid":"CE8A66994AA34F76B5BB52B2D60A85C5","previous_id":1846291,"previous_guid":"69741265A5E74C14A38099F4794DBE66","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"1nu4n\",\"text\":\"The HCD MS/MS spectra (1.6 m/z isolation width, 28%\\ncollision energy, 1 x 105 AGC target, 100 ms maximum ion time) were\\nacquired at a resolution of 17,500 at m/z 200. Dynamic exclusion was set to\\nexclude previously sequenced precursor ions for 30 seconds. Precursor ions with\\n+1, and +7, +8, or higher charge states were excluded from sequencing. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"sup\",\"offset\":76,\"length\":1}],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90827,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"5","cases":[],"critical":null}],"template_id":1,"title":"Liquid Chromatography with Tandem Mass Spectrometry (LC-MS/MS)","title_html":"Liquid Chromatography with Tandem Mass Spectrometry (LC-MS/MS)","type_id":1,"units":[{"id":1,"type_id":3,"name":"µL","can_manage":0,"read_only":0},{"id":2,"