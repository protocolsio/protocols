tical":null},{"id":1919918,"guid":"3A0EB346B59A46B785D6B46084B7285C","previous_id":1919914,"previous_guid":"CD793B2CFF3A4435B2BE783C5FF4133F","section":"\u003cp\u003eMethods\u003c/p\u003e","section_color":"#A492FF","section_duration":3600,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"6oerf\",\"text\":\"Clean each maze with 70% ethanol and let dry before beginning each test.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"7k48j\",\"text\":\"\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"88nln\",\"text\":\" \",\"type\":\"atomic\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[{\"key\":0,\"offset\":0,\"length\":1}],\"data\":{}},{\"key\":\"ca91a\",\"text\":\"\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{\"0\":{\"type\":\"image\",\"mutability\":\"IMMUTABLE\",\"data\":{\"height\":229,\"id\":438785,\"is_video\":false,\"legend\":\"{\\\"blocks\\\":[{\\\"key\\\":\\\"d7usl\\\",\\\"text\\\":\\\"Figure 1.  Maze set-up.\\\",\\\"type\\\":\\\"unstyled\\\",\\\"depth\\\":0,\\\"inlineStyleRanges\\\":[{\\\"style\\\":\\\"bold\\\",\\\"offset\\\":0,\\\"length\\\":11},{\\\"style\\\":\\\"italic\\\",\\\"offset\\\":0,\\\"length\\\":11}],\\\"entityRanges\\\":[],\\\"data\\\":{}}],\\\"entityMap\\\":{}}\",\"mime\":\"image/png\",\"originalFile\":{},\"original_name\":\"image.png\",\"placeholder\":\"https://content.protocols.io/files/pnsdcbnsx.jpg\",\"source\":\"https://content.protocols.io/files/pnsbcbnsx.jpg\",\"type\":1,\"webp_placeholder\":\"https://content.protocols.io/files/pnsccbnsx.webp\",\"webp_source\":\"https://content.protocols.io/files/pnsacbnsx.webp\",\"width\":280}}}}","data":null,"protocol_id":94223,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"2","cases":[],"critical":null},{"id":1919919,"guid":"1433FD919ADF4194B5824629E05B08A0","previous_id":1919918,"previous_guid":"3A0EB346B59A46B785D6B46084B7285C","section":"\u003cp\u003eMethods\u003c/p\u003e","section_color":"#A492FF","section_duration":3600,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"1cihr\",\"text\":\"     \\nStart Ethovision recording before placing rodents in center of maze (if you have multiple Y mazes, you can run cage mates simultaneously).\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94223,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"3","cases":[],"critical":null},{"id":1919920,"guid":"5AAB3ABD9620464A9953766E91EC6E82","previous_id":1919919,"previous_guid":"1433FD919ADF4194B5824629E05B08A0","section":"\u003cp\u003eMethods\u003c/p\u003e","section_color":"#A492FF","section_duration":3600,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"cjj9i\",\"text\":\"Leave the room and record for 8 min before returning animals to their home cage.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94223,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4","cases":[],"critical":null},{"id":1919921,"guid":"A7B4093AE2D7476EB44E4B2780788499","previous_id":1919920,"previous_guid":"5AAB3ABD9620464A9953766E91EC6E82","section":"\u003cp\u003eData Analysis\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"d3qb5\",\"text\":\"The rodent is considered to have entered an arm when their full body minus their tail is in the arm. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94223,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"5","cases":[],"critical":null},{"id":1919924,"guid":"8F4D5222FF17420F939274B780E5FE7B","previous_id":1919921,"previous_guid":"A7B4093AE2D7476EB44E4B2780788499","section":"\u003cp\u003eData Analysis\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":true,"step":"{\"blocks\":[{\"key\":\"9qahj\",\"text\":\"Percent alternation is defined as the number of spontaneous alternations/the total possible triads. For example in this maze, if the animal enters the arms in this order: B C A C A B, you would score themas having 2 spontaneous alternations (BCA and CAB) out of 4 possible triads (BCA, CAC, ACA, and CAB) or 50% alternation.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"bold\",\"offset\":0,\"length\":19},{\"style\":\"italic\",\"offset\":0,\"length\":19}],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94223,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"5.1","cases":[],"critical":null},{"id":1919925,"guid":"20C69A15AA8748F1A4F78C8CE89B37C9","previous_id":1919924,"previous_guid":"8F4D5222FF17420F939274B780E5FE7B","section":"\u003cp\u003eData Analysis\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":true,"step":"{\"blocks\":[{\"key\":\"3den5\",\"text\":\"EthoVision software reports number of alternations and max entries to represent the number of possible triads so percent alternation is simply calculated by number of alternations/max entries. Percent alternation for a healthy mouse should be around 70%.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94223,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"5.2","cases":[],"critical":null},{"id":1919927,"guid":"82731CCA22A844ABB682519C5C7721ED","previous_id":1919925,"previous_guid":"20C69A15AA8748F1A4F78C8CE89B37C9","section":"\u003cp\u003eData Analysis\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":true,"step":"{\"blocks\":[{\"key\":\"eupkj\",\"text\":\"Controls: Check that max entries or distance traveled does not correlate with percent alternation. Correlation between these two variables suggests that the amount the rodent is moving may be skewing the percent alternation redouts and is no longer accurately reflecting working memory performance.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"bold\",\"offset\":0,\"length\":9},{\"style\":\"italic\",\"offset\":0,\"length\":9}],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94223,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"5.3","cases":[],"critical":null}],"template_id":4,"title":"Y-Maze Protocol","title_html":"Y-Maze Protocol","type_id":1,"units":[{"id":1,"type_id":3,"name":"µL","can_manage":0,"read_only":0},{"id":2,"type_id":3,"name":"mL","can_manage":0,"read_only":0},{"id":3,"type_id":3,"name":"L","can_manage":0,"read_only":0},{"id":4,"type_id":3,"name":"µg","can_manage":0,"read_only":0},{"id":5,"type_id":3,"name":"mg","can_manage":0,"read_only":0},{"id":6,"type_id":3,"name":"g","can_manage":0,"read_only":0},{"id":7,"type_id":3,"name":"kg","can_manage":0,"read_only":0},{"id":8,"type_id":3,"name":"ng","can_manage":0,"read_only":0},{"id":9,"type_id":3,"name":"Hz","can_manage":0,"read_only":0},{"id":10,"type_id":24,"name":"°C","can_manage":0,"read_only":0},{"id":11,"type_id":24,"name":"°К","can_manage":0,"read_only":0},{"id":12,"type_id":24,"name":"°F","can_manage":0,"read_only":0},{"id":13,"type_id":25,"name":"Mass Percent","can_manage":0,"read_only":0},{"id":14,"type_id":25,"name":"% volume","can_manage":0,"read_only":0},{"id":15,"type_id":25,"name":"Mass / % volume","can_manage":0,"read_only":0},{"id":16,"type_id":25,"name":"Parts per Million (PPM)","can_manage":0,"read_only":0},{"id":17,"type_id":25,"name":"Parts per Billion (PPB)","can_manage":0,"read_only":0},{"id":18,"type_id":25,"name":"Parts per Trillion (PPT)","can_manage":0,"read_only":0},{"id":19,"type_id":25,"name":"Mole Fraction","can_manage":0,"read_only":0},{"id":20,"type_id":25,"name":"Mole