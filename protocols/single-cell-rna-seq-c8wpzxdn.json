_substep":false,"step":"{\"blocks\":[{\"key\":\"45qka\",\"text\":\"Samples were trypsinised or scraped from the culture surface and placed in a 15mL conical tube.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94895,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"1","cases":[],"critical":null},{"id":1935130,"guid":"7D73D1DB72244AF69300A520DDD1A01B","previous_id":1935129,"previous_guid":"CB30D83CF66345989691160879784ACC","section":"\u003cp\u003eHarvesting cells for single-cell RNA-sequencing\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"2c16t\",\"text\":\"Tubes were centrifuged at 800g in a refrigerated centrifuge for 5 minutes, and the culture media was decanted.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94895,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"2","cases":[],"critical":null},{"id":1935132,"guid":"2C163E21DA494E71B62712D3EF9F3630","previous_id":1935130,"previous_guid":"7D73D1DB72244AF69300A520DDD1A01B","section":"\u003cp\u003eHarvesting cells for single-cell RNA-sequencing\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"9pqmf\",\"text\":\"The pellet was resuspended in 10mL chilled PBS per tube by pipetting, then centrifuged again using the above parameters before decanting the PBS.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94895,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"3","cases":[],"critical":null},{"id":1935133,"guid":"C24544403CDC4907BC1C42EF0D18E523","previous_id":1935132,"previous_guid":"2C163E21DA494E71B62712D3EF9F3630","section":"\u003cp\u003eHarvesting cells for single-cell RNA-sequencing\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"2bj6h\",\"text\":\"Pellets were resuspended in 1mL PBS, and 100,000 cells were transferred to a 1.5mL falcon tube.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94895,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4","cases":[],"critical":null},{"id":1935134,"guid":"381BE222115644A6B6E6CA0E279BF1ED","previous_id":1935133,"previous_guid":"C24544403CDC4907BC1C42EF0D18E523","section":"\u003cp\u003eHarvesting cells for single-cell RNA-sequencing\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"6mcqa\",\"text\":\"These were centrifuged at 1000rpm for 3 minutes at 4°C.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94895,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"5","cases":[],"critical":null},{"id":1935135,"guid":"99636DBDA1A84E1BB8D69130BA1F4F74","previous_id":1935134,"previous_guid":"381BE222115644A6B6E6CA0E279BF1ED","section":"\u003cp\u003eHarvesting cells for single-cell RNA-sequencing\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"f1qvh\",\"text\":\"Cells were resuspended in 20μL chilled DPS.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"2isqd\",\"text\":\"\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94895,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"6","cases":[],"critical":null},{"id":1935136,"guid":"420588B6B8AA4E59919873AA9482B306","previous_id":1935135,"previous_guid":"99636DBDA1A84E1BB8D69130BA1F4F74","section":"\u003cp\u003eHarvesting cells for single-cell RNA-sequencing\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"9n3kp\",\"text\":\"180μL chilled 100% methanol was added dropwise to the cells while gently vortexing to prevent cell clumps, before fixing the cells on ice for 15 minutes. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94895,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"7","cases":[],"critical":null},{"id":1935137,"guid":"E0C9803CA65D4B5FA5D7CA14B9598FFE","previous_id":1935136,"previous_guid":"420588B6B8AA4E59919873AA9482B306","section":"\u003cp\u003eSingle-cell RNA-sequencing protocol\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"3cmqc\",\"text\":\"Between 2400 to 4000 cells were loaded for each sample into a separate channel of a Chromium Chip G for use in the 10X Chromium Controller (cat: PN-1000120). \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94895,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"8","cases":[],"critical":null},{"id":1935139,"guid":"3F07D1C1BD9F4487A3D816DC57DDC85B","previous_id":1935137,"previous_guid":"E0C9803CA65D4B5FA5D7CA14B9598FFE","section":"\u003cp\u003eSingle-cell RNA-sequencing protocol\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"9qbrs\",\"text\":\"The cells were partitioned into nanoliter scale Gel Beads in emulsions (GEMs) and lysed using the 10x Genomics Single Cell 3′ Chip V3.1 GEM, Library and Gel Bead Kit (cat: PN-1000121). \\n\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94895,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"9","cases":[],"critical":null},{"id":1935140,"guid":"D5E90C3232414733864CEC8620D11851","previous_id":1935139,"previous_guid":"3F07D1C1BD9F4487A3D816DC57DDC85B","section":"\u003cp\u003eSingle-cell RNA-sequencing protocol\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"9i4kb\",\"text\":\"cDNA synthesis and library construction were performed as per the manufacturer’s instructions.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94895,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"10","cases":[],"critical":null},{"id":1935141,"guid":"393DDE8335DA48A89642C561DB917D53","previous_id":1935140,"previous_guid":"D5E90C3232414733864CEC8620D11851","section":"\u003cp\u003eSingle-cell RNA-sequencing protocol\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"feqp0\",\"text\":\"The RNA was reversed transcribed and amplified using 12 cycles of PCR.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94895,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"11","cases":[],"critical":null},{"id":1935142,"guid":"8D8394B656ED4E78A04B59F40C3410D2","previous_id":1935141,"previous_guid":"393DDE8335DA48A89642C561DB917D53","section":"\u003cp\u003eSingle-cell RNA-sequencing protocol\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"io6n\",\"text\":\"Libraries were prepared from 10µL of the cDNA and 13 cycles of amplification. Each library was prepared using Single Index Kit T Set A (cat: PN-1000213) and sequenced on the HiSeq4000 system (Illumina) using 100 bp paired-end run at a depth of 20-50 million reads. Libraries were generated in independent runs for the different samples.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94895,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"12","cases":[],"critical":null},{"id":1935143,"guid":"6F8EAC9605FE4CBAB768D9295C50432B","previous_id":1935142,"previous_guid":"8D8394B656ED4E78A04B59F40C3410D2","section":"\u003cp\u003eSingle-cell RNA-sequencing data processing\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"a6nh6\",\"text\":\"Reads were aligned to the human reference genome (Ensembl release 93, GRCh38) using Cell Ranger v3.0.2.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94895,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"13","cases":[],"critical":null},{"id":1935144,"guid":"337D91AF3BB544688D26EDDF97856782","previous_id":1935143,"previous_guid":"6F8EAC9605FE4CBAB768D9295C50432B","section":"\u003cp\u003eSingle-cell RNA-sequencing data processing\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"9vllh\",\"text\":\"The analysis was carried out using Seurat v3.0 (REF- Butler, 2018 ,Stuart, 2019 #645) following Seurat’s standard workflow. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94895,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"14","cases":[],"critical":null},{"id":1935145,"guid":"F2538EA6D4F348FF9C02907971366BA8","previous_id":1935144,"previous_guid":"337D91AF3BB544688D26EDDF97856782","section":"\u003cp\u003eSingle-cell RNA-sequencing data processing\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"347jh\",\"text\":\"Cells expressing fewer than 200 genes were excluded from the subsequent analysis. In addition, we excluded cells with more than 3000 detected genes to remove suspected cell doublets or multiplets.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94895,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"15","cases":[],"critical":null},{"id":1935146,"guid":"FA7B8B75C51F43F7A4EEAFCDC95585D2","previous_id":1935145,"previous_guid":"F2538EA6D4F348FF9C02907971366BA8","section":"\u003cp\u003eSingle-cell RNA-sequencing data processing\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"7e4a9\",\"text\":\"Due to the nature of the cells used for this experiment, a  10% cut-off was applied for the percentage of mitochondrial genes expressed to filter out likely apoptotic cells.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94895,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"16","cases":[],"critical":null},{"id":1935147,"guid":"0620B9E0F02F49BFBBCF53B923D8F327","previous_id":1935146,"previous_guid":"FA7B8B75C51F43F7A4EEAFCDC95585D2","section":"\u003cp\u003eSingle-cell RNA-sequencing data processing\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"a9sca\",\"text\":\"Using default parameters of Seurat, data for each sample were log normalised across cells and the 2000 most highly variable genes identified.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94895,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"17","cases":[],"critical":null},{"id":1935148,"guid":"44093970B0004AB1B302DC4C9CCDE4A7","previous_id":1935147,"previous_guid":"0620B9E0F02F49BFBBCF53B923D8F327","section":"\u003cp\u003eSingle-cell RNA-sequencing data processing\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"f3kq3\",\"text\":\"Using the canonical correlation analysis (‘CCA’) to identify anchors, we integrated the samples using Seurat v 3 (Butler, 2018; Stuart, 2019), followed by regression of the effect of cell cycle and scaling of the data. Dimensional reduction was performed using 50 PCs.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94895,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"18","cases":[],"critical":null},{"id":1935149,"guid":"8E1598EB327E4E0F98CB37B9142FB171","previous_id":1935148,"previous_guid":"44093970B0004AB1B302DC4C9CCDE4A7","section":"\u003cp\u003eSingle-cell RNA-sequencing data processing\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"dlf5q\",\"text\":\"Clustree v0.4.4 and Seurat's plots functions were used to visualise the expression of astrocytic and neuronal marker genes across different cluster resolutions.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94895,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"19","cases":[],"critical":null},{"id":1935150,"guid":"BB53732C884F4280A61853BE3A7C7C19","previous_id":1935149,"previous_guid":"8E1598EB327E4E0F98CB37B9142FB171","section":"\u003cp\u003eSingle-cell RNA-sequencing data processing\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"fv3q6\",\"text\":\"A clustering resolution of 0.25 was selected.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null