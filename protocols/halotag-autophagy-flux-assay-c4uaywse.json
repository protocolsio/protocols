":[],"peer_reviewed":false,"protocol_references":"","public":true,"public_fork_note":"","published_on":1709785327,"references":[],"related_equipments":[],"related_materials":[],"reserved_doi":"10.17504/protocols.io.x54v9pwkmg3e/v1","retraction_reason":null,"samples":{},"shared_access_id":0,"show_comparison":false,"sign_info":null,"space_access":{"can_view":false,"can_edit":false,"can_remove":false,"can_add":false,"can_publish":true,"can_get_doi":true,"can_share":false,"can_move":false,"can_move_outside":false,"can_transfer":false,"can_download":false,"limited_run":false,"limited_private_links":false,"limited_blind_links":false,"is_locked":false},"space_id":34457,"state_version_id":126,"stats":{"is_voted":false,"number_of_views":2,"number_of_steps":24,"number_of_bookmarks":0,"number_of_comments":0,"number_of_bookmarked_comments":0,"number_of_steps_comments":0,"number_of_protocol_comments":0,"number_of_exports":0,"number_of_runs":0,"number_of_votes":0,"number_of_reagents":0,"number_of_equipments":0,"number_of_collections":1,"number_of_forks":{"private":0,"public":0},"number_of_accessible_forks":0},"status":{"id":1,"info":""},"steps":[{"id":1844457,"guid":"E3A50CC60FFF49808DC2CB8C082753DF","previous_id":0,"previous_guid":null,"section":"\u003cp\u003eDay 1: Seeding plates \u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"24rmr\",\"text\":\"Seed 50,000 HeLa cells in each chamber of a 12-well treated tissue culture plate. Maintain cells in DMEM + 10% FBS + 1x Pen-strep.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90722,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"1","cases":[],"critical":null},{"id":1844458,"guid":"3E27A220AF4F4537B3D5217118929109","previous_id":1844457,"previous_guid":"E3A50CC60FFF49808DC2CB8C082753DF","section":"\u003cp\u003eDay 1: Seeding plates \u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"8qesv\",\"text\":\"Grow for 2 days until cells are ~80% confluent\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90722,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"2","cases":[],"critical":null},{"id":1844460,"guid":"F29B4D6A94D245B2BBAE6EAFE6BD90DB","previous_id":1844458,"previous_guid":"3E27A220AF4F4537B3D5217118929109","section":"\u003cp\u003eDay 3: Transient Transfection\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"e4ns6\",\"text\":\"If cells do not need to be transfected, skip to “day 5”\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90722,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"3","cases":[],"critical":null},{"id":1844461,"guid":"1A398C55B2904B05BE36B758F77E7E78","previous_id":1844460,"previous_guid":"F29B4D6A94D245B2BBAE6EAFE6BD90DB","section":"\u003cp\u003eDay 3: Transient Transfection\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"fah48\",\"text\":\"Wash cells into DMEM + 10% FBS without antibiotic prior to transfection.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90722,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4","cases":[],"critical":null},{"id":1844462,"guid":"4204182B29514A9AA5C72C210EDF58CB","previous_id":1844461,"previous_guid":"1A398C55B2904B05BE36B758F77E7E78","section":"\u003cp\u003eDay 3: Transient Transfection\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"1raq\",\"text\":\"Prepare reagents for lipofection with lipofectamine 3000. Each well requires 1.2 µg of pmCherry vector or construct. Prepare a master mix consisting of 1.2 µg of DNA, 125 µL of opti-mem low serum medium, and 2.4 µL of P3000 reagent per well. Prepare an additional mix consisting of 125 µL of opti-mem and 2.4 µL of lipofectamine 3000 per well. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90722,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"5","cases":[],"critical":null},{"id":1844463,"guid":"C19416B531E44E659712F4E8287126FB","previous_id":1844462,"previous_guid":"4204182B29514A9AA5C72C210EDF58CB","section":"\u003cp\u003eDay 3: Transient Transfection\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"c0o5d\",\"text\":\"Combine the DNA + P3000 and lipofectamine solutions together, and allow to incubate for 10 minutes at room temperature.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90722,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"6","cases":[],"critical":null},{"id":1844464,"guid":"C0D228D92B274B588B8FC1320B389911","previous_id":1844463,"previous_guid":"C19416B531E44E659712F4E8287126FB","section":"\u003cp\u003eDay 3: Transient Transfection\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"dhu1n\",\"text\":\"After 10 min, dispense 250 µL of combined DNA/P3000/lipofectamine mixture into each well to be transfected.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90722,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"7","cases":[],"critical":null},{"id":1844465,"guid":"3FE498DA93744118AA2B10BF2A338C8C","previous_id":1844464,"previous_guid":"C0D228D92B274B588B8FC1320B389911","section":"\u003cp\u003eDay 3: Transient Transfection\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"ft13u\",\"text\":\"Return plates to incubator, and incubate for two days before conducting experiment\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90722,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"8","cases":[],"critical":null},{"id":1844497,"guid":"F6270BA8CEDE48BF9674EA9A05C7EF01","previous_id":1844465,"previous_guid":"3FE498DA93744118AA2B10BF2A338C8C","section":"\u003cp\u003eDay 4: Swap medium\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"c0k6h\",\"text\":\"Swap medium of transfected cells with fresh DMEM + 10% FBS + Pen-strep\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90722,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"9","cases":[],"critical":null},{"id":1844499,"guid":"55596326FA1046249628DD98BF2EB0E8","previous_id":1844497,"previous_guid":"F6270BA8CEDE48BF9674EA9A05C7EF01","section":"\u003cp\u003eDay 5: Autophagy Induction and Cell Collection\u003c/p\u003e","section_color":"#FFED92","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"1sp60\",\"text\":\"Replace growth medium with labeling medium consisting of growth medium + 100 nM Janelia Fluor 549-conjugated HaloLigand.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90722,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"10","cases":[],"critical":null},{"id":1844500,"guid":"E2F323B6C08A4F949CE032A1129533C0","previous_id":1844499,"previous_guid":"55596326FA1046249628DD98BF2EB0E8","section":"\u003cp\u003eDay 5: Autophagy Induction and Cell Collection\u003c/p\u003e","section_color":"#FFED92","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"f0f6s\",\"text\":\"Incubate at 37 C for 20 min.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90722,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"11","cases":[],"critical":null},{"id":1844501,"guid":"A0115B23C27E4C66BB457BC8311B0D5A","previous_id":1844500,"previous_guid":"E2F323B6C08A4F949CE032A1129533C0","section":"\u003cp\u003eDay 5: Autophagy Induction and Cell Collection\u003c/p\u003e","section_color":"#FFED92","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"4rfal\",\"text\":\"Aspirate off labeling medium and wash with PBS or growth medium.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90722,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"12","cases":[],"critical":null},{"id":1844502,"guid":"A1110F5511204F6F845858714208DD67","previous_id":1844501,"previous_guid":"A0115B23C27E4C66BB457BC8311B0D5A","section":"\u003cp\u003eDay 5: Autophagy Induction and Cell Collection\u003c/p\u003e","section_color":"#FFED92","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"8ut7f\",\"text\":\"Wash cells into either growth medium + DMSO, growth medium + 10 µM Oligomycin/ 5 µM Antimycin, or Earle’s Balanced Salt solution to start the experiment.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90722,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"13","cases":[],"critical":null},{"id":1844503,"guid":"8357300F78034554B874A48D0B3EB2AE","previous_id":1844502,"previous_guid":"A1110F5511204F6F845858714208DD67","section":"\u003cp\u003eDay 5: Autophagy Induction and Cell Collection\u003c/p\u003e","section_color":"#FFED92","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"5luh9\",\"text\":\"Return cells to incubator for either 3 H (for starvation autophagy measurements via EBSS) or 6 H (for mitochondrial depolarization via OA).\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90722,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"14","cases":[],"critical":null},{"id":1844504,"guid":"AEFC67D0196A4418A19C8FB1A5E5E9CF","previous_id":1844503,"previous_guid":"8357300F78034554B874A48D0B3EB2AE","section":"\u003cp\u003eDay 5: Autophagy Induction and Cell Collection\u003c/p\u003e","section_color":"#FFED92","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"5l6o9\",\"text\":\"Collect cells via scraping into ice-cold PBS.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90722,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"15","cases":[],"critical":null},{"id":1844505,"guid":"6A2E65D4390045789D30D64B580D7005","previous_id":1844504,"previous_guid":"AEFC67D0196A4418A19C8FB1A5E5E9CF","section":"\u003cp\u003eDay 5: Autophagy Induction and Cell Collection\u003c/p\u003e","section_color":"#FFED92","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"64c14\",\"text\":\"Pellet cells via tabletop centrifuge, aspirate off supernatant, and snap freeze in liquid nitrogen. Store at -80 C until next day.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90722,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"16","cases":[],"critical":null},{"id":1844508,"guid":"4C7807A221244F218905CA5F5EC831FD","previous_id":1844505,"previous_guid":"6A2E65D4390045789D30D64B580D7005","section":"\u003cp\u003eDay 6: HaloTag gel shift measurement and quantification\u003c/p\u003e","section_color":"#EA9F6C","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"193v1\",\"text\":\"Lyse cells via resuspension in 30 µL lysis buffer and 30 minute incubation at 4 C (50 mM HEPES pH 7.4, 150 mM NaCl, 0.5% NP-40, 1 mM TCEP, cOmplete protease inhibition tablet (Roche).\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90722,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"17","cases":[],"critical":null},{"id":1844509,"guid":"8EE06EE7180648F8B1D369D1BFEB0EB8","previous_id":1844508,"previous_guid":"4C7807A221244F218905CA5F5EC831FD","section":"\u003cp\u003eDay 6: HaloTag gel shift measurement and quantification\u003c/p\u003e","section_color":"#EA9F6C","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"50bqh\",\"text\":\"Measure protein concentration via a 96-well plate BCA assay. Briefly, pipette 200 µL of reconstituted BCA reagent (Thermo Fisher) in each well of a 96-well plate. Include 5 additional wells for a 3 mg/mL, 2 mg/mL, 1 mg/mL, PBS blank, and Lysis buffer blank. Into each 200 µL well, pipette 1 µL of sample, standard, or blank. Cover plate and incubate at 37 C for 30 min before measuring the absorbance of each well at 560 nm. If protein concentration is especially high, it might be necessary to make a dilute sample of each well sample prior to performing BCA assay.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90722,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"18","cases":[],"critical":null},{"id":1844510,"guid":"067044773C494E9BBE790827BE5A04EF","previous_id":1844509,"previous_guid":"8EE06EE7180648F8B1D369D1BFEB0EB8","section":"\u003cp\u003eDay 6: HaloTag gel shift measurement and quantification\u003c/p\u003e","section_color":"#EA9F6C","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"36ruc\",\"text\":\"In excel, subtract blank readings from measured absorbances, and plot a standard curve using the BSA standards. Using the standard curve, determine the concentration of each protein samples, and the volume necessary to load 20 µg each.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90722,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"19","cases":[],"critical":null},{"id":1844511,"guid":"FF65E791E6CC4747BB7512ED8A1902D5","previous_id":1844510,"previous_guid":"067044773C494E9BBE790827BE5A04EF","section":"\u003cp\u003eDay 6: HaloTag gel shift measurement and quantification\u003c/p\u003e","section_color":"#EA9F6C","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"9hg9i\",\"text\":\"Prepare samples for SDS-PAGE via addition of loading buffer and 25 mM TCEP. Heat samples at 95 C for 2 min before loading onto gel. Omit a ladder.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90722,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"20","cases":[],"critical":null},{"id":1844512,"guid":"A80C750FA5644CE3A0842395453E5398","previous_id":1844511,"previous_guid":"FF65E791E6CC4747BB7512ED8A1902D5","section":"\u003cp\u003eDay 6: HaloTag gel shift measurement and quantification\u003c/p\u003e","section_color":"#EA9F6C","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"8lbp5\",\"text\":\"Run gel at 120 V until the dye band has reached the bottom of the gel. Gently cut off dye band at the bottom of the gel, and transfer the gel to a clean container. Rinse in deionized water before imaging on a ChemiDoc using the AlexaFluor 548 channel. Use AutoExpose protocol unless trying to image particularly faint bands.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90722,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"21","cases":[],"critical":null},{"id":1844513,"guid":"C46FDFE52AFC473EB19B179416CFAF2B","previous_id":1844512,"previous_guid":"A80C750FA5644CE3A0842395453E5398","section":"\u003cp\u003eDay 6: HaloTag gel shift measurement and quantification\u003c/p\u003e","section_color":"#EA9F6C","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"82g4j\",\"text\":\"Open image file in Fiji, define rectangular selection that encompasses the first lane, and use the Gel Analyzer tool to highlight each lane before plotting the trace.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90722,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"22","cases":[],"critical":null},{"id":1844514,"guid":"0E205A971EF446CB80461BF5B22C23DA","previous_id":1844513,"previous_guid":"C46FDFE52AFC473EB19B179416CFAF2B","section":"\u003cp\u003eDay 6: HaloTag gel shift measurement and quantification\u003c/p\u003e","section_color":"#EA9F6C","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"5o1qc\",\"text\":\"In the trace window, define a baseline, and then measure the area under each section of the curve that describes the band of interest. The highest MW band should represent the Su9-halo or the LC3-halo reporter, while the lowest MW band should represent processed halo.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90722,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"23","cases":[],"critical":null},{"id":1844515,"guid":"0461A2F77AC14A5DB98BDB80A0C8BA47","previous_id":1844514,"