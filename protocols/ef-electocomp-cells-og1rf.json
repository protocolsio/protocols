{"id":44857,"title":"Ef_electocomp_cells_OG1RF","title_html":"<p>Ef_electocomp_cells_OG1RF<\/p>","image":{"source":"https:\/\/www.protocols.io\/img\/default_protocol.png","placeholder":"https:\/\/www.protocols.io\/img\/default_protocol.png"},"doi":null,"doi_status":0,"uri":"ef-electocomp-cells-og1rf-bp2zmqf6","type_id":1,"template_id":4,"published_on":1606234247,"parent_protocols":[],"parent_collections":[],"cited_protocols":[],"version_id":0,"version_data":{"id":"0","code":"bp2zmqf6","parent_id":0,"parent_uri":null,"is_same_owner":false,"has_pending_merge_request":false,"has_approved_merge_request":false},"created_on":1606232054,"modified_on":null,"categories":null,"public":1,"is_unlisted":0,"creator":{"name":"Eadewunm ","affiliation":null,"affiliations":[],"username":"w2z23403r203","note":null,"link":null,"image":{"source":"\/img\/avatars\/002.png","placeholder":"\/img\/avatars\/002.png"},"badges":[{"id":4,"image":{"source":"\/img\/badges\/gold.svg","placeholder":"\/img\/badges\/gold.svg"},"name":"Gold power author!"}],"verified":0,"is_verified_user":false,"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false},"journal":null,"journal_name":null,"journal_link":null,"article_citation":null,"has_versions":0,"link":null,"total_collections":0,"number_of_steps":14,"authors":[{"name":"Elizabeth Fozo","affiliation":"In-house protocol","affiliations":[],"username":"elizabeth-fozo","note":null,"link":null,"image":{"source":"\/img\/avatars\/017.png","placeholder":"\/img\/avatars\/017.png"},"badges":[],"verified":1,"is_verified_user":true,"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false}],"versions":[],"groups":[],"is_owner":1,"has_subprotocols":0,"is_subprotocol":0,"is_bookmarked":0,"can_claim_authorship":0,"can_accept_authorship":0,"can_be_copied":1,"can_remove_fork":1,"fork_id":null,"url":"https:\/\/www.protocols.io\/view\/ef-electocomp-cells-og1rf-bp2zmqf6","forks_count":{"private":0,"public":0},"access":{"can_view":1,"can_remove":1,"can_add":0,"can_edit":0,"can_publish":0,"can_get_doi":1,"can_share":1,"can_move":1,"can_move_outside":1,"can_transfer":1,"can_download":1,"is_locked":0},"guid":"AB61A8C02E6911EB923A21FC28F9E72F","state_version_id":235,"steps":[{"id":1081201,"guid":"F98647302E6A11EB923A21FC28F9E72F","previous_id":null,"previous_guid":null,"modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"7571A3F123BF40E5A3C08EE8998BB93D","order_id":1,"type_id":6,"title":"Section","source":{"title":"E. faecalis Electrocompetent Cells (with Lysozyme)"}},{"id":1054724,"guid":"94171C8998E04B5FAD740215C750283F","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Inoculate 5-10 mL BHI or THB (I add fusidic acid at 25 ug\/mL but not rifampicin) and incubate o\/n at 37\u00b0C.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1081203,"guid":"0B6B45902E6B11EB923A21FC28F9E72F","previous_id":1081201,"previous_guid":"F98647302E6A11EB923A21FC28F9E72F","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"A74DA5AE7789443BA91F7A3FA5F5BC00","order_id":1,"type_id":6,"title":"Section","source":{"title":"E. faecalis Electrocompetent Cells (with Lysozyme)"}},{"id":1054724,"guid":"94FDD811FF9E484792A5453CC8B3E95A","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Dilute o\/n culture 1:50 or 1:100 with in 100 mL THB (with FA25) and incubate until culture OD at 600 nm reaches 0.5-1.0 (original protocol dilutes 1:10)<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1081204,"guid":"1C392EA02E6B11EB923A21FC28F9E72F","previous_id":1081203,"previous_guid":"0B6B45902E6B11EB923A21FC28F9E72F","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"88653F28E95046749EA19C91BABF4DAB","order_id":1,"type_id":6,"title":"Section","source":{"title":"E. faecalis Electrocompetent Cells (with Lysozyme)"}},{"id":1054724,"guid":"44DCB8FD0CF64B3E92D7F636201474B1","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Chill cells on ice for 15-20 min. Use floor centrifuge to pellet cells (in Oakridge tubes or 50 mL Falcon tubes) at 6,000 rpm for 10 minutes (this step doesn\u2019t need to be at 4 oC).<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1081205,"guid":"24A937602E6B11EB923A21FC28F9E72F","previous_id":1081204,"previous_guid":"1C392EA02E6B11EB923A21FC28F9E72F","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"3B25F04E91214DB9BB241CD209C734D2","order_id":1,"type_id":6,"title":"Section","source":{"title":"E. faecalis Electrocompetent Cells (with Lysozyme)"}},{"id":1054724,"guid":"C300CED5F9B44165B1BF7BA5C4624F82","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Resuspend pellet in 2 mL 10% glycerol or sterile H2O and split into two 1.5 mL tubes. Pellet 13,000 rpm for 1 min. in a tabletop centrifuge (this step doesn\u2019t need to be at 4 oC).<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1081206,"guid":"2AD4A7502E6B11EB923A21FC28F9E72F","previous_id":1081205,"previous_guid":"24A937602E6B11EB923A21FC28F9E72F","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"E77195471AC347DF9D2692F468CAED88","order_id":1,"type_id":6,"title":"Section","source":{"title":"E. faecalis Electrocompetent Cells (with Lysozyme)"}},{"id":1054724,"guid":"7FD1A2BD94CA4CFE9F2AC1DB15A73552","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span>Resuspend each pellet in 500 \u00b5L lysozyme solution (LS) containing <\/span><span style = \"font-weight:bold;\">25 \u03bcg\/ml lysozyme* (add just before use-see recipe in Materials<\/span><span>). Incubate at 37\u00b0C for 20 min (water bath or incubator).<\/span><\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1081207,"guid":"2FEA3A702E6B11EB923A21FC28F9E72F","previous_id":1081206,"previous_guid":"2AD4A7502E6B11EB923A21FC28F9E72F","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"66917B0B150D40E9A5ACC326883C0ED7","order_id":1,"type_id":6,"title":"Section","source":{"title":"E. faecalis Electrocompetent Cells (with Lysozyme)"}},{"id":1054724,"guid":"10C5E5B31F674DFE90C05EDCFB004E18","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Pellet as in step 4.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1081208,"guid":"34FB39B02E6B11EB923A21FC28F9E72F","previous_id":1081207,"previous_guid":"2FEA3A702E6B11EB923A21FC28F9E72F","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"5A1D92EE80974DE2B213BBD0B04CC0C8","order_id":1,"type_id":6,"title":"Section","source":{"title":"E. faecalis Electrocompetent Cells (with Lysozyme)"}},{"id":1054724,"guid":"CE3ECC6204FA46BA922B9502F948D975","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span>Wash 3 times with 1 ml ice-cold electroporation buffer (EB \u2013 see recipe below).<\/span><span style = \"font-weight:bold;\"> Keep pellets and buffers cold from this point forward!<\/span><span><\/span><\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1081209,"guid":"3B16A4102E6B11EB923A21FC28F9E72F","previous_id":1081208,"previous_guid":"34FB39B02E6B11EB923A21FC28F9E72F","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"B7219DF03D414AD7BABD2E49E171DBA7","order_id":1,"type_id":6,"title":"Section","source":{"title":"E. faecalis Electrocompetent Cells (with Lysozyme)"}},{"id":1054724,"guid":"DD6564C9FAB944318BF93E58B02A1483","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Resuspend each tube in 300 uL EB\/tube (total from both tubes pooled will be approx. 500-600 uL, including volume of cells) and split into 100 uL aliquots. Store at -80\u00b0C. Efficiency will decrease after freezing<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1081210,"guid":"694574B02E6B11EB923A21FC28F9E72F","previous_id":1081209,"previous_guid":"3B16A4102E6B11EB923A21FC28F9E72F","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"165D620F1C004B6D945D2BACA36E3D4C","order_id":1,"type_id":6,"title":"Section","source":{"title":"Electroporation"}},{"id":1054724,"guid":"32740216208647A2A6BE86378AD10DCB","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Thaw cells on ice. I use ~50 uL per electroporation.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1081214,"guid":"C1E63F902E6C11EB923A21FC28F9E72F","previous_id":1081210,"previous_guid":"694574B02E6B11EB923A21FC28F9E72F","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"2F09FF0B5AD04D0FACA2FBCB6FDECD09","order_id":1,"type_id":6,"title":"Section","source":{"title":"Electroporation"}},{"id":1054724,"guid":"080ED09D6B5B4EDEB89EFF82A3D31C09","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Chill DNA and tubes for recovery (1 tube per electroporation). Transfer an appropriate volume of STHB to a test tube (do NOT put on ice). Chill electrocuvettes (1 per electroporation and a few extras, we use 0.1 cm cuvettes).<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1081215,"guid":"C882C0D02E6C11EB923A21FC28F9E72F","previous_id":1081214,"previous_guid":"C1E63F902E6C11EB923A21FC28F9E72F","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"9D2BBFB01FF54FC2B4A941A7AFD4EB3B","order_id":1,"type_id":6,"title":"Section","source":{"title":"Electroporation"}},{"id":1054724,"guid":"DF104FE4563148D0912B3B0B3B4F09B6","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Open program on Gene Pulser (Dawn has a standard E. faecalis one saved, settings are .6 kV, 200 \u03a9, 25 uF).<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1081217,"guid":"D62B75102E6C11EB923A21FC28F9E72F","previous_id":1081215,"previous_guid":"C882C0D02E6C11EB923A21FC28F9E72F","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"4B75E219115D4ABA8A63D5782E2B7CCB","order_id":1,"type_id":6,"title":"Section","source":{"title":"Electroporation"}},{"id":1054724,"guid":"62A74E3A67DC40429A297D86B8841DE2","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span>Transfer 50 uL cells to the first microfuge tube. Add ice-cold DNA (~100 ng, 1-3 uL of most minipreps will be fine) and pipette a couple of times to mix. Immediately transfer to pre-chilled electrocuvette and electroporate. Immediately add 1 mL STHB to the electrocuvette after the pulse is completed. The time constant should be ~5.0 (higher for pure cells, lower with more or low-quality DNA). Continue even if it\u2019s lower or it arcs. Pipette up and down a few times to wash cells out of cuvette. Transfer back to the microfuge tube you mixed your cells and DNA in during step 2. <\/span><span style = \"font-weight:bold;\">Keep at room temp\/recovery temperature from here on out!<\/span><span><\/span><\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1081218,"guid":"086BAF402E6D11EB923A21FC28F9E72F","previous_id":1081217,"previous_guid":"D62B75102E6C11EB923A21FC28F9E72F","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"1D205D97DBC84F5EB4AEB1CB78F3F0DB","order_id":1,"type_id":6,"title":"Section","source":{"title":"Electroporation"}},{"id":1054724,"guid":"0CC473117D2B4E068146DA66F93637AC","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Recover cells with static incubation at 37\u00b0C (or whatever temperature you need) for 2-4 hours<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1081220,"guid":"37BD2DF02E6D11EB923A21FC28F9E72F","previous_id":1081218,"previous_guid":"086BAF402E6D11EB923A21FC28F9E72F","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"129D41049B2B45C88C88D061DFF3F0FD","order_id":1,"type_id":6,"title":"Section","source":{"title":"Electroporation"}},{"id":1054724,"guid":"3E0F06CBA4C24E8F81791AEBA0C6E57C","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Plate 50 uL undiluted cells. Pellet the remaining volume and resuspend in 100 uL STHB. Plate 50 uL of the resuspended cells. Save the rest on your benchtop overnight. Incubate plates at 37 oC until colonies appear (1-2 days).<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0}],"document":null,"materials":[],"description":null,"changed_on":1606234247}