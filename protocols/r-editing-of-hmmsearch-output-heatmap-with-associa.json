uping<\/div><div class = \"text-block\">dat <- read.csv(file=\"data.csv\", header=T, na.strings=c(\"\",\"-\"))<\/div><div class = \"text-block\">dat2  <- data.frame(dat$group,dat$protein_ID)<\/div><div class = \"text-block\">dat2 <- dat2[!duplicated(dat2$dat.protein_ID),]<\/div><div class = \"text-block\">colnames(dat2) <- c(\"group\",\"protein_ID\")<\/div><div class = \"text-block\">#import groups<\/div><div class = \"text-block\">chlorophytes <- subset(dat2,dat2$group==\"chlorophytes\")<\/div><div class = \"text-block\">charophytes <- subset(dat2,dat2$group==\"charophytes\")<\/div><div class = \"text-block\">liverworts<- subset(dat2,dat2$group==\"liverworts\")<\/div><div class = \"text-block\">mosses<- subset(dat2,dat2$group==\"mosses\")<\/div><div class = \"text-block\">lycophytes<- subset(dat2,dat2$group==\"lycophytes\")<\/div><div class = \"text-block\">gymnosperms<- subset(dat2,dat2$group==\"gymnosperms\")<\/div><div class = \"text-block\">angiosperms<- subset(dat2,dat2$group==\"angiosperms\")<\/div><div class = \"text-block\">monocots<- subset(dat2,dat2$group==\"monocots\")<\/div><div class = \"text-block\">eudicots<- subset(dat2,dat2$group==\"eudicots\")<\/div><div class = \"text-block\">cls <- list(a.chrolophytes=chlorophytes$protein_ID,<\/div><div class = \"text-block\">b.charophytes=charophytes$protein_ID,<\/div><div class = \"text-block\">c.liverworts=liverworts$protein_ID,<\/div><div class = \"text-block\">d.mosses=mosses$protein_ID,<\/div><div class = \"text-block\">e.lycophytes=lycophytes$protein_ID,<\/div><div class = \"text-block\">f.gymnosperms=gymnosperms$protein_ID,<\/div><div class = \"text-block\">g.angiosperms=angiosperms$protein_ID,<\/div><div class = \"text-block\">h.monocots=monocots$protein_ID,<\/div><div class = \"text-block\">i.eudicots=eudicots$protein_ID)<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1020205,"guid":"852196E0EDC611EAA1805F4F720D9F76","previous_id":1020152,"previous_guid":"A5950D10EDAF11EAA440010544A70BB8","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"0ED87A8D88BF49EA9A457CEF77BF2A35","order_id":1,"type_id":6,"title":"Section","source":{"title":"tree drawing"}},{"id":1054724,"guid":"5B6140716E684744ADA361FF08DE29B2","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">#add group to tree<\/div><div class = \"text-block\">tree3 <- groupOTU(tree2,cls)<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1020206,"guid":"077C1A70EDC711EAA1805F4F720D9F76","previous_id":1020207,"previous_guid":"154B9090EDC711EAA1805F4F720D9F76","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"1C7B978C01FD48D2910DCE0812BD0ED8","order_id":1,"type_id":6,"title":"Section","source":{"title":"tree drawing"}},{"id":1054724,"guid":"5DC60EB5C72746A6B323FCA813A88646","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">#only includes support values > 0.7<\/div><div class = \"text-block\">q <- ggtree(tree3)<\/div><div class = \"text-block\">d <- q$data<\/div><div class = \"text-block\">d <- d[!d$isTip,]<\/div><div class = \"text-block\">d$label <- as.numeric(d$label)<\/div><div class = \"text-block\">d <- d[d$label > 0.7,]<\/div><div class = \"text-block\">#tree drawing<\/div><div class = \"text-block\">obj <- ggtree(tree3) + aes(color=group) +<\/div><div class = \"text-block\">scale_color_manual(values=c(\"deepskyblue\",\"green\",\"seagreen\",\"seagreen4\",<\/div><div class = \"text-block\">\"orange2\",\"chocolate4\",\"red\",\"blue\",\"gray40\")) +<\/div><div class = \"text-block\">geom_tiplab(size=6) +<\/div><div class = \"text-block\">geom_text(data=d,aes(label=label,hjust=1.5,vjust=-0.5,size=6))<\/div><div class = \"text-block\">ggsave(\"strongsupport.tree.pdf\",obj,width=60, height=90,limitsize=FALSE)<\/div><div class = \"text-block\">#done<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1020207,"guid":"154B9090EDC711EAA1805F4F720D9F76","previous_id":1020205,"previous_guid":"852196E0EDC611EAA1805F4F720D9F76","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"6B280BF168674E6781DD67E0FE7AFCB5","order_id":1,"type_id":6,"title":"Section","source":{"title":"tree drawing"}},{"id":1054724,"guid":"1B51BE253476440CBC2B2E59F6FA0C2A","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">#tree drawing<\/div><div class = \"text-block\">obj <- ggtree(tree3) + aes(color=group) +<\/div><div class = \"text-block\">scale_color_manual(values=c(\"deepskyblue\",\"green\",\"seagreen\",<\/div><div class = \"text-block\">\"seagreen4\",\"orange2\",\"chocolate4\",\"red\",\"blue\",\"gray40\")) +<\/div><div class = \"text-block\">geom_tiplab() +<\/div><div class = \"text-block\">geom_text2(aes(subset=!isTip,label=label,hjust=1.5,vjust=-0.5))<\/div><div class = \"text-block\">ggsave(\"all_support_value.tree.pdf\",obj, width=30, height=50, limitsize=FALSE)<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1020208,"guid":"51EF6620EDC711EAA1805F4F720D9F76","previous_id":1020206,"previous_guid":"077C1A70EDC711EAA1805F4F720D9F76","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"36FC2924344843B3AACDCBB273C91429","order_id":1,"type_id":6,"title":"Section","source":{"title":"Making heatmap from output of HMMER website search"}},{"id":1054724,"guid":"AA56F999C61E408DAE9ED92E7CA83931","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">#libraries<\/div><div class = \"text-block\">library(reshape2)<\/div><div class = \"text-block\">library(dplyr)<\/div><div class = \"text-block\">library(ggplot2)<\/div><div class = \"text-block\">library(aplot)<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#84CE84","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1020209,"guid":"797C3740EDC711EAA1805F4F720D9F76","previous_id":1020208,"previous_guid":"51EF6620EDC711EAA1805F4F720D9F76","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"ACADB1F097644D7CAB455FF630678BAC","order_id":1,"type_id":6,"title":"Section","source":{"title":"Making heatmap from output of HMMER website search"}},{"id":1054724,"guid":"BCD846A25B004B44BE965A613C17FAA4","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">#generate data frame from \"dat\"<\/div><div class = \"text-block\">dat3 <- data.frame(dat$protein_ID,dat$hmm_name,dat$i.Evalue)<\/div><div class = \"text-block\">colnames(dat3) <- c(\"protein_ID\",\"hmm_name\",\"i.Evalue\")<\/div><div class = \"text-block\">#remove duplicate, \"i.Evalue\" is ordered in the ascending manner <\/div><div class = \"text-block\">dat4 <- dat3[!duplicated(dat3[1:2]),]<\/div><div class = \"text-block\">#make table of i.Evalue, ordered by protein_ID and hmm_hame<\/div><div class = \"text-block\">dat5 <- dcast(dat4,dat4$protein_ID ~ dat4$hmm_name)<\/div><div class = \"text-block\">#format for ggtree<\/div><div class = \"text-block\">dat6 <- melt(dat5)<\/div><div class = \"text-block\">colnames(dat6) <- c(\"protein_ID\",\"hmm_name\",\"i.Evalue\")<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#84CE84","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1020210,"guid":"9FA4E480EDC711EAA1805F4F720D9F76","previous_id":1020209,"previous_guid":"797C3740EDC711EAA1805F4F720D9F76","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"9EB84E8958D044CE923F39D13FFF9CEE","order_id":1,"type_id":6,"title":"Section","source":{"title":"Making heatmap from output of HMMER website search"}},{"id":1054724,"guid":"13AD47694DC34E0DBAA36F5941F71C89","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">#order of domain, by preference<\/div><div class = \"text-block\">domorder <- c(\"PLAC8\",\"MCAfunc\",\"DUF2985\",\"Pkinase_Tyr\",\"UDPGT\", \"WI12\",\"WRKY\")<\/div><div class = \"text-block\">dat7 <- dat6 %>%<\/div><div class = \"text-block\">mutate(hmm_name=factor(hmm_name,levels=domorder),ordered=TRUE)<\/div><div class = \"text-block\">#dat7 is the data for heatmap<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#84CE84","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1020211,"guid":"AB1B9D90EDC711EAA1805F4F720D9F76","previous_id":1020212,"previous_guid":"FA7B7090EDC711EAA1805F4F720D9F76","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"10492F957258499483E0917606BEA7D6","order_id":1,"type_id":6,"title":"Section","source":{"title":"Combining tree and heatmap"}},{"id":1054724,"guid":"D7BD28899D1042C9858F7F7DC06B76E0","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">#drawing<\/div><div class = \"text-block\">obj2 <- ggtree(tree2) + geom_tiplab(align = TRUE,size=0)<\/div><div class = \"text-block\">hm <- ggplot(dat7, aes(x=hmm_name,y=protein_ID)) +<\/div><div class = \"text-block\">geom_tile(aes(fill=i.Evalue),color=\"gray50\") +<\/div><div class = \"text-block\">scale_fill_gradient(low=\"blue\",high=\"yellow\",na.value=\"gray80\",limits=c(0,0.001)) +<\/div><div class = \"text-block\">scale_x_discrete(expand=c(1,0)) +<\/div><div class = \"text-block\">th