orking"},"steps":[{"id":1764621,"guid":"9D40A8603AC611EEA29DB1D0701B3C69","previous_id":0,"previous_guid":null,"section":"\u003cp\u003eSetup\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"92p3k\",\"text\":\"Lay a bench pad on the table and place the beam on top of the two empty cages ~3-4 cm of overhang.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":86461,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"1","cases":[],"critical":null},{"id":1764623,"guid":"B0155DF03AC611EEA29DB1D0701B3C69","previous_id":1764621,"previous_guid":"9D40A8603AC611EEA29DB1D0701B3C69","section":"\u003cp\u003eSetup\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"a34db\",\"text\":\"Place the test subjectâ€™s home cage on the narrow end sideways so that the bream rests on top of the side wall.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":86461,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"2","cases":[],"critical":null},{"id":1764624,"guid":"B80B2DA03AC611EEA29DB1D0701B3C69","previous_id":1764623,"previous_guid":"B0155DF03AC611EEA29DB1D0701B3C69","section":"\u003cp\u003eSetup\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"31cuh\",\"text\":\"Clean the beam with 70% ethanol and dry thoroughly before starting and after completing each cage of mice. During test days, spray paper towels with ethanol and wipe down the wire grid.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":86461,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"3","cases":[],"critical":null},{"id":1764625,"guid":"C97B98403AC611EEA29DB1D0701B3C69","previous_id":1764624,"previous_guid":"B80B2DA03AC611EEA29DB1D0701B3C69","section":"\u003cp\u003eTraining (Days 1-2)\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"5tom1\",\"text\":\"Gently place the mouse at the widest edge of the beam and allow it to traverse across the beam.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":86461,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4","cases":[],"critical":null},{"id":1764626,"guid":"D3163E503AC611EEA29DB1D0701B3C69","previous_id":1764625,"previous_guid":"C97B98403AC611EEA29DB1D0701B3C69","section":"\u003cp\u003eTraining (Days 1-2)\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":true,"step":"{\"blocks\":[{\"key\":\"af4ih\",\"text\":\"For the first trial, the mouse will need to be gently guided to cross. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":86461,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4.1","cases":[],"critical":null},{"id":1764629,"guid":"DBCA06D03AC611EEA29DB1D0701B3C69","previous_id":1764626,"previous_guid":"D3163E503AC611EEA29DB1D0701B3C69","section":"\u003cp\u003eTraining (Days 1-2)\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":true,"step":"{\"blocks\":[{\"key\":\"al8d4\",\"text\":\"If the mouse is stationary or attempts to turn, nudge it in the forward direction.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":86461,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4.2","cases":[],"critical":null},{"id":1764630,"guid":"E0AAA7403AC611EEA29DB1D0701B3C69","previous_id":1764629,"previous_guid":"DBCA06D03AC611EEA29DB1D0701B3C69","section":"\u003cp\u003eTraining (Days 1-2)\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":true,"step":"{\"blocks\":[{\"key\":\"b8h1i\",\"text\":\"Additional mice in the home cage can be placed in a spare, clean cage or left within the home cage.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":86461,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4.3","cases":[],"critical":null},{"id":1764631,"guid":"F865F0B03AC611EEA29DB1D0701B3C69","previous_id":1764630,"previous_guid":"E0AAA7403AC611EEA29DB1D0701B3C69","section":"\u003cp\u003eTraining (Days 1-2)\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"5aesf\",\"text\":\"Once the mouse has reached the end and touched any part of the home cage, the trial is complete.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":86461,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"5","cases":[],"critical":null},{"id":1764632,"guid":"051A59E03AC711EEA29DB1D0701B3C69","previous_id":1764631,"previous_guid":"F865F0B03AC611EEA29DB1D0701B3C69","section":"\u003cp\u003eTraining (Days 1-2)\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"7pkbt\",\"text\":\"Clean any debris off the beam and train the next mouse. Repeat until all mice in the cage have been trained 5 times on the beam.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":86461,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"6","cases":[],"critical":null},{"id":1764633,"guid":"0B8A4CE03AC711EEA29DB1D0701B3C69","previous_id":1764632,"previous_guid":"051A59E03AC711EEA29DB1D0701B3C69","section":"\u003cp\u003eTraining (Days 1-2)\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":true,"step":"{\"blocks\":[{\"key\":\"9k9gq\",\"text\":\"Mice should need minimal guidance to cross the beam by the 3rd trial even on the first day. They should require little if any guidance by the second day.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":86461,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"6.1","cases":[],"critical":null},{"id":1764634,"guid":"0F14AC203AC711EEA29DB1D0701B3C69","previous_id":1764633,"previous_guid":"0B8A4CE03AC711EEA29DB1D0701B3C69","section":"\u003cp\u003eTraining (Days 1-2)\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"1fna8\",\"text\":\"Train the next mouse and repeat until all mice with the same cage have been trained 5 times. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":86461,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"7","cases":[],"critical":null},{"id":1764635,"guid":"16592D303AC711EEA29DB1D0701B3C69","previous_id":1764634,"previous_guid":"0F14AC203AC711EEA29DB1D0701B3C69","section":"\u003cp\u003eTraining (Days 1-2)\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"8ncmo\",\"text\":\"Return all mice into their home cage. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":86461,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"8","cases":[],"critical":null},{"id":1764637,"guid":"220B68A03AC711EEA29DB1D0701B3C69","previous_id":1764635,"previous_guid":"16592D303AC711EEA29DB1D0701B3C69","section":"\u003cp\u003eTraining (Days 1-2)\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"3eaga\",\"text\":\"Clean the beam with ethanol.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":86461,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"9","cases":[],"critical":null},{"id":1764638,"guid":"26188C703AC711EEA29DB1D0701B3C69","previous_id":1764637,"previous_guid":"220B68A03AC711EEA29DB1D0701B3C69","section":"\u003cp\u003eTraining (Days 1-2)\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"1l7d3\",\"text\":\"Remove any bedding/debris from the bench pad.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":86461,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"10","cases":[],"critical":null},{"id":1764641,"guid":"3AE430003AC711EEA29DB1D0701B3C69","previous_id":1764638,"previous_guid":"26188C703AC711EEA29DB1D0701B3C69","section":"\u003cp\u003eTest Day (Day 3)\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"c9ktq\",\"text\":\"Place the wire grid (squares - 1 cm2) on the beam surface leaving a 1 cm space between the grid and the beam surface.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":86461,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"11","cases":[],"critical":null},{"id":1764643,"guid":"59373FC03AC711EEA29DB1D0701B3C69","previous_id":1764641,"previous_guid":"3AE430003AC711EEA29DB1D0701B3C69","section":"\u003cp\u003eTest Day (Day 3)\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":true,"step":"{\"blocks\":[{\"key\":\"fnvkh\",\"text\":\"The grid should not wobble. If it does, tighten all the bottom where it touches the clear part of the beam. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":86461,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"11.1","cases":[],"critical":null},{"id":1764645,"guid":"609B2FB03AC711EEA29DB1D0701B3C69","previous_id":1764643,"previous_guid":"59373FC03AC711EEA29DB1D0701B3C69","section":"\u003cp\u003eTest Day (Day 3)\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"8qi51\",\"text\":\"Write down the mouse ID for all mice in the cage on the index card and leave room for tally marks before each ID to show the trial number.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":86461,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"12","cases":[],"critical":null},{"id":1764647,"guid":"660AF0C03AC711EEA29DB1D0701B3C69","previous_id":1764645,"previous_guid":"609B2FB03AC711EEA29DB1D0701B3C69","section":"\u003cp\u003eTest Day (Day 3)\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"88fab\",\"text\":\"Start recording on the camcorder to capture the mouse ID and trial number. Without stopping the recording, gently place the mouse at the widest part of the beam and allow it to cross the beam.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":86461,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"13","cases":[],"critical":null},{"id":1764649,"guid":"748C03A03AC711EEA29DB1D0701B3C69","previous_id":1764647,"previous_guid":"660AF0C03AC711EEA29DB1D0701B3C69","section":"\u003cp\u003eTest Day (Day 3)\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":true,"step":"{\"blocks\":[{\"key\":\"79lkl\",\"text\":\"Make sure to hold the camera perpendicular to the beam and at the same height to obtain the clearest recordings of the mouseâ€™s paws on the wire gird.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":86461,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"13.1","cases":[],"critical":null},{"id":1764650,"guid":"791360803AC711EEA29DB1D0701B3C69","previous_id":1764649,"previous_guid":"748C03A03AC711EEA29DB1D0701B3C69","section":"\u003cp\u003eTest Day (Day 3)\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":true,"step":"{\"blocks\":[{\"key\":\"cv500\",\"text\":\"Adjust the distance of the camera from the mouse, depending on its speed (faster mice=longer distance) to capture the whole body at all times\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":86461,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"13.2","cases":[],"critical":null},{"id":1764651,"guid":"7D8E61503AC711EEA29DB1D0701B3C69","previous_id":1764650,"previous_guid":"791360803AC711EEA29DB1D0701B3C69","section":"\u003cp\u003eTest Day (Day 3)\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"279q6\",\"text\":\"Once the mouse has reached the end and touched any part of the home cage, the trial is complete. Stop the video recording.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":86461,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"14","cases":[],"critical":null},{"id":1764652,"guid":"81F257B03AC711EEA29DB1D0701B3C69","previous_id":1764651,"previous_guid":"7D8E61503AC711EEA29DB1D0701B3C69","section":"\u003cp\u003eTest Day (Day 3)\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"3h1es\",\"text\":\"Clean any debris off the beam and train the next mouse. Repeat until all mice in the cage have been tested 5 times on the beam.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":86461,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"15","cases":[],"critical":null},{"id":1764653,"guid":"8974A5103AC711EEA29DB1D0701B3C69","previous_id":1764652,"previous_guid":"81F257B03AC711EEA29DB1D0701B3C69","section":"\u003cp\u003eTest Day (Day 3)\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"9a77s\",\"text\":\"Test the next mouse and repeat until all mice with the same cage have been tested 5 times. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":86461,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"16","cases":[],"critical":null},{"id":1764654,"guid":"8E1B49C03AC711EEA29DB1D0701B3C69","previous_id":1764653,"previous_guid":"8974A5103AC711EEA29DB1D0701B3C69","section":"\u003cp\u003eTest Day (Day 3)\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"3k8ok\",\"text\":\"Return all mice into their home cage. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":86461,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"17","cases":[],"critical":null},{"id":1764655,"guid":"929623803AC711EEA29DB1D0701B3C69","previous_id":1764654,"previous_guid":"8E1B49C03AC711EEA29DB1D0701B3C69","section":"\u003cp\u003eTest Day (Day 3)\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"k