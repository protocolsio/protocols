vate":0,"public":0},"access":{"can_view":1,"can_remove":1,"can_add":0,"can_edit":0,"can_publish":0,"can_get_doi":1,"can_share":1,"can_move":1,"can_move_outside":1,"can_transfer":1,"can_download":1,"is_locked":0},"guid":"A7AF9140511B11EBA6B829D490996442","state_version_id":251,"steps":[{"id":1101209,"guid":"04A74410512611EBB7D6A5FF82BDEB64","previous_id":null,"previous_guid":null,"modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"AE70213AF87E42F6A11952DAD36E77B1","order_id":1,"type_id":6,"title":"Section","source":{"title":"steps"}},{"id":1054724,"guid":"4E860355F0F84680BF3E06AAD1307414","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Image plates with photo doc machine. Choose plates where colonies are well spread, and not clumped - especially against the sides, where the image will be cropped.\u00a0<\/div><\/div>"}},{"id":1054725,"guid":"F81F8C710D4F43F5A1CF54BADDF7EB2B","order_id":2,"type_id":26,"title":"notes","source":{"id":0,"parent_id":0,"uri":"","title":"","body":"<div class = \"text-blocks\"><div class = \"text-block\">Overcrowded plates have inherently smaller colonies. Ensure that the images are all taken at the same distance\/zoom.\u00a0<\/div><\/div>","created_on":0,"changed_on":0,"creator":{"name":" ","affiliation":null,"affiliations":[],"username":"","note":null,"link":null,"image":{"source":"","placeholder":""},"badges":[],"verified":0,"is_verified_user":false,"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false},"comments":[]}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1101210,"guid":"23DB3DF0512611EBB7D6A5FF82BDEB64","previous_id":1101209,"previous_guid":"04A74410512611EBB7D6A5FF82BDEB64","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"0676084EEFB1463BBD7B9B22122615F2","order_id":1,"type_id":6,"title":"Section","source":{"title":"steps"}},{"id":1054724,"guid":"A08E291A068B4A50B3FE2C7078FE9733","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Download FIJI from \u201c<\/div><div class = \"text-block\"><a href=\"https:\/\/imagej.net\/Fiji\" style = \"text-decoration:underline;color:blue;cursor:pointer;\"><span style = \":;\">https:\/\/imagej.net\/Fiji<\/span><\/a><\/div><div class = \"text-block\">\u201d if you don\u2019t already have the program. This is an open-source distribution of ImageJ with many tools\/plugins built-in.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1101211,"guid":"397BBDB0512611EBB7D6A5FF82BDEB64","previous_id":1101210,"previous_guid":"23DB3DF0512611EBB7D6A5FF82BDEB64","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"0455B9644DB84A18B5D8A323C12CFBCC","order_id":1,"type_id":6,"title":"Section","source":{"title":"steps"}},{"id":1054724,"guid":"4CA5115BD4284FFA9C96CB3DFEF84E52","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span>Open FIJI, and open your image (<\/span><span style = \"font-style:italic;\">File > Open... > [Select yourFIle] > Open<\/span><span style = \"font-style:italic;\">)<\/span><\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1101212,"guid":"4A86FDE0512611EBB7D6A5FF82BDEB64","previous_id":1101211,"previous_guid":"397BBDB0512611EBB7D6A5FF82BDEB64","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"27E76581FCAB4C958D019C90EFE17062","order_id":1,"type_id":6,"title":"Section","source":{"title":"steps"}},{"id":1054724,"guid":"3431401DAF974911B1ABB8EFCEDB041C","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">First, we must select your image and crop out most of the non-particle background (in this case, the walls of the plate). You have two options here:<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1101213,"guid":"541A02D0512611EBB7D6A5FF82BDEB64","previous_id":1101212,"previous_guid":"4A86FDE0512611EBB7D6A5FF82BDEB64","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"4D4F2594B93D4C8D91DE5B5B79609283","order_id":1,"type_id":6,"title":"Section","source":{"title":"steps"}},{"id":1054724,"guid":"8B32A334732549598F836895A756C467","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span style = \"font-weight:bold;\">Recommended:<\/span><span style = \"font-weight:bold;font-style:italic;\"> <\/span><span style = \"font-style:italic;\">Edit > Selection > Fit Circle to Image. <\/span><span>This will create a circular selection area that contains the majority of your colonies. It may not be perfectly centered on the plate, but generally, it's good enough to get a representative selection of colonies. The default threshold is usually fine, but you may test other values. Optionally, you can also use <\/span><span style = \"font-style:italic;\">Edit > Selection > Enlarge <\/span><span>and input a value to grow or shrink the selection (a negative value to shrink) if you think this better represents the colonies.<\/span><\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1101214,"guid":"81FB9DD0512611EBB7D6A5FF82BDEB64","previous_id":1101213,"previous_guid":"541A02D0512611EBB7D6A5FF82BDEB64","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"E6783165FF044A6185542534CD223CAD","order_id":1,"type_id":6,"title":"Section","source":{"title":"steps"}},{"id":1054724,"guid":"8900E45FB42146DC819A76BA9635497E","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Use the oval select tool to manually draw out the region of the plate containing the colonies, cropping out the sides of the plate. This option is not suited for batching but may help resolve anomalous data from batching.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1101215,"guid":"96567E30512611EBB7D6A5FF82BDEB64","previous_id":1101214,"previous_guid":"81FB9DD0512611EBB7D6A5FF82BDEB64","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"9E9F61CC468E4950838C85BCA567A949","order_id":1,"type_id":6,"title":"Section","source":{"title":"steps"}},{"id":1054724,"guid":"9F9070B151F44B0EB9E860463EBD0D4F","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span style = \"font-style:italic;\">Edit > Clear Outside <\/span><span>to remove most of the background.\u00a0<\/span><\/div><\/div>"}},{"id":1054725,"guid":"57DFBCED10A54983930D22B84AEB1FF9","order_id":2,"type_id":26,"title":"notes","source":{"id":0,"parent_id":0,"uri":"","title":"","body":"<div class = \"text-blocks\"><div class = \"text-block\">We do not want to use the crop tool here, because this changes the resolution of the image, which influences the particle analyzer - by using a set of images all the same resolution, we can properly compare colony size between images.<\/div><\/div>","created_on":0,"changed_on":0,"creator":{"name":" ","affiliation":null,"affiliations":[],"username":"","note":null,"link":null,"image":{"source":"","placeholder":""},"badges":[],"verified":0,"is_verified_user":false,"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false},"comments":[]}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1101216,"guid":"CCAC8EC0512611EBB7D6A5FF82BDEB64","previous_id":1101215,"previous_guid":"96567E30512611EBB7D6A5FF82BDEB64","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"840532B5D2BF440D8A10717993BCB2AB","order_id":1,"type_id":6,"title":"Section","source":{"title":"steps"}},{"id":1054724,"guid":"3720D1610C934215A2157B3C37DB00C3","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span style = \"font-style:italic;\">Image > Type > 8-Bit<\/span><span>\u00a0- This allows the remaining commands to work properly.<\/span><\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1101217,"guid":"DCF14AA0512611EBB7D6A5FF82BDEB64","previous_id":1101216,"previous_guid":"CCAC8EC0512611EBB7D6A5FF82BDEB64","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"088F4836BC5B4D689F48CEAE13F40ED5","order_id":1,"type_id":6,"title":"Section","source":{"title":"steps"}},{"id":1054724,"guid":"D9AA0F660A0749B2BC1F8A3CBCBF6D96","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span style = \"font-style:italic;\">Process > Subtract Background <\/span><span>- This seems to make the thresholding step more reliable but may be optional. Try with and without, and see what works better.<\/span><\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1101218,"guid":"F6D71530512611EBB7D6A5FF82BDEB64","previous_id":1101217,"previous_guid":"DCF14AA0512611EBB7D6A5FF82BDEB64","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"7B12C254E8054DD18B14F83782213567","order_id":1,"type_id":6,"title":"Section","source":{"title":"steps"}},{"id":1054724,"guid":"DFE9F654129347E6BBA3AE4E776FFE8C","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">The thresholding step has a large impact on the final analysis, and again you have two options here:<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1101219,"guid":"0F2A7FF0512711EBB7D6A5FF82BDEB64","previous_id":1101218,"previous_guid":"F6D71530512611EBB7D6A5FF82BDEB64","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"C12F37A18F0142B684BB6D1BF6CD8760","order_id":1,"type_id":6,"title":"Section","source":{"title":"steps"}},{"id":1054724,"guid":"7A47DBFD60694DBF8902E8B300E9903A","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span>Manually:<\/span><span style = \"font-style:italic;\"> Image > Adjust > Threshold <\/span><span>- From here you must play with the settings until you feel you have filtered out any non-colony particles but not caused any colonies to shrink\/change shape. The ideal setting will not be consistent between plates due to differences in image brightness. This option is not suited for batch processing.<\/span><\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1101220,"guid":"1CBD1420512711EBB7D6A5FF82BDEB64","previous_id":1101219,"previous_guid":"0F2A7FF0512711EBB7D6A5FF82BDEB64","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"DEA8D42D6A2E4C81A7128FFB9D87C930","order_id":1,"type_id":6,"title":"Section","source":{"title":"steps"}},{"id":1054724,"guid":"72471142558D4A209F24F7EC793D52FC","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span style = \"font-weight:bold;\">Recommended:<\/span><span style = \"font-weight:bold;font-style:italic;\"> <\/span><span style = \"font-style:italic;\">Image > Adjust > Auto Threshold<\/span><span> - You can test it with Try All method first to see the difference between the options (the names will be below each image in a tiny font) - MaxEntropy seems to be a decent middle ground that removes erroneous particles while preserving colony shape.<\/span><\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1101222,"guid":"793AC080512711EBB7D6A5FF82BDEB64","previous_id":1101220,"previous_guid":"1CBD1420512711EBB7D6A5FF82BDEB64","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"3472BED3200E495793BAD0FC7627AABE","order_id":1,"type_id":6,"title":"Section","source":{"title":"steps"}},{"id":1054724,"guid":"1AECEAFA9CA14893A42563049BB524FB","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span style = \"font-style:italic;\">Process > Binary > Make Binary<\/span><span>\u00a0- Required for steps 10 and 11<\/span><\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1101223,"guid":"79440F50512711EBB7D6A5FF82BDEB64","previous_id":1101222,"previous_guid":"793AC080512711EBB7D6A5FF82BDEB64","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"FCD3487FD493487E8E291E0EAA6FF576","order_id":1,"type_id":6,"title":"Section","source":{"title":"steps"}},{"id":1054724,"guid":"A19DCFEF52DE435D8DD40C69A8DE179D","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span style = \"font-style:italic;\">Process > Binary > Fill Holes<\/span><span>\u00a0- This will fill in holes in the particles, which may appear if colonies are particularly shiny.<\/span><\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1101224,"guid":"794D8530512711EBB7D6A5FF82BDEB64","previous_id":1101223,"previous_guid":"79440F50512711EBB7D6A5FF82BDEB64","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"CEDB4AA2BD8F44B4A8AAA686ACE8276D","order_id":1,"type_id":6,"title":"Section","source":{"title":"steps"}},{"id":1054724,"guid":"2C26C74599A84199A1E1F0C5398B451B","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span style = \"font-style:italic;\">Process > Binary > Watershed<\/span><span> -\u00a0This draws a one-pixel wide line where the software detects two separate particles\/colonies are merging. If you aren\u2019t confident in the software or zero colonies are touching, you may exclude this step.<\/span><\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1101225,"guid":"79574930512711EBB7D6A5FF82BDEB64","previous_id":1101224,"previous_guid":"794D8530512711EBB7D6A5FF82BDEB64","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"8050AF7658DA4DE3B191F360E3961BD4","order_id":1,"type_id":6,"title":"Section","source":{"title":"steps"}},{"id":1054724,"guid":"272C59A91CC941B6870EA9D879D10C84","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span style = \"font-style:italic;\">Analyze > Analyze Particles<\/span><span>\u00a0- There are a few settings potentially worth modifying from their default options :<\/span><\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1101226,"guid":"8CF7DA40512711EBB7D6A5FF82BDEB64","previous_id":1101225,"previous_guid":"79574930512711EBB7D6A5FF82BDEB64","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"3AA8C0006C014E29BB72C7937C46DFD1","order_id":1,"type_id":6,"title":"Section","source":{"title":"steps"}},{"id":1054724,"guid":"E312B9B05A2746DABBD6D560FCB00EFD","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Size - This setting is not intuitive, as the units (squared inches\/pixels) don't easily correspond to the real world colony size. I recommend running this first default and using the \u201cArea\u201d analysis output to get a reasonable idea of how small your smallest colonies are - from here you can set more exact size boundaries.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1101227,"guid":"9819E3A0512711EBB7D6A5FF82BDEB64","previous_id":1101226,"previous_guid":"8CF7DA40512711EBB7D6A5FF82BDEB64","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"4103C558D6F24AE5991B75F2548EAD24","order_id":1,"type_id":6,"title":"Section","source":{"title":"steps"}},{"id":1054724,"guid":"FB1CC9398BB041D98E73D22FC5BA88AE","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Circularity - This essential setting defines how circular our particles must be to get included in the analysis. Typically 0.60 - 1.00 is a safe range to exclude all remaining background.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1101228,"guid":"A0BA7240512711EBB7D6A5FF82BDEB64","previous_id":1101227,"previous_guid":"9819E3A0512711EBB7D6A5FF82BDEB64","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"D1CBD2A4242B4B9F9B75C138A6F61E2E","order_id":1,"type_id":6,"title":"Section","source":{"title":"steps"}},{"id":1054724,"guid":"4EC4B9C70A5A4EC09D641D221CC72BD5","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Checkboxes: Display Results, Exclude on edges, Clear results, Summarize<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1101229,"guid":"BBDC6650512711EBB7D6A5FF82BDEB64","previous_id":1101228,"previous_guid":"A0BA7240512711EBB7D6A5FF82BDEB64","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"DEED7557D2304DF4828F943C9D48CC6A","order_id":1,"type_id":6,"title":"Section","source":{"title":"steps"}},{"id":1054724,"guid":"BAD649CECE334A648206000ADFC0E14C","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Check the resulting image to ensure only colonies were analyzed. Particles will be numbered on the new image, so you may reference them against the tabulated data. A summary window will also be generated containing the averaged data of each particle analysis you run. The tables can be copied directly to Excel.\u00a0<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0}],"document":"","materials":[],"description":"<div class = \"text-blocks\"><div class = \"text-block\">ImageJ\/FIJI Particle Analyzer<\/div><\/div>","changed_on":1610052485}