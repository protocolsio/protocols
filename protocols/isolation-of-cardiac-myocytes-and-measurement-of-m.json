[{"id":1054723,"guid":"9A1D967C34D442FF875F2AFED3232973","order_id":1,"type_id":6,"title":"Section","source":{"title":"Experimental setup"}},{"id":1054724,"guid":"C1164812903F4D60BAF6F99166F60DD2","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Connect a thermometer to the outflow of the Langendorff apparatus, to measure the temperature of the heated solution.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#84CE84","section_duration":0,"critical":[],"critical_id":null,"duration":0},{"id":837472,"guid":"482BAFE0FBF611E989469F4A167D63E5","previous_id":837471,"previous_guid":"4824AB00FBF611E989469F4A167D63E5","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"E16F2B9380994691830D2EF7926085AF","order_id":1,"type_id":6,"title":"Section","source":{"title":"Experimental setup"}},{"id":1054724,"guid":"9788325276034BC4962AC22724546EF7","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span>Fill the Langendorff apparatus with pre-heated buffer solution with 1,25 mM of Ca<\/span><span style = \"vertical-align:super;\">2+<\/span><span>. Eliminate possible air bubbles. Adjust the peristaltic pump to a flow rate of 5,5 mL\/min at outflow from the cannula.<\/span><\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#84CE84","section_duration":0,"critical":[],"critical_id":null,"duration":0},{"id":837474,"guid":"483B1930FBF611E989469F4A167D63E5","previous_id":837472,"previous_guid":"482BAFE0FBF611E989469F4A167D63E5","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"EBADA1F2156A4DF4A8F4C0847780AD17","order_id":1,"type_id":6,"title":"Section","source":{"title":"Myocyte isolation"}},{"id":1054724,"guid":"BEAFCB77F42C4D739D8C664544C857DF","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Inject mouse with heparin solution (5000 U\/Kg), via intraperitoneal injection.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#FFED92","section_duration":0,"critical":[],"critical_id":null,"duration":0},{"id":837475,"guid":"4842BA50FBF611E989469F4A167D63E5","previous_id":837474,"previous_guid":"483B1930FBF611E989469F4A167D63E5","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"A127F48A95204777B7A6D1CF022F7FBC","order_id":1,"type_id":6,"title":"Section","source":{"title":"Myocyte isolation"}},{"id":1054724,"guid":"4AEFF5970E5C4CB59EC3EA07418F1C6D","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">After 30 min, anesthetize rat with a single intraperitoneal injection of sodium thiopental (50 mg\/kg). When fully anesthetized (no response to strong foot pinch), euthanize the animal by cervical dislocation.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#FFED92","section_duration":0,"critical":[],"critical_id":null,"duration":0},{"id":837476,"guid":"484AF7B0FBF611E989469F4A167D63E5","previous_id":837475,"previous_guid":"4842BA50FBF611E989469F4A167D63E5","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"B9AF845CDAB44FCBB7F9D8360700D2E1","order_id":1,"type_id":6,"title":"Section","source":{"title":"Myocyte isolation"}},{"id":1054724,"guid":"AA0AE486A1C44A708A19E85C2C631A90","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span>Open the chest and identify the aorta. Excise the heart, being careful not to damage the aorta. Place the heart in 4 \u00b0C, pre-oxygenated buffer solution with 1,25 mM Ca<\/span><span style = \"vertical-align:super;\">2+<\/span><span>.<\/span><\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#FFED92","section_duration":0,"critical":[],"critical_id":null,"duration":0},{"id":837477,"guid":"48530E00FBF611E989469F4A167D63E5","previous_id":837476,"previous_guid":"484AF7B0FBF611E989469F4A167D63E5","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"88BE012EE41F4BFD8A9371C3E23F846D","order_id":1,"type_id":6,"title":"Section","source":{"title":"Myocyte isolation"}},{"id":1054724,"guid":"0CD48F7F41CF49D99F8933AF4452C8CE","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Cannulate the heart by the aorta in the cannula attached to the Langendorff system. Secure the aorta to the cannula by tying a loop of silk suture.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#FFED92","section_duration":0,"critical":[],"critical_id":null,"duration":0},{"id":837478,"guid":"485D4730FBF611E989469F4A167D63E5","previous_id":837477,"previous_guid":"48530E00FBF611E989469F4A167D63E5","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"A243ED90785141ECBBE4E7CAE7BFE8C3","order_id":1,"type_id":6,"title":"Section","source":{"title":"Myocyte isolation"}},{"id":1054724,"guid":"A69C22E7FF514CF49BBE0D066E8AEE31","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span>Perfuse the heart with a buffer solution with 1,25 mM Ca<\/span><span style = \"vertical-align:super;\">2+<\/span><span> at a flow of 5,5 ml\/min for 10 min.<\/span><\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#FFED92","section_duration":0,"critical":[],"critical_id":null,"duration":0},{"id":837479,"guid":"4866E420FBF611E989469F4A167D63E5","previous_id":837478,"previous_guid":"485D4730FBF611E989469F4A167D63E5","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"E82B252610794BE1B748830676D26CDE","order_id":1,"type_id":6,"title":"Section","source":{"title":"Myocyte isolation"}},{"id":1054724,"guid":"BD5F6112E0054A259B50BE8A04F06E7A","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span>Switch the solution to Ca<\/span><span style = \"vertical-align:super;\">2+<\/span><span> free buffer and perfuse the heart with this solution at a flow of 7,5 ml\/min for 5 min.<\/span><\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#FFED92","section_duration":0,"critical":[],"critical_id":null,"duration":0},{"id":837480,"guid":"4870A820FBF611E989469F4A167D63E5","previous_id":837479,"previous_guid":"4866E420FBF611E989469F4A167D63E5","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"B2162A79EDF547F1A88772A459AB2EE5","order_id":1,"type_id":6,"title":"Section","source":{"title":"Myocyte isolation"}},{"id":1054724,"guid":"298264AAF1EC403E9F94C9F44A06C719","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Switch the solution to digestion buffer and perfuse the heart with this solution at a flow of 7,5 ml\/min for 5 min. Recirculate the solution with the enzyme. Keep the perfusion until the heart become flaccid and pale (20 \u2013 30 minutes approximately).<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#FFED92","section_duration":0,"critical":[],"critical_id":null,"duration":0},{"id":837481,"guid":"487AE150FBF611E989469F4A167D63E5","previous_id":837480,"previous_guid":"4870A820FBF611E989469F4A167D63E5","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"3C4635F41F6E4F4CB09A86D4D5941F34","order_id":1,"type_id":6,"title":"Section","source":{"title":"Myocyte isolation"}},{"id":1054724,"guid":"6928DAF2EDFC4B3E8A5885A9CCF4B22D","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span>Remove heart from cannula and place in petri dish with buffer solution with 50 \u00b5M Ca<\/span><span style = \"vertical-align:super;\">2+<\/span><span>. Cut the heart into small chunks and pipette several times with a transfer pipette to further disperse cells.<\/span><\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#FFED92","section_duration":0,"critical":[],"critical_id":null,"duration":0},{"id":837482,"guid":"4884A550FBF611E989469F4A167D63E5","previous_id":837481,"previous_guid":"487AE150FBF611E989469F4A167D63E5","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"1BA7D6C85D0E4F639740DD8032991263","order_id":1,"type_id":6,"title":"Section","source":{"title":"Myocyte isolation"}},{"id":1054724,"guid":"A6A2551333AB46BABB41817363F88F22","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Filter the cell suspension into two 15 ml conical tube through a 250 \u03bcm nylon mesh filter.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#FFED92","section_duration":0,"critical":[],"critical_id":null,"duration":0},{"id":837483,"guid":"488F2CA0FBF611E989469F4A167D63E5","previous_id":837482,"previous_guid":"4884A550FBF611E989469F4A167D63E5","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"44919F87172B477780C3EEC74F225C8F","order_id":1,"type_id":6,"title":"Section","source":{"title":"Myocyte isolation"}},{"id":1054724,"guid":"0546441FDB3D48249FB02DDD0005EA37","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Centrifuge the cell suspension for 3 minutes, 35g.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#FFED92","section_duration":0,"critical":[],"critical_id":null,"duration":0},{"id":837484,"guid":"4899DB00FBF611E989469F4A167D63E5","previous_id":837483,"previous_guid":"488F2CA0FBF611E989469F4A167D63E5","