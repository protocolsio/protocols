rk":1,"fork_id":48107,"url":"https:\/\/www.protocols.io\/view\/rapid-quantification-of-cellulose-nanocrystals-by-bwgvpbw6","forks_count":{"private":0,"public":0},"access":{"can_view":1,"can_remove":0,"can_add":0,"can_edit":0,"can_publish":0,"can_get_doi":0,"can_share":1,"can_move":1,"can_move_outside":1,"can_transfer":1,"can_download":1,"is_locked":0},"guid":"324D8DE0F94C478D9D779C9BEB24926B","state_version_id":0,"steps":[{"id":1210993,"guid":"75CCE7530A784A51AF6FBB218FD78161","previous_id":null,"previous_guid":null,"modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"14869ABF301F4F708166E5774B0B7B93","order_id":1,"type_id":6,"title":"Section","source":{"title":"Standard calibration curve"}},{"id":1054724,"guid":"142B53DDA54C468EAB323616FD276F25","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">For calibration curve, prepare different CNC solutions in DI water (up to 0.5% w\/v).<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1210994,"guid":"8FAB2637F08C4FAEB54CBD687894E968","previous_id":1210993,"previous_guid":"75CCE7530A784A51AF6FBB218FD78161","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"71564C2F696D4160AB00BF3666BF5FD9","order_id":1,"type_id":6,"title":"Section","source":{"title":"Standard calibration curve"}},{"id":1054724,"guid":"78249EB02E6548B68DC063B66FBCD38E","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">In flat-bottomed black 96 plate, place 100 \u00b5L of sample, 20 \u00b5L of 5M KOH and 80 \u00b5L of CW reagent (in this order). KOH is needed for basic conditions (pH=12-13).<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1210995,"guid":"4D83A319A66F4AA2AE8349AC2CE6D26C","previous_id":1210994,"previous_guid":"8FAB2637F08C4FAEB54CBD687894E968","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"DC988721C7434D339465842EF1AA53B6","order_id":1,"type_id":6,"title":"Section","source":{"title":"Standard calibration curve"}},{"id":1054724,"guid":"9985B89697884B7F9C9DE7592F03E3FE","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Fluorescence signal can be quantified using plate reader and report in the instrument\u2019s Relative Fluorescence Units (RFU). Excitation is measured at 355 nm and emission at 433 nm. The plate should be continuously shaken to prevent CNC sedimentation and temperature should be kept at 30 \u00b0C. <\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1210996,"guid":"7DCBEBD4BF4B4EE490158A693258D87F","previous_id":1210995,"previous_guid":"4D83A319A66F4AA2AE8349AC2CE6D26C","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"B1F3739663154A548B6D53A8B6E7FDD3","order_id":1,"type_id":6,"title":"Section","source":{"title":"Standard calibration curve"}},{"id":1054724,"guid":"7735971DCDD34648A269320EEFEE6783","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Fluorescence is read in kinetic mode every 10 min with maximal incubation time of 60 min until plateau is reached. <\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":3600},{"id":1210997,"guid":"616BCD662B6340ADB5082364FA1C4683","previous_id":1210996,"previous_guid":"7DCBEBD4BF4B4EE490158A693258D87F","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"9D9C475F76104B918624F92A8DC4E4D9","order_id":1,"type_id":6,"title":"Section","source":{"title":"CNC production from Whatman filter paper and total estimation"}},{"id":1054724,"guid":"AE2CC314FC20407F9ABD6E14278E2ADB","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Grind and dry Whatman filter paper 1.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":3600,"critical":null,"critical_id":null,"duration":0},{"id":1210998,"guid":"97B07597554F483FB1388D9F44474171","previous_id":1210997,"previous_guid":"616BCD662B6340ADB5082364FA1C4683","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"CB1B763CCA25471AB9A47E709E9D6A86","order_id":1,"type_id":6,"title":"Section","source":{"title":"CNC production from Whatman filter paper and total estimation"}},{"id":1054724,"guid":"6C04282EB9C54E21BAA4C2EFB6CAAA9C","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Add the ground dried filter paper sample to 64% aqueous sulfuric acid at 1:20 ratio (w\/w).<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":3600,"critical":null,"critical_id":null,"duration":0},{"id":1210999,"guid":"A194825AC92E4315A7A8E970A9B7B75E","previous_id":1210998,"previous_guid":"97B07597554F483FB1388D9F44474171","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"09720902064B4C69B5373F5F8D4B94DD","order_id":1,"type_id":6,"title":"Section","source":{"title":"CNC production from Whatman filter paper and total estimation"}},{"id":1054724,"guid":"8939316B38914A09AAC162BCE34FD995","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Incubate the mixture for 45 minutes at 45 \u00b0C. <\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":3600,"critical":null,"critical_id":null,"duration":0},{"id":1211000,"guid":"6CCDB5C32D8948BCAD9564FBEB7511D6","previous_id":1210999,"previous_guid":"A194825AC92E4315A7A8E970A9B7B75E","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"7BD5D488078E4B41BF7294C3016C464C","order_id":1,"type_id":6,"title":"Section","source":{"title":"CNC production from Whatman filter paper and total estimation"}},{"id":1054724,"guid":"3CFA29DD37AE4036BE1AC17769853159","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Stop the reaction by diluting the mixture 10-fold with cold water.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":3600,"critical":null,"critical_id":null,"duration":0},{"id":1211001,"guid":"C0EED3DCDA084E12855D76054E3A49EC","previous_id":1211000,"previous_guid":"6CCDB5C32D8948BCAD9564FBEB7511D6","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"CF9AA13C8D144D9C926EB2E816FF15AA","order_id":1,"type_id":6,"title":"Section","source":{"title":"CNC production from Whatman filter paper and total estimation"}},{"id":1054724,"guid":"ABEAEB2279DE401E95BDF26C2934B71F","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Remove the acid from the CNCs by centrifugation (10,000 rpm) for 15 min at 10 \u00b0C.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":3600,"critical":null,"critical_id":null,"duration":900},{"id":1211002,"guid":"3F90A1511AD74408B48854ACF81D9361","previous_id":1211001,"previous_guid":"C0EED3DCDA084E12855D76054E3A49EC","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"F86CA514D96C42D4A84C11410A21E093","order_id":1,"type_id":6,"title":"Section","source":{"title":"CNC production from Whatman filter paper and total estimation"}},{"id":1054724,"guid":"583B3AFB567A4108B9546FF49730F6E1","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Decanting the liquid, and re-suspend the pellet in DIW for three repeating washes.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":3600,"critical":null,"critical_id":null,"duration":2700},{"id":1211003,"guid":"E59BDF3A6F2A4C6088A87B34FAC84295","previous_id":1211002,"previous_guid":"3F90A1511AD74408B48854ACF81D9361","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"4BAA63A57BAD471C8EE6532CC0B41226","order_id":1,"type_id":6,"title":"Section","source":{"title":"CNC production from Whatman filter paper and total estimation"}},{"id":1054724,"guid":"D232305724604049A38081E478A40D2B","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"> Re-suspend the CNC in 30 ml DIW, and dialyze against deionized water until a constant pH is reached. <\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":3600,"critical":null,"critical_id":null,"duration":0},{"id":1211004,"guid":"73DCFC77419D41FAAB2DE99DD2D0F93B","previous_id":1211003,"previous_guid":"E59BDF3A6F2A4C6088A87B34FAC84295","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"90B01F36800642CFA5965F43C19ED695","order_id":1,"type_id":6,"title":"Section","source":{"title":"CNC production from Whatman filter paper and total estimation"}},{"id":1054724,"guid":"0BB407C66D924D7DA4AD30D1D75ED38D","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><div class = \"justify\" style = \"text-align:left\">Follow the CNC fluorescence procedure as mentioned above, and compare the results to the calibration curve, in order to establish CNCs concentration.<\/div><\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":3600,"critical":null,"critical_id":null,"duration":0}],"document":"","materials":[],"description":"<div class = \"text-blocks\"><div class = \"text-block\"><div class = \"justify\" style = \"text-align:left\">Cellulose nanocrystals (CNCs) have gained worldwide interest for their properties. CNCs are mostly produced by acid hydrolysis of cellulose-rich biomass, but its\u2019 quantification is still complicated and tedious. Here, we describe a rapid method for the determination of CNCs concentration using Calcofluor White (CW) fluorescence dye. This method is robust, easy, selective for crystalline cellulose and suitable for routine measurement in CNCs production.<\/div><\/div><div class = \"text-block\"><div class = \"justify\" style = \"text-align:left\"><\/div><\/div><\/div>","changed_on":1625856391}