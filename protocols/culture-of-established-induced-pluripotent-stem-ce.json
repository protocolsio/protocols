id":"63F8486498C040D7A7232208694E23EC","order_id":1,"type_id":6,"title":"Section","source":{"title":"iPSC Passaging"}},{"id":1054724,"guid":"D35555CEB49B4D25BC32703F6E3B5B32","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span style = \"font-weight:bold;\">Preparation<\/span><\/div><div class = \"text-block\">Coat a 6 well plate(s) with vitronectin and incubate according to the manufacturer's instructions. <\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#7ed321","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":954314,"guid":"FA850670A59A11EAB8D76102C4791949","previous_id":954310,"previous_guid":"ABEA3120A59A11EAB8D76102C4791949","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"2464C34B8C0743538ADBF93596CCEA04","order_id":1,"type_id":6,"title":"Section","source":{"title":"iPSC Thawing"}},{"id":1054724,"guid":"09273A32E6DC442FA63A02AEE512138B","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Media change the cells every 24 hours with culture media until they reach 70-80% confluency; at this point they need to be passaged.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#f5a623","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":954323,"guid":"F0588B30A59B11EAB8D76102C4791949","previous_id":954313,"previous_guid":"EACD6F60A59A11EAB8D76102C4791949","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"33BB3BC096254AE6AADC5758DDD9022C","order_id":1,"type_id":6,"title":"Section","source":{"title":"iPSC Passaging"}},{"id":1054724,"guid":"3E083763E39341BCAAD0E1968FCF946D","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Prepare fresh 0.5mM EDTA by diluting UltraPure 0.5M EDTA, pH 8.0 with DPBS(-\/-). Store at room temperature and use on day of preparation only.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#7ed321","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":954324,"guid":"F7A856E0A59B11EAB8D76102C4791949","previous_id":954323,"previous_guid":"F0588B30A59B11EAB8D76102C4791949","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"9EC01897EB414B60B239DD5CE80D7F7A","order_id":1,"type_id":6,"title":"Section","source":{"title":"iPSC Passaging"}},{"id":1054724,"guid":"B69C80812D52490BBECC27985C94F549","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Aspirate the spent medium from cells and wash with 2ml of DPBS(-\/-) per well.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#7ed321","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":954325,"guid":"04AF25D0A59C11EAB8D76102C4791949","previous_id":954324,"previous_guid":"F7A856E0A59B11EAB8D76102C4791949","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"CB46D1F94C2345AD9731DC1A31AAA195","order_id":1,"type_id":6,"title":"Section","source":{"title":"iPSC Passaging"}},{"id":1054724,"guid":"2F2B42F8C171459BA19ACC8181304ECF","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Aspirate DPBS(-\/-) and add 1-2ml of 0.5mM EDTA per well. Rock the dishes to cover the surface of the cells and incubate for 4 min at room temperature.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#7ed321","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":954326,"guid":"0A660200A59C11EAB8D76102C4791949","previous_id":954325,"previous_guid":"04AF25D0A59C11EAB8D76102C4791949","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"4541B002A