is protocol and it's working"},"steps":[{"id":1927082,"guid":"F59445A262A6465D8B7C70CE517C4639","previous_id":0,"previous_guid":null,"section":"\u003cp\u003eEMGain 0 Camera gain and offset measurement protocol\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"ds0mt\",\"text\":\"Place a lens tissue on the sample stage above the objective lens to serve as a diffuser.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94556,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"1","cases":[],"critical":null},{"id":1927084,"guid":"36599058C966410780255BA542FFF7FD","previous_id":1927082,"previous_guid":"F59445A262A6465D8B7C70CE517C4639","section":"\u003cp\u003eEMGain 0 Camera gain and offset measurement protocol\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"cgc7m\",\"text\":\"Acquire brightfield mode images from the microscope with a 100ms exposure time, capturing at least 500 frames consecutively under no light. Ensure EM Gain is off.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94556,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"2","cases":[],"critical":null},{"id":1927085,"guid":"32C5545703D94EB9B73663E82A478220","previous_id":1927084,"previous_guid":"36599058C966410780255BA542FFF7FD","section":"\u003cp\u003eEMGain 0 Camera gain and offset measurement protocol\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"41ogn\",\"text\":\"Repeat step 2 with increasing exposure times (at least 5 times, including step 2).\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94556,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"3","cases":[],"critical":null},{"id":1927086,"guid":"EE51660F4C6B4FC5B956F094E9C45744","previous_id":1927085,"previous_guid":"32C5545703D94EB9B73663E82A478220","section":"\u003cp\u003eEMGain 0 Camera gain and offset measurement protocol\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"9ns6s\",\"text\":\"Save the images taken under different exposure times in the 'camera_calibration' folder in .tif format and prefix the file names with 'exposure_'. For instance, the first image captured under no light should be named '0_filename', and the second image taken with slightly increasing exposure should be named '1_filename'.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94556,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4","cases":[],"critical":null},{"id":1927087,"guid":"98A6C189EB754F4F8330E0B8DB2243B3","previous_id":1927086,"previous_guid":"EE51660F4C6B4FC5B956F094E9C45744","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"","data":null,"protocol_id":94556,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"5","cases":[],"critical":null},{"id":1927088,"guid":"D851A747AFAB4B45A17C96A8BD4487BF","previous_id":1927087,"previous_guid":"98A6C189EB754F4F8330E0B8DB2243B3","section":"\u003cp\u003eEMGain on Camera gain and offset measurement protocol\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"5jrp9\",\"text\":\"Acquire brightfield mode images from the microscope with a 100ms exposure time, capturing at least 500 frames consecutively under no light. Ensure EM Gain is set to the setting of interest (e.g. 250).\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"italic\",\"offset\":190,\"length\":4}],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94556,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"6","cases":[],"critical":null},{"id":1927089,"guid":"412C54DA6E844D598A8B956F3DA5AFF9","previous_id":1927088,"previous_guid":"D851A747AFAB4B45A17C96A8BD4487BF","section":"\u003cp\u003eEMGain on Camera gain and offset measurement protocol\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"1u304\",\"text\":\"Repeat step 2 with increasing exposure times (at least 5 times, including step 2). Ensure no pixels are above/at saturation.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94556,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"7","cases":[],"critical":null},{"id":1927100,"guid":"F8B4E77EF28C401FBC36378FCB43D4E0","previous_id":1927089,"previous_guid":"412C54DA6E844D598A8B956F3DA5AFF9","section":"\u003cp\u003eEMGain on Camera gain and offset measurement protocol\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"6m790\",\"text\":\"Save the images taken under different exposure times in the 'camera_calibration_EMGain_N' folder in .tif format and prefix the file names with 'exposure_'. For instance, the first image captured under no light should be named '0_filename', and the second image taken with slightly increasing exposure should be named '1_filename'.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94556,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"8","cases":[],"critical":null},{"id":1927102,"guid":"7545B3873AA44F739DCC469E096F4F9C","previous_id":1927100,"previous_guid":"F8B4E77EF28C401FBC36378FCB43D4E0","section":"\u003cp\u003eGain calculation\u003c/p\u003e","section_colo