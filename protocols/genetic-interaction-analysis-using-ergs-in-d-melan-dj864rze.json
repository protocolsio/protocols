ks":0,"number_of_comments":0,"number_of_bookmarked_comments":0,"number_of_steps_comments":0,"number_of_protocol_comments":0,"number_of_exports":0,"number_of_runs":0,"number_of_votes":0,"number_of_reagents":0,"number_of_equipments":0,"number_of_collections":0,"number_of_forks":{"private":0,"public":0},"number_of_accessible_forks":0},"status":{"id":1,"info":"We use this protocol and it's working"},"steps":[{"id":2204585,"guid":"5847087627AF429FA86E6AC41FDD962F","previous_id":0,"previous_guid":null,"section":"\u003cp\u003eElectroretinograms (ERGs)\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"7sp4v\",\"text\":\"For every measurement male flies are collected (including the control) 4±1 d after eclosion from the transheterozugous crosses reared in a temperature and light-controlled incubator\\nat   and 12-hour light-dark cycle \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[{\"key\":0,\"offset\":185,\"length\":1}],\"data\":{}}],\"entityMap\":{\"0\":{\"type\":\"temperature\",\"mutability\":\"MUTABLE\",\"data\":{\"error_temperature\":false,\"guid\":\"B17A0CB3646D11EF8E380A58A9FEAC02\",\"id\":0,\"label\":\"\",\"scientificNotation\":false,\"temperature\":\"25\",\"unit\":10}}}}","data":null,"protocol_id":106494,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"1","cases":[],"critical":null},{"id":2204603,"guid":"3751EAE92CF347C29CCBD2ECAA5FBEF6","previous_id":2204585,"previous_guid":"5847087627AF429FA86E6AC41FDD962F","section":"\u003cp\u003eElectroretinograms (ERGs)\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"v76n\",\"text\":\"Flies are sedated with CO2 and immobilized on glass microscope slides using double-sided tape\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":106494,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"2","cases":[],"critical":null},{"id":2204606,"guid":"4DF1C4138F99492CBA3BDF43F97A2462","previous_id":2209255,"previous_guid":"5EC2C9745D13460B9ECE82333499A1AE","section":"\u003cp\u003eElectroretinograms (ERGs)\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"doson\",\"text\":\"Glass electrodes (borosilicate, 1.5 mm outer diameter) filled with 3 M NaCl are placed in the thorax as a reference and on the fly eye for recordings\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":106494,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4","cases":[],"critical":null},{"id":2204613,"guid":"14D81D413B004C7F8869CA90F99D35ED","previous_id":2204606,"previous_guid":"4DF1C4138F99492CBA3BDF43F97A2462","section":"\u003cp\u003eElectroretinograms (ERGs)\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"boi83\",\"text\":\"The flies are exposed to darkness for 3 seconds, followed by 1 second of LED light illumination inside a Faraday Cage.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":106494,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"5","cases":[],"critical":null},{"id":2204616,"guid":"60290C90B87E4639BBBB40DE09BDDD8B","previous_id":2204613,"previous_guid":"14D81D413B004C7F8869CA90F99D35ED","section":"\u003cp\u003eElectroretinograms (ERGs)\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"7fhh2\",\"text\":\"This is repeated 4 times for each fly (5 times in total).\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":106494,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"6","cases":[],"critical":null},{"id":2204618,"guid":"331113456B314B50A36917B600A82B02","previous_id":2204616,"previous_guid":"60290C90B87E4639BBBB40DE09BDDD8B","section":"\u003cp\u003eElectroretinograms (ERGs)\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"3gqm6\",\"text\":\"Light-evoked signals are amplified by a DC amplifier and the amplified signal is processed\\nby a data acquisition device (Clampex) and Axosope 10.7, connected to a PC running Clampfit 10.7 software (Molecular Devices). \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":106494,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"7","cases":[],"critical":null},{"id":2204681,"guid":"87DCB7ABE35F4520B480C4E939C610CB","previous_id":2204618,"previous_guid":"331113456B314B50A36917B600A82B02","section":"\u003cp\u003eElectroretinograms (ERGs)\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"9u63p\",\"text\":\"First control flies are recorded to to assess whether the recording is stable and conditions are comparable to previous recordings\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":106494,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"8","cases":[],"critical":null},{"id":2204683,"guid":"0A3F5683C31F48E0A6CFA4C90E1CACB2","previous_id":2209254,"previous_guid":"B529DE9296104A2C8E65AB9D9BC4A332","section":"\u003cp\u003eAnalysis for genetic interaction using non-interacting model\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"c2id1\",\"text\":\"For all ERGs the depolarization amplitude is determined and normalized to the mean depolarization amplitude of the control of the individual experiment.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":106494,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"10","cases":[],"critical":null},{"id":2204726,"guid":"41B2382036D44D3AAEFBA7978D006025","previous_id":2204683,"previous_guid":"0A3F5683C31F48E0A6CFA4C90E1CACB2","section":"\u003cp\u003eAnalysis for genetic interaction using non-interacting model\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"bo0jr\",\"text\":\"The expected depolarization amplitude for every gene pair is calculated by multiplying the normalized mean depolarization amplitude of the single heterozygous mutants.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":106494,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"11","cases":[],"critical":null},{"id":2204856,"guid":"12373DE37AE0423099CC65C6DCC52F92","previous_id":2204726,"previous_guid":"41B2382036D44D3AAEFBA7978D006025","section":"\u003cp\u003eAnalysis for genetic interaction using non-interacting model\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"3arvp\",\"text\":\"The genetic interaction strength is calculated by subtracting the modelled expected depolarization (step 9) from the individual observed depolarization amplitude of double heterozygous gene pairs\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":106494,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"12","cases":[],"critical":null},{"id":2209254,"guid":"B529DE9296104A2C8E65AB9D9BC4A332","previous_id":2204681,"previous_guid":"87DCB7ABE35F4520B480C4E939C610CB","section":"\u003cp\u003eElectroretinograms (ERGs)\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"70scr\",\"text\":\"ERG traces are analyzed for depolarisation amplitude and On/Off peaks in IGOR Pro 6.37 (WaveMetrics) using a custom-made macro.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":106494,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"9","cases":[],"critical":null},{"id":2209255,"guid":"5EC2C9745D13460B9ECE82333499A1AE","previous_id":2204603,"previous_guid":"3751EAE92CF347C29CCBD2ECAA5FBEF6","section":"\u003cp\u003eElectroretinograms (ERGs)\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"881hn\",\"text\":\"Filamented glass micropipettes are used to generate electrodes using the Laser-pipette-puller P2000. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":106494,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"3","cases":[],"critical":null}],"template_id":0,"title":"Genetic interaction analysis using ERGs in D. melanogaster","title_html":"Genetic interaction analysis using ERGs in D. melanogaster","type_id":1,"units":[{"id":1,"type_id":3,"name":"µL","can_manage":0,"read_only":0},{"id":2,"type_id":3,"name":"mL","can_manage":0,"read_only":0},{"id":3,"type_id":3,"name":"L","can_manage":0,"read_only":0},{"id":4,"type_id":3,"name":"µg","can_manage":0,"read_only":0},{"id":5,"type_id":3,"name":"mg","can_manage":0,"read_only":0},{"id":6,"type_id":3,"name":"g","can_manage":0,"read_only":0},{"id":7,"type_id":3,"name":"kg","can_manage":0,"read_only":0},{"id":8,"type_id":3,"name":"ng","can_manage":0,"read_only":0},{"id":9,"type_id":3,"name":"Hz","can_manage":0,"read_only":0},{"id":10,"type_id":24,"name":"°C","can_manage":0,"read_only":0},{"id":11,"type