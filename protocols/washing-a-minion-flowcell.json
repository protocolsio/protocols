id":0,"components":[{"id":1054723,"guid":"9C8470E2CA244D8ABA131352FB422217","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"CFC6FD9FA57B4EE6AA4DE0CD6131944A","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span>Place the tube of Wash Solution A on ice.<\/span><span style = \"font-weight:bold;\"> Do not vortex<\/span><span style = \"font-weight:bold;font-weight:bold;\"> <\/span><span style = \"font-weight:bold;\">the tube.<\/span><\/div><\/div>"}},{"id":1054725,"guid":"85D81F440555476BBEFF369D2B0713CC","order_id":2,"type_id":26,"title":"notes","source":{"id":0,"parent_id":0,"uri":"","title":"","body":"<div class = \"text-blocks\"><div class = \"text-block\"><span>Solution A contains<\/span><span style = \"font-weight:bold;\"> <\/span><span>DNase I, which is especially sensitive to physical denaturation. Mixing should only be carried out by gently inverting the tube. <\/span><\/div><\/div>","created_on":0,"changed_on":0,"creator":{"name":" ","affiliation":"","affiliations":[],"username":"","link":null,"image":{"source":"","placeholder":""},"badges":[],"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false},"comments":[]}}],"cases":[],"data":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":893446,"guid":"B6DB5F80618B11EAB3B5A92795EA430E","previous_id":893445,"previous_guid":"B6D9D8E0618B11EAB3B5A92795EA430E","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"A0933796F37D431594F831B6EC3E0585","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"81C81FFCC450444A97F5ABBD150901BF","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Thaw one tube of Wash Solution B at room temperature. Mix thoroughly by vortexing, spin down briefly and place on ice.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":893448,"guid":"B6DE45B0618B11EAB3B5A92795EA430E","previous_id":893446,"previous_guid":"B6DB5F80618B11EAB3B5A92795EA430E","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"DD29F7E1F5454DB79060B57B233B8E26","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"06E0E19A936C4A0F912D8900834EAA24","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">In a clean 1.5 ml Eppendorf DNA LoBind tube, prepare the following Wash Mix:<\/div><div class = \"text-block\"><span style = \"font-weight:bold;\">Component<\/span><span>                        <\/span><span style = \"font-weight:bold;\">Volume<\/span><\/div><div class = \"text-block\">Wash Solution A                 <\/div><div class = \"text-block\">Wash Solution B                <\/div><div class = \"text-block\">Mix well by pipetting, and place on ice. Do not vortex the tube.<\/div><\/div>"}},{"id":1054725,"guid":"422750D0618C11EAB3B5A92795EA430E","order_id":2,"type_id":3,"title":"amount","source":{"amount":20,"unit":"\u00b5l","title":""}},{"id":1054726,"guid":"44E92FF0618C11EAB3B5A92795EA430E","order_id":3,"type_id":3,"title":"amount","source":{"amount":380,"unit":"\u00b5l","title":""}}],"cases":[],"data":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":893450,"guid":"B6E23D50618B11EAB3B5A92795EA430E","previous_id":893448,"previous_guid":"B6DE45B0618B11EAB3B5A92795EA430E","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"D5DC39C3E7C546398428CC32DB847329","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"37F90C5B9B3148CBADB8A9ADA5F2E181","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">If you are still sequencing on the flowcell: stop or pause the sequencing experiment in MinKNOW, and leave the flow cell in the device.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":893451,"guid":"B6E46030618B11EAB3B5A92795EA430E","previous_id":893450,"previous_guid":"B6E23D50618B11EAB3B5A92795EA430E","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"604215EFD31A4F26BB65E03EEB401C3F","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"5968F522B32A4C52911D462D182AC855","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span style = \"font-weight:bold;\">Ensure that the priming port cover and SpotON sample port cover are closed<\/span><span> and remove all fluid from the waste channel using a P1000 pipette.<\/span><\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":893453,"guid":"5B40F1C0618C11EAB3B5A92795EA430E","previous_id":893451,"previous_guid":"B6E46030618B11EAB3B5A92795EA430E","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"0824208499AB449FB42DE280BF8263E5","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"9C5107E7A10D46C79D6D93F3676CA26B","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Rotate the flow cell priming port cover clockwise 90\u00b0 so that the priming port is visible. <\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":893455,"guid":"CA843880618C11EAB3B5A92795EA430E","previous_id":893453,"previous_guid":"5B40F1C0618C11EAB3B5A92795EA430E","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"3734654839BD4F09AC7DC18411D32AC5","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"C42A3FF6802F4D20BFA71607D0EC0F91","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Check for air between the priming port and the sensor array. If necessary, using a P1000 draw back a small volume to remove any air:<\/div><div class = \"text-block\"><ul style = \"list-style-type:disc;\"><li style = \"counter-reset:ol0;list-style-type:disc;\">Set a P1000 pipette to 200 \u00b5l<\/li><li style = \"counter-reset:ol0;list-style-type:disc;\">Insert the tip into the priming port, holding the pipette perpendicular to the flowcell to ensure no air gets in the port<\/li><li style = \"counter-reset:ol0;list-style-type:disc;\">Turn the wheel anti-clockwise until the dial shows 220-230 \u00b5l, or until you can see a small volume of buffer\/liquid entering the pipette tip (do not remove too much liquid or you risk exposing the flowcell to air- a few \u00b5l is sufficient)<\/li><li style = \"counter-reset:ol0;list-style-type:disc;\"><span>Visually check that there is <\/span><span style = \":UNDERLINE;font-style:italic;\">continuous buffer <\/span><span>from the priming port across the sensor array<\/span><\/li><\/ul><\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":893697,"guid":"753635F061EA11EAB3B5A92795EA430E","previous_id":893455,"previous_guid":"CA843880618C11EAB3B5A92795EA430E","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"490F8585DE284E1BAF8C00F4CAD91AF4","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"0040A5D5C9824304946CF66AC2F6FAFE","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Load 400 \u00b5l of the prepared Wash Mix into the flow cell via the priming port, avoiding the introduction of air. Do this as slowly as possibly- you may want to use the same method of pipette control as above i.e. turning the dial clockwise to dispense the liquid into the flowcell.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":893699,"guid":"7C84A21061EA11EAB3B5A92795EA430E","previous_id":893697,"previous_guid":"753635F061EA11EAB3B5A92795EA430E","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"1F89A6051FFB4FF2A6E665839D310B94","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"1F20E6ABADFB4BEF9010B3BAFEFC7F45","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Close the priming port and wait for 30 minutes.<\/div><\/div>"}},{"id":1054725,"guid":"D9999DAFA5E74316A1042D319AFF10BB","order_id":2,"type_id":26,"title":"notes","source":{"id":0,"parent_id":0,"uri":"","title":"","body":"<div class = \"text-blocks\"><div class = \"text-block\">You can monitor the digestion by starting or resuming a run in MinKNOW. The number of pores in strand should eventually drop to zero and the number of single pores should increase as they unblock. <\/div><\/div>","created_on":0,"changed_on":0,"creator":{"name":" ","affiliation":"","affiliations":[],"username":"","link":null,"image":{"source":"","placeholder":""},"badges":[],"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false},"comments":[]}}],"cases":[],"data":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0}],"document":null,"materials":[{"id":350131,"mol_weight":0,"name":"Flowcell Wash Kit","linfor":"","url":"http:\/\/","sku":"EXP-WSH003","cas_number":"","rrid":"","public":0,"vendor":{"name":"Oxford Nanopore Technologies","affiliation":null,"affiliations":[],"username":null,"link":"https:\/\/nanoporetech.com\/","image":{"source":"https:\/\/www.protocols.io\/img\/vendors\/placeholder.png","placeholder":"https:\/\/www.protocols.io\/img\/vendors\/placeholder.png"},"badges":[],"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false,"id":859},"can_edit":0,"stats":{"total_protocols":0}}],"description":"<div class = \"text-blocks\"><div class = \"text-block\">This protocol details the steps required to wash a MInION flowcell using Oxford Nanopore's Flowcell Wash kit (EXP-WSH003). The kit contains a DNase I to digest the previous library and unblock pores to prepare the flowcell membrane for another run. <\/div><\/div>","changed_on":1583766199}