ious_guid":"779E1E21FF4911ED8BC347773C33EDA2","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"6td86\",\"text\":\"GUV Assay   \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":82660,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"2","cases":[],"critical":null},{"id":1697613,"guid":"77A08F20FF4911ED8BC347773C33EDA2","previous_id":1697612,"previous_guid":"779F56A1FF4911ED8BC347773C33EDA2","section":"","section_color":"","section_duration":0,"is_substep":true,"step":"{\"blocks\":[{\"key\":\"7f0si\",\"text\":\"Set up the reaction in an eight-well observation chamber (Lab Tek) at room temperature.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":82660,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"2.1","cases":[],"critical":null},{"id":1697614,"guid":"77A1C7A1FF4911ED8BC347773C33EDA2","previous_id":1697613,"previous_guid":"77A08F20FF4911ED8BC347773C33EDA2","section":"","section_color":"","section_duration":0,"is_substep":true,"step":"{\"blocks\":[{\"key\":\"aqftt\",\"text\":\"Coat the chamber with 1 mg/ml β casein for 10 min.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":82660,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"2.2","cases":[],"critical":null},{"id":1697615,"guid":"77A32731FF4911ED8BC347773C33EDA2","previous_id":1697614,"previous_guid":"77A1C7A1FF4911ED8BC347773C33EDA2","section":"","section_color":"","section_duration":0,"is_substep":true,"step":"{\"blocks\":[{\"key\":\"epa4s\",\"text\":\"Wash the coated chamber with reaction buffer (25 mM HEPES at pH 7.4, 150 mM NaCl and 1 mM TCEP).\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data