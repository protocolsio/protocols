ta\":{},\"depth\":0,\"entityRanges\":[],\"inlineStyleRanges\":[],\"key\":\"7g6vg\",\"text\":\"Avoid subculture at low-density as it will age the cells.\",\"type\":\"unstyled\"}],\"entityMap\":{}}}}}","data":null,"protocol_id":90642,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"13","cases":[],"critical":null},{"id":1843133,"guid":"C2CF9CC58BCF407CBA0BF4DD4BC52126","previous_id":1843132,"previous_guid":"ED4D6A945D3E42828B47F6CE9708CBB9","section":"\u003cp\u003eThawing, subculturing and freezing fibroblasts\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"dfvkb\",\"text\":\"If want to freeze the cells, after centrifugation (Step 11), remove the supernatant, and add 1 to 2 mL of Recovery Cell Culture Freezing Medium (Thermo Fisher Scientific). \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"1qco4\",\"text\":\" \",\"type\":\"atomic\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[{\"key\":0,\"offset\":0,\"length\":1}],\"data\":{}},{\"key\":\"firtd\",\"text\":\"\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{\"0\":{\"type\":\"notes\",\"mutability\":\"IMMUTABLE\",\"data\":{\"blocks\":[{\"data\":{},\"depth\":0,\"entityRanges\":[],\"inlineStyleRanges\":[],\"key\":\"319s7\",\"text\":\"Recommend freezing cells at a high concentration.\",\"type\":\"unstyled\"}],\"entityMap\":{}}}}}","data":null,"protocol_id":90642,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"14","cases":[],"critical":null},{"id":1843134,"guid":"7DC2B7E585EC49A0B80898C367F8356F","previous_id":1843133,"previous_guid":"C2CF9CC58BCF407CBA0BF4DD4BC52126","section":"\u003cp\u003eThawing, subculturing and freezing fibroblasts\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"1n8jr\",\"text\":\"Gently pipette to resuspend the cells and transfer the cell-containing medium to a cryogenic vial. Then store the cryogenic vial in a cryo-freezing container that limits the decrease in temperature at ~1°C per min at -80°C.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90642,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"15","cases":[],"critical":null},{"id":1843135,"guid":"D68A467BE17F468C8D6EBB47F7EAE5F0","previous_id":1843134,"previous_guid":"7DC2B7E585EC49A0B80898C367F8356F","section":"\u003cp\u003eThawing, subculturing and freezing fibroblasts\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"2klep\",\"text\":\"For long-term preservation, transfer the cryogenic vial from a -80°C freezer to a liquid nitrogen tank.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90642,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"16","cases":[],"critical":null}],"template_id":4,"title":"Human primary fibroblast culture","title_html":"Human primary fibroblast culture","type_id":1,"units":[{"id":1,"type_id":3,"name":"µL","can_manage":0,"read_only":0},{"id":2,"type_id":3,"name":"mL","can_manage":0,"read_only":0},{"id":3,"type_id":3,"name":"L","can_manage":0,"read_only":0},{"id":4,"type_id":3,"name":"µg","can_manage":0,"read_only":0},{"id":5,"type_id":3,"name":"mg","can_manage":0,"read_only":0},{"id":6,"type_id":3,"name":"g","can_manage":0,"read_only":0},{"id":7,"type_id":3,"name":"kg","can_manage":0,"read_only":0},{"id":8,"type_id":3,"name":"ng","can_manage":0,"read_only":0},{"id":9,"type_id":3,"name":"Hz","can_manage":0,"read_only":0},{"id":10,"type_id":24,"name":"°C","can_manage":0,"read_only":0},{"id":11,"type_id":24,"name":"°К","can_manage":0,"read_only":0},{"id":12,"type_id":24,"name":"°F","can_manage":0,"read_only"