"url":null},"is_member":1,"description":null,"research_interests":null,"website":null,"location":null,"affiliation":null,"status":{"is_visible":true,"access_level":0},"stats":{"files":[],"total_members":0,"total_followers":0,"total_child_groups":0,"total_parent_groups":0,"has_collaborations":0},"user_status":{"is_member":true,"is_confirmed":true,"is_invited":false,"is_owner":true,"is_admin":false,"is_following":false},"join_link":null,"token":null,"owner":{"name":" ","affiliation":null,"affiliations":[],"username":null,"note":null,"link":null,"image":{"source":null,"placeholder":null},"badges":[],"verified":0,"is_verified_user":false,"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false},"is_protocol_requested":0,"is_group_requested":0,"is_my":false,"is_request":false,"is_confirmed":1,"is_declined":0,"requester":{"name":" ","affiliation":null,"affiliation_url":null,"username":null,"link":null},"protocol":{"id":0,"title":"FCMPASS - Acquisition and gating of light scatter reference materials","title_html":"FCMPASS - Acquisition and gating of light scatter reference materials","image":{"source":null,"placeholder":null},"doi":null,"doi_status":0,"uri":"fcmpass-acquisition-and-gating-of-light-scatter-re-bjcnkive","type_id":1,"template_id":0,"published_on":null,"stats":{"number_of_views":0,"number_of_steps":0,"number_of_bookmarks":0,"number_of_comments":0,"number_of_exports":0,"number_of_runs":0,"number_of_votes":0,"is_voted":0},"parent_protocols":[],"parent_collections":[],"cited_protocols":[]},"created_on":1596592052,"resolve_on":0,"resolved_user":{"name":" ","affiliation":null,"affiliations":[],"username":null,"note":null,"link":null,"image":{"source":null,"placeholder":null},"badges":[],"verified":0,"is_verified_user":false,"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false},"shared":false}}],"is_owner":1,"has_subprotocols":0,"is_subprotocol":1,"is_bookmarked":0,"can_claim_authorship":0,"can_accept_authorship":0,"can_be_copied":1,"can_remove_fork":1,"fork_id":null,"url":"https:\/\/www.protocols.io\/view\/fcmpass-acquisition-and-gating-of-light-scatter-re-bjcnkive","forks_count":{"private":0,"public":0},"access":{"can_view":1,"can_remove":0,"can_add":0,"can_edit":0,"can_publish":0,"can_get_doi":0,"can_share":1,"can_move":1,"can_move_outside":1,"can_transfer":1,"can_download":1,"is_locked":0},"guid":"A7EDDDF0D6B511EA8727B3FC1E9BE860","state_version_id":203,"steps":[{"id":995597,"guid":"DAEE81F0D6B511EA8727B3FC1E9BE860","previous_id":null,"previous_guid":null,"modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"90B212EE38CD4772B186B6A06BF0B643","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"A937CAF612AD43CA91FA57E562FEAF6F","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span>Calculate the stock traceable size calibration reference bead particle concentration using percent solids value and particle density provided by the manufacturer and the following formula, whereis the concentration (particles mL<\/span><span style = \"vertical-align:super;\">-1<\/span><span>),, is the percent solids,is the particle density (g mL<\/span><span style = \"vertical-align:super;\">-1<\/span><span>), andis the average diameter (\u00b5m).<\/span><\/div><div style = \"text-align :; float : ;\"><span style = \"\">N_P=\\frac{(W_V\\%,\u00d7 6\u00d710^{12})} {(\u03c0\u03c1_\u03c1 D^3 )}<\/span><\/div><\/div>"}},{"id":1054725,"guid":"1710073E10E84C23BE2578746ADD1097","order_id":2,"type_id":26,"title":"notes","source":{"id":0,"parent_id":0,"uri":"","title":"","body":"<div class = \"text-blocks\"><div class = \"text-block\"><span>For example, 100 nm polystyrene beads at 1% with 1.05 g mL<\/span><span style = \"vertical-align:super;\">-1<\/span><span>would be calculated using:<\/span><\/div><div class = \"text-block\"><div class = \"justify\" style = \"text-align:center\"><a href=\"#\" style = \"text-decoration:underline;color:blue;cursor:pointer;\"><span style = \":;\"> <\/span><\/a><span style = \":;\"> <\/span><\/div><\/div><div style = \"text-align :; float : ;\"><span style = \"\">1.82\u00d710^{13}=\\frac{(0.01\u00d7 6\u00d710^{12})} {(\u03c0\u00d71.05\u00d7 0.1^3 )}<\/span><\/div><\/div>","created_on":0,"changed_on":0,"creator":{"name":" ","affiliation":null,"affiliations":[],"username":"","note":null,"link":null,"image":{"source":"","placeholder":""},"badges":[],"verified":0,"is_verified_user":false,"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false},"comments":[]}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":995598,"guid":"A31E8620D6B611EA8727B3FC1E9BE860","previous_id":995597,"previous_guid":"DAEE81F0D6B511EA8727B3FC1E9BE860","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"672955997E5F40BC8E5266EF56FD177C","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"07A5E2CC1D504862B041C2BB189E1A7D","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Thoroughly vortex the traceable size calibration reference bead stock bottles to homogenize the mixtures before dispensing 1 drop (~50 \u00b5L) into separate 500 \u00b5L low-protein binding Eppendorf.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":995599,"guid":"4CBB20C0D6B811EA8727B3FC1E9BE860","previous_id":995598,"previous_guid":"A31E8620D6B611EA8727B3FC1E9BE860","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"315BA513E80945C4B635D0835C330736","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"262756666C714B3EBC7FC0C9004E98C3","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span>Using the working stock from step 2, make up 500 \u00b5L solution at 1x10<\/span><span style = \"vertical-align:super;\">7 <\/span><span>particles mL<\/span><span style = \"vertical-align:super;\">-1<\/span><span>.<\/span><\/div><\/div>"}},{"id":1054725,"guid":"5FA15E7A25F64D058C6B1B654A4A3D65","order_id":2,"type_id":26,"title":"notes","source":{"id":0,"parent_id":0,"uri":"","title":"","body":"<div class = \"text-blocks\"><div class = \"text-block\">It is recommended that serial dilutions are used and volumes of no less than 10 \u00b5L to avoid pipetting errors. The optimal particle concentration at which to run the reference materials will vary depending on several factors, including the flow rate, beam height, and electronic sampling rate. If running for the first time, it is recommended that serial dilutions are performed to determine the optimal concentration for preparation of the beads.<\/div><\/div>","created_on":0,"changed_on":0,"creator":{"name":" ","affiliation":null,"affiliations":[],"username":"","note":null,"link":null,"image":{"source":"","placeholder":""},"badges":[],"verified":0,"is_verified_user":false,"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false},"comments":[]}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":995600,"guid":"742B4220D6B811EA8727B3FC1E9BE860","previous_id":995599,"previous_guid":"4CBB20C0D6B811EA8727B3FC1E9BE860","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"45B94C9421F746F6A2DA15FBB7517DE8","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"83906BDD851F4B8E8EB680AE5B5769E2","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">On the flow cytometer, set the triggering threshold to the most sensitive light scatter detector and ensure the parameter is using log-scaling (not linear or biexponential).<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":995601,"guid":"7AE1DB10D6B811EA8727B3FC1E9BE860","previous_id":995600,"previous_guid":"742B4220D6B811EA8727B3FC1E9BE860","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"D59FDEBE677043C28A8586E8972CA31C","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"091BBE27536C4E3FAD7115776DE31CF4","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Running DPBS, lower the triggering threshold until the noise floor of the instrument becomes visible. This is most clearly when using a histogram.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":995602,"guid":"81013D10D6B811EA8727B3FC1E9BE860","previous_id":995601,"previous_guid":"7AE1DB10D6B811EA8727B3FC1E9BE860","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"E95705E1E2F841248BDE930FFA3BA3C8","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"0C03280E98C44593A364D8881707B060","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Plotting the trigger-channel height parameter against time and monitoring while running DPBS is a good indication for determining whether an instrument is clean. If the spread of noise (and event rate) decreases over time, it is indicative that the instrument was dirty and is becoming cleaner.<\/div><\/div>"}},{"id":1054725,"guid":"E3BA49E12AC94E4D91664EDC869C54C7","order_id":2,"type_id":26,"title":"notes","source":{"id":0,"parent_id":0,"uri":"","title":"","body":"<div class = \"text-blocks\"><div class = \"text-block\">There are a number of cleaning solutions. Some shared resource lab managers have a preference. These include bleach, contrad 70, micro 90, surfanol<\/div><\/div>","created_on":0,"changed_on":0,"creator":{"name":" ","affiliation":null,"affiliations":[],"username":"","note":null,"link":null,"image":{"source":"","placeholder":""},"badges":[],"verified":0,"is_verified_user":false,"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false},"comments":[]}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":995603,"guid":"B3DD9030D6B811EA8727B3FC1E9BE860","previous_id":995606,"previous_guid":"D33FC600D6B811EA8727B3FC1E9BE860","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"A8C199D2310343D18951122CF9D80A65","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"5567CE5D97244C61A9EE0913871D3489","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Analyze each bead sample at the same acquisition settings until >5000 bead events are recorded.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":995604,"guid":"BAD59EF0D6B811EA8727B3FC1E9BE860","previous_id":995602,"previous_guid":"81013D10D6B811EA8727B3FC1E9BE860","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"7C9DDABC559F49C1B1448D77A7E82D8B","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"C8B8BA58EBDE47EBAEDC5089BAFD889B","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">The extent to which the opto-electronic noise of an instrument can be sampled will vary between instruments. Legacy flow cytometers will tolerate a couple of 1000-2000 events\/second whilst allowing room to sample desired events, while high-speed jet-in-air sorters are capable of sample 10,000+ events per second.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":995605,"guid":"BE1894F0D6B811EA8727B3FC1E9BE860","previous_id":995604,"previous_guid":"BAD59EF0D6B811EA8727B3FC1E9BE860","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"7D7D5D28FBCE482AA8E4A6BC022EF9E2","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"9FEDBF752BE8481F8F3EE5FFB368BC11","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Triggering using a light scatter parameter on the opto-electronic noise of the instrument has benefits in determining and tracking the lower limit of detection, as well as being informative for buffer + reagent controls where background fluorescence will show clear shifts due to many events being triggered from sampling the noise. The use of this method comes at the cost of having high event rates and therefore larger files. Before utilizing this method the instrument should be validated to determine: 1) its ability to detect and accurately process particles, 2) the event rate at which single small particles are detected, and 3) the degree to which the opto-electronic noise can be sampled without creating artefacts or reducing the ability to detect genuine events.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":995606,"guid":"D33FC600D6B811EA8727B3FC1E9BE860","previous_id":995605,"previous_guid":"BE1894F0D6B811EA8727B3FC1E9BE860","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"A8E9039153E4425F90F9AC8AF69EB9D4","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"9F3864AE52E7441BAED94D51C65D6284","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">On some instruments that utilize peristaltic pumps there can appear to be an increase and decrease of the baseline corresponding to the turnover of the pump. This is a result of the threshold being set close to (but above) the electronic noise, resulting in the increase and decrease in trigger events in light scatter. This can be overcome by lowering the threshold so that the noise is being sampled regardless of the peristaltic pump turnover or increasing the threshold and therefore decreasing the instrument\u2019s limit of sensitivity.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":995607,"guid":"F370E490D6B811EA8727B3FC1E9BE860","previous_id":995603,"previous_guid":"B3DD9030D6B811EA8727B3FC1E9BE860","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"4F36EE9CEC3D45178A34410545A60D89","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"043F817C75624DDA82B99DF57F863EFC","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">It is preferable to analyze and store bead populations individually. This will minimize population overlap, aggregates, background noise, and artifacts.<\/div><div style = \"text-align :; float : ;\"><img style = \"\" src = \"https:\/\/s3.amazonaws.com\/protocols-files\/public\/8129a42555104200975866e18b5e0c31c95251181efbe2f172e8a4d712a958f1\/cgmibcmax.png\" \/><\/div><\/div>"}},{"id":1054725,"guid":"506F042F7FA04391B70AF5F071FE4F74","order_id":2,"type_id":1,"title":"description","source":{"description":"<div style = \"text-align :; float : ;\"><img style = \"\" src = \"https:\/\/s3.amazonaws.com\/protocols-files\/public\/8129a42555104200975866e18b5e0c31c95251181efbe2f172e8a4d712a958f1\/cgmibcmax.png\" \/><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":995608,"guid":"0B5783C0D6B911EA8727B3FC1E9BE860","previous_id":995607,"previous_guid":"F370E490D6B811EA8727B3FC1E9BE860","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"F0CCDE626A7B49F783F503F2FA7F151C","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"DF29052490614CF9805FE27496C2EB6A","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Gate each bead population using the parameter Height vs. Area in a dot-plot to remove doublets\/aggregates and then use a histogram on the light scatter parameter (Height) to obtain statistics for each population. The light scatter parameter should use log scaling.<\/div><div style = \"text-align :; float : ;\"><img style = \"\" src = \"https:\/\/s3.amazonaws.com\/protocols-files\/public\/8129a42555104200975866e18b5e0c31c95251181efbe2f172e8a4d712a958f1\/cgmebcmax.png\" \/><\/div><\/div>"}},{"id":1054725,"guid":"6994B5AAB13244AABAC6FAA1C988CF79","order_id":2,"type_id":1,"title":"description","source":{"description":"<div style = \"text-align :; float : ;\"><img style = \"\" src = \"https:\/\/s3.amazonaws.com\/protocols-files\/public\/8129a42555104200975866e18b5e0c31c95251181efbe2f172e8a4d712a958f1\/cgmebcmax.png\" \/><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":995609,"guid":"44FDA9B0D6B911EA8727B3FC1E9BE860","previous_id":995608,"previous_guid":"0B5783C0D6B911EA8727B3FC1E9BE860","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"7530D70AC4C04AB2A5039F2DB5861A0C","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"0F6C6BCEC9394337B8F1998B1F67E33A","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Obtain the median statistic for each of the bead populations.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":995610,"guid":"4BCF92D0D6B911EA8727B3FC1E9BE860","previous_id":995609,"previous_guid":"44FDA9B0D6B911EA8727B3FC1E9BE860","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"D6A96D66F916427AAB1A33CB0C85E884","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"1E4544E378C14D1B92D1E39246AE2ACE","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">By default, flow cytometers trigger the acquisition of an event using the pulse height parameter. In cases where a trigger threshold is being defined (e.g. SSC), it is recommended that the pulse-height is used so that the limit of detection can be defined in calibrated units. There is no consensus within the small particle community over the use of pulse height vs. area. We recommend that, in general, if the parameter being calibrated was not used as a trigger channel the pulse area statistic should be used due to the tendency for low signal intensities to be linear and therefore a more reliable method for extrapolation.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0}],"document":null,"materials":[{"id":4222,"mol_weight":0,"name":"Flow Cytometer","linfor":"","url":"","sku":"","cas_number":"","rrid":"","public":0,"vendor":{"name":"Contributed by users","affili