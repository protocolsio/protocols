ges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"ejlnp\",\"text\":\"\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"4hard\",\"text\":\" \",\"type\":\"atomic\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[{\"key\":0,\"offset\":0,\"length\":1}],\"data\":{}},{\"key\":\"370rc\",\"text\":\"\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{\"0\":{\"type\":\"notes\",\"mutability\":\"IMMUTABLE\",\"data\":{\"blocks\":[{\"data\":{},\"depth\":0,\"entityRanges\":[],\"inlineStyleRanges\":[],\"key\":\"3jce1\",\"text\":\"The chambers should be at least 30-40 cm tall so that the mice are not able to easily escape. During habituation some mice may attempt to jump out.\",\"type\":\"unstyled\"}],\"entityMap\":{}}}}}","data":null,"protocol_id":86295,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"2","cases":[],"critical":null},{"id":1761188,"guid":"45C4D4D036FC11EEA2931F7A18138282","previous_id":1761187,"previous_guid":"BFD4F58036FB11EEA2931F7A18138282","section":"\u003cp\u003eOpen Field Behavior\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"4ep4g\",\"text\":\"On day 3, perform test session by placing mice in chamber for 30 minutes and recording their location using an overhead mounted camera\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":86295,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"3","cases":[],"critical":null},{"id":1761189,"guid":"6BF0B66036FC11EEA2931F7A18138282","previous_id":1761188,"previous_guid":"45C4D4D036FC11EEA2931F7A18138282","section":"\u003cp\u003eOpen Field Behavior\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"7ug0e\",\"text\":\"Calculate average movement velocity and total distance traveled using commercial video tracking software (Noldus EthoVision)\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":86295,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4","cases":[],"critical":null},{"id":1761191,"guid":"D33B956036FC11EEA2931F7A18138282","previous_id":1761189,"previous_guid":"6BF0B66036FC11EEA2931F7A18138282","section":"\u003cp\u003eT-Maze\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"fkd50\",\"text\":\"Prior to T-Maze task, handle mice every day for one week and calorie restrict to reduce weight to ~85% of original body weight; measure weight daily.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":86295,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"5","cases":[],"critical":null},{"id":1761192,"guid":"035B398036FD11EEA2931F7A18138282","previous_id":1761191,"previous_guid":"D33B956036FC11EEA2931F7A18138282","section":"\u003cp\u003eT-Maze\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"8vemv\",\"text\":\"For two days before the task\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"5i63e\",\"text\":\"familiarize mice with the food reward by providing 10 reward pellets in their home cage\",\"type\":\"unordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"b3st3\",\"text\":\"familiarize mice the T-maze by allowing each mouse to explore the maze freely for 10 minutes with 5 rewards in each arm\",\"type\":\"unordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":86295,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"6","cases":[],"critical":null},{"id":1761268,"guid":"50771A8036FE11EEA2931F7A18138282","previous_id":1761192,"previous_guid":"035B398036FD11EEA2931F7A18138282","section":"\u003cp\u003eT-Maze\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"9ffud\",\"text\":\"Simple uncued task (food provided in the left arm of the T-maze); repeat for 10 trials on each test day\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":86295,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"7","cases":[],"critical":null},{"id":1761272,"guid":"9B27211036FE11EEA2931F7A18138282","previous_id":1761268,"previous_guid":"50771A8036FE11EEA2931F7A18138282","section":"\u003cp\u003eT-Maze\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":true,"step":"{\"blocks\":[{\"key\":\"b3dfq\",\"text\":\"Place mouse in the start area for 60 seconds\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":86295,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"7.1","cases":[],"critical":null},{"id":1761273,"guid":"A4B2ABF036FE11EEA2931F7A18138282","previous_id":1761272,"previous_guid":"9B27211036FE11EEA2931F7A18138282","section":"\u003cp\u003eT-Maze\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":true,"step":"{\"blocks\":[{\"key\":\"bakrb\",\"text\":\"Remove sliding door, give mice 60 seconds to choose the left or right arm; if choice is not made within 60 seconds, nudge into either the left or right arm on an alternating basis\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":86295,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"7.2","cases":[],"cri