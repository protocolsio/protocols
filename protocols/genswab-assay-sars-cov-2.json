_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Remove 2ul of Dilution 1 to 198ul of 0.1% Tween-20 in TE. This is Dilution 2.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1084084,"guid":"7CAFC27BC363446EB0F0898213ECE25B","previous_id":1084083,"previous_guid":"AA225F5EBE25417F8D93788E78A72DC3","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"2290D69E468A4B789EB61D1D735EFE06","order_id":1,"type_id":6,"title":"Section","source":{"title":"Pre-PCR: S2 Spike-in Dilution"}},{"id":1054724,"guid":"4BFD8C6A9AE84E87996675BD22CA2ED4","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Remove 2ul of Dilution 2 to 198ul of 0.1% Tween-20 in TE. This is Dilution 3.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1084085,"guid":"3357DC0493A74EB0AC89A5340BCEA14F","previous_id":1084084,"previous_guid":"7CAFC27BC363446EB0F0898213ECE25B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"5C0E9206568047F99868DF7BF6B814DE","order_id":1,"type_id":6,"title":"Section","source":{"title":"Pre-PCR: S2 Spike-in Dilution"}},{"id":1054724,"guid":"376A9BF5D8444BBE9F05219A5FC983F7","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Make a final 10x dilution by adding 10ul of Dilution 3 to 90ul of 0.1% Tween-20 in TE. Keep on ice until use.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1084086,"guid":"717BEEC192A8406E89692F0F8C0D497C","previous_id":1084085,"previous_guid":"33