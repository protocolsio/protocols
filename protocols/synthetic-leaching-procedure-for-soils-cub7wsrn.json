ership_history":null,"parent_collections":[],"parent_protocols":[],"peer_reviewed":false,"protocol_references":"{\"blocks\":[{\"key\":\"bedl5\",\"text\":\"EPA Method 1312. (1994). Synthetic Precipitation Leaching Procedure. https://www.epa.gov/sites/default/files/2015-12/documents/1312.pdf\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[{\"key\":0,\"offset\":69,\"length\":66}],\"data\":{}}],\"entityMap\":{\"0\":{\"type\":\"link\",\"mutability\":\"MUTABLE\",\"data\":{\"guid\":\"EF20CFDEF4C011EDB2C30A58A9FEAC02\",\"url\":\"https://www.epa.gov/sites/default/files/2015-12/documents/1312.pdf\"}}}}","public":true,"public_fork_note":"","published_on":1684336545,"references":[],"related_equipments":[],"related_materials":[],"reserved_doi":"","retraction_reason":null,"samples":{},"shared_access_id":100,"show_comparison":false,"sign_info":null,"state_version_id":756,"stats":{"is_voted":false,"number_of_views":0,"number_of_steps":6,"number_of_bookmarks":0,"number_of_comments":0,"number_of_bookmarked_comments":0,"number_of_steps_comments":0,"number_of_protocol_comments":0,"number_of_exports":0,"number_of_runs":0,"number_of_votes":0,"number_of_reagents":0,"number_of_equipments":0,"number_of_collections":0,"number_of_forks":{"private":0,"public":0},"number_of_accessible_forks":0},"status":{"id":1,"info":"A standard protocol. "},"steps":[{"id":1687766,"guid":"57963280F4C111EDA51E01219A8767DA","previous_id":0,"previous_guid":null,"section":"\u003cp\u003ePreparation of SLP acid mixture.\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"cp425\",\"text\":\"Add 2.4 g of H2SO4 and 1.6 g of HNO3 to about 30 mL of DI water in a graduated 50 mL centrifuge tube. Adjust the final volume to 40 mL. Add 1 mL of this to about 40 mL of DI water in a second graduated 50 mL centrifuge tube. Adjust the final volume to 50 mL.  Wear proper PPE at all times.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":82015,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"1","cases":[],"critical":null},{"id":1687798,"guid":"92FD73F0F4C211EDA51E01219A8767DA","previous_id":1687766,"previous_guid":"57963280F4C111EDA51E01219A8767DA","section":"\u003cp\u003ePreparation of SLP extractant.\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"3umjt\",\"text\":\"Calibrate a pH meter. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":82015,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"2","cases":[],"critical":null},{"id":1687802,"guid":"C172EA80F4C211EDA51E01219A8767DA","previous_id":1687798,"previous_guid":"92FD73F0F4C211EDA51E01219A8767DA","section":"\u003cp\u003ePreparation of SLP extractant.\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"9rdcm\",\"text\":\"Fill a beaker with a volume of DI water sufficient for your needs. Add a stir bar to the beaker and place it on a stir plate. Insert the pH meter into the the DI water and turn the stir plate on. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":82015,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"3","cases":[],"critical":null},{"id":1687816,"guid":"00A16290F4C311EDA51E01219A8767DA","previous_id":1687802,"previous_guid":"C172EA80F4C211EDA51E01219A8767DA","section":"\u003cp\u003ePreparation of SLP extractant.\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"5mgd\",\"text\":\"Using a disposable transfer pipette, add your SLP acid mixture (step 1) dropwise to the stirring DI water. Add one or two drops at a time and wait for the pH reading to stabilize. If you are extracting soils from east of the Mississippi River, adjust the final pH to 4.2. For soils west of the Mississippi River, adjust to the final pH to 5.0. This should require 10-15 drops of the SLP acid mixture for about 1 L of extractant. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":82015,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4","cases":[],"critical":null},{"id":1687821,"guid":"8B32B670F4C311EDA51E01219A8767DA","previous_id":1687816,"previous_guid":"00A16290F4C311EDA51E01219A8767DA","section":"\u003cp\u003eSLP Extraction\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"c42ia\",\"text\":\"SLP extractions are done at a soil:solution ratio of 1:20. I typically use 2g:40 mL, however, for biochar screening experiments, I'll use 40g:800 mL in 1L bottles. Weigh soil into a properly sized vessel with a secure lid. Add SLP extract at a volume that is 20x the mass of soil. Cap the vessel and transfer to a platform shaker for 18 hours. (I usually start these at 4:00 PM and take them off at 10:00 AM the next morning.) \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"bold\",\"offset\":334,\"length\":8}],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":82015,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"5","cases":[],"critical":null},{"id":1687825,"guid":"8F807040F4C411EDA51E01219A8767DA","previous_id":1687821,"previous_guid":"8B32B670F4C311EDA51E01219A8767DA","section":"\u003cp\u003eSLP Extraction\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"2ribs\",\"text\":\"Filter the liquid through a 0.45 um filter. This can be analyzed for solutes of interest according to the proper method. \",\"type\":\"unstyled\",\"d