k":"","image":{"source":"/img/avatars/004.png","placeholder":"/img/avatars/004.png","webp_source":""}},"stats":null}],"parent_protocols":[],"peer_reviewed":false,"protocol_references":"","public":true,"public_fork_note":"","published_on":1680512427,"references":[],"related_equipments":[],"related_materials":[],"reserved_doi":"10.17504/protocols.io.5jyl8jo89g2w/v1","retraction_reason":null,"samples":{},"shared_access_id":16,"show_comparison":false,"sign_info":null,"state_version_id":162,"stats":{"is_voted":false,"number_of_views":1,"number_of_steps":11,"number_of_bookmarks":0,"number_of_comments":0,"number_of_bookmarked_comments":0,"number_of_steps_comments":0,"number_of_protocol_comments":0,"number_of_exports":0,"number_of_runs":0,"number_of_votes":0,"number_of_reagents":0,"number_of_equipments":0,"number_of_collections":2,"number_of_forks":{"private":0,"public":0},"number_of_accessible_forks":0},"status":{"id":1,"info":"We use this protocol and it's working"},"steps":[{"id":1645359,"guid":"A79DDCA0D08211EDAACC0B48D8B66585","previous_id":0,"previous_guid":null,"section":"\u003cp\u003eGeneration of induced pluripotent stem cells and gene correction\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"c4qcj\",\"text\":\"Skin fibroblasts were reprogrammed by nucleofection with pCXLE- hOct3/4 (RRID:Addgene_27076), pCXLE-hSK (RRID:Addgene_27078),using the Amaxa nucleofection kit for human dermal fibroblasts (Lonza, VPD-100) and program P-022 of the Nucleofector 2b (Lonza).\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":79876,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"1","cases":[],"critical":null},{"id":1645360,"guid":"CE0F2BA0D08211EDAACC0B48D8B66585","previous_id":1645359,"previous_guid":"A79DDCA0D08211EDAACC0B48D8B66585","section":"\u003cp\u003eGeneration of induced pluripotent stem cells and gene correction\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"45g14\",\"text\":\"Nucleofected fibroblasts were plated in six-well plates coated with Matrigel (Corning)\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"457hu\",\"text\":\"in DMEM supplemented with 10% FBS (Gibco) and 1% GlutaMAX Supplement (Gibco).\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":79876,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"2","cases":[],"critical":null},{"id":1645361,"guid":"D6CB7FA0D08211EDAACC0B48D8B66585","previous_id":1645360,"previous_guid":"CE0F2BA0D08211EDAACC0B48D8B66585","section":"\u003cp\u003eGeneration of induced pluripotent stem cells and gene correction\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"1sk11\",\"text\":\"The following day, the medium was changed to DMEM+/+ (DMEM with 10% FBS and 1% GlutaMAX Supplement and 1% Pen/Strep (Millipore)) supplemented with 2 ng/ml recombinant\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"fnnfu\",\"text\":\"basic human fibroblast growth factor (FGF2, Peprotech).\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":79876,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"3","cases":[],"critical":null},{"id":1645362,"guid":"DFD61AB0D08211EDAACC0B48D8B66585","previous_id":1645361,"previous_guid":"D6CB7FA0D08211EDAACC0B48D8B66585","section":"\u003cp\u003eGeneration of induced pluripotent stem cells and gene correction\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"7n47e\",\"text\":\"On day 3 or4 post nucleofection, the medium was changed to E8 medium com-\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"34m1a\",\"text\":\"posed of DMEM F12 with HEPES (Gibco), 128 ng/ml ascorbic acid (Sigma\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"5n8p0\",\"text\":\"–Aldrich), 1x insulin-transferrin-selenium (Thermo Fisher\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"dg4s5\",\"text\":\"Scientific), 10 ng/mL FGF2 (Peprotech), 500 ng/ml heparin (Sigma-Aldrich), and 2 ng/ml TGFβ1 (Peprotech). E8 medium was supplemented with 100 μm sodium butyrate and 0.1% Pen/Strep.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":79876,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4","cases":[],"critical":null},{"id":1645363,"guid":"F4B76920D08211EDAACC0B48D8B66585","previous_id":1645362,"previous_guid":"DFD61AB0D08211EDAACC0B48D8B66585","section":"\u003cp\u003eGeneration of induced pluripotent stem cells and gene correction\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"t47g\",\"text\":\"Colonies started to appear from day 14 onward.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":79876,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"5","cases":[],"critical":null},{"id":1645364,"guid":"FAFD17D0D08211EDAACC0B48D8B66585","previous_id":1645363,"previous_guid":"F4B76920D08211EDAACC0B48D8B66585","section":"\u003cp\u003eGeneration of induced pluripotent stem cells and gene correction\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"6ur27\",\"text\":\" Induced pluripotent stem cells (iPSCs) were cultured on Vitronectin XF (StemCell Technologies) in E8 medium.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":79876,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"6","cases":[],"critical":null},{"id":1645365,"guid":"0E248140D08311EDAACC0B48D8B66585","previous_id":1645364,"previous_guid":"FAFD17D0D08211EDAACC0B48D8B66585","section":"\u003cp\u003eGeneration of induced pluripotent stem cells and gene correction\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"a7m56\",\"text\":\"Gene correction for the L444P mutation was performed as previously described in (Schöndorf, D. C. et al., 2014)\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":79876,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"7","cases":[],"critical":null},{"id":1645366,"guid":"24D1B610D08311EDAACC0B48D8B66585","previous_id":1645365,"previous_guid":"0E248140D08311EDAACC0B48D8B66585","section":"\u003cp\u003eGeneration of induced pluripotent stem cells and gene correction\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"fv10m\",\"text\":\"One hour before nucleofection, 10μM Rockinhibitor was added to the iPSC medium. About 240 nM crRNA (IDT): Atto550-labeled tracrRNA (IDT) duplex was complexed with 124μM Cas9 to form the ribonucleoprotein complex (RNP complex).\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":79876,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"8","cases":[],"critical":null},{"id":1645367,"guid":"649FF090D08311EDAACC0B48D8B66585","previous_id":1645366,"previous_guid":"24D1B610D08311EDAACC0B48D8B66585","section":"\u003cp\u003eGeneration of induced pluripotent stem cells and gene correction\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"9a92h\",\"text\":\"iPSCs(1.6 × 10^6) were nucleofected with the RNP complex and 16μg of ssODN using 100μl of Ingenio nucleofection solution (Mirus) with program B-016 of Nucleofector 2b.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":79876,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"9","cases":[],"critical":null},{"id":1645368,"guid":"7C2BC590D08311EDAACC0B48D8B66585","previous_id":1645367,"previous_guid":"649FF090D08311EDAACC0B48D8B66585","section":"\u003cp\u003eGeneration of induced pluripotent stem cells and gene correction\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"83nrh\",\"text\":\"Following nucleofection, the cells were FACSsorted for Atto550-positive cells using a FACS Aria II with a 100-μmnozzle. After sorting, 1 × 10^4 cells were plated per 10-cm dish.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":79876,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"10","cases":[],"critical":null},{"id":1645369,"guid":"8BB87C10D08311EDAACC0B48D8B66585","previous_id":1645368,"previous_guid":"7C2BC590D08311EDAACC0B48D8B66585","section":"\u003cp\u003eGeneration of induced pluripotent stem cells and gene correction\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"ce6mu\",\"text\":\"Colonies were picked and sequenced by Sanger sequencing.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":79876,"case_id":0,"critical_ids":"","duration":0,"original