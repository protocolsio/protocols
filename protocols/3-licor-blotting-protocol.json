:0},"access":{"can_view":1,"can_remove":1,"can_add":0,"can_edit":0,"can_publish":0,"can_get_doi":1,"can_share":1,"can_move":1,"can_move_outside":1,"can_transfer":1,"can_download":1,"is_locked":0},"guid":"07430CD0461E11EBA9099F80C80C21AD","state_version_id":203,"steps":[{"id":1097315,"guid":"4C2B6E50461E11EBA9099F80C80C21AD","previous_id":null,"previous_guid":null,"modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"40CCBF4E1A3F4DF78F1714741570BC6E","order_id":1,"type_id":6,"title":"Section","source":{"title":"Blotting Protocol for use on LiCor Odyssey imager."}},{"id":1054724,"guid":"48065E9DB7194E259B8051D026737BFB","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span>Use only the <\/span><span style = \"font-weight:bold;\">Licor Immobilon FL<\/span><span> PVDF membranes.\u00a0 Standard PVDF has an unacceptable background signal in the IR.\u00a0Handle the membranes with forceps at the very edges, and minimize contact with gloves. Damage to the membrane can show up on the image of the blot. <\/span><span style = \"font-weight:bold;\">Use only dishes that are clean and have not contained Coomassie blue<\/span><span>\u2013 the instrument is very sensitive and will detect traces of blue dye on the blot.\u00a0 The blue dye in the pre-stained markers will be detected, which is convenient.\u00a0The red marker in the set will not be detected<\/span><\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1097316,"guid":"AA451770461E11EBA9099F80C80C21AD","previous_id":1097315,"previous_guid":"4C2B6E50461E11EBA9099F80C80C21AD","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"911FC1919B684EFEB2A8AFC5822BEB65","order_id":1,"type_id":6,"title":"Section","source":{"title":"Blotting Protocol for use on LiCor Odyssey imager."}},{"id":1054724,"guid":"A46CF3BA79D641F799AC3E17989D0E27","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Place the membrane in a clean dish containing 100% methanol and activate for 30 seconds.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1097317,"guid":"B58A1220461E11EBA9099F80C80C21AD","previous_id":1097316,"previous_guid":"AA451770461E11EBA9099F80C80C21AD","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"DE041F084E0A4961A53E3E7224D76188","order_id":1,"type_id":6,"title":"Section","source":{"title":"Blotting Protocol for use on LiCor Odyssey imager."}},{"id":1054724,"guid":"F5AE963966774B7D91B7457470713474","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Wash for 5 seconds with Milli Q water, and then incubate in transfer buffer(standard formulation)at room temperature until ready to use.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1097318,"guid":"DEAA5480461E11EBA9099F80C80C21AD","previous_id":1097317,"previous_guid":"B58A1220461E11EBA9099F80C80C21AD","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"3354765F07034F1E8FF90F404F1EB368","order_id":1,"type_id":6,"title":"Section","source":{"title":"Blotting Protocol for use on LiCor Odyssey imager."}},{"id":1054724,"guid":"1F9B15BB506F44B1AE81B85136087ED4","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Assemble the cassette for the mini blot module.\u00a0The transfer is performed in the cold room at 20V for 50 minutes.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1097319,"guid":"00986550461F11EBA9099F80C80C21AD","previous_id":1097318,"previous_guid":"DEAA5480461E11EBA9099F80C80C21AD","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"B24B0CC9161747449BA835222E06FDA6","order_id":1,"type_id":6,"title":"Section","source":{"title":"Blotting Protocol for use on LiCor Odyssey imager."}},{"id":1054724,"guid":"6FA3FCEA73704F27857DC18D33DFCC3F","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">After transfer, the membranes are trimmed to the size of the gel and placed on Whatman paper until completely dry.\u00a0 This fixes the protein onto the membrane and improves signal intensity.\u00a0 Allow a minimum of1 hour for drying, or they can be left overnight.\u00a0\u00a0<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1097320,"guid":"19BC8D90461F11EBA9099F80C80C21AD","previous_id":1097319,"previous_guid":"00986550461F11EBA9099F80C80C21AD","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"8EF98A836C06436BAEE5F2B754607F8E","order_id":1,"type_id":6,"title":"Section","source":{"title":"Blotting Protocol for use on LiCor Odyssey imager."}},{"id":1054724,"guid":"A51B4E1FD21C4F07B6992A00FA808606","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">We use the commercialLicorBlocking buffer stock for the IRblots.\u00a0 This gives excellent results with respect to the background. Prepare just what you need for a single day \u2013 do not store diluted buffer overnight or you will get an increased background.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1097321,"guid":"A626CAB0462011EBA9099F80C80C21AD","previous_id":1097320,"previous_guid":"19BC8D90461F11EBA9099F80C80C21AD","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"3682741FFE44475887EC8A8DC526A61C","order_id":1,"type_id":6,"title":"Section","source":{"title":"Blotting Protocol for use on LiCor Odyssey imager."}},{"id":1054724,"guid":"B0915748714A48D5A573F49A769F2350","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">When ready to continue processing the blot, activate with 100% methanol for 30 seconds, then wash for 5 seconds wash with Milli Q water.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1097322,"guid":"C6E33950462011EBA9099F80C80C21AD","previous_id":1097321,"previous_guid":"A626CAB0462011EBA9099F80C80C21AD","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"05396BB52455443AA0A3F65209085768","order_id":1,"type_id":6,"title":"Section","source":{"title":"Blotting Protocol for use on LiCor Odyssey imager."}},{"id":1054724,"guid":"E0C4F9E182A5406A834255614B0E2DF9","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span>Dilute the commercial blocking buffer stock 1:1 with TBS (50% Licor blocking buffer, 50% TBS =<\/span><span style = \"font-weight:bold;\"> blocking buffer<\/span><span>).\u00a0\u00a0 Incubate in 10 ml blocking buffer(per square dish) for 1 hour.\u00a0\u00a0<\/span><\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1097323,"guid":"02233920462111EBA9099F80C80C21AD","previous_id":1097322,"previous_guid":"C6E33950462011EBA9099F80C80C21AD","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"BDEAA73ADFE549EEB1A75716CF755B4E","order_id":1,"type_id":6,"title":"Section","source":{"title":"Blotting Protocol for use on LiCor Odyssey imager."}},{"id":1054724,"guid":"7D45B7E93E2B489B8FBA5428D75BC9F3","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span>For antibody incubation, the membranes will be incubated in a <\/span><span style = \"font-weight:bold;\">blocking buffer supplemented with 0.1% Tween 20 (=Antibody Buffer)<\/span><span>.\u00a0Primary antibody concentrations are 1:10,000 for both FLAG and1:50000 for OmpA.\u00a0 Both antibodies are added at the same time. Incubate with primary antibodies for 1 hour.<\/span><\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1097324,"guid":"3B286420462111EBA9099F80C80C21AD","previous_id":1097323,"previous_guid":"02233920462111EBA9099F80C80C21AD","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"7F586708744C40789FC620CBD8840EEF","order_id":1,"type_id":6,"title":"Section","source":{"title":"Blotting Protocol for use on LiCor Odyssey imager."}},{"id":1054724,"guid":"327709F3E50140DF9460E27F4DC7C9B9","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span>Wash the blots 4 X 5\u2019 with <\/span><span style = \"font-weight:bold;\">TBST wash buffer<\/span><span> (TBS plus 0.1% Tween 20). Note: this is just TBST and does not contain any of the LiCor commercial blocking buffers.<\/span><\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1097325,"guid":"7924EBE0462111EBA9099F80C80C21AD","previous_id":1097324,"previous_guid":"3B286420462111EBA9099F80C80C21AD","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"C499A67126A240AE8C069CB05B80D49F","order_id":1,"type_id":6,"title":"Section","source":{"title":"Blotting Protocol for use on LiCor Odyssey imager."}},{"id":1054724,"guid":"762C3500D17C4320BAB6A4BC731BA6F7","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span>Incubate with IR dye conjugated secondary antibodies, diluted in<\/span><span style = \"font-weight:bold;\"> Antibody Buffer <\/span><span>at the following concentrations: IR dye 680 Goat anti-rat, 1:10,000 (detects FLAG Ab)and IR dye 800 Anti-rabbit (ask from Reynold's lab), 1:10,000 (detects OmpA Ab).<\/span><\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1097326,"guid":"E64CF3C0462111EBA9099F80C80C21AD","previous_id":1097325,"previous_guid":"7924EBE0462111EBA9099F80C80C21AD","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"08B4C74FCC4840B4A83770D382370AF5","order_id":1,"type_id":6,"title":"Section","source":{"title":"Blotting Protocol for use on LiCor Odyssey imager."}},{"id":1054724,"guid":"C9726C54F0944A74A75FF0834A1E4419","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span>Wash 4 X 5\u2019 with <\/span><span style = \"font-weight:bold;\">TBST wash buffer <\/span><span>and take for imaging on the LiCor Odyssey instrument.<\/span><\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0}],"document":"","materials":[],"description":"<div class = \"text-blocks\"><div class = \"text-block\">Blotting Protocol for use on LiCor Odyssey imager.<\/div><\/div>","changed_on":1608840043}