ted_blind_links":false,"is_locked":false},"space_id":3610,"state_version_id":227,"stats":{"is_voted":false,"number_of_views":3,"number_of_steps":12,"number_of_bookmarks":0,"number_of_comments":0,"number_of_bookmarked_comments":0,"number_of_steps_comments":0,"number_of_protocol_comments":0,"number_of_exports":0,"number_of_runs":0,"number_of_votes":0,"number_of_reagents":0,"number_of_equipments":0,"number_of_collections":0,"number_of_forks":{"private":0,"public":0},"number_of_accessible_forks":0},"status":{"id":1,"info":"We use this protocol and it's working"},"steps":[{"id":2261904,"guid":"826E0FEF851B11EFB4B80A58A9FEAC02","previous_id":null,"previous_guid":null,"section":"\u003cp\u003eReceiving Samples\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"at28m\",\"text\":\"When acinar samples are received from the various isolation centers, match what is physically sent with sample info list sent with package and make sure all samples are accounted for. Take note of the condition of the samples. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":109320,"case_id":0,"critical_ids":"","duration":0,"original_id":1867310,"number":"1","cases":[],"critical":null},{"id":2261905,"guid":"826EC87C851B11EFB4B80A58A9FEAC02","previous_id":2261904,"previous_guid":"826E0FEF851B11EFB4B80A58A9FEAC02","section":"\u003cp\u003eReceiving Samples\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"4eajl\",\"text\":\"Let isolation center know samples were received and in what condition. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":109320,"case_id":0,"critical_ids":"","duration":0,"original_id":1867311,"number":"2","cases":[],"critical":null},{"id":2261906,"guid":"826F713A851B11EFB4B80A58A9FEAC02","previous_id":2261905,"previous_guid":"826EC87C851B11EFB4B80A58A9FEAC02","section":"\u003cp\u003eReceiving Samples\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"8nv2o\",\"text\":\"Print QR barcodes for the acinar samples in this format and tape onto the tubes. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"ftp62\",\"text\":\"\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"8fp5u\",\"text\":\"HIGI#ACIN (# being the next sequential number)\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":109320,"case_id":0,"critical_ids":"","duration":0,"original_id":1867312,"number":"3","cases":[],"critical":null},{"id":2261907,"guid":"82701BDE851B11EFB4B80A58A9FEAC02","previous_id":2261906,"previous_guid":"826F713A851B11EFB4B80A58A9FEAC02","section":"\u003cp\u003eReceiving Samples\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"e4qvp\",\"text\":\"Store tubes in Yalow -80Â°C freezer in  \\\"IIDP - HIGI Acinar Samples\\\" box.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":109320,"case_id":0,"critical_ids":"","duration":0,"original_id":1867313,"number":"4","cases":[],"critical":null},{"id":2261908,"guid":"8270C52E851B11EFB4B80A58A9FEAC02","previous_id":2261907,"previous_guid":"82701BDE851B11EFB4B80A58A9FEAC02","section":"\u003cp\u003eExtracting DNA\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"euelj\",\"text\":\"Thaw acinar tubes on ice. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":109320,"case_id":0,"critical_ids":"","duration":0,"original_id":1867314,"number":"5","cases":[],"critical":null},{"id":2261909,"guid":"82717111851B11EFB4B80A58A9FEAC02","previous_id":2261908,"previous_guid":"8270C52E851B11EFB4B80A58A9FEAC02","section":"\u003cp\u003eExtracting DNA\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"fs8e3\",\"text\":\"Use DNA extraction protocol for DNeasy Blood \\u0026 Tissue Kit (https://protocols.io/view/dna-extraction-qiagen-dneasy-blood-amp-tissue-kit-bjngkmbw). \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[{\"key\":0,\"offset\":59,\"length\":84}],\"data\":{}}],\"entityMap\":{\"0\":{\"type\":\"link\",\"mutability\":\"\",\"data\":{\"guid\":\"69BF618C878C11ECB1A00A58A9FEAC02\",\"url\":\"https://protocols.io/view/dna-extraction-qiagen-dneasy-blood-amp-tissue-kit-bjngkmbw\"}}}}","data":null,"protocol_id":109320,"case_id":0,"critical_ids":"","duration":0,"original_id":1867315,"number":"6","cases":[],"critical":null},{"id":2261910,"guid":"82725688851B11EFB4B80A58A9FEAC02","previous_id":2261909,"previous_guid":"82717111851B11EFB4B80A58A9FEAC02","section":"\u003cp\u003eQC\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"359c1\",\"text\":\"After extraction, measure the concentration by nanodrop, taking account of A260/280 and A260/230 values as well. A260/280 value should be close to 1.8 and A260/230 should be around 1.8-2.0. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":109320,"case_id":0,"critical_ids":"","duration":0,"original_id":1867316,"number":"7","cases":[],"critical":null},{"id":2261911,"guid":"82731913851B11EFB4B80A58A9FEAC02","previous_id":2261910,"previous_guid":"82725688851B11EFB4B80A58A9FEAC02","section":"\u003cp\u003eQC\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"664l5\",\"text\":\"Take a portion of the sample to measure concentration by Qubit. Protocol can be found here: https://protocols.io/view/qubit-dsdna-assays-bnstmeen. For genotyping, Qubit concentration should be above 50 ng/uL and total DNA should be above 750 ng. In case the DNA concentration is below 50 ng/uL repeat the DNA extraction with a fresh piece of acinar tissue from the same donor and QC the extracted DNA. Keep repeating this till you are able to extract at least 750ng of DNA for that donor (it is okay if the concentration of the extracted DNA is still less than 50ng/uL) - for these samples, when you submit them for genotyping (step 10), make sure that you pool together and submit all the DNA you have extracted for that donor and request for the sample to be 'speed-vac'ed at the SFGF core - this will help bring the concentration up to 50ng/uL, while still making sure that there is 750ng of DNA, thereby meeting the QC requirements for genotyping.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[{\"key\":0,\"offset\":92,\"length\":53}],\"data\":{}}],\"entityMap\":{\"0\":{\"type\":\"link\",\"mutability\":\"\",\"data\":{\"guid\":\"E6FAC99F878C11ECB1A00A58A9FEAC02\",\"url\":\"https://protocols.io/view/qubit-dsdna-assays-bnstmeen\"}}}}","data":null,"protocol_id":109320,"case_id":0,"critical_ids":"","duration":0,"original_id":1867317,"number":"8","cases":[],"critical":null},{"id":2261912,"guid":"82740328851B11EFB4B80A58A9FEAC02","previous_id":2261913,"previous_guid":"8274F0E8851B11EFB4B80A58A9FEAC02","section":"\u003cp\u003eGenotyping\u003c/p\u003e","section_color":"#FFED92","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"b7tlm\",\"text\":\"Record sample info, nanodrop QC and Qubit information in genotyping QC spreadsheet, which will be sent to the SFGF core whenever the entire genotyping batch is ready (follow this protocol: https://www.protocols.io/edit/sop54v1-tgd-genotypingsubmissionforcollaborators-b8jaruie).\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[{\"key\":0,\"offset\":189,\"length\":87}],\"data\":{}}],\"entityMap\":{\"0\":{\"type\":\"link\",\"mutability\":\"MUTABLE\",\"data\":{\"guid\":\"6FC84E171CA611ED84CD0A58A9FEAC02\",\"url\":\"https://www.protocols.io/edit/sop54v1-tgd-genotypingsubmissionforcollaborators-b8jaruie\"}}}}","data":null,"protocol_id":109320,"case_id":0,"critical_ids":"","duration":0,"original_id":1867318,"number":"10","cases":[],"critical":null},{"id":2261913,"guid":"8274F0E8851B11EFB4B80A58A9FEAC02","previous_id":2261911,"previous_guid":"82731913851B11EFB4B80A58A9FEAC02","section":"\u003cp\u003eQC\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"bglb2\",\"text\":\"Create QR codes that include sample name in this format: HIGI#DNA. Label should also include nanodrop concentration and QC info. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":109320,"case_id":0,"critical_ids":"","duration":0,"original_id":1867319,"number":"9","cases":[],"critical":null},{"id":2261914,"guid":"82759FA8851B11EFB4B80A58A9FEAC02","previous_id":2261912,"previous_guid":"82740328851B11EFB4B80A58A9FEAC02","section":"\u003cp\u003eGenotyping\u003c/p\u003e","section_color":"#FFED92","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"8pses\",\"text\":\"As of 2023-12-01, samples were sent to be genotyped on Illumina's Infinium Omni Exome 2.5 arrays.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"5a3vr\",\"text\":\"As of 2024-10-31, samples were sent to be genotyped on Illumina's Infinium Global Diversity Array-8+ v1.0   Omni2.5-Exome-8v1.6 arrays. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":109320,"case_id":0,"critical_ids":"","duration":0,"original_id":1867320,"number":"11","cases":[],"critical":null},{"id":2263919,"guid":"F7094B2A9E824DC99FF60F45F4EB41D2","previous_id":2261914,"previous_guid":"82759FA8851B11EFB4B80A58A9FEAC02","section":"\u003cp\u003eGenotyping\u003c/p\u003e","section_color":"#FFED92","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"5lp4m\",\"text\":\"Following the QC of the genotyping data, if the data for a donor fails QC, resend the DNA from that same donor on another genotyping batch.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":109320,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"12","cases":[],"critical":null}],"template_id":4,"title":"SOP52v1_TGD_IIDP-HIGISampleProcessing_Public","title_html":"SOP52v1_TGD_IIDP-HIGISampleProcessing_Public","type_id":1,"units":[{"id":1,"type_id":3,"name":"ÂµL","can_manage":0,"read_only":0},{"id":2,"type_id":3,"name":"mL","can_manage":0,"read_only":0},{"id":3,"type_id":3,"name":"L","can_manage":0,"read_only":0},{"id":4,"type_id":3,"name":"Âµg","can_manage":0,"read_only":0},{"id":5,"type_id":3,"name":"mg","can_manage":0,"read_only":0},{"id":6,"type_id":3,"name":"g","can_manage":0,"read_only":0},{"id":7,"type_id":3,"name":"kg","can_manage":0,"read_only":0},{"id":8,"type_id":3,"name":"ng","can_manage":0,"read_only":0},{"id":9,"type_id":3,"name":"Hz","can_manage":0,"read_only":0},{"id":10,"type_id":24,"name":"Â°C","can_manage":0,"read_only":0},{"id":11,"type_id":24,"name":"Â°Ð","can_manage":0,"read_only":0},{"id":12,"type_id":24,"name":"Â°F","can_manage":0,"read_only":0},{"id":13,"type_id":25,"name":"Mass Percent","can_manage":0,"read_only":0},{"id":14,"type_id":25,"name":"% volume","can_manage":0,"read_only":0},{"id":15,"type_id":25,"name":"Mass / % volume","can_manage":0,"read_only":0},{"id":16,"type_id":25,"name":"Parts per Million (PP