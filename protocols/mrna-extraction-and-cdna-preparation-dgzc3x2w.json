,"section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"462nm\",\"text\":\"**Thaw and Resuspend Cells** - Thaw cells stored in TRIzol (15596026, Invitrogen). - Resuspend cells in 1 mL of TRIzol.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":103172,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"1","cases":[],"critical":null},{"id":2133702,"guid":"770EFC3597904A028E38BD0362B2D4E5","previous_id":2133701,"previous_guid":"A33E7E52E84445B889E46B2B34B137E4","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"2fn3v\",\"text\":\"**Add Chloroform** - Add 200 μL of chloroform to the samples.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":103172,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"2","cases":[],"critical":null},{"id":2133703,"guid":"980882D526CE4F7497C4D1AC0F158163","previous_id":2133702,"previous_guid":"770EFC3597904A028E38BD0362B2D4E5","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"cvh0e\",\"text\":\"**Centrifuge Samples** - Centrifuge the samples at 12,000 g for 15 minutes at 4°C.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":103172,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"3","cases":[],"critical":null},{"id":2133704,"guid":"897B36C113BB40C0BA92C410AC6339FC","previous_id":2133703,"previous_guid":"980882D526CE4F7497C4D1AC0F158163","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"3dv20\",\"text\":\"**Collect Aqueous Phase** - Carefully collect the aqueous phase.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\