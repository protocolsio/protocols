":[],"created_on":1710200463,"creator":{"name":"Alexander Ferrera","affiliation":"UC Berkeley","affiliation_url":null,"username":"alexander-ferrera","link":null,"image":{"source":"/img/avatars/012.png","placeholder":"/img/avatars/012.png","webp_source":""},"badges":[{"id":2,"name":"Author","image":{"source":"/img/badges/bronze.svg","placeholder":"/img/badges/bronze.svg"}},{"id":6,"name":"Science accelerator","image":{"source":"/img/badges/accelerator.svg","placeholder":"/img/badges/accelerator.svg"}}],"affiliations":[{"affiliation":"UC Berkeley","url":null,"job_title":"","is_default":true}]},"cross_cloud_origin":null,"description":"{\"blocks\":[{\"key\":\"f0vgg\",\"text\":\"Invasive Plant Identification On Onetahi Motu, Tetiaroa Within French Polynesia\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"8vvie\",\"text\":\"This protocol describes a means of sampling plant species on Tetiaroa's Onetahi Motu in order to identify and address harmful invasive species impacting their environment. By dividing the island into five sections, invasive plant species can be identified using iNaturalist and cross-checking with expert Dr. Jean-Yves Meyer, an esteemed terrestrial botanist specializing in Polynesian flora.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","disclaimer":"{\"blocks\":[{\"key\":\"8u3rm\",\"text\":\"Don't forget to wear sun protective clothing/gear, apply sunscreen, bring ample water, and wear sturdy walking shoes. Be sure to also bring a recording utensil, your waterproof field notebook, cameral and/or a phone with iNaturalist downloaded to help you identify plants as you go. See the materials page for a full list of what to bring. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","document":"","documents":[{"id":451665,"guid":"","type_id":0,"filename":"","key":"","color":"","height":0,"width":0,"is_document":null,"original_file_id":null,"thumb_url":"https://www.protocols.io/img/extensions/png.png","size":2355433,"is_private":0,"is_new":0,"ofn":"Screenshot 2024-03-11 at 2.54.20â€¯PM.png","url":"https://content.protocols.io/files/p3ctccx3p.png","bucket_name":null,"s3_webp_url":"","file_id":451665}],"doi":"dx.doi.org/10.17504/protocols.io.bp2l6xom5lqe/v1","doi_status":1,"ethics_statement":null,"fork_id":null,"fork_info":null,"forks":[],"funders":[],"groups":[],"guid":"0AD2A6F8F0F64CBBB4C283C9A28065C8","guidelines":"","has_references":false,"has_step_reagents":false,"has_versions":false,"id":96524,"image":{"source":"https://content.protocols.io/files/p3jkccx3p.jpg","webp_source":"https://content.protocols.io/files/p3jjccx3p.webp","placeholder":"https://content.protocols.io/files/p3b7ccx3p.png","webp_placeholder":""},"image_attribution":"{\"blocks\":[{\"key\":\"eteo0\",\"text\":\"\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"dju9d\",\"text\":\" \",\"type\":\"atomic\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[{\"key\":0,\"offset\":0,\"length\":1}],\"data\":{}},{\"key\":\"d8mrb\",\"text\":\"\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"ctujk\",\"text\":\" \",\"type\":\"atomic\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[{\"key\":1,\"offset\":0,\"length\":1}],\"data\":{}},{\"key\":\"8ovro\",\"text\":\"\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"dshes\",\"text\":\" \",\"type\":\"atomic\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[{\"key\":2,\"offset\":0,\"length\":1}],\"data\":{}},{\"key\":\"barr4\",\"text\":\"\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{\"0\":{\"type\":\"image\",\"mutability\":\"IMMUTABLE\",\"data\":{\"height\":533,\"id\":451666,\"is_video\":fal