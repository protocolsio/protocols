s.io/img/default_protocol.png","webp_source":null,"placeholder":"https://www.protocols.io/img/default_protocol.png","webp_placeholder":null},"doi":"dx.doi.org/10.17504/protocols.io.bp2l6xyrklqe/v1","uri":"vu-m-a-t-et-al-2024-targeted-micro-fiber-arrays-fo-c5x8y7rw","published_on":1706060417,"modified_on":0,"version_class":0,"version_id":0,"version_code":"","version_uri":"","created_on":1701821998,"categories":null,"type_id":0,"creator":{"name":"Cláudia C. Mendes","affiliation":null,"affiliation_url":null,"username":"n4ple1y1w1v4yle1","link":null,"image":{"source":"/img/avatars/002.png","placeholder":"/img/avatars/002.png","webp_source":""}},"stats":null}],"parent_protocols":[],"peer_reviewed":false,"protocol_references":"{\"blocks\":[{\"key\":\"ft2tl\",\"text\":\"Hopkins, T.M., Heilman, A.M., Liggett, J.A., LaSance, K., Little, K.J., Hom, D.B., Minteer, D.M., Marra, K.G., and Pixley, S.K. (2015). Combining micro-computed tomography with histology to analyze biomedical implants for peripheral nerve repair. J. Neurosci. Methods 255, 122–130. 10.1016/j.jneumeth.2015.08.016.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"italic\",\"offset\":268,\"length\":3}],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","public":true,"public_fork_note":"","published_on":1706060419,"references":[],"related_equipments":[],"related_materials":[],"reserved_doi":"10.17504/protocols.io.rm7vzxed5gx1/v1","retraction_reason":null,"samples":{},"shared_access_id":0,"show_comparison":false,"sign_info":null,"space_access":{"can_view":false,"can_edit":false,"can_remove":false,"can_add":false,"can_publish":true,"can_get_doi":true,"can_share":false,"can_move":false,"can_move_outside":false,"can_transfer":false,"can_download":false,"limited_run":false,"limited_private_links":false,"limited_blind_links":false,"is_locked":false},"space_id":8782,"state_version_id":314,"stats":{"is_voted":false,"number_of_views":3,"number_of_steps":14,"number_of_bookmarks":0,"number_of_comments":0,"number_of_bookmarked_comments":0,"number_of_steps_comments":0,"number_of_protocol_comments":0,"number_of_exports":0,"number_of_runs":0,"number_of_votes":0,"number_of_reagents":0,"number_of_equipments":0,"number_of_collections":1,"number_of_forks":{"private":0,"public":0},"number_of_accessible_forks":0},"status":{"id":1,"info":""},"steps":[{"id":1898020,"guid":"85CDAC3EF70A4A7DA3876062567C8B3B","previous_id":0,"previous_guid":null,"section":"\u003cp\u003eRemoving Lugol\u0026#x27;s contrast agent from tissue\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"7tu5i\",\"text\":\"In preparation for immunohistology, Lugol’s solution was removed from brain tissue after CT imaging by soaking the implanted brains in a solution of 5% sodium thiosulfate (STS, Carolina) in 1% PBS for 4-6 days (Hopkins et al., 2015).\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":93202,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"1","cases":[],"critical":null},{"id":1898021,"guid":"AE4F55DDEBD9407380A6CCE790A12121","previous_id":1898020,"previous_guid":"85CDAC3EF70A4A7DA3876062567C8B3B","section":"\u003cp\u003eRemoving Lugol\u0026#x27;s contrast agent from tissue\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"2l2i8\",\"text\":\"The implant was then removed, the brain returned to the STS solution for one hour.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"6b6v\",\"text\":\"\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":93202,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"2","cases":[],"critical":null},{"id":1898022,"guid":"FB87F00AC4C9482583FC439BAD0B1BCF","previous_id":1898031,"previous_guid":"F161F678EBFF403DA90153ADCF321F06","section":"\u003cp\u003eSlicing\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"5v0e5\",\"text\":\"Once the brains sank, coronal sections (50 µm) were sliced with a cryostat (Leica CM3050 S) and transferred to PBS for storage.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":93202,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4","cases":[],"critical":null},{"id":1898023,"guid":"DD63FF0AA48447D1B99BB14BAEDFE16F","previous_id":1898022,"previous_guid":"FB87F00AC4C9482583FC439BAD0B1BCF","section":"\u003cp\u003eImmunostaining\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"5jgrn\",\"text\":\"To stain for dLight1.3b or ChR2 expression:\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"3b0v2\",\"text\":\"\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":93202,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"5","cases":[],"critical":null},{"id":1898024,"guid":"C6DF58C89A42492AB2A4F28E06D1512F","previous_id":1898033,"previous_guid":"DEA035B2BDB342FA829F7F72578151C4","section":"\u003cp\u003eImmunostaining\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":true,"step":"{\"blocks\":[{\"key\":\"abkqt\",\"text\":\"Treated sections were then incubated for 24-48 hours at 4°C in 5% NGS, 0.2% triton, and a GFP primary antibody (chicken polyclonal antibody; 1:1000, ThermoFisher Scientific, No. A10262).\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":93202,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"5.2","cases":[],"critical":null},{"id":1898025,"guid":"8A02BE3982EC490ABA0D003BCC02FF6E","previous_id":1898024,"previous_guid":"C6DF58C89A42492AB2A4F28E06D1512F","section":"\u003cp\u003eImmunostaining\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":true,"step":"{\"blocks\":[{\"key\":\"41thb\",\"text\":\"Slices were then washed, and incubated for 2 hours at room temperature in 5% NGS, 0.2% PBST, and an Alexa 488 secondary antibody (goat anti-chicken; 1:200, ThermoFisher Scientific, No.A-11039).\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":93202,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"5.3","cases":[],"critical":null},{"id":1898026,"guid":"0AE622A02308488487A993DAE1B8CD5A","previous_id":1898025,"previous_guid":"8A02BE3982EC490ABA0D003BCC02FF6E","section":"\u003cp\u003eImmunostaining\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"gnmq\",\"text\":\"For neuronal staining:\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"dqkra\",\"text\":\"\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":93202,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"6","cases":[],"critical":null},{"id":1898027,"guid":"363AFDDD2DE54D88AE4F3DA7C414BB93","previous_id":1898034,"previous_guid":"BFE8A8729A104689B852795D24D79618","section":"\u003cp\u003eImmunostaining\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":true,"step":"{\"blocks\":[{\"key\":\"2ousu\",\"text\":\"Treated sections were then incubated for 24-48 hours at 4°C in 5% BSA, 0.2% triton, and a NeuN primary antibody (guinea pig antibody; 1:500, Synaptic Systems, 266 004).\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":93202,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"6.2","cases":[],"critical":null},{"id":1898028,"guid":"CE8B622265D8482C85CED8CB4575A9F1","previous_id":1898027,"previous_guid":"363AFDDD2DE54D88AE4F3DA7C414BB93","section":"\u003cp\u003eImmunostaining\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":true,"step":"{\"blocks\":[{\"key\":\"lvuf\",\"text\":\"Slices were then washed, and incubated for 2 hours at room temperature in 5% BSA, 0.2% PBST, and an Alexa 647 secondary antibody (goat anti-guinea pig; 1:200, ThermoFisher Scientific, No. A-21450).\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":93202,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"6.3","cases":[],"critical":null},{"id":1898029,"guid":"900BED27DF3F4780804D7B0982C4620D","previous_id":1898028,"previous_guid":"CE8B622265D8482C85CED8CB4575A9F1","section":"\u003cp\u003eMounting and imaging\u003c/p\u003e","section_color":"#FFED92","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"e4mke\",\"text\":\"Finally, stained sections were mounted onto glass slides using Vectashield Mounting Medium with DAPI (Vector Laboratories, H-2000).\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":93202,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"7","cases":[],"critical":null},{"id":1898030,"guid":"EFBD6E1F9EEB4624B512FBE732834BB6","previous_id":1898029,"previous_guid":"900BED27DF3F4780804D7B0982C4620D","section":"\u003cp\u003eMounting and imaging\u003c/p\u003e","section_color":"#FFED92","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"2knt2\",\"text\":\"Confocal images were acquired with a Zeiss LSM 800.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":93202,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"8","cases":[],"critical":null},{"id":1898031,"guid":"F161F678EBFF403DA90153ADCF321F06","previous_id":1898021,"previous_guid":"AE4F55DDEBD9407380A6CCE790A12121","section":"\u003cp\u003eSlicing\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"8gnmk\",\"text\":\"The brains were then moved to a solution of 30-40% sucrose in PBS.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":93202,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"3","cases":[],"critical":null},{"id":1898033,"guid":"DEA035B2BDB342FA829F7F72578151C4","previous_id":1898023,"previous_guid":"DD63FF0AA48447D1B99BB14BAEDFE16F","section":"\u003cp\u003eImmunostaining\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":true,"step":"{\"blocks\":[{\"key\":\"949fr\",\"text\":\"Sections were then blocked and treated in a 5% normal goat serum (NGS) and 0.2% PBS triton (Sigma Aldrich) solution (PBST).\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":93202,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"5.1","cases":[],"critical":null},{"id":1898034,"guid":"BFE8A8729A104689B852795D24D79618","previous_id":1898026,"previous_guid":"0AE622A02308488487A993DAE1B8CD5A","section":"\u003cp\u003eImmunostaining\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":true,"step":"{\"blocks\":[{\"key\":\"9mkrh\",\"text\":\"Sections were then blocked and treated in a 5% bovine serum albumin (BSA) and 0.2% PBS triton (Sigma Aldrich) solution (PBST).\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":93202,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"6.1","cases":[],"critical":null}],"template_id":1,"title":"Immunohistology","title_html":"Immunohistology","type_id":1,"units":[{"id":1,"type_id":3,"name":"µL","can_manage":0,"read_only":0},{"id":2,"type_id":3,"name":"mL","can_manage":0,"read_only":0},{"id":3,"type_id":3,"name":"L","can_manage":0,"read_only":0},{"id":4,"type_id":3,"name":"µg","can_manage":0,"read_only":0},{"id":5,"type_id":3,"name":"mg","can_manage":0,"read_only":0},{"id":6,"type_id":3,"name":"g","can_manage":0,"read_only":0},{"id":7,"type_id":3,"name":"kg","can_manage":0,"read_only":0},{"id":8,"type_id":3,"name":"ng","can_manage":0,"read_only":0},{"id":9,"type_id":3,"name":"Hz","can_manage":0,"read_only":0},{"id":10,"type_id":24,"name":"°C","can_manage":0,"read_only":0},{"id":11,"type_id":24,"name":"°К","can_manage":0,"read_only":0},{"id":12,"type_id":24,"name":"°F","can_manage":0,"read_only":0},{"id":13,"type_id":25,"name":"Mass Percent","can_manage":0,"read_only":0},{"id":14,"type_id":25,"name":"% volume","can_manage":0,"read_only":0},{"id":15,"type_id":25,"name":"Mass / % volume","can_manage":0,"read_only":0},{"id":16,"type_id":25,"name":"Parts per Million (PPM)","can_manage":0,"read_only":0},{"id":17,"type_id":25,"name":"Parts per Billion (PPB)","can_manage":0,"read_only":0},{"id":18,"type_id":25,"name":"Parts per Trillion (PPT)","can_manage":0,"read_only":0},{"id":19,"type_id":25,"name":"Mole Fraction","can_manage":0,"read_only":0},{"id":20,"type_id":25,"name":"Mole Percent","can_manage":0,"read_only":0},{"id":21,"type_id":25,"name":"Molarity (M)","can_manage":0,"read_only":1},{"id":22,"type_id":25,"name":"Molarity (m)","can_manage":0,"read_only":0},{"id":23,"type_id":25,"name":"Genome copies per ml","can_manage":0,"read_only":0},{"id":24,"type_id":3,"name":"μV","can_manage":0,"read_only":0},{"id":25,"type_id":3,"name":"ms","can_manage":0,"read_only":0},{"