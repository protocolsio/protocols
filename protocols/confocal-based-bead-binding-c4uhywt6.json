iewed":false,"protocol_references":"","public":true,"public_fork_note":"","published_on":1709785330,"references":[],"related_equipments":[],"related_materials":[],"reserved_doi":"10.17504/protocols.io.8epv5xdbng1b/v1","retraction_reason":null,"samples":{},"shared_access_id":0,"show_comparison":false,"sign_info":null,"space_access":{"can_view":false,"can_edit":false,"can_remove":false,"can_add":false,"can_publish":true,"can_get_doi":true,"can_share":false,"can_move":false,"can_move_outside":false,"can_transfer":false,"can_download":false,"limited_run":false,"limited_private_links":false,"limited_blind_links":false,"is_locked":false},"space_id":34457,"state_version_id":320,"stats":{"is_voted":false,"number_of_views":0,"number_of_steps":9,"number_of_bookmarks":0,"number_of_comments":0,"number_of_bookmarked_comments":0,"number_of_steps_comments":0,"number_of_protocol_comments":0,"number_of_exports":0,"number_of_runs":0,"number_of_votes":0,"number_of_reagents":0,"number_of_equipments":0,"number_of_collections":1,"number_of_forks":{"private":0,"public":0},"number_of_accessible_forks":0},"status":{"id":1,"info":""},"steps":[{"id":1846033,"guid":"F0C4936D3ADC49E59D4392F6121CA716","previous_id":0,"previous_guid":null,"section":"\u003cp\u003eProtein preparation\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"bdd1b\",\"text\":\" In order to prepare proteins for a fluorescent bead-binding experiment, you need one protein that can bind a resin (in my case MBP-Rubicon RH domain, which can bind amylose resin) and a fluorescently labelled bait protein (in my case, Rab7-AlexaFluor 647).\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"dmspa\",\"text\":\"\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"cn8s0\",\"text\":\"To prepare my AlexaFluor 647, used an NHS ester salt, and followed standard manufacturer procedures for labelling, quenching, and cleaning the protein (https://www.sigmaaldrich.com/US/en/technical-documents/protocol/genomics/pcr/nhs-ester-oligonucleotide-conjugation)\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[{\"key\":0,\"offset\":152,\"length\":114}],\"data\":{}},{\"key\":\"e5th7\",\"text\":\"\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"2qf86\",\"text\":\"Measure the degree of labelling of your sample. My degree of labelling was 0.9 fluorophores per Rab7 copy\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{\"0\":{\"type\":\"link\",\"mutability\":\"MUTABLE\",\"data\":{\"guid\":\"A6621C72801911EEA1FC0A58A9FEAC02\",\"url\":\"https://www.sigmaaldrich.com/US/en/technical-documents/protocol/genomics/pcr/nhs-ester-oligonucleotide-conjugation\"}}}}","data":null,"protocol_id":90729,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"1","cases":[],"critical":null},{"id":1846034,"guid":"8EA0A8A5903542BCA43B776C140DE753","previous_id":1846033,"previous_guid":"F0C4936D3ADC49E59D4392F6121CA716","section":"\u003cp\u003eProtein preparation\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"6vkth\",\"text\":\"Here, I wanted to assess the impact of phosphorylation on binding. In order to keep the samples as comparable as possible, I fluorescently labelled my Rab7 with 647, then separated the Rab7 into two pools. One pool I phosphorylated using TBK1, the other pool I treated with the same dilution and recovery steps, but omitted the TBK1. See the TBK1 phosphorylation protocol for details.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"1c1sd\",\"text\":\"\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90729,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"2","cases":[],"critical":null},{"id":1846035,"guid":"A7AAA8AE79BE47E6BA837D859B635AE8","previous_id":1846034,"previous_guid":"8EA0A8A5903542BCA43B776C140DE753","section":"\u003cp\u003eBead-binding and confocal\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"2c273\",\"text\":\"Washed and prepared a 40 uL suspension of 25% amylose resin in a 50 mM HEPES 7.5, 150 mM NaCl, 2 mM MgCl2, and 10 mM TCEP buffer. Incubated this resin with 500 nM MBP-RH domain and ~ 4 uM fluorescently-labelled Rab7.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"feb8q\",\"text\":\"\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"dtj3q\",\"text\":\"Prepare a negative control consisting of the experimental sample absent the resin-binding partner, in this case MBP-Rubicon RH\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90729,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"3","cases":[],"critical":null},{"id":1846037,"guid":"994659DCE48E4382BC4DCC4B51540641","previous_id":1846035,"previous_guid":"A7AAA8AE79BE47E6BA837D859B635AE8","section":"\u003cp\u003eBead-binding and confocal\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"2qbkc\",\"text\":\"Incubate samples on rocker at room temperature for 1 H. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90729,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4","cases":[],"critical":null},{"id":1846038,"guid":"6367F31A91A944849B080ADA9735FF25","previous_id":1846037,"previous_guid":"994659DCE48E4382BC4DCC4B51540641","section":"\u003cp\u003eBead-binding and confocal\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"c943p\",\"text\":\"Collect resin at bottom of tube using a tabletop centrifuge, and aspirate and discard supernatant.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90729,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"5","cases":[],"critical":null},{"id":1846039,"guid":"0002CB3A85D84B49B5738A645B2B4417","previous_id":1846038,"previous_guid":"6367F31A91A944849B080ADA9735FF25","section":"\u003cp\u003eBead-binding and confocal\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"ckvfp\",\"text\":\"Wash resin 3 times with 1 mL of room temperature buffer\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90729,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"6","cases":[],"critical":null},{"id":1846040,"guid":"06FFB32293194768B2594ED96F9A034A","previous_id":1846039,"previous_guid":"0002CB3A85D84B49B5738A645B2B4417","section":"\u003cp\u003eBead-binding and confocal\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"14h9h\",\"text\":\"Resuspend resin in 200 uL of buffer, then transfer to an 8-well confocal imaging chamber.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90729,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"7","cases":[],"critical":null},{"id":1846041,"guid":"BA595A9C973C4AED880BCD6EC6B23176","previous_id":1846040,"previous_guid":"06FFB32293194768B2594ED96F9A034A","section":"\u003cp\u003eBead-binding and confocal\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"32eco\",\"text\":\"Focus sample and image resin. Adjust imaging parameters if necessary.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90729,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"8","cases":[],"critical":null},{"id":1846042,"guid":"BEA05A8F8A3B45239BEA5D85341080F5","previous_id":1846041,"previous_guid":"BA595A9C973C4AED880BCD6EC6B23176","section":"\u003cp\u003eAnalysis\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"41hs4\",\"text\":\"In order to measure the brightness of an individual bead, export the images to FIJI, and define an ROI that contains a bead edge. Measure this ROI, and record the maximum brightness. This brightness should correspond to the edge of the bead. Repeat this for ~20 cells, and average to acquire a single biological replicate.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"kn8r\",\"text\":\"\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"cgdk2\",\"text\":\"Repeat experiment at least twice more to acquire two more biological replicates. Perform a T-test between the comparison groups in the experiment\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90729,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"9","cases":[],"critical":null}],"template_id":4,"title":"Confocal-based bead-binding ","title_html":"Confocal-based bead-binding ","type_id":1,"units":[{"id":1,"type_id":3,"name":"µL","can_manage":0,"read_only":0},{"id":2,"type_id":3,"name":"mL","can_manage":0,"read_only":0},{"id":3,"type_id":3,"name":"L","can_manage":0,"read_only":0},{"id":4,"typ