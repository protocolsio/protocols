ocks\":[{\"key\":\"2e9l4\",\"text\":\"Clean and prepare injector needle.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"12sov\",\"text\":\"Rinse 3x with 70% ethanol.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"b66gs\",\"text\":\"Rinse 3x with saline.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"5q209\",\"text\":\"\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":58411,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"2","cases":[],"critical":null},{"id":1349644,"guid":"0F31C5A1907B11ECAEC5D16D716F6222","previous_id":1349639,"previous_guid":"0F2FA2C0907B11ECAEC5D16D716F6222","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"fsnva\",\"text\":\"Animals: adult mice.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"ecasa\",\"text\":\"Coordinators: from lambda AP +0.09 cm, ML+0.12 cm, DV -0.44 cm\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"17o3p\",\"text\":\"Injection rate: 12 µl/hour, total 2 µl in 10 min.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":58411,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"3","cases":[],"critical":null},{"id":1349648,"guid":"0F33E881907B11ECAEC5D16D716F6222","previous_id":1349644,"previous_guid":"0F31C5A1907B11ECAEC5D16D716F6222","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"8hb6p\",\"text\":\"Anesthesia: IP animals with Avertin solution\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"e81e4\",\"text\":\"15 ml/10 g body weight. After approximately 3-5 minutes animal should be completely anesthetized. Verify this by observing no response to tail pinch.  \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":58411,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4","cases":[],"critical":null},{"id":1349651,"guid":"0F363270907B11ECAEC5D16D716F6222","previous_id":1349648,"previous_guid":"0F33E881907B11ECAEC5D16D716F6222","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"v7s4\",\"text\":\"Fill the glass micropipette needle with virus solution (20 µl) by drawing the syringe plunger slowly. You can see a line between mineral oil and the solution. Adjust the rate to 12 µl/hour, volume to 2 µl. Pre-run the micro pump until you see a drop of liquid in the needle tip. Resume the volume to 2 µl.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":58411,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"5","cases":[],"critical":null},{"id":1349652,"guid":"0F36F5C1907B11ECAEC5D16D716F6222","previous_id":1349651,"previous_guid":"0F363270907B11ECAEC5D16D716F6222","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"dhc1u\",\"text\":\"Make a midline scalp incision and identify both the bregma and lambda coronal sutures.  Place animal on platform in prone position and place head in ear bars (ear bars should be aligned with the area just anterior to external ear). Place snout in incisor bar and tighten loosely.  Equilibrate bregma and lambda by measuring height of each using the stereotaxic arm to lower cannula until it contacts the exposed skull surface. A difference of less than 0.02 cm is acceptable. Adjust by raising or lowering snout height using incisor bar (be careful not to tighten snout holder since it will force wrong head angle).\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":58411,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"6","cases":[],"critical":null},{"id":1349653,"guid":"0F37B911907B11ECAEC5D16D716F6222","previous_id":1349652,"previous_guid":"0F36F5C1907B11ECAEC5D16D716F6222","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"8afc0\",\"text\":\"From lambda dial in the following coordinates: AP: +0.09 cm, ML: +0.12 cm, lower needle tip to exposed dura and dial in: DV: -0.44 cm.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":58411,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"7","cases":[],"critical":null},{"id":1349654,"guid":"0F38CA81907B11ECAEC5D16D716F6222","previous_id":1349653,"previous_guid":"0F37B911907B11ECAEC5D16D716F6222","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"fgvgc\",\"text\":\"Infuse AAV alpha synuclein, at a rate of 12 µl/hour for 10 min (total 2 µL). Wait 2 minute and slowly withdraw needle 1 minutes after the end of the infusion\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":58411,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"8","cases":[],"critical":null},{"id":1349655,"guid":"0F39B4E1907B11ECAEC5D16D716F6222","previous_id":1349654,"previous_guid":"0F38CA81907B11ECAEC5D16D716F6222","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"fpt86\",\"text\":\"Suture scalp and place animal in incubator for recovery from anesthesia.   \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":58411,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"9","cases":[],"critical":null}],"template_id":4,"title":"Unilateral intranigral AAV alpha synuclein mouse model","title_html":"\u003cp\u003eUnilateral intranigral AAV alpha synuclein mouse model\u003c/p\u003e","type_id":1,"units":[{"id":1,"type_id":3,"name":"µL","can_manage":0},{"id":2,"type_id":3,"name":"mL","can_manage":0},{"id":3,"type_id":3,"name":"L","can_manage":0},{"id":4,"type_id":3,"name":"µg","can_manage":0},{"id":5,"type_id":3,"name":"mg","can_manage":0},{"id":6,"type_id":3,"name":"g","can_manage":0},{"id":7,"type_id":3,"name":"kg","can_manage":0},{"id":8,"type_id":3,"name":"ng","can_manage":0},{"id":9,"type_id":3,"name":"Hz","can_manage":0},{"id":10,"type_id":24,"name":"°C","can_manage":0},{"id":11,"type_id":24,"name":"°К","can_manage":0},{"id":12,"type_id":24,"name":"°F","can_manage":0},{"id":13,"type_id":25,"name":"Mass Percent","can_manage":0},{"id":14,"type_id":25,"name":"% volume","can_manage":0},{"id":15,"type_id":25,"name":"Mass / % volume","can_manage":0},{"id":16,"type_id":25,"name":"Parts per Million (PPM)","can_manage":0},{"id":17,"type_id":25,"name":"Parts per Billion (PPB)","can_manage":0},{"id":18,"type_id":25,"name":"Parts per Trillion (PPT)","can_manage":0},{"id":19,"type_id":25,"name":"Mole Fraction","can_manage":0},{"id":20,"type_id":25,"name":"Mole Percent","can_manage":0},{"id":21,"type_id":25,"name":"Molarity (M)","can_manage":0},{"id":22,"type_id":25,"name":"Molarity (m)","can_manage":0},{"id":23,"type_id":25,"name":"Genome copies per ml","can_manage":0},{"id":24,"type_id":3,"name":"μV","can_manage":0},{"id":25,"type_id":3,"name":"ms","can_manage":0},{"id":26,"type_id":3,"name":"pg","can_manage":0},{"id":27,"type_id":25,"name":"Molarity dilutions","can_manage":0},{"id":28,"type_id":25,"name":"millimolar (mM)","can_manage":0},{"id":29,"type_id":25,