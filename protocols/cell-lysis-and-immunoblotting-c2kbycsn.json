er_of_steps":6,"number_of_bookmarks":0,"number_of_comments":0,"number_of_bookmarked_comments":0,"number_of_steps_comments":0,"number_of_protocol_comments":0,"number_of_exports":0,"number_of_runs":0,"number_of_votes":0,"number_of_reagents":0,"number_of_equipments":0,"number_of_collections":0,"number_of_forks":{"private":0,"public":0},"number_of_accessible_forks":0},"status":{"id":1,"info":"We use this protocol and it's working"},"steps":[{"id":1800691,"guid":"10AF84F58D3D40A893441AE91109ABFF","previous_id":0,"previous_guid":null,"section":"\u003cp\u003eHarvest and Lyse cells\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"9340q\",\"text\":\"Cells were cultured in the presence of the corresponding stress to 60-80% confluency in 6-well plates, 10 cm or 15 cm dishes. After removing the media, the cells were washed with DPBS three times. \\n\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":88419,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"1","cases":[],"critical":null},{"id":1800694,"guid":"6792A186BA0D42528151D7C1F7E40D5A","previous_id":1800691,"previous_guid":"10AF84F58D3D40A893441AE91109ABFF","section":"\u003cp\u003eHarvest and Lyse cells\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"7q9ae\",\"text\":\"To lyse cell urea buffer (8M urea, 50 mM TRIS 7.5, 150 mM NaCl, containing mammalian protease inhibitor cocktail (Sigma), Phos-STOP, and 20 unit/ml Benzonase (Millipore)) was added directly onto the cells. Cell lysates were collected by cell scrapers and sonicated on ice for 10 seconds at level 5, and lysates were cleared by centrifugation (15000 rpm, 10 min at 4 oC). \\n\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"sup\",\"offset\":365,\"length\":2}],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":88419,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"2","cases":[],"critical":null},{"id":1800708,"guid":"A0C4F80B56974E22B6C098C8CBE30C70","previous_id":1800694,"previous_guid":"6792A186BA0D42528151D7C1F7E40D5A","section":"\u003cp\u003eRun SDS-page gel and transfer to membrane for immunoblotting\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"7i4bt\",\"text\":\"The concentration of the supernatant was measured by BCA assay. For immunoblotting, the whole cell lysate was denatured by the addition of LDS sample buffer supplemented with 100 mM DTT, followed by boiling at 95oC for 5 minutes\\n\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"sup\",\"offset\":212,\"length\":1}],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":88419,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"3","cases":[],"critical":null},{"id":1800733,"guid":"F52D7B058E2D479F9127AF645F620CBE","previous_id":1800708,"previous_guid":"A0C4F80B56974E22B6C098C8CBE30C70","section":"\u003cp\u003eRun SDS-page gel and transfer to membrane for immunoblotting\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"ef7c4\",\"text\":\"10-20 mg of each lysate was loaded onto the 4-20% Tris-Glycine gel (Thermo Fisher Scientific) or 4-12% NuPAGE Bis-Tris gel (Thermo Fisher Scientific), followed by SDS-PAGE with Tris-Glycine SDS running buffer (Thermo Fisher Scientific) or MOPS SDS running buffer (Thermo Fisher Scientific), respectively. \\n\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":88419,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4","cases":[],"critical":null},{"id":1800734,"guid":"F2D8FDB510EE4C62BC4796B517D4F908","previous_id":1800733,"previous_guid":"F52D7B058E2D479F9127AF645F620CBE","section":"\u003cp\u003eRun SDS-page gel and transfer to membrane for immunoblotting\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"e5p98\",\"text\":\"For Chemiluminescence westerns: The proteins were electro-transferred to PVDF membranes (0.45 µm, Millipore), and then the total protein was stained using Ponceau (Thermo Fisher Scientific). The membrane was then blocked with 5% non-fat milk (r.t., 60 min) incubated with the indicated primary antibodies (4oC, overnight), washed three times with TBST (total 30 min), and further incubated either with HRP conjugated anti-Rabbit and anti-mouse secondaries at (1:5,000) for 1h. After thorough wash with TBST for 30 min membranes were treated with Lightning‱ Plus Chemiluminescence Reagent (PerkinElmer, NEL104001EA) after mixing the Enhanced Luminol Reagent and the Oxidizing Reagent 1:1. Mixed Chemiluminescence Reagent was added to blot and rocked gently for 1 minute and imaged using BioRad ChemiDoc Imaging System\\n\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"italic\",\"offset\":0,\"length\":31},{\"style\":\"sup\",\"offset\":307,\"length\":1}],\"entityRanges\":[{\"key\":0,\"offset\":555,\"length\":1}],\"data\":{}}],\"entityMap\":{\"0\":{\"type\":\"emoji\",\"mutability\":\"MUTABLE\",\"data\":{\"guid\":\"49CE8A685C9811EE89760A58A9FEAC02\",\"name\":\":tm:\"}}}}","data":null,"protocol_id":88419,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"5","cases":[],"critical":null},{"id":1800758,"guid":"0C329B652A694DBE93FDEA48B3AD867E","previous_id":1800734,"previous_guid":"F2D8FDB510EE4C62BC4796B517D4F908","section":"\u003cp\u003eRun SDS-page gel and transfer to membrane for immunoblotting\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"cpfum\",\"text\":\"\\n\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"ethfd\",\"text\":\"For LI-COR westerns: The proteins were electro-transferred to nitrocellulose membranes and then the total protein was stained using Ponceau (Thermo Fisher Scientific). The membrane was then blocked with LI-COR blocking buffer at room temperature for 1h. Then membranes were incubated with the indicated primary antibodies (4oC, overnight), washed three times with TBST (total 30 min), and further incubated either with fluorescent IRDye 680RD Goat anti-Mouse IgG H+L, or IRDye 800CW Goat anti-Rabbit IgG H+L secondary antibody at (1:10,000) at room temperature for 1h. After thorough wash with TBST for 30 min, near infrared signal was detected using OdysseyCLx imager and quantified using ImageStudioLite (LI-COR).\",\"type\":\"align-justify\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"italic\",\"offset\":0,\"length\":20},{\"style\":\"sup\",\"offset\":324,\"length\":1}],\"entityRanges\":[],\"data\":{}},{\"key\":\"blccm\",\"text\":\"\\n\\n\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":88419,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"6","cases":[],"critical":null}],"template_id":1,"title":"Cell lysis and immunoblotting","title_html":"Cell lysis and immunoblotting","type_id":1,"units":[{"id":1,"type_id":3,"name":"µL","can_manage":0,"read_only":0},{"id":2,"type_id":3,"name":"mL","can_manage":0,"read_only":0},{"id":3,"type_id":3,"name":"L","can_manage":0,"read_only":0},{"id":4,"type_id":3,"name":"µg","can_manage":0,"read_only":0},{"id":5,"type_id":3,"name":"mg","can_manage":0,"read_only":0},{"id":6,"type_id":3,"name":"g","can_manage":0,"read_only":0},{"id":7,"type_id":3,"name":"kg","can_manage":0,"read_only":0},{"id":8,"type_id":3,"name":"ng","can_manage":0,"read_only":0},{"id":9,"type_id":3,"name":"Hz","can_manage":0,"read_only":0},{"id":10,"type_id":24,"name":"°C","can_manage":0,"read_only":0},{"id