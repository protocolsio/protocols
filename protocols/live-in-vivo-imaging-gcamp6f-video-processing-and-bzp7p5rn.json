":false,"sign_info":null,"state_version_id":52,"stats":{"is_voted":false,"number_of_views":29,"number_of_steps":10,"number_of_bookmarks":0,"number_of_comments":0,"number_of_bookmarked_comments":0,"number_of_steps_comments":0,"number_of_protocol_comments":0,"number_of_exports":0,"number_of_runs":0,"number_of_votes":0,"number_of_reagents":0,"number_of_equipments":0,"number_of_collections":0,"number_of_forks":{"private":0,"public":0},"number_of_accessible_forks":0},"status":{"id":1,"info":"We use this protocol and it’s working"},"steps":[{"id":1281119,"guid":"835468E03BFB11ECBD1C5900AC158EC3","previous_id":0,"previous_guid":null,"section":"\u003cp\u003ePreparation\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"3r0ga\",\"text\":\"AAV-PHP.S-CAG-GCaMP6F (1012VGs) was delivered systemically to WT C57.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"sup\",\"offset\":25,\"length\":2}],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":54751,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"1","cases":[],"critical":null},{"id":1281120,"guid":"8DF9C8303BFB11ECBD1C5900AC158EC3","previous_id":1281119,"previous_guid":"835468E03BFB11ECBD1C5900AC158EC3","section":"\u003cp\u003ePreparation\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"9c4gb\",\"text\":\"3-4 weeks after infection, mice were anesthetized with 2% isoflurane on a heating pad (Kent Scientific, Torrington, CT-DCT15) with plastic sleeve covers (Kent Scientific, Torrington, CT-DCT1520P). \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":54751,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"2","cases":[],"critical":null},{"id":1281123,"guid":"9F0D1EB03BFB11ECBD1C5900AC158EC3","previous_id":1281120,"previous_guid":"8DF9C8303BFB11ECBD1C5900AC158EC3","section":"\u003cp\u003ePreparation\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"f8usd\",\"text\":\"The abdominal cavity was surgically opened to expose the intestines. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":54751,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"3","cases":[],"critical":null},{"id":1281126,"guid":"B111DA103BFB11ECBD1C5900AC158EC3","previous_id":1281123,"previous_guid":"9F0D1EB03BFB11ECBD1C5900AC158EC3","section":"\u003cp\u003eImaging\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"72bqg\",\"text\":\"The proximal colon was identified, and this portion was placed on top of a stack of 4-6 glass microscopy slides (depending on size of animal) (VWR, Radnor PA-Cat. No. 48300-026). \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":54751,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4","cases":[],"critical":null},{"id":1281128,"guid":"BA8BB1B03BFB11ECBD1C5900AC158EC3","previous_id":1281126,"previous_guid":"B111DA103BFB11ECBD1C5900AC158EC3","section":"\u003cp\u003eImaging\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"dppc6\",\"text\":\"Tissue was secured onto glass slide with a biorthogonal silicon elastomer (Kwik-Sil) (World Precision Instruments, Sarasota, FL-KWIK-SIL), and a glass coverslip.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":54751,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"5","cases":[],"critical":null},{"id":1281130,"guid":"BFB011E03BFB11ECBD1C5900AC158EC3","previous_id":1281128,"previous_guid":"BA8BB1B03BFB11ECBD1C5900AC158EC3","section":"\u003cp\u003eImaging\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"d7j5j\",\"text\":\" Elastomer stiffens within 1 minute of application and coverslip must be held steadily to ensure a flat imaging surface. Anesthetized mouse is placed under an upright confocal microscope (Zeiss LSM 880). \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":54751,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"6","cases":[],"critical":null},{"id":1281132,"guid":"C434FDC03BFB11ECBD1C5900AC158EC3","previous_id":1281130,"previous_guid":"BFB011E03BFB11ECBD1C5900AC158EC3","section":"\u003cp\u003eImaging\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"ci7l4\",\"text\":\"Using a 10X objective, GCaMP6F fluorescence was taken at 5Hz (1 image every 200ms). \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":54751,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"7","cases":[],"critical":null},{"id":1281133,"guid":"C92C81903BFB11ECBD1C5900AC158EC3","previous_id":1281132,"previous_guid":"C434FDC03BFB11ECBD1C5900AC158EC3","section":"\u003cp\u003eImaging\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"79ubi\",\"text\":\"Periods of movement of tissue and luminal contents is normal during live imaging. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":54751,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"8","cases":[],"critical":null},{"id":1281137,"guid":"D86C9EB03BFB11ECBD1C5900AC158EC3","previous_id":1281133,"previous_guid":"C92C81903BFB11ECBD1C5900AC158EC3","section":"\u003cp\u003eProcessing \u0026amp; Analysis\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"e3mjq\",\"text\":\"Cell tracking was performed in 2D by using TrackMate ImageJ plugin (https://imagej.net/TrackMate) and fluorescence intensity was recorded from cells. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[{\"key\":0,\"offset\":68,\"length\":28}],\"data\":{}}],\"entityMap\":{\"0\":{\"type\":\"link\",\"mutability\":\"\",\"data\":{\"guid\":\"DC377F2A3BFB11ECA98F0A58A9FEAC02\",\"url\":\"https://imagej.net/TrackMate\"}}}}","data":null,"protocol_id":54751,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"9","cases":[],"critical":null},{"id":1281138,"guid":"DFCF08003BFB11ECBD1C5900AC158EC3","previous_id":1281137,"previous_guid":"D86C9EB03BFB11ECBD1C5900AC158EC3","section":"\u003cp\u003eProcessing \u0026amp; Analysis\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"805b6\",\"text\":\"Average background was determined by taking the fluorescence of a region of interest that did not contain a cell, over the duration of the video. Background was subtracted from cell fluorescence intensities.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":54751,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"10","cases":[],"critical":null}],"template_id":4,"title":"Live, in vivo, imaging (GCaMP6F), Video Processing, and Analysis","title_html":"\u003cp\u003eLive, in vivo, imaging (GCaMP6F), Video Processing, and Analysis\u003c/p\u003e","type_id":1,"units":[{"id":1,"type_id":3,"name":"µL","can_manage":0},{"id":2,"type_id":3,"name":"mL","can_manage":0},{"id":3,"type_id":3,"name":"L","can_manage":0},{"id":4,"type_id":3,"name":"µg","can_manage