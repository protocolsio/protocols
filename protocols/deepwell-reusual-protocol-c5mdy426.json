arked_comments":0,"number_of_steps_comments":0,"number_of_protocol_comments":0,"number_of_exports":0,"number_of_runs":0,"number_of_votes":0,"number_of_reagents":0,"number_of_equipments":0,"number_of_collections":0,"number_of_forks":{"private":0,"public":0},"number_of_accessible_forks":0},"status":{"id":1,"info":"We use this protocol and it's working"},"steps":[{"id":1863182,"guid":"116BCE103E064D4BB17BB2BFEDD01662","previous_id":0,"previous_guid":null,"section":"\u003cp\u003eGrowing cultures in a deep well\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"fdgtt\",\"text\":\"Cover a deep well with two layers of aluminium foil and tape the front and back to hold it securely. The tape also turns the aluminium foil into a lid for the deep well by temporarily peeling off the front tape.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":91525,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"1","cases":[],"critical":null},{"id":1863183,"guid":"D54C4DC40AD54BFE94CF7BB4B7F00AD5","previous_id":1863182,"previous_guid":"116BCE103E064D4BB17BB2BFEDD01662","section":"\u003cp\u003eGrowing cultures in a deep well\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"frd3\",\"text\":\"Autoclave at 120C for 20min. After autoclaving, keep in a heated oven (\\u003e 50C) overnight for drying and desiccation.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":91525,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"2","cases":[],"critical":null},{"id":1863184,"guid":"A8649A38EE73453F84305F09EE7709F9","previous_id":1863183,"previous_guid":"D54C4DC40AD54BFE94CF7BB4B7F00AD5","section":"\u003cp\u003eGrowing cultures in a deep well\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"bj7v8\",\"text\":\"Do not fill the deep wells more than 60% of their max volume if they are to be placed in a shaking incubator to avoid spillage.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":91525,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"3","cases":[],"critical":null},{"id":1863187,"guid":"C9C8B2D8B32D46AC9DDC1454B95AC385","previous_id":1863184,"previous_guid":"A8649A38EE73453F84305F09EE7709F9","section":"\u003cp\u003eDeep well Decontamination\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"19d4p\",\"text\":\"Tape around the deep well over the aluminium foil to prevent spillage.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":91525,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4","cases":[],"critical":null},{"id":1863188,"guid":"7BEBD4ADA8934D35A604A60BB15C86B9","previous_id":1863187,"previous_guid":"C9C8B2D8B32D46AC9DDC1454B95AC385","section":"\u003cp\u003eDeep well Decontamination\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"29ooi\",\"text\":\"While carrying the deep well outside the lab space, enclose it in a waterproof autoclave bag. Keep sufficient absorbent paper on hand in case of accidental spillage.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":91525,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"5","cases":[],"critical":null},{"id":1863189,"guid":"24B7B2E50EA842DBB5D341262CA75E02","previous_id":1863188,"previous_guid":"7BEBD4ADA8934D35A604A60BB15C86B9","section":"\u003cp\u003eDeep well Decontamination\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"en31o\",\"text\":\"In the autoclave facility, autoclave the deep wells at 120C for \\u003e30min to decontaminate.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":91525,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"6","cases":[],"critical":null},{"id":1863190,"guid":"8C4E7BA4B9B54406A6C90BBF336060D2","previous_id":1863189,"previous_guid":"24B7B2E50EA842DBB5D341262CA75E02","section":"\u003cp\u003eDeep well Decontamination\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"dm9r5\",\"text\":\"Once autoclaved, the deep wells will be brought back to the lab\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":91525,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"7","cases":[],"critical":null},{"id":1863191,"guid":"3BD2EA36D461493C82BA12636D07752A","previous_id":1863190,"previous_guid":"8C4E7BA4B9B54406A6C90BBF336060D2","section":"\u003cp\u003eDeep well Decontamination\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"8llr0\",\"text\":\"Carefully peel the aluminium foil and dispose the cultures and aluminium foil into a disposal bag containing paper roll to absorb the liquid.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":91525,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"8","cases":[],"critical":null},{"id":1863192,"guid":"58ADC1B52B7D455396663C7660C59127","previous_id":1863191,"previous_guid":"3BD2EA36D461493C82BA12636D07752A","section":"\u003cp\u003eDeep well Decontamination\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"39o6j\",\"text\":\"Send decontaminated culture for incineration (yellow bin route).\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":91525,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"9","cases":[],"critical":null},{"id":1863193,"guid":"902C622A9288432EABCA263178EE2209","previous_id":1863192,"previous_guid":"58ADC1B52B7D455396663C7660C59127","section":"\u003cp\u003eDeep well Decontamination\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"du71n\",\"text\":\"Rinse the deep well plate with water and a brush to clean before future use.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":91525,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"10","cases":[],"critical":null},{"id":1863194,"guid":"A3B735FBB3594E6D833385CB65458271","previous_id":1863193,"previous_guid":"902C622A9288432EABCA263178EE2209","section":"\u003cp\u003eDeep well Decontamination\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"29cms\",\"text\":\"Keep in a heated oven (\\u003e 50C) overnight for drying and desiccation\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":91525,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"11","cases":[],"critical":null},{"id":1863196,"guid":"21E9D34176EE4AC38E0FF9225EEDF979","previous_id":1863194,"previous_guid":"A3B735FBB3594E6D833385CB65458271","section":"\u003cp\u003eMajor Risks\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"db0pv\",\"text\":\"Using deep wells presents similar risk as using a microtiter plate and similar precautions need to be taken.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":91525,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"12","cases":[],"critical":null},{"id":1863197,"guid":"A72C5F18E2404F229E48D4E83B5CF597","previous_id":1863196,"previous_guid":"21E9D34176EE4AC38E0FF9225EEDF979","section":"\u003cp\u003eMajor Risks\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"9st3i\",\"text\":\"Spilling of culture during shaking - At speeds of 180rpm, the spillage is be negligible\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":91525,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"13","cases":[],"critical":null},{"id":1863198,"guid":"482D78C52CD1452E9ACD8458C78E88B5","previous_id":1863197,"previous_guid":"A72C5F18E2404F229E48D4E83B5CF597","section":"\u003cp\u003eMajor Risks\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"86kne\",\"text\":\"Spillage while handling - If handled with reasonable care and awareness, the risk of spillage is negligible. Presence of the aluminium foil and tape reduces this risk. Even if inverted, outside spillage would be minimal, however care would have to be taken while removing the aluminium foil lid post autoclaving.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":91525,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"14","cases":[],"critical":null}],"template_id":1,"title":"Deepwell Reusual Protocol ","title_html":"Deepwell Reusual Protocol ","type_id":1,"units":[{"id":1,"type_id":3,"name":"µL","can_manage":0,"read_