ent_uri":null,"is_same_owner":false,"is_parent_public":false,"has_pending_merge_request":false,"has_approved_merge_request":false,"merge_request":null},"creator":{"name":"Emily A Aery Jones","username":"emily-aery-jones","affiliation":"Stanford University","affiliation_url":"","link":"","is_verified_user":true,"image":{"source":"https://lh3.googleusercontent.com/a/AATXAJynClPYaIY3SSIbhneMGoDr_T8dtqhWyYO4s0Lo=s96-c","webp_source":null,"placeholder":"https://lh3.googleusercontent.com/a/AATXAJynClPYaIY3SSIbhneMGoDr_T8dtqhWyYO4s0Lo=s96-c","webp_placeholder":null}},"image":{"source":"https://content.protocols.io/files/kiepbqrq7.jpg","webp_source":null,"placeholder":"https://content.protocols.io/files/kiepbqrq7.jpg","webp_placeholder":null},"uri":"freely-moving-recording-chronic-recoverable-neurop-cnr7vd9n","private_link":null,"blind_link":null}}],"created_on":1688059299,"creator":{"name":"Emily A Aery Jones","affiliation":"Stanford University","affiliation_url":"","username":"emily-aery-jones","link":"","image":{"source":"https://lh3.googleusercontent.com/a/AATXAJynClPYaIY3SSIbhneMGoDr_T8dtqhWyYO4s0Lo=s96-c","placeholder":"https://lh3.googleusercontent.com/a/AATXAJynClPYaIY3SSIbhneMGoDr_T8dtqhWyYO4s0Lo=s96-c","webp_source":""},"badges":[{"id":3,"name":"Power author!","image":{"source":"/img/badges/silver.svg","placeholder":"/img/badges/silver.svg"}}],"affiliations":[{"affiliation":"Stanford University","url":"","is_default":true}]},"description":"{\"blocks\":[{\"key\":\"bfqe\",\"text\":\"This protocol collection explains how to build a low-cost, lightweight system to implant 1 Neuropixels 1.0 probe or 2 Neuropixels 2.0 probes into mice, record during freely moving behavior, then recover the probes for future use. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"4sad\",\"text\":\"\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"4a437\",\"text\":\"Other chronic recoverable designs:\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"bold\",\"offset\":0,\"length\":34}],\"entityRanges\":[],\"data\":{}},{\"key\":\"9ocp7\",\"text\":\" single 1.0 probe in mice: Juavinett et al, 2019 (https://elifesciences.org/articles/47188)\",\"type\":\"unordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[{\"key\":0,\"offset\":50,\"length\":41}],\"data\":{}},{\"key\":\"3ko6a\",\"text\":\" multiple 1.0 probes in rats: Luo \\u0026 Bondy et al, 2020 (https://elifesciences.org/articles/59716)\",\"type\":\"unordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[{\"key\":1,\"offset\":55,\"length\":41}],\"data\":{}},{\"key\":\"92r4f\",\"text\":\" single 2.0 probe in mice and dual or moveable 2.0 probes in rats: van Daal, Aydin, \\u0026 Michon et al, 2021 (https://www.nature.com/articles/s41596-021-00539-9)\",\"type\":\"unordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[{\"key\":2,\"offset\":106,\"length\":50}],\"data\":{}},{\"key\":\"dc48b\",\"text\":\" overview of unpublished designs: Neuropixels 2021 course lecture 3.7 by Yoh Isogai (https://www.youtube.com/watch?v=7cZqYIiGvBQ\\u0026list=PLfhWmWntvjl64ti_a-MzHlwqwEU0ZlALb\\u0026index=18) \",\"type\":\"unordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[{\"key\":3,\"offset\":85,\"length\":93}],\"data\":{}},{\"key\":\"6f7ee\",\"text\":\"\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"8chdq\",\"text\":\"Advantages of this design:\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"bold\",\"offset\":0,\"length\":26}],\"entityRanges\":[],\"data\":{}},{\"key\":\"4tlid\",\"text\":\"Lightweight (entire assembly, including headbar, dental cement, and tape, weighs \\u003c3g for single 1.0 probe or \\u003c4g for dual 2.0 probes)\",\"type\":\"unordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"amp64\",\"text\":\"Allows headfixed or freely moving recordings\",\"type\":\"unordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"dt48j\",\"text\":\"Quickly attaches to a headstage holder, which provides LEDs for tracking and a counterweight to encourage running\",\"type\":\"unordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"dlj1i\",\"text\":\"Position and angle of internal mount on the probe adjustable before gluing, allowing implantation into any brain region, but custom adjustment is not required, allowing the same probe to be re-inserted into a variety of sites\",\"type\":\"unordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"3d7f2\",\"text\":\"Unlike completely enclosed designs, the shank is uncovered during insertion for better visualization, yet doesn't require delicately surrounding the shank with a glue column\",\"type\":\"unordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"7h30k\",\"text\":\"Dual 2.0 probes: probes are independently insertable, so can set custom positions, depths, and angles for each\",\"type\":\"unordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"57r3q\",\"text\":\"\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"fq9gt\",\"text\":\"Disadvantages of this design:\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"bold\",\"offset\":0,\"length\":29}],\"entityRanges\":[],\"data\":{}},{\"key\":\"7rt6n\",\"text\":\"Not suitable for larger animals (lightweight design likely can't withstand larger forces, flex cable remains exposed, moisture known to wick up shank into PCB in rats)\",\"type\":\"unordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"aap7m\",\"text\":\"External components are assembled around the probe during surgery rather than during assembly, so surgeries take slightly longer\",\"type\":\"unordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"65c59\",\"text\":\"Less elegant than completely enclosed designs and requires a larger skull surface area for gluing\",\"type\":\"unordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"cas5\",\"text\":\"Dual 2.0 probes: how close together the probes can get is limited by some of the components\",\"type\":\"unordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"cjlu2\",\"text\":\"\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"c1o7s\",\"text\":\"Assembly preview:\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"bold\",\"offset\":0,\"length\":17}],\"entityRanges\":[],\"data\":{}},{\"key\":\"fsv2t\",\"text\":\"Single 1.0 probe\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"6bumh\",\"text\":\" \",\"type\":\"atomic\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[{\"key\":4,\"offset\":0,\"length\":1}],\"data\":{}},{\"key\":\"3g2f7\",\"text\":\"\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"2lgph\",\"text\":\"Dual 2.0 beta probes\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"cd97u\",\"text\":\"\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"5qmac\",\"text\":\" \",\"type\":\"atomic\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[{\"key\":5,\"offset\":0,\"length\":1}],\"data\":{}},{\"key\":\"38rb8\",\"text\":\"\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{\"0\":{\"type\":\"link\",\"mutability\":\"MUTABLE\",\"data\":{\"guid\":\"93782625A37611EDA2B90A58A9FEAC02\",\"url\":\"https://elifesciences.org/articles/47188)\"}},\"1\":{\"type\":\"link\",\"mutability\":\"MUTABLE\",\"data\":{\"guid\":\"8718CE8EA37311EDA2B90A58A9FEAC02\",\"url\":\"https://elifesciences.org/articles/59716)\"}},\"2\":{\"type\":\"link\",\"mutability\":\"MUTABLE\",\"data\":{\"guid\":\"EE82F58AA37511EDA2B90A58A9FEAC02\",\"url\":\"https://www.nature.com/articles/s41596-021-00539-9\"}},\"3\":{\"type\":\"link\",\"mutability\":\"MUTABLE\",\"data\":{\"guid\":\"8718CF26A37311EDA2B90A58A9FEAC02\",\"url\":\"https://www.youtube.com/watch?v=7cZqYIiGvBQ\\u0026list=PLfhWmWntvjl64ti_a-MzHlwqwEU0ZlALb\\u0026index=18)\"}},\"4\":{\"type\":\"video\",\"mutability\":\"IMMUTABLE\",\"data\":{\"guid\":\"20762F60A37911EDB3D751F14CC67362\",\"height\":0,\"id\":335821,\"is_video\":true,\"legend\":\"\",\"mime\":\"video/mp4\",\"nestedSelector\":null,\"original_name\":\"chronic_NPX.mp4\",\"placeholder\":\"https://content.protocols.io/files/kh8qbqrq7.jpg\",\"shadow\":false,\"source\":\"https://content.protocols.io/files/kh8pbqrq7.mp4\",\"width\":0}},\"5\":{\"type\":\"video\",\"mutability\":\"IMMUTABLE\",\"data\":{\"guid\":\"67EEFFC0A37911EDB3D751F14CC67362\",\"height\":0,\"id\":335823,\"is_video\":true,\"legend\":\"\",\"mime\":\"video/mp4\",\"nestedSelector\":null,\"original_name\":\"dual_20_beta_probe_assembly.mp4\",\"placeholder\":\"https://content.protocols.io/files/kh8sbqrq7.jpg\",\"shadow\":false,\"source\":\"https://content.protocols.io/files/kh8rbqrq7.mp4\",\"width\":0}}}}","disclaimer":"","document":"","documents":[],"doi":"dx.doi.org/10.17504/protocols.io.e6nvwjo87lmk/v2","doi_status":1,"ethics_statement":null,"fork_id":null,"fork_info":null,"forks":[],"funders":[{"funder_name":"Simons Collaboration on the Global Brain","grant_id":"542987SPI"},{"funder_name":"Stanford School of Medicine Dean's Postdoctoral Fellowship","grant_id":""},{"funder_name":"A