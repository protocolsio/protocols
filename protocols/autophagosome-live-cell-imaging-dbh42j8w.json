:0,"number_of_bookmarked_comments":0,"number_of_steps_comments":0,"number_of_protocol_comments":0,"number_of_exports":0,"number_of_runs":0,"number_of_votes":0,"number_of_reagents":0,"number_of_equipments":0,"number_of_collections":0,"number_of_forks":{"private":0,"public":0},"number_of_accessible_forks":0},"status":{"id":1,"info":"We use this protocol and it's working"},"steps":[{"id":1998186,"guid":"B31D5C48ED024BA18C4AC2EABEADA1AA","previous_id":0,"previous_guid":null,"section":"\u003cp\u003eCell Culture\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"eg1bk\",\"text\":\"Seed Parkin-expressing HeLa cells for ~80% confluence on day of transfection on tissue-culture coated glass slides, suitable for use on a confocal microscope.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":97564,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"1","cases":[],"critical":null},{"id":1998187,"guid":"FE703B0C492849B9924494A7F221AF8F","previous_id":1998186,"previous_guid":"B31D5C48ED024BA18C4AC2EABEADA1AA","section":"\u003cp\u003eCell Culture\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"1e1hj\",\"text\":\"On day of transfection, transfect pCIG2-LAMP1-mNeon Green, pHaloTag-LC3B, and an autophagy regulating protein tagged with mcherry via lipofectamine 3,000 (ThermoFisher). Briefly, dilute plasmids in Opti-mem reduced serum medium and add P3000 reagent in accordance with manufacturer instructions. Also dilute lipofectamine 3000 reagent in optimem, then mix the dilute plasmid and lipofectamine solutions together. Incubate for ~10 minutes, then add dropwise to adhered cells.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":97564,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"2","cases":[],"critical":null},{"id":1998188,"guid":"0778CB7BFF7940EF9878288ECAFA613F","previous_id":1998187,"previous_guid":"FE703B0C492849B9924494A7F221AF8F","section":"\u003cp\u003eCell Culture\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"eouvl\",\"text\":\"Allow cells to recover overnight\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":97564,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"3","cases":[],"critical":null},{"id":1998189,"guid":"0157AD4BF89A4E10A8B3E9F51BC591AC","previous_id":1998188,"previous_guid":"0778CB7BFF7940EF9878288ECAFA613F","section":"\u003cp\u003eCell Culture\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"9old4\",\"text\":\"On day of experiment swap cells into growth medium containing 10 µM Oligomycin A and 5 µM Antimycin A. Return to incubator for 4 H\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":97564,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4","cases":[],"critical":null},{"id":1998190,"guid":"EA49723B4EB545C394ECA4DD15250F80","previous_id":1998189,"previous_guid":"0157AD4BF89A4E10A8B3E9F51BC591AC","section":"\u003cp\u003eImaging and Analysis\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"5h1bk\",\"text\":\"When ready to image, swap medium with Leibovitz’s L-15 medium (Gibco 11415064) supplemented\\nwith 10% fetal bovine serum and 1% Glutamax along with Antimycin A and\\nOligomycin A. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":97564,"case_id":0,"critical_ids":"","duration":0,"or