{"access":{"can_view":true,"can_edit":false,"can_remove":false,"can_add":false,"can_publish":false,"can_get_doi":false,"can_share":true,"can_move":true,"can_move_outside":true,"can_transfer":true,"can_download":true,"is_locked":false},"authors":[{"name":"Jacob   Waldbauer","affiliation":"University of Chicago","affiliation_url":null,"username":"jacob--waldbauer","link":null,"image":{"source":"/img/avatars/016.png","placeholder":"/img/avatars/016.png","webp_source":""},"note":"","is_verified_author":true,"is_verified_user":true},{"name":"Lichun Zhang","affiliation":"University of Chicago","affiliation_url":null,"username":"","link":null,"image":{"source":"","placeholder":"","webp_source":""},"note":"","is_verified_author":true,"is_verified_user":false}],"before_start":"{\"blocks\":[{\"key\":\"33juh\",\"text\":\"\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":{}}","book_chapter":null,"can_accept_authorship":false,"can_be_copied":true,"can_claim_authorship":false,"can_remove_fork":false,"can_sign":false,"child_steps":{},"cited_protocols":[],"collection_items":[],"created_on":1642793620,"creator":{"name":"Jacob  Waldbauer","affiliation":"","affiliation_url":null,"username":"jacob--waldbauer","link":null,"image":{"source":"/img/avatars/016.png","placeholder":"/img/avatars/016.png","webp_source":""},"badges":[{"id":3,"name":"Power author!","image":{"source":"/img/badges/silver.svg","placeholder":"/img/badges/silver.svg"}},{"id":5,"name":"Early adopter","image":{"source":"/img/badges/star.svg","placeholder":"/img/badges/star.svg"}}],"affiliations":[{"affiliation":null,"url":null,"is_default":true}]},"description":"{\"blocks\":[{\"key\":\"ej6pj\",\"text\":\"Isobaric peptide termini labeling (IPTL) is a quantification method which permits relative quantification using quantification points distributed throughout the whole tandem mass spectrometry (MS/MS) spectrum. It is based on the complementary derivatization of peptide termini with different isotopes resulting in isobaric peptides.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":{}}","disclaimer":"","document":"","documents":null,"doi":"dx.doi.org/10.17504/protocols.io.b36bqran","doi_status":2,"fork_id":1834,"fork_info":{"id":1834,"title":"diDO-IPTL protocol","title_html":null,"image":{"source":"https://www.protocols.io/img/default_protocol.png","webp_source":null,"placeholder":"https://www.protocols.io/img/default_protocol.png","webp_placeholder":null},"doi":"dx.doi.org/10.17504/protocols.io.d2i8cd","uri":"dido-iptl-protocol-d2i8cd","published_on":1508275583,"modified_on":0,"version_class":0,"version_id":2,"version_code":"","version_uri":"","created_on":1445280659,"categories":null,"creator":{"name":"Lichun Zhang","affiliation":"","affiliation_url":"","username":"lichun-zhang","link":"","image":{"source":"/img/avatars/014.png","placeholder":"/img/avatars/014.png","webp_source":""}},"stats":null},"forks":[],"funders":[],"groups":[],"guid":"09689C117AF111EC897E0A58A9FEAC02","guidelines":"{\"blocks\":[{\"key\":\"9au6t\",\"text\":\"\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":{}}","has_references":false,"has_versions":false,"id":57251,"image":{"source":"https://www.protocols.io/img/default_protocol.png","webp_source":null,"placeholder":"https://www.protocols.io/img/default_protocol.png","webp_placeholder":null},"image_attribution":"","in_trash":false,"is_bookmarked":false,"is_contact_suspended":false,"is_content_confidential":false,"is_content_warning":false,"is_in_transfer":false,"is_owner":true,"is_retracted":false,"is_shared_directly":false,"is_subprotocol":null,"is_unlisted":false,"item_id":866871,"journal":null,"journals":[],"keywords":null,"last_modified":1642793866,"link":"","location":null,"manuscript_citation":"","materials":[],"materials_text":"","ownership_history":null,"parent_collections":[],"parent_protocols":[],"peer_reviewed":false,"public":true,"public_fork_note":"","published_on":1642793866,"references":[],"related_equipments":[],"retraction_reason":null,"shared_access_id":16,"show_comparison":true,"sign_info":null,"state_version_id":0,"stats":{"is_voted":false,"number_of_views":151,"number_of_steps":12,"number_of_bookmarks":0,"number_of_comments":0,"number_of_bookmarked_comments":0,"number_of_steps_comments":0,"number_of_protocol_comments":0,"number_of_exports":0,"number_of_runs":0,"number_of_votes":0,"number_of_reagents":0,"number_of_equipments":0,"number_of_collections":0,"number_of_forks":{"private":0,"public":0},"number_of_accessible_forks":0},"status":{"id":1,"info":"We use this protocol and it’s working"},"steps":[{"id":1326417,"guid":"097278307AF111EC897E0A58A9FEAC02","previous_id":null,"previous_guid":null,"section":"DAY 1","section_color":null,"section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"e3hu9\",\"text\":\"Prepare dried MS-grade trypsin, 2ug per sample to be labeled. Note that separate aliquots of dried trypsin must be used for 16O- and 18O-labeling.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"sup\",\"offset\":124,\"length\":2},{\"style\":\"sup\",\"offset\":133,\"length\":2}],\"entityRanges\":[],\"data\":[]}],\"entityMap\":{}}","data":null,"protocol_id":57251,"case_id":0,"critical_ids":"","duration":0,"original_id":134486,"number":"1","cases":[],"critical":null},{"id":1326418,"guid":"09729E0A7AF111EC897E0A58A9FEAC02","previous_id":1326417,"previous_guid":"097278307AF111EC897E0A58A9FEAC02","section":"DAY 1","section_color":null,"section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"dr8g6\",\"text\":\"Prepare two separate N-methylmorpholine (NMM) and acetic acid O-isotope exchange buffers with 16O and 18O (\\u003e98%) enriched water. Volume ratio of buffer components is 48.5 H2O : 1 NMM : 0.5 acetic acid (pH 7.4). The total volume of buffer to prepare depends on how many peptide samples are to be labeled. The protocol below is for 40µL of O-isotope exchange buffer per sample (~10-20 µg of peptide). For samples with small amounts of peptide, the concentration can be raised by adding only 20µL buffer per sample and halving the volumes of all subsequent reagent additions.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"sup\",\"offset\":94,\"length\":2},{\"style\":\"sup\",\"offset\":102,\"length\":2},{\"style\":\"sub\",\"offset\":172,\"length\":1}],\"entityRanges\":[],\"data\":[]}],\"entityMap\":{}}","data":null,"protocol_id":57251,"case_id":0,"critical_ids":"","duration":0,"original_id":134487,"number":"2","cases":[],"critical":null},{"id":1326419,"guid":"0972BCD57AF111EC897E0A58A9FEAC02","previous_id":1326418,"previous_guid":"09729E0A7AF111EC897E0A58A9FEAC02","section":"DAY 1","section_color":null,"section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"fb5k8\",\"text\":\"Redissolve dried trypsin aliquots in their respective O-isotope exchange buffers and transfer 40µl to each tube of dried peptide sample. KEEP TRACK of which tubes received which buffer.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":{}}","data":null,"protocol_id":57251,"case_id":0,"critical_ids":"","duration":0,"original_id":134488,"number":"3","cases":[],"critical":null},{"id":1326420,"guid":"0972D9AE7AF111EC897E0A58A9FEAC02","previous_id":1326419,"previous_guid":"0972BCD57AF111EC897E0A58A9FEAC02","section":"DAY 1","section_color":null,"section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"6c3lv\",\"text\":\"Parafilm the tops of the tubes and incubate at 37ºC overnight.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":{}}","data":null,"protocol_id":57251,"case_id":0,"critical_ids":"","duration":0,"original_id":134489,"number":"4","cases":[],"critical":null},{"id":1326421,"guid":"0972F79D7AF111EC897E0A58A9FEAC02","previous_id":1326420,"previous_guid":"0972D9AE7AF111EC897E0A58A9FEAC02","section":"DAY 2","section_color":null,"section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"erhfc\",\"text\":\"After overnight incubation, add 2µl of 11.3M monochloroacetic acid (prepared in LC-MS grade water) to each peptide tube to lower pH down to 2.6.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":{}}","data":null,"protocol_id":57251,"case_id":0,"critical_ids":"","duration":0,"original_id":134490,"number":"5","cases":[],"critical":null},{"id":1326422,"guid":"097317457AF111EC897E0A58A9FEAC02","previous_id":1326421,"previous_guid":"0972F79D7AF111EC897E0A58A9FEAC02","section":"DAY 2","section_color":null,"section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"2icra\",\"text\":\"Prepare 16% CH2O and CD2O formaldehyde solutions, at least 2µl per sample, diluting stocks with LC-MS grade water if necessary.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"sub\",\"offset\":14,\"length\":1},{\"style\":\"sub\",\"offset\":23,\"length\":1}],\"entityRanges\":[],\"data\":[]}],\"entityMap\":{}}","data":null,"protocol_id":57251,"case_id":0,"critical_ids":"","duration":0,"original_id":134491,"number":"6","cases":[],"critical":null},{"id":1326423,"guid":"097335097AF111EC897E0A58A9FEAC02","previous_id":1326422,"previous_guid":"097317457AF111EC897E0A58A9FEAC02","section":"DAY 2","section_color":null,"section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"8mhrg\",\"text\":\"Prepare 4.8M NaBH3CN in LC-MS water. Tare 1.5mL tube, add a small amount of NaBH3CN powder in fume hood, weigh, dissolve.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"sub\",\"offset\":17,\"length\":1},{\"style\":\"sub\",\"offset\":80,\"length\":1}],\"entityRanges\":[],\"data\":[]}],\"entityMap\":{}}","data":null,"protocol_id":57251,"case_id":0,"critical_ids":"","duration":0,"original_id":134492,"number":"7","cases":[],"critical":null},{"id":1326424,"guid":"0973526A7AF111EC897E0A58A9FEAC02","previous_id":1326423,"previous_guid":"097335097AF111EC897E0A58A9FEAC02","section":"DAY 2","section_color":null,"section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"48j03\",\"text\":\"Add 2µL of CH2O to each 18O tube and 2µL of CD2O to each 16O tube.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"sub\",\"offset\":13,\"length\":1},{\"style\":\"sub\",\"offset\":46,\"length\":1},{\"style\":\"sup\",\"offset\":24,\"length\":2},{\"style\":\"sup\",\"offset\":57,\"length\":2}],\"entityRanges\":[],\"data\":[]}],\"entityMap\":{}}","data":null,"protocol_id":57251,"case_id":0,"critical_ids":"","duration":0,"original_id":134493,"number":"8","cases":[],"critical":null},{"id":1326425,"guid":"09736F587AF111EC897E0A58A9FEAC02","previous_id":1326424,"previous_guid":"0973526A7AF111EC897E0A58A9FEAC02","section":"DAY 2","section_color":null,"section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"e8ch4\",\"text\":\"Add 2µL 4.8M NaBH3CN to each tube.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"sub\",\"offset\":17,\"length\":1}],\"entityRanges\":[],\"data\":[]}],\"entityMap\":{}}","data":null,"protocol_id":57251,"case_id":0,"critical_ids":"","duration":0,"original_id":134494,"number":"9","cases":[],"critical":null},{"id":1326426,"guid":"09738B8E7AF111EC897E0A58A9FEAC02","previous_id":1326425,"previous_guid":"09736F587AF111EC897E0A58A9FEAC02","section":"DAY 2","section_color":null,"section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"djjql\",\"text\":\"Mix well and incubate at 45ºC for 1 hr. While waiting, prepare 5M ammonium formate in LC-MS water.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":{}}","data":null,"protocol_id":57251,"case_id":0,"critical_ids":"","duration":0,"original_id":134495,"number":"10","cases":[],"critical":null},{"id":1326427,"guid":"0973AD0A7AF111EC897E0A58A9FEAC02","previous_id":1326426,"previous_guid":"09738B8E7AF111EC897E0A58A9FEAC02","section":"DAY 2","section_color":null,"section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"4cqoq\",\"text\":\"Add 2µL 5M ammonium formate to each tube and mix well.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":{}}","data":null,"protocol_id":57251,"case_id":0,"critical_ids":"","duration":0,"original_id":134496,"number":"11","cases":[],"critical":null},{"id":1326428,"guid":"0973CEF87AF111EC897E0A58A9FEAC02","previous_id":1326427,"previous_guid":"0973AD0A7AF111EC897E0A58A9FEAC02","section":"DAY 2","section_color":null,"section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"cibs8\",\"text\":\"Add 8µL of formic acid to each tube and mix well. Total sample volume is ~56µl. 16O- and 18O-labeled samples are ready to be mixed and analyzed by LC-MS.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"sup\",\"offset\":80,\"length\":2},{\"style\":\"sup\",\"offset\":89,\"length\":2}],\"entityRanges\":[],\"data\":[]}],\"entityMap\":{}}","data":null,"protocol_id":57251,"case_id":0,"critical_ids":"","duration":0,"original_id":134497,"number":"12","cases":[],"critical":null}],"template_id":1,"title":"diDO-IPTL protocol","title_html":"diDO-IPTL protocol","type_id":1,"units":[{"id":1,"type_id":3,"name":"µL","can_manage":0},{"id":2,"type_id":3,"name":"mL","can_manage":0},{"id":3,"type_id":3,"name":"L","can_manage":0},{"id":4,"type_id":3,"name":"µg","can_manage":0},{"id":5,"type_id":3,"name":"mg","can_manage":0},{"id":6,"type_id":3,"name":"g","can_manage":0},{"id":7,"type_id":3,"name":"kg","can_manage":0},{"id":8,"type_id":3,"name":"ng","can_manage":0},{"id":9,"type_id":3,"name":"Hz","can_manage":0},{"id":10,"type_id":24,"name":"°C","can_manage":0},{"id":11,"type_id":24,"name":"°К","can_manage":