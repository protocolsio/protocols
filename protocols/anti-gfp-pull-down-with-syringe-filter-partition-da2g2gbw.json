","previous_id":1987045,"previous_guid":"7A63944F1A254C7DAE95199C49341BDB","section":"\u003cp\u003eSample lysis and filter partition\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"2hmik\",\"text\":\"supernatant was then centrifuged at 16000g for 10min at 4°C to pellet aggregates and the associated proteins\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":97064,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4","cases":[],"critical":null},{"id":1987050,"guid":"08DE08373E9B4341B71CCF43DADEC97C","previous_id":1987047,"previous_guid":"FD16506022AB4F3AB14B075BA2C75811","section":"\u003cp\u003eSample lysis and filter partition\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"f5po8\",\"text\":\"The pellet was washed once in PBS to remove soluble Htt64Q-GFP and was then dissolved in PBS, and passed through pre-equilibrated syringe filters (in the same buffer) with 0.2um or 0.4um pore sizes (Millipore). \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":97064,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"5","cases":[],"critical":null},{"id":1987052,"guid":"93D70C1DF5274EB4B92DFEB07148BECB","previous_id":1987050,"previous_guid":"08DE08373E9B4341B71CCF43DADEC97C","section":"\u003cp\u003eSample lysis and filter partition\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"e74sg\",\"text\":\"The filter blocked fractions were then collected by washing it with a syringe, in the same buffer, and pelleted again at 16,000g for 10min at 4°C.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_i