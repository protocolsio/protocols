_guid":"CC152261083811EB91C773D191B3688B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"71C9B8383C02409CB4D2B1404B895A13","order_id":1,"type_id":6,"title":"Section","source":{"title":"Procedure"}},{"id":1054724,"guid":"4E2CEC4B506A498E8A89DFBD77D7D8A3","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Clip the biopsy area with clippers and wipe down with ethanol-soaked gauze. We typically sample the thorax (middle of 5 or 6th rib) - or shoulder area<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1046094,"guid":"CC18CBE0083811EB91C773D191B3688B","previous_id":1046093,"previous_guid":"CC16D011083811EB91C773D191B3688B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"83D2425C19A5435CB83261D283F9CEA0","order_id":1,"type_id":6,"title":"Section","source":{"title":"Procedure"}},{"id":1054724,"guid":"28300449BA2A4A35A7689BCA10A94C10","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Wipe area with a sterile dry gauze.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1046095,"guid":"CC1B15D0083811EB91C773D191B3688B","previous_id":1046094,"previous_guid":"CC18CBE0083811EB91C773D191B3688B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"8AEFEB832CB1482686F1DDCB13193882","order_id":1,"type_id":6,"title":"Section","source":{"title":"Procedure"}},{"id":1054724,"guid":"472BCC347B864B798639B4B841E04B04","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Spray 5% lidocaine onto the biopsy area and wait for 30 sec.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1046096,"guid":"CC1DADE0083811EB91C773D191B3688B","previous_id":1046095,"previous_guid":"CC1B15D0083811EB91C773D191B3688B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"C0922133257847928855E76C3FBAD2E4","order_id":1,"type_id":6,"title":"Section","source":{"title":"Procedure"}},{"id":1054724,"guid":"31C146F3A7074425BDAF4B721C8D0F09","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Collect skin sample by rotating the 5 mm skin punch while pressing downwards with mild force.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1046097,"guid":"CC206D01083811EB91C773D191B3688B","previous_id":1046096,"previous_guid":"CC1DADE0083811EB91C773D191B3688B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"1190D7FF8B574D2CBD87E9CFCCEE0FFE","order_id":1,"type_id":6,"title":"Section","source":{"title":"Procedure"}},{"id":1054724,"guid":"AF598FDD63D440C1A90C563C445DDB6B","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Clean the skin punch by immersing in 70% ethanol. The skin punch can be used three to four times.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1046098,"guid":"CC237A41083811EB91C773D191B3688B","previous_id":1046097,"previous_guid":"CC206D01083811EB91C773D191B3688B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"F5EDC25408AA4DF8A36CF4933951D1EB","order_id":1,"type_id":6,"title":"Section","source":{"title":"Procedure"}},{"id":1054724,"guid":"63F01015BF6C49C7919BD544089878E0","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Take the skin sample from skin punch with a needle or tweezers, and place it into a cassette between the sponges. Close the cassettes.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1046099,"guid":"CC26D5A1083811EB91C773D191B3688B","previous_id":1046098,"previous_guid":"CC237A41083811EB91C773D191B3688B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"BF5F40CC683941DFA2A3E007F1292D4F","order_id":1,"type_id":6,"title":"Section","source":{"title":"Procedure"}},{"id":1054724,"guid":"AC6EF456C29B4EC884F3CFA553A8C70D","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Place the cassette into the container with 10% NBF at room temperature.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1046100,"guid":"CC2A7F21083811EB91C773D191B3688B","previous_id":1046099,"previous_guid":"CC26D5A1083811EB91C773D191B3688B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"C41117E81FE543A79BD8255BFD146C62","order_id":1,"type_id":6,"title":"Section","source":{"title":"Procedure"}},{"id":1054724,"guid":"9AFAF83F4FD14B0A90EFDBB5B552B476","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Place sterile gauze on the biopsy site to remove excess blood or stop bleeding.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1046101,"guid":"CC2E4FB1083811EB91C773D191B3688B","previous_id":1046100,"previous_guid":"CC2A7F21083811EB91C773D191B3688B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"8E04263809924E23B72048125AC1D53C","order_id":1,"type_id":6,"title":"Section","source":{"title":"Procedure"}},{"id":1054724,"guid":"C1881A5F59ED4784B60FD1A3C941C303","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Place some super glue into the wound site. Wait 10 seconds.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1046102,"guid":"CC326E61083811EB91C773D191B3688B","previous_id":1046101,"previous_guid":"CC2E4FB1083811EB91C773D191B3688B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"84AAFBECD05044C3A5086E29AF0A53CC","order_id":1,"type_id":6,"title":"Section","source":{"title":"Procedure"}},{"id":1054724,"guid":"08B4E77107B644F49A1080942BC41BD6","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Spray tick spray around the biopsy site.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1046103,"guid":"2ABB5410083911EB91C773D191B3688B","previous_id":1046102,"previous_guid":"CC326E61083811EB91C773D191B3688B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"7D11B63ACB884B4E839123B0E469DC04","order_id":1,"type_id":6,"title":"Section","source":{"title":"Procedure"}},{"id":1054724,"guid":"FD4EDF9DB09248CE9DC2695F4B51146E","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">To process the biopsy, leave the skin samples in NBF for 16-20 h at room temperature. Rinse the skin-containing cassettes in PBS twice, 15 min each time. Cut the skin samples in half and place them back into the cassette (use gloves). Store cassettes in a container containing PBS.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0}],"document":null,"materials":[],"description":"<div class = \"text-blocks\"><div class = \"text-block\">This is a simple protocol for collecting skin biopsies from cattle. The procedure described here was used to collect skin for histological analysis but it could also be used to collect skin for PCR or other purposes. Our experience indicates that there are few complications with the procedure. The most common is that occasionally there is a slight amount of bleeding that can be treated by applying pressure using a gauze. Healing occurs rapidly and biopsy sites are difficult to detect 14 days after the procedure.  The protocol is best read by using the attached pdf file. <\/div><\/div>","changed_on":1602032913}