n":0},{"id":977403,"guid":"35D0BEC0C22611EAB0D8670F838C3A5B","previous_id":977402,"previous_guid":"319FBF40C22611EAB0D8670F838C3A5B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"CA559A4031C942F2B06AD0A76571FA6A","order_id":1,"type_id":6,"title":"Section","source":{"title":"ICV Surgery Procedure"}},{"id":1054724,"guid":"DB4D320E9DD44FC8B2FCDB47E0D453D7","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Incision down the middle of the head, between the eyes<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":977404,"guid":"37BC8930C22611EAB0D8670F838C3A5B","previous_id":977403,"previous_guid":"35D0BEC0C22611EAB0D8670F838C3A5B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"73487B1B79F34515B066D435715CBFD5","order_id":1,"type_id":6,"title":"Section","source":{"title":"ICV Surgery Procedure"}},{"id":1054724,"guid":"C61DB60BCCAA4FF0A75E3C92EE3AF99A","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Clip the 4 sides of the skin<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":977406,"guid":"3B61C370C22611EAB0D8670F838C3A5B","previous_id":977404,"previous_guid":"37BC8930C22611EAB0D8670F838C3A5B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"6E7BEEF2735E400ABEED74ECCFCF398D","order_id":1,"type_id":6,"title":"Section","source":{"title":"ICV Surgery Procedure"}},{"id":1054724,"guid":"EDDB1248A67349A5827AACE5F957E63D","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Scrape off everything from the skull. Want it to not bleed. Can use the stone for the blood spots<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":977407,"guid":"3F9470A0C22611EAB0D8670F838C3A5B","previous_id":977406,"previous_guid":"3B61C370C22611EAB0D8670F838C3A5B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"4DC304B5FD044C33A0622D47FC3C5296","order_id":1,"type_id":6,"title":"Section","source":{"title":"ICV Surgery Procedure"}},{"id":1054724,"guid":"FA288EC5B00F480C943066BA429DD963","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Screw drivers: use #1 first to make 4 holes, one in each quadrant. Want to make the hole all the way through (will hear a pop). Then use #2 in each of the holes to ensure the holes are good<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":977408,"guid":"441BCD80C22611EAB0D8670F838C3A5B","previous_id":977407,"previous_guid":"3F9470A0C22611EAB0D8670F838C3A5B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"BC5B8EB766234E229933B7139CE0A625","order_id":1,"type_id":6,"title":"Section","source":{"title":"ICV Surgery Procedure"}},{"id":1054724,"guid":"989297E6FEEF4DC4B96FF6266EF5CEA6","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Screws: use the rounded ones, not the one with the sharper sides. Put one screw into each hole made. Want the screws to have a little bit of space so the cement can go under (not completely against the skull).<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":977409,"guid":"5447AA30C22611EAB0D8670F838C3A5B","previous_id":977408,"previous_guid":"441BCD80C22611EAB0D8670F838C3A5B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"7F831A6B297245B383B102A6342CF7B0","order_id":1,"type_id":6,"title":"Section","source":{"title":"ICV Surgery Procedure"}},{"id":1054724,"guid":"BC1B549A65B04CF99F35A69E3D894D58","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Put the arm on to the stereotax<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":977410,"guid":"5B1CA090C22611EAB0D8670F838C3A5B","previous_id":977409,"previous_guid":"5447AA30C22611EAB0D8670F838C3A5B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"FA9D0344C3BE46C59D8756E727739CC2","order_id":1,"type_id":6,"title":"Section","source":{"title":"ICV Surgery Procedure"}},{"id":1054724,"guid":"F3D985510900429D9BEFC80CF3C995DF","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Put the cannula into the arm, longer part of the metal pointing down.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":977411,"guid":"5F0859B0C22611EAB0D8670F838C3A5B","previous_id":977410,"previous_guid":"5B1CA090C22611EAB0D8670F838C3A5B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"354153D4EDBB491EAB103F168136437C","order_id":1,"type_id":6,"title":"Section","source":{"title":"ICV Surgery Procedure"}},{"id":1054724,"guid":"679A3A54E1FE4D0AA467D057F3AC8FEB","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Find the middle of Bregma (the line right in the middle of the skull). If bregma is not straight, estimate the middle.\u00a0<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":977412,"guid":"631CD080C22611EAB0D8670F838C3A5B","previous_id":977411,"previous_guid":"5F0859B0C22611EAB0D8670F838C3A5B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"9EDF0476565F41A1B1D50A29FFDA009D","order_id":1,"type_id":6,"title":"Section","source":{"title":"ICV Surgery Procedure"}},{"id":1054724,"guid":"B805EFE1771645C8A31450807AD269E1","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Calculations: look at the dials on the side. AP is the one running perpendicular to the rat (pointing away from you), LAT is the one you have to stand to see, attached to the one going up\/down. ALSO mark the point of DV (the depth) of where the cannula just touches the skull.\u00a0<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":977413,"guid":"680E39D0C22611EAB0D8670F838C3A5B","previous_id":977412,"previous_guid":"631CD080C22611EAB0D8670F838C3A5B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"DC1BD10186D84DA6B025030869FBA464","order_id":1,"type_id":6,"title":"Section","source":{"title":"ICV Surgery Procedure"}},{"id":1054724,"guid":"06306AA1D5CD4F1D81289A4C1B38F0A1","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Adjust the AP and LAT o get to the ventricle. AP is -0.6, LAT is +\/-2. Want to do half rats +, the other half - so have equal amount of rats. Make a little mark at that adjustment\u00a0<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":977414,"guid":"6D6BD270C22611EAB0D8670F838C3A5B","previous_id":977413,"previous_guid":"680E39D0C22611EAB0D8670F838C3A5B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"01B3698F37AC4EF08E3E6B78B5FE8690","order_id":1,"type_id":6,"title":"Section","source":{"title":"ICV Surgery Procedure"}},{"id":1054724,"guid":"4B936583454845729289A75D38775A55","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Move the arm, then drill a hole at the marked spot, just enough to go through the skull.\u00a0<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":977415,"guid":"71CE9050C22611EAB0D8670F838C3A5B","previous_id":977414,"previous_guid":"6D6BD270C22611EAB0D8670F838C3A5B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"6627BBD3A3E04B8382915FE0B2737A27","order_id":1,"type_id":6,"title":"Section","source":{"title":"ICV Surgery Procedure"}},{"id":1054724,"guid":"2D75BE7467EA4A969EF7AA345822D09A","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Poke a hole into the drilled hole so that the dura is broken.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":977416,"guid":"76081B50C22611EAB0D8670F838C3A5B","previous_id":977415,"previous_guid":"71CE9050C22611EAB0D8670F838C3A5B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"34D67754BE4D42F9BE994CEC06FB517F","order_id":1,"type_id":6,"title":"Section","source":{"title":"ICV Surgery Procedure"}},{"id":1054724,"guid":"9BA43B726AC9459491830A07AAC35B24","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Take DV and subtract 3.2. This is the depth you should adjust the cannula to.\u00a0<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":977417,"guid":"7A1B59A0C22611EAB0D8670F838C3A5B","previous_id":977416,"previous_guid":"76081B50C22611EAB0D8670F838C3A5B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"A108269301CD4C8984990CBEC0BEE5E4","order_id":1,"type_id":6,"title":"Section","source":{"title":"ICV Surgery Procedure"}},{"id":1054724,"guid":"AEF0C4ADF70449388407E46DE5FF788A","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Make sure there is no blood on the skull! Use q-tips to cleanup blood<\/div><div class = \"text-block\">First glue: Japanese glue. One scoop of powder, 4 drips of orange bottle, one drop of sealant (syringe). This is the ratio you need to use, but might need to make some more (double up on everything!). Mix it and put it around the cannula and 4 screws.\u00a0<\/div><div class = \"text-block\">Can put locktight between first and second gluemake sure there is no blood on the skull! Use q-tips to clear<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":977418,"guid":"9E736590C22611EAB0D8670F838C3A5B","previous_id":977417,"previous_guid":"7A1B59A0C22611EAB0D8670F838C3A5B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"0A3FD53ECAC44FB3B9886AFD1534B2CE","order_id":1,"type_id":6,"title":"Section","source":{"title":"ICV Surgery Procedure"}},{"id":1054724,"guid":"365149FE5BDC474FA028E9E0753F2A65","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Second glue: sun shine. Put the paste around the entire skull, making sure to cover the cannula and the 4 screws. Press down to make sure it is on the skull.\u00a0<\/div><div class = \"text-block\">Use the UV light to go over the sun shine. Go over all parts of it! Should be rock hard at the end.\u00a0<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":977419,"guid":"B83F1870C22611EAB0D8670F838C3A5B","previous_id":977418,"previous_guid":"9E736590C22611EAB0D8670F838C3A5B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"52ECEF1EB89B49F0A627C68115CF0953","order_id":1,"type_id":6,"title":"Section","source":{"title":"ICV Surgery Procedure"}},{"id":1054724,"guid":"82DD5C3311AA4559A72B63AE4034F531","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Now you can lift the arm off. Unscrew a little, put your spatula in the middle to open some more, then lift the arm.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":977420,"guid":"BD78D560C22611EAB0D8670F838C3A5B","previous_id":977419,"previous_guid":"B83F1870C22611EAB0D8670F838C3A5B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"19106CE50B254788A4B356B90BF4ADA2","order_id":1,"type_id":6,"title":"Section","source":{"title":"ICV Surgery Procedure"}},{"id":1054724,"guid":"9D7DBAAAE76B4137816845D2B515855C","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Flatten the skull back to 0<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":977421,"guid":"C31DB030C22611EAB0D8670F838C3A5B","previous_id":977420,"previous_guid":"BD78D560C22611EAB0D8670F838C3A5B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"7D4007FCA12343C3AB2