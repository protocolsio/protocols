false,"protocol_references":"","public":true,"public_fork_note":"","published_on":1709785328,"references":[],"related_equipments":[],"related_materials":[],"reserved_doi":"10.17504/protocols.io.q26g7pwd3gwz/v1","retraction_reason":null,"samples":{},"shared_access_id":0,"show_comparison":false,"sign_info":null,"space_access":{"can_view":false,"can_edit":false,"can_remove":false,"can_add":false,"can_publish":true,"can_get_doi":true,"can_share":false,"can_move":false,"can_move_outside":false,"can_transfer":false,"can_download":false,"limited_run":false,"limited_private_links":false,"limited_blind_links":false,"is_locked":false},"space_id":34457,"state_version_id":336,"stats":{"is_voted":false,"number_of_views":0,"number_of_steps":11,"number_of_bookmarks":0,"number_of_comments":0,"number_of_bookmarked_comments":0,"number_of_steps_comments":0,"number_of_protocol_comments":0,"number_of_exports":0,"number_of_runs":0,"number_of_votes":0,"number_of_reagents":0,"number_of_equipments":0,"number_of_collections":1,"number_of_forks":{"private":0,"public":0},"number_of_accessible_forks":0},"status":{"id":1,"info":""},"steps":[{"id":1846013,"guid":"7F6F2EE05E4144F8ABD7668C507550E9","previous_id":0,"previous_guid":null,"section":"\u003cp\u003eProtein prep\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"d5h2\",\"text\":\"On day prior to experiment, codialyze both binding partners to use in the the same beaker.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90731,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"1","cases":[],"critical":null},{"id":1846014,"guid":"D3054C5EE08245FA82D6377F7D40B3F2","previous_id":1846013,"previous_guid":"7F6F2EE05E4144F8ABD7668C507550E9","section":"\u003cp\u003eProtein prep\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"cp89u\",\"text\":\"Prepare 2 L of 50 mM HEPES 7.5, 150 mM NaCl, 10 mM TCEP, and degas thoroughly at room temperature under vacuum with stir bar agitation, then chill in fridge until ready for dialysis.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90731,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"2","cases":[],"critical":null},{"id":1846015,"guid":"DA767C523AE34C99BFD35EE7F4F31D5B","previous_id":1846014,"previous_guid":"D3054C5EE08245FA82D6377F7D40B3F2","section":"\u003cp\u003eProtein prep\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"ejcb7\",\"text\":\"Calculate sufficient protein for the assay: for a single run, want ~ 100 uL of 1 mM Rab7, and ~500 uL of 100 uM MBP-RUBCN RH domain. Samples can be more dilute, but should have enough total mass of protein to concentrate down later.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90731,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"3","cases":[],"critical":null},{"id":1846016,"guid":"ABACB8BC09B04A698CE43D3889E923A3","previous_id":1846015,"previous_guid":"DA767C523AE34C99BFD35EE7F4F31D5B","section":"\u003cp\u003eProtein prep\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"bn5fd\",\"text\":\"In seperate dialysis bags, dispense purified Rab7 and RUBCN RH. Clip the ends of the dialysis bags twice, and place both dialysis bags in a 2 L beaker of the degassed and chilled dialysis buffer.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90731,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4","cases":[],"critical":null},{"id":1846017,"guid":"66B3389BAA824DBEAFE0746469AB3F64","previous_id":1846016,"previous_guid":"ABACB8BC09B04A698CE43D3889E923A3","section":"\u003cp\u003eProtein prep\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"dq29b\",\"text\":\"Incubate for 24 H\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90731,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"5","cases":[],"critical":null},{"id":1846019,"guid":"45620366D6824CEBB0F523697260E4B6","previous_id":1846017,"previous_guid":"66B3389BAA824DBEAFE0746469AB3F64","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"3gbbc\",\"text\":\"After dialysis has completed, recover the protein samples and concentrate down to 1 mM for Rab7 and 100 uM for Rubicon. Spin protein samples at max speed on a tabletop centrifuge in order to pellet any protein aggregates, and allow protein samples to come to room temperature on a benchtop. Samples are now ready for ITC.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"8uapr\",\"text\":\"\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"9sj66\",\"text\":\"Note, changes in sample temperature can encourage the evolution of bubbles, which severely damages the quality of your ITC run. Make sure centrifuges are pre-warmed to room temperature, and keep all samples on benchtop rather than in ice bucket until ITC runs are completed.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90731,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"6","cases":[],"critical":null},{"id":1846020,"guid":"3B1CD7F84EB14D298410D00033D3A913","previous_id":1846019,"previous_guid":"45620366D6824CEBB0F523697260E4B6","section":"\u003cp\u003eIsothermal Titration Calorimetry\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"dq1bd\",\"text\":\"In a Malvern PEAQ-ITC, set the resting temperature to room temperature, here ~ 22 C\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90731,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"7","cases":[],"critical":null},{"id":1846021,"guid":"CFFE7AF7C1EE4D7D83F1BD60F40D2BF9","previous_id":1846020,"previous_guid":"3B1CD7F84EB14D298410D00033D3A913","section":"\u003cp\u003eIsothermal Titration Calorimetry\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"34plg\",\"text\":\"Prepare for run by loading the chamber with ~ 300 uL of the Rubicon RH stock, and follow the automated syringe loading protocol to load the syringe with ~75 uL of the Rab7 stock. Gently \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90731,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"8","cases":[],"critical":null},{"id":1846022,"guid":"E503E08DC33544C5AB0F72F3927624A2","previous_id":1846021,"previous_guid":"CFFE7AF7C1EE4D7D83F1BD60F40D2BF9","section":"\u003cp\u003eIsothermal Titration Calorimetry\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"cgi4b\",\"text\":\"Used the following parameters for this run, though changes can be made to suit your particular experiment\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"e88ba\",\"text\":\"\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"fb7us\",\"text\":\"Reference power :41.9 uW\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"52qkk\",\"text\":\"Temperature: 25 C\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"3ktf6\",\"text\":\"Stir speed: 750 rpm\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"9d1a2\",\"text\":\"Initial delay: 60 sec\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"epidu\",\"text\":\"Injection parameters: 13 injections, 3 uL each, 6 second duration, spaced by 150 sec. Initial inject was 0.4 uL with a duration of 00.8 sec, and not included in the analysis\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"4q00o\",\"text\":\"\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"cm5lf\",\"text\":\"Initiate run. Make sure that power baseline nears the reference power during the equilibration phase. If it does not, it is highly likely that a bubble was introduced, and the run should be aborted and the chamber reset. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90731,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"9","cases":[],"critical":null},{"id":1846023,"guid":"BA5AB112F8CC415FB016FEE2A12576C4","previous_id":1846022,"previous_guid":"E503E08DC33544C5AB0F72F3927624A2","section":"\u003cp\u003eIsothermal Titration Calorimetry\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"7ad6u\",\"text\":\"Open the native Malvern analysis software and visually inspect the automatically fitted baselines for quality.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90731,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"10","cases":[],"critical":null},{"id":1846024,"guid":"A6B058BB45CA48AEA7CAA2397AB0CF27","previous_id":1846023,"previous_guid":"BA5AB112F8CC415FB016FEE2A12576C4","section":"\u003cp\u003eIsothermal Titration Calorimetry\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"d1s8\",\"text\":\"Generate curve of best fit using the software and export. Inspect c-value of curve, and ensure that it falls within 10-100 in order to get an accurate KD determination.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90731,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"11","cases":[],"critical":null}],"template_id":4,"title":"Isothermal Titration Calorimetry of the Rubicon RH domain and Rab7","title_html":"Isothermal Titration Calorimetry of the Rubicon RH domain and Rab7","type_id":1,"units":[{"id":1,"type_id":3,"name":"µL","can_manage":0,"read_only":0},{"id":2,"type_id":3,"name":"mL","can_manage":0,"read_only":0},{"id":3,"type_id":3,"name":"L","can_manage":0,"read_only":0},{"id":4,"type_id":3,"name":"µg","can_manage":0,"read_only":0},{"id":5,"type_id":3,"name":"mg","can_manage":0,"read_only":0},{"id":6,"type_id":3,"name":"g","can_manage":0,"read_only":0},{"id":7,"type_id":3,"name":"kg","can_manage":0,"read_only":0},{"id":8,"type_id":3,"name":"ng","can_manage":0,"read_only":0},{"id":9,"type_id":3,"name":"Hz","can_manage":0,"read_only":0},{"id":10,"type_id":24,"name":"°C","can_manage":0,"read_only":0},{"id":11,"type_id":24,"name":"°К","can_manage":0,"read_only":0},{"id":12,"type_id":24,"name":"°F","can_manage":0,"read_only":0},{"id":13,"type_id":25,"name":"Mass Percent","can_manage":0,"read_only":0},{"id":14,"type_id":25,"name":"% volume","can_manage":0,"read_only":0},{"id":15,"type_id":25,"name":"Mass / % volume","can_manage":0,"read_only":0},{"id