cal_id":null,"duration":0},{"id":1220736,"guid":"3F77D5E0ED8A11EBA606AF81B7AB2BD8","previous_id":1220735,"previous_guid":"2FE8AE60ED8A11EBA606AF81B7AB2BD8","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"9404A8AAC54246EEAF953B52EF70274E","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"DDF809187E7943B9B5F2C56099282B71","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Incorporate the chitosan <\/div><div class = \"text-block\">  into the water\/ vinegar solution in small parts. Be sure to add the chitosan slowly while constantly mixing so that the chitosan can dissolve evenly into the solution.<\/div><br\/><\/div>"}},{"id":1054725,"guid":"361624B0ED8B11EBA606AF81B7AB2BD8","order_id":2,"type_id":3,"title":"amount","source":{"amount":13.5,"unit":"g","title":""}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1220737,"guid":"46C9EB80ED8A11EBA606AF81B7AB2BD8","previous_id":1220736,"previous_guid":"3F77D5E0ED8A11EBA606AF81B7AB2BD8","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"8E708EE603EE40F5AE2EAAE94BEFCE9A","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"140D209F5E124F88920194F398E56A14","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Slowly add the chitosan\/vinegar\/water <\/div><div class = \"text-block\"> solution into the bowl containing Hemp Protein Powder\/ Gluten\/ Orange peel.\u00a0 Bring the mixture together with a silicone spatula as the solution is added. Switch to hand kneading the material if the Chitosan and the gluten become too sticky for the spatula. Stop mixing when the material resembles a brown dough like consistency.<\/div><br\/><\/div>"}},{"id":1054725,"guid":"4DB819C0ED8B11EBA606AF81B7AB2BD8","order_id":2,"type_id":3,"title":"amount","source":{"amount":45,"unit":"mL","title":""}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1220738,"guid":"4E9E0260ED8A11EBA606AF81B7AB2BD8","previous_id":1220737,"previous_guid":"46C9EB80ED8A11EBA606AF81B7AB2BD8","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"9104BCAB2C81427BA36AFBCC9E16A3B8","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"A46DB652C93F487EB0D9F4D226D3CABD","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Press the material into a mold or onto a cooking sheet. Place in the oven at 200F until dry or leave the mixture to air dry for a few days.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1220739,"guid":"58E2F230ED8A11EBA606AF81B7AB2BD8","previous_id":1220738,"previous_guid":"4E9E0260ED8A11EBA606AF81B7AB2BD8","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"ECAD471A9F1D407E9592D4248C87E929","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"76AC49162F15417799AAC41C0CE4801F","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span style = \"font-style:italic;\">As part of a biodesign research group, we reimagined where waste products could be used. This material looks to live harmoniously with the lifetime of product use, so by using all natural ingredients, it biodegrades sooner rather than later.\u00a0 This also creates a collaboration between different industries and people as the ingredients are usually discarded. Essentially building a material to create with nature. Through this material, we were a