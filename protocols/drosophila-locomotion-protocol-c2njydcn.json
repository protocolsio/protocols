omments":0,"number_of_bookmarked_comments":0,"number_of_steps_comments":0,"number_of_protocol_comments":0,"number_of_exports":0,"number_of_runs":0,"number_of_votes":0,"number_of_reagents":0,"number_of_equipments":0,"number_of_collections":0,"number_of_forks":{"private":0,"public":0},"number_of_accessible_forks":0},"status":{"id":1,"info":"We use this protocol and it's working"},"steps":[{"id":1802108,"guid":"0CEEBE601D1044E1A7E3B2B45223BEDA","previous_id":0,"previous_guid":null,"section":"\u003cp\u003eDay -10: Set up crosses \u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"a262f\",\"text\":\"Set up fly crosses as needed.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":88491,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"1","cases":[],"critical":null},{"id":1802109,"guid":"B8EDE9CD70104E1BBAE90B6D411FE641","previous_id":1802108,"previous_guid":"0CEEBE601D1044E1A7E3B2B45223BEDA","section":"\u003cp\u003eDay 0-6: Collect progeny\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"e4006\",\"text\":\"~10 days after setting up the crosses, progeny will begin to eclose. Once daily, collect those that are desired, discarding any undesired genotypes. Flies should be collected in vials with 9-14 flies per vial. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"bold\",\"offset\":0,\"length\":1}],\"entityRanges\":[],\"data\":{}},{\"key\":\"cbnnd\",\"text\":\" \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"eg0k9\",\"text\":\"6 replicates per genotype is desirable for the assay, so once 6 vials (of 9-14 flies each) have been collected, the crosses may be discarded even if they are still producing additional flies. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"bo60h\",\"text\":\" \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"1pvqk\",\"text\":\"Note: Flies are sensitive to the effects of anesthesia, so collecting them at the time of eclosion is the only time they should be exposed to CO2.  \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"sub\",\"offset\":144,\"length\":1}],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":88491,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"2","cases":[],"critical":null},{"id":1802110,"guid":"4B6DBDE9E0204AEE9BF57561E59BACEE","previous_id":1802109,"previous_guid":"B8EDE9CD70104E1BBAE90B6D411FE641","section":"\u003cp\u003eDay 1-10: Aging flies\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"f0k31\",\"text\":\"Flies should be flipped (without anesthesia) to new food every 2-3 days while aging and stored at   in an incubator. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[{\"key\":0,\"offset\":98,\"length\":1}],\"data\":{}},{\"key\":\"3dqff\",\"text\":\"\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{\"0\":{\"type\":\"temperature\",\"mutability\":\"MUTABLE\",\"data\":{\"error_temperature\":false,\"guid\":\"\",\"id\":0,\"label\":\"\",\"scientificNotation\":false,\"temperature\":\"25\",\"unit\":10}}}}","data":null,"protocol_id":88491,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"3","cases":[],"critical":null},{"id":1802111,"guid":"C1BDDB8344F04EED9BFC9AF1C786926D","previous_id":1802110,"previous_guid":"4B6DBDE9E0204AEE9BF57561E59BACEE","section":"\u003cp\u003eDay 10*: Locomotion assay\u003c/p\u003e","section_color":"#FFED92","section_duration":60,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"emo7e\",\"text\":\"Supplies needed: \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"UNDERLINE\",\"offset\":0,\"length\":15}],\"entityRanges\":[],\"data\":{}},{\"key\":\"dslu\",\"text\":\"Timer\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"fvps2\",\"text\":\"Empty vials and buzz plugs\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"13fil\",\"text\":\"White paper\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"djs81\",\"text\":\"iPhone or iPod, or alternate recording device, optional \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"ee23v\",\"text\":\" \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"bold\",\"offset\":0,\"length\":1}],\"entityRanges\":[],\"data\":{}},{\"key\":\"8qk9k\",\"text\":\"*Day 10 is a standard time point but the assay can be performed at any desired timepoint and will show age related decline in locomotion over at least the course of 3-20 days.  \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":88491,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4","cases":[],"critical":null},{"id":1802130,"guid":"AC5A0A9D995B43C0AF62DEDA7AEAD912","previous_id":1802111,"previous_guid":"C1BDDB8344F04EED9BFC9AF1C786926D","section":"\u003cp\u003eDay 10*: Locomotion assay\u003c/p\u003e","section_color":"#FFED92","section_duration":60,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"amruc\",\"text\":\"Perform assay in groups of 4-7 vials at a time when possible. For example, if 30 vials are to be assayed on one day, divide them into 5 groups of 6 or 6 groups of 5.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":88491,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"5","cases":[],"critical":null},{"id":1802131,"guid":"A6546C8EB88144ACBAE93BC02C817442","previous_id":1802130,"previous_guid":"AC5A0A9D995B43C0AF62DEDA7AEAD912","section":"\u003cp\u003eDay 10*: Locomotion assay\u003c/p\u003e","section_color":"#FFED92","section_duration":60,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"asr50\",\"text\":\"Flip flies to new polystyrene or polypropylene vials, without food, plugged with a Buzz Plug. Write the genotype or code on the bottom of the vial so that it does not interfere with seeing the flies when the tube is on its side. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":88491,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"6","cases":[],"critical":null},{"id":1802132,"guid":"B34D9AAD03CB4AD19E803C867652B5DF","previous_id":1802133,"previous_guid":"8C8293E5E5D04BBDBD24E0F58E59D6AE","section":"\u003cp\u003eDay 10*: Locomotion assay\u003c/p\u003e","section_color":"#FFED92","section_duration":60,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"67pd\",\"text\":\"Start Timer. Briefly but firmly tap the first vial 3 times, strong enough to knock the flies to the bottom on the vial, then immediately lay it on its side on top of a white piece of paper and note the time on the timer. Wait 15 seconds, watching the time count down on the timer. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":88491,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"8","cases":[],"critical":null},{"id":1802133,"guid":"8C8293E5E5D04BBDBD24E0F58E59D6AE","previous_id":1802131,"previous_guid":"A6546C8EB88144ACBAE93BC02C817442","section":"\u003cp\u003eDay 10*: Locomotion assay\u003c/p\u003e","section_color":"#FFED92","section_duration":60,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"chr87\",\"text\":\"Allow flies to acclimate for   before beginning.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[{\"key\":0,\"offset\":29,\"length\":1}],\"data\":{}},{\"key\":\"6rs08\",\"text\":\"\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{\"0\":{\"type\":\"duration\",\"mutability\":\"MUTABLE\",\"data\":{\"duration\":60,\"guid\":\"\",\"id\":0,\"label\":\"\",\"pause\":null,\"preset\":[],\"start\":null}}}}","data":null,"protocol_id":88491,"case_id":0,"critical_ids":"","duration":60,"original_id":0,"number":"7","cases":[],"critical":null},{"id":1802188,"guid":"03F13C0A5D00471887ECB81E8A1F5D7C","previous_id":1802132,"previous_guid":"B34D9AAD03CB4AD19E803C867652B5DF","section":"\u003cp\u003eDay 10*: Locomotion assay\u003c/p\u003e","section_color":"#FFED92","section_duration":60,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"eo6v4\",\"text\":\"After the 15 second delay, record a 3 second video of the flies moving in the vial using an iPhone or iPod or equivalent. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"8rp2s\",\"text\":\"(As an alternative, counts can be performed manually). \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":88491,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"9","cases":[],"critical":null},{"id":1802189,"guid":"D92F730B970D43669A3332BE6F6554D9","previous_id":1802188,"previous_guid":"03F13C0A5D00471887ECB81E8A1F5D7C","section":"\u003cp\u003eDay 10*: Locomotion assay\u003c/p\u003e","section_color":"#FFED92","section_duration":60,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"b6n60\",\"text\":\"After the video is recorded, put the vial upright and set aside for now. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":88491,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"10","cases":[],"critical":null},{"id":1802190,"guid":"923DF1DB86B043CCB12573CA06F511AE","previous_id":1802189,"previous_guid":"D92F730B970D43669A3332BE6F6554D9","section":"\u003cp\u003eDay 10*: Locomotion assay\u003c/p\u003e","section_color":"#FFED92","section_duration":60,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"82qke\",\"text\":\"Repeat steps 8-10 for each additional vial, until each vial has been recorded 1 time. Then start back again with vial #1, completing a total of 6 sets of recordings. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":88491,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"11","cases":[],"critical":null},{"id":1802191,"guid":"BD543242A1534B6C8AAB3FE821635B73","previous_id":1802190,"previous_guid":"923DF1DB86B043CCB12573CA06F511AE","section":"\u003cp\u003eDay 10*: Locomotion assay\u003c/p\u003e","section_color":"#FFED92","section_duration":60,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"akpb7\",\"text\":\"At the end of the assay flies are anesthetized and fixed or frozen as needed for downstream assays. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":88491,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"12","cases":[],"critical":null},{"id":1802202,"guid":"575003D874EE4A90B803AC6EC654DD52","previous_id":1802191,"previous_guid":"BD543242A1534B6C8AAB3FE821635B73","section":"\u003cp\u003eDay 10*: Locomotion assay\u003c/p\u003e","section_color":"#FFED92","section_duration":60,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"7sr91\",\"text\":\"Analyze movement data in a blinded fashion.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entit