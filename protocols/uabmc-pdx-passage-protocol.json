_authorship":0,"can_accept_authorship":0,"can_be_copied":1,"can_remove_fork":1,"fork_id":null,"url":"https:\/\/www.protocols.io\/view\/uabmc-pdx-passage-protocol-bg9zjz76","forks_count":{"private":0,"public":0},"access":{"can_view":1,"can_remove":1,"can_add":0,"can_edit":0,"can_publish":0,"can_get_doi":1,"can_share":1,"can_move":1,"can_move_outside":1,"can_transfer":1,"can_download":1,"is_locked":0},"guid":"E3183630A93F11EA9E30390138005F10","state_version_id":106,"steps":[{"id":956695,"guid":"AE76BE00A94011EA9E30390138005F10","previous_id":null,"previous_guid":null,"modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"8FCECAC60D8A4E7DBD894C4A051DBC79","order_id":1,"type_id":6,"title":"Section","source":{"title":"TUMOR HARVEST"}},{"id":1054724,"guid":"C2BAD9EFD9024AAF8FFED1DCD72B77DF","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><div class = \"justify\" style = \"text-align:justify\"><\/div><style>\n\t\t\t\t\t\t\t  .justify:after {\n\t\t\t\t\t\t\t    content: \"\";\n\t\t\t\t\t\t\t    display:inline-block;\n\t\t\t\t\t\t\t    width: 100%;\n\t\t\t\t\t\t\t  }\n\t\t\t\t\t\t\t<\/style><\/div><div class = \"text-block\">Select 1 to 2 mice that have tumors that measure no more than 8-10mm in greatest diameter, are not ulcerated and appear to have grown in a consistent fashion.<\/div><\/div>"}},{"id":1054725,"guid":"440BFED51A2A4ED5A62B1F0C3F007CAE","order_id":2,"type_id":26,"title":"notes","source":{"id":0,"parent_id":0,"uri":"","title":"","body":"<div class = \"text-blocks\"><div class = \"text-block\">(This must be done aseptically using sterile surgical techniques<\/div><\/div>","created_on":0,"changed_on":0,"creator":{"name":" ","affiliation":null,"affiliations":[],"username":"","link":null,"image":{"source":"","placeholder":""},"badges":[],"verified":0,"is_verified_user":false,"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false},"comments":[]}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":956696,"guid":"BDBC5960A94011EA9E30390138005F10","previous_id":956695,"previous_guid":"AE76BE00A94011EA9E30390138005F10","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"261C81F15EE5493384C0E1879C89DEBE","order_id":1,"type_id":6,"title":"Section","source":{"title":"TUMOR HARVEST"}},{"id":1054724,"guid":"6DE1DC0ADA2449B38D0F2460EFD9E7E3","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Euthanize one mouse at a time with CO2 asphyxiation in designated inhalation chamber attached via a two-stage regulator to a CO2 tank.  Perform cervical dislocation or bilateral thoracotomy to ensure the mouse is dead.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":956699,"guid":"BDBFDBD1A94011EA9E30390138005F10","previous_id":956696,"previous_guid":"BDBC5960A94011EA9E30390138005F10","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"404A51CE009E4144B63A021E373D5492","order_id":1,"type_id":6,"title":"Section","source":{"title":"TUMOR HARVEST"}},{"id":1054724,"guid":"5795E663677342EC9621666435F2DD8F","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Spray the mouse with 3% chlorhexidine sanitizing solution (over the sink) and transfer mouse to a sterile convertor cloth.  Sanitize the skin around the tumor with betadine scrub solution.  Wait at least 2 mins before proceeding.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":956700,"guid":"BDC18980A94011EA9E30390138005F10","previous_id":956699,"previous_guid":"BDBFDBD1A94011EA9E30390138005F10","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"C53631B25C64497A87ED71D122EF4F94","order_id":1,"type_id":6,"title":"Section","source":{"title":"TUMOR HARVEST"}},{"id":1054724,"guid":"47E5AA9774C142F78B1FD01AE5D97280","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Use sterile forceps and one scalpel with a #11 blade to produce an incision in the skin forming a half-circle about 5mm away from the tumor on the ventral (lower) side of the tumor.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":956701,"guid":"BDC31021A94011EA9E30390138005F10","previous_id":956700,"previous_guid":"BDC18980A94011EA9E30390138005F10","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"E0CBCF2F1D00467D9E6FE30FD9FCDD04","order_id":1,"type_id":6,"title":"Section","source":{"title":"TUMOR HARVEST"}},{"id":1054724,"guid":"6DA9F7DF072E4ECF88F00E534A96F835","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Use the forceps to lift the edge of the skin and use a new sterile scalpel with #11 blade to carefully dissect under the tumor freeing it from the underlying muscle tissues. Blunt dissection works best, where possible.  Use your ring finger of the hand holding the forceps to push the tumor forward during this dissection.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":956702,"guid":"BDC50BF0A94011EA9E30390138005F10","previous_id":956701,"previous_guid":"BDC31021A94011EA9E30390138005F10","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"4D20A329587F43098D724EA868805951","order_id":1,"type_id":6,"title":"Section","source":{"title":"TUMOR HARVEST"}},{"id":1054724,"guid":"645744D99AA24584A8F2B611EFA97A7E","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">After you have dissected the tumor free from the body of the mouse, continue to dissect the tumor from the overlying skin.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":956703,"guid":"BDC72ED0A94011EA9E30390138005F10","previous_id":956702,"previous_guid":"BDC50BF0A94011EA9E30390138005F10","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"CFC713606A3C4114BB0534B36DA5D764","order_id":1,"type_id":6,"title":"Section","source":{"title":"TUMOR HARVEST"}},{"id":1054724,"guid":"3100581846A245AC9EF31F09944D96D0","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Open one of the glass Petri dishes and set the cover aside.  Continue the dissection of the tumor such that it \u201cdrops\u201d into the open dish. Cover the Dish and transfer to a laminar flow biological safety cabinet.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":956704,"guid":"BDC978C0A94011EA9E30390138005F10","previous_id":956703,"previous_guid":"BDC72ED0A94011EA9E30390138005F10","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"BF7342471D094977AB030ABDF92B5922","order_id":1,"type_id":6,"title":"Section","source":{"title":"TUMOR HARVEST"}},{"id":1054724,"guid":"B804D3B29D934F1698FD314E0CFEA718","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Dice tumor into large pieces (chunks) and dissect out any necrotic (dead) tissue. Necrosis can be white, yellowish or dark red and bloody.  Transfer healthy-looking tumor tissue chunks to a new glass Petri dish, if necessary.  Place Petri dish on an ice bath until all tumor chunks have been harvested and processed to this point.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":956705,"guid":"BDCC10D0A94011EA9E30390138005F10","previous_id":956704,"previous_guid":"BDC978C0A94011EA9E30390138005F10","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"690C8A86A8D94FDFBE22A8C6AD5CEFB9","order_id":1,"type_id":6,"title":"Section","source":{"title":"TUMOR HARVEST"}},{"id":1054724,"guid":"7D232D2095354E2491B3C8E02C14D0AD","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Place the carcass in the black plastic sack and proceed to euthanize the next mouse and repeat the process.  Ensure that the carcass bag is well tied and disposed in the Morgue in Animal Resource Program space.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":956708,"guid":"EF8F79E0A94011EA9E30390138005F10","previous_id":956705,"previous_guid":"BDCC10D0A94011EA9E30390138005F10","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"3B5848B87DC447EF9F4F02F58F1C67A3","order_id":1,"type_id":6,"title":"Section","source":{"title":"PDX REIMPLANTATION IN MOUSE FLANK"}},{"id":1054724,"guid":"15A9B7B0E3464F048BF27E5DEA20DBDD","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Rinse tissue chunks a couple of times with 5-10 ml each of sterile PBS; aspirate as much rinse solution as possible each time from Petri dish.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":956709,"guid":"EF9482F0A94011EA9E30390138005F10","previous_id":956708,"previous_guid":"EF8F79E0A94011EA9E30390138005F10","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"9F482DF687004BD9977CA23AC7C20DC3","order_id":1,"type_id":6,"title":"Section","source":{"title":"PDX REIMPLANTATION IN MOUSE FLANK"}},{"id":1054724,"guid":"86B809340E1A46CDA09C82020B20B970","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Mince rinsed PDX tissue chunks finely using 2 #11 scalpel blades in a scissor-like action until the pieces are small enough to fit through the bore of a 10ml pipet.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":956710,"guid":"EF998C00A94011EA9E30390138005F10","previous_id":956709,"previous_guid":"EF9482F0A94011EA9E30390138005F10","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"851DA46F923846B5A893978DFA039A32","order_id":1,"type_id":6,"title":"Section","source":{"title":"PDX REIMPLANTATION IN MOUSE FLANK"}},{"id":1054724,"guid":"F5F3968B79D9476F89F19784A5C035BD","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Add <\/div><div class = \"text-block\"> PBS to the petri dish with the minced tissue.<\/div><\/div>"}},{"id":1054725,"guid":"F74D2290A94011EA9E30390138005F10","order_id":2,"type_id":3,"title":"amount","source":{"amount":200,"unit":"\u00b5l","title":""}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":956711,"guid":"EF9FA680A94011EA9E30390138005F10","previous_id":956710,"previous_guid":"EF998C00A94011EA9E30390138005F10","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"2223005103BA48C793A677CB5C77A2D7","order_id":1,"type_id":6,"title":"Section","source":{"title":"PDX REIMPLANTATION IN MOUSE FLANK"}},{"id":1054724,"guid":"85114CB2B302408EA54CC2FA1BBBEDFA","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Using a sterile 1ml slip-tip tuberculin syringe (without needle attached), draw up the tissue<\/div><\/div>"}},{"id":1054725,"guid":"7F129C3414D4499DB86054E8A0EC515C","order_id":2,"type_id":26,"title":"notes","source":{"id":0,"parent_id":0,"uri":"","title":"","body":"<div class = \"text-blocks\"><div class = \"text-block\">We typically implant 100 \u2013 250ul of tissue per mouse. The hub of the needle will have about 100ul of \u201cdead\u201d space, so draw up enough tissue to compensate for this loss.<\/div><\/div>","created_on":0,"changed_on":0,"creator":{"name":" ","affiliation":null,"affiliations":[],"username":"","link":null,"image":{"source":"","placeholder":""},"badges":[],"verified":0,"is_verified_user":false,"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false},"comments":[]}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":956712,"guid":"EFA599F0A94011EA9E30390138005F10","previous_id":956711,"previous_guid":"EF9FA680A94011EA9E30390138005F10","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"26F5A979E1074F8780CB89B8AC829075","order_id":1,"type_id":6,"title":"Section","source":{"title":"PDX REIMPLANTATION IN MOUSE FLANK"}},{"id":1054724,"guid":"DD5D5D6A6C86424DBA92004DF458003B","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Fit a 16 gauge 1\u00bd inch needle on the syringe tip and expel any air bubbles and excess liquid. Place the syringe on ice.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":956713,"guid":"EFAC29A0A94011EA9E30390138005F10","previous_id":956712,"previous_guid":"EFA599F0A94011EA9E30390138005F10","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"799E5DEC177E4A5E83A0F015E42462D0","order_id":1,"type_id":6,"title":"Section","source":{"title":"PDX REIMPLANTATION IN MOUSE FLANK"}},{"id":1054724,"guid":"E0C02472180D44A5BD3A2B26E91582B9","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Anesthetize the mice using ketamine\/xylazine by intraperitoneal injection or isoflurane by inhalation. (Refer to Anesthesia SOP#_______)<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":956714,"guid":"EFB30770A94011EA9E30390138005F10","previous_id":956713,"previous_guid":"EFAC29A0A94011EA9E30390138005F10","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"353C9B2B49954D8A9C4B8ACCBE8F1F8D","order_id":1,"type_id":6,"title":"Section","source":{"title":"PDX REIMPLANTATION IN MOUSE FLANK"}},{"id":1054724,"guid":"C0828F724A774340B8983E8BE368B1EF","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Place the mice on a sterile convertor towel on their side, right flank facing up. Apply Chlorhexidine scrub with a sterile cotton-tipped applicator to the skin on the Right flank, wait a couple of minutes, then wipe with sterile isopropanol wipe to remove excess chlorhexidine.  Allow alcohol to dry.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":956715,"guid":"EFBACFA0A94011EA9E30390138005F10","previous_id":956714,"previous_guid":"EFB30770A94011EA9E30390138005F10","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"C22A379E674140BD8460D56B11D462AA","order_id":1,"type_id":6,"title":"Section","source":{"title":"PDX REIMPLANTATION IN MOUSE FLANK"}},{"id":1054724,"guid":"D54F06717EF14936A1C770A6226A06B3","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Re-sanitize your gloves.  Pull the skin of the right flank between your fingers, smoothing it out and lift gently but tightly.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":956716,"guid":"EFC30D00A94011EA9E30390138005F10","previous_id":956715,"previous_guid":"EFBACFA0A94011EA9E30390138005F10","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"795E3A4CB3AB4DF480BF0AE05F22471A","order_id":1,"type_id":6,"title":"Section","source":{"title":"PDX REIMPLANTATION IN MOUSE FLANK"}},{"id":1054724,"guid":"4ACCA263C449477F9CFDCB2B8FD72284","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Using your middle finger as a guide underneath the skin fold, slowly insert the needle just under the skin. The needle should be visible through the skin. Slowly push the desired amount of tissue chunks into the area on the mouse\u2019s side between the front and rear legs. Draw the needle out slowly and pinch the skin together with a pair of semi-curved forceps as the needle retracts. Place a small drop of tissue glue on the needle entry wound.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":956717,"guid":"EFCB7171A94011EA9E30390138005F10","previous_id":956716,"previous_guid":"EFC30D00A94011EA9E30390138005F10","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"E378B259FF6745D4BF5C773402DE2F62","order_id":1,"type_id":6,"title":"Section","source":{"title":"PDX REIMPLANTATION IN MOUSE FLANK"}},{"id":1054724,"guid":"9730EC40783B41EB8BCA96F4FD0BF051","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Place the mice back in a clean sterile cage; lay the mice on their left side in the cage). Place the cage half on, half off of a heating pad set to low (<\/div><div class = \"text-block\">) until the mice are awake and able to move around normally.<\/div><\/div>"}},{"id":1054725,"guid":"0C1A9AE0A94111EA9E30390138005F10","order_id":2,"type_id":24,"title":"temperature","source":{"temperature":84,"unit":"\u00b0F","title":""}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0}],"document":null,"materials":[],"description":null,"changed_on":1591590816}