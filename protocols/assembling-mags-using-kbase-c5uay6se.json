n_edit":false,"can_remove":false,"can_add":false,"can_publish":true,"can_get_doi":true,"can_share":false,"can_move":false,"can_move_outside":false,"can_transfer":false,"can_download":false,"limited_run":false,"limited_private_links":false,"limited_blind_links":false,"is_locked":false},"state_version_id":56,"stats":{"is_voted":false,"number_of_views":3,"number_of_steps":14,"number_of_bookmarks":0,"number_of_comments":0,"number_of_bookmarked_comments":0,"number_of_steps_comments":0,"number_of_protocol_comments":0,"number_of_exports":0,"number_of_runs":0,"number_of_votes":0,"number_of_reagents":0,"number_of_equipments":0,"number_of_collections":0,"number_of_forks":{"private":0,"public":0},"number_of_accessible_forks":0},"status":{"id":1,"info":"We use this protocol and it's working"},"steps":[{"id":1867529,"guid":"9A443F022894406690D03256B6AAA725","previous_id":0,"previous_guid":null,"section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"9ukdf\",\"text\":\"Create an acount with KBase, login, and start a new narrative.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":91746,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"1","cases":[],"critical":null},{"id":1867530,"guid":"0AFE4A8F30FD4B8A90EE819C4B2F404E","previous_id":1867529,"previous_guid":"9A443F022894406690D03256B6AAA725","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"ft8oc\",\"text\":\"Import metagenomic data as SRA reads by selecting the “Import SRA File as Reads from Web” app. Provide the SRA file URL, the sequencing technology, and an output name for the file to be generated. Run the app and locate output in My Data.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":91746,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"2","cases":[],"critical":null},{"id":1867531,"guid":"5161A4F557E84155B212A94F228C0030","previous_id":1867530,"previous_guid":"0AFE4A8F30FD4B8A90EE819C4B2F404E","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"2vjla\",\"text\":\"To assess quality of the reads, run the “Assess Read Quality with FastQC” app. Load the uploaded data from the SRA upload and run the app.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":91746,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"3","cases":[],"critical":null},{"id":1867532,"guid":"1C618F14F35448468F458FE2D6C30EF2","previous_id":1867531,"previous_guid":"5161A4F557E84155B212A94F228C0030","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"523d2\",\"text\":\"Review the quality scores and determine if any of the reads need to be removed for low quality scores.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":91746,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4","cases":[],"critical":null},{"id":1867533,"guid":"218494E91A8744598F78F68E0319C3CE","previous_id":1867532,"previous_guid":"1C618F14F35448468F458FE2D6C30EF2","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"fk7gf\",\"text\":\"To trim reads of adapters and to remove low quality reads, load the data into the “Trim Reads with Trimmomatic” app. There are several parameters to adjust, but these can be left at the default unless specific criteria are needed for the analysis. Define an output file name and run the app.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":91746,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"5","cases":[],"critical":null},{"id":1867534,"guid":"85F7AAC8E5A34CCF9BD43DB776097C1F","previous_id":1867533,"previous_guid":"218494E91A8744598F78F68E0319C3CE","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"ouot\",\"text\":\"Review the quality scores and compare to the pre-trimmed quality report.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":91746,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"6","cases":[],"critical":null},{"id":1867535,"guid":"2C7995BBF8104A73A0F0A26DECE9E558","previous_id":1867534,"previous_guid":"85F7AAC8E5A34CCF9BD43DB776097C1F","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"hjbq\",\"text\":\"Prior to assembly, the reads can be used to assign taxonomy. Load the trimmed data file into the “Classify Taxonomy of Metagenomic Reads with Kaiju” app. Select the taxonomic level of classification and the database to be used and run the app.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":91746,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"7","cases":[],"critical":null},{"id":1867536,"guid":"FA48E08EF64644F4A9B95B411CF188B1","previous_id":1867535,"previous_guid":"2C7995BBF8104A73A0F0A26DECE9E558","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"d3laq\",\"text\":\"Analyze the data to look at the taxonomic classifications present in the sample.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":91746,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"8","cases":[],"critical":null},{"id":1867537,"guid":"C75CAD3F8C3940E3BF07379E1DD9DEEB","previous_id":1867536,"previous_guid":"FA48E08EF64644F4A9B95B411CF188B1","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"cj7ra\",\"text\":\"To run the assembly, loaded the trimmed reads into the “Assemble Reads with metaSPAdes” app. Select the minimum contig length, identify an output object, and run the app.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":91746,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"9","cases":[],"critical":null},{"id":1867538,"guid":"89769F8971214BAB91B478C7EA8C4CB0","previous_id":1867537,"previous_guid":"C75CAD3F8C3940E3BF07379E1DD9DEEB","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"29ldf\",\"text\":\"Bin the contigs by loading the assembled read object into the “Bin Contigs using MaxBin2” app. Choose an output object and run the app.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":91746,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"10","cases":[],"critical":null},{"id":1867539,"guid":"B2A0AB71484745CFB97A3698378B9F54","previous_id":1867538,"previous_guid":"89769F8971214BAB91B478C7EA8C4CB0","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"2pv50\",\"text\":\"10.  Load the result of the binning into the “Assess Genome Quality with CheckM” and run the app.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":91746,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"11","cases":[],"critical":null},{"id":1867540,"guid":"41E513439F9C49F485681C52955650FD","previous_id":1867539,"previous_guid":"B2A0AB71484745CFB97A3698378B9F54","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"99gem\",\"text\":\"11.  Analyze and determine which bins are acceptable for extraction. This would be any bins with more than 75% completion and less than 5% contamination.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":91746,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"12","cases":[],"critical":null},{"id":1867541,"guid":"1E8E5D633E624C6D906E23C727124AA9","previous_id":1867540,"previous_guid":"41E513439F9C49F485681C52955650FD","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"87spa\",\"text\":\"12.  Extract the selected genomes using the “Extract Bins as Assemblies from BinnedContigs”.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":91746,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"13","cases":[],"critical":null},{"id":1867542,"guid":"3CE7810697D64873B5975A221CFFFBEF","previous_id":1867541,"previous_guid":"1E8E5D633E624C6D906E23C727124AA9","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"2tno\",\"text\":\"13.  Metagenome assembled genomes are now ready to be annotated and further analyzed in KBase, or exported for analysis by other methods.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":91746,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"14","cases":[],"critical":null}],"template_id":5,"title":"Assembling MAGs using KBase","title_html":"Assembling MAGs using KBase","type_id":1,"units":[{"id":1,"type_id":3,"name":"µL","can_manage":0,"read_only":0},{"id":2,"type_id":3,"name":"mL","can_manage":0,"read_onl