":0,"number_of_runs":0,"number_of_votes":0,"number_of_reagents":0,"number_of_equipments":0,"number_of_collections":1,"number_of_forks":{"private":0,"public":0},"number_of_accessible_forks":0},"status":{"id":1,"info":""},"steps":[{"id":1919140,"guid":"413D7DE54B664B258C1140348DA75B69","previous_id":0,"previous_guid":null,"section":"\u003cp\u003eTissue dissociation \u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"a9fj9\",\"text\":\"After animal sacrifice (see protocol “intracardiac perfusion”), the dissected and collected cerebral region of interest (SNpc and striatum) is immersed in D-PBS with high glucose buffer at 4°C\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94139,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"1","cases":[],"critical":null},{"id":1919141,"guid":"2B1A43C5175D4E608A6723D32C11ECD1","previous_id":1919140,"previous_guid":"413D7DE54B664B258C1140348DA75B69","section":"\u003cp\u003eTissue dissociation \u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"34h2b\",\"text\":\"Immediately after, the brain regions are dissociated to single-cell suspension by enzymatic degradation and myelin removal using the adult brain dissociation kit and GentleMACS dissociator (Miltenyi Biotec.)\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94139,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"2","cases":[],"critical":null},{"id":1919142,"guid":"F861B795D8D646E3AEB249128780C723","previous_id":1919141,"previous_guid":"2B1A43C5175D4E608A6723D32C11ECD1","section":"\u003cp\u003eTissue dissociation \u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"23bro\",\"text\":\"The isolated tissue are placed in C-tubes and a mix containing Enzyme P and Buffer Z is added\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94139,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"3","cases":[],"critical":null},{"id":1919143,"guid":"A9D60997142A48229CB3FDBF8C1C4025","previous_id":1919142,"previous_guid":"F861B795D8D646E3AEB249128780C723","section":"\u003cp\u003eTissue dissociation \u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"pler\",\"text\":\"Add a second mix containing Enzyme A and Buffer Y, tightly close C-tubes and attach upside down onto the sleeve of the gentleMACS Octo Dissociator with Heaters (Miltenyi Biotec.)\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94139,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4","cases":[],"critical":null},{"id":1919144,"guid":"C5CB0344063E46A7AA3EA636092498EB","previous_id":1919143,"previous_guid":"A9D60997142A48229CB3FDBF8C1C4025","section":"\u003cp\u003eTissue dissociation \u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"c6cvv\",\"text\":\"Run the appropriate program 37C_ABDK_02 for 30 min\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94139,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"5","cases":[],"critical":null},{"id":1919145,"guid":"7B5D113EBC4442B39A186EE8161A4683","previous_id":1919144,"previous_guid":"C5CB0344063E46A7AA3EA636092498EB","section":"\u003cp\u003eTissue dissociation \u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"6o7sq\",\"text\":\"Detach the C-tube and centrifugate to collect sample\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94139,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"6","cases":[],"critical":null},{"id":1919146,"guid":"039B85E1B5DE4DE690059E13AAD5210E","previous_id":1919145,"previous_guid":"7B5D113EBC4442B39A186EE8161A4683","section":"\u003cp\u003eTissue dissociation \u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"eom3t\",\"text\":\"Resuspend and filter to a 70 um MACS SmartStrainer with 10 ml of cold D-PBS with high glucose\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94139,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"7","cases":[],"critical":null},{"id":1919147,"guid":"7CE65CF530B640E785692DAB9F9201AA","previous_id":1919146,"previous_guid":"039B85E1B5DE4DE690059E13AAD5210E","section":"\u003cp\u003eTissue dissociation \u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"vkn5\",\"text\":\"After centrifugation at 300g for 10 minutes, resuspend the sample in the debris removal solution, gently overlay in cold D-PBS with high glucose and centrifuge at 3000g for 10 minutes with full acceleration brake.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94139,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"8","cases":[],"critical":null},{"id":1919148,"guid":"DA67B6C5CDD94A3FB37347148026A188","previous_id":1919147,"previous_guid":"7CE65CF530B640E785692DAB9F9201AA","section":"\u003cp\u003eTissue dissociation \u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"1n9jm\",\"text\":\"Discard the two phases and resuspend the sample in cold D-PBS with high glucose\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94139,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"9","cases":[],"critical":null},{"id":1919149,"guid":"1B1FAB9120D54094B530DABED62C9AD5","previous_id":1919148,"previous_guid":"DA67B6C5CDD94A3FB37347148026A188","section":"\u003cp\u003eTissue dissociation \u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"75v3b\",\"text\":\"Centrifuge at 1000g for 10 minutes\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94139,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"10","cases":[],"critical":null},{"id":1919150,"guid":"69131485323743A9AA51091A8C371429","previous_id":1919149,"previous_guid":"1B1FAB9120D54094B530DABED62C9AD5","section":"\u003cp\u003eTissue dissociation \u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"2kv8v\",\"text\":\"Resuspend in the staining buffer (sample is ready for cell count and flow cytometry staining)\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94139,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"11","cases":[],"critical":null},{"id":1919156,"guid":"D89D14CEFA8B490AB544EB10E27DFA80","previous_id":1919150,"previous_guid":"69131485323743A9AA51091A8C371429","section":"\u003cp\u003eImmunophenotyping of brain tissues by flow cytometry\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"fe5ii\",\"text\":\"Stain single suspension cells with PerCP5.5-conjugated anti-CD45 (1:100, Biolegend), APC-Vio770-conjugated anti-CD11b/c (1:100, REAffinity Miltenyi Biotec.), APC-conjugated anti-CD161 (1:100, Biolegend) or CD86 (1:100, Miltenyi Biotec.), PE-conjugated anti-CD45RA (1:100, Biolegend), PE-Cy7-conjugated anti-CD3 (1:100, Miltenyi Biotec.) or anti-MHC-II (1:100, REAffinity Miltenyi Biotec.), and FITC-conjugated anti-granulocytes (1:100 Miltenyi Biotec.) or VioBright-conjugated anti-ACSA-2/O4/CD31 (1:100 REAffinity Miltenyi Biotec.) for 15 minutes at 4°C in the dark\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94139,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"12","cases":[],"critical":null},{"id":1919157,"guid":"53B68F17CB934162A7BF7EF4D350F542","previous_id":1919156,"previous_guid":"D89D14CEFA8B490AB544EB10E27DFA80","section":"\u003cp\u003eImmunophenotyping of brain tissues by flow cytometry\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"50j4g\",\"text\":\"Wash and resuspend in PBS\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94139,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"13","cases":[],"critical":null},{"id":1919158,"guid":"6428E5A518174CBA9EC04670CC0C9328","previous_id":1919157,"previous_guid":"53B68F17CB934162A7BF7EF4D350F542","section":"\u003cp\u003eImmunophenotyping of brain tissues by flow cytometry\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"dcqds\",\"text\":\"Acquire cells on an appropriate flow cytometer\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94139,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"14","cases":[],"critical":null},{"id":1919159,"guid":"CFD320C70A50457698AA38D7188FEEED","previous_id":1919158,"previous_guid":"6428E5A518174CBA9EC04670CC0C9328","section":"\u003cp\u003eImmunophenotyping of brain tissues by flow cytometry\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"1t3ss\",\"text\":\"Exclude astrocytes, oligodendrocytes, and endothelial cells based on their expression of ACSA-2, O4, and CD31 (Lineage). The remaining cells are identified as CD45+ total leucocytes/resident immune cells\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94139,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"15","cases":[],"critical":null},{"id":1919160,"guid":"2BC8AD6222E84F4E91BCCFB1424ACB80","previous_id":1919159,"previous_guid":"CFD320C70A50457698AA38D7188FEEED","section":"\u003cp\u003eImmunophenotyping of brain tissues by flow cytometry\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"f1qph\",\"text\":\"Inside this gate, microglial cells are identified as CD45lowCD11b/c+ cells and infiltrated myeloid cells as CD45highCD11bhigh, subsequently identified as macrophages according to the expression of F4/80. The remaining of CD45highCD11blow cells are further gated to identify T-lymphocytes as CD3+, B-lymphocytes as CD45RA+, and NK cells as CD161+. The expression of CD68 and MHC-II is further assessed inside the microglial cell population\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94139,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"16","cases":[],"critical":null},{"id":1919161,"guid":"D356D8B05916454288416A54BB9455F2","previous_id":1919160,"previous_guid":"2BC8AD6222E84F4E91BCCFB1424ACB80","section":"\u003cp\u003eImmunophenotyping of brain tissues by flow cytometry\u003c/p\u003e","section_co