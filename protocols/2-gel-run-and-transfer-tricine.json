:{"private":0,"public":0},"access":{"can_view":1,"can_remove":1,"can_add":0,"can_edit":0,"can_publish":0,"can_get_doi":1,"can_share":1,"can_move":1,"can_move_outside":1,"can_transfer":1,"can_download":1,"is_locked":0},"guid":"93538B403E4A11EBAD4F5551AAC39FD1","state_version_id":307,"steps":[{"id":1092642,"guid":"C24955603E4A11EBAD4F5551AAC39FD1","previous_id":null,"previous_guid":null,"modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"A9FF96D963714687B51A7D2BCE5C0589","order_id":1,"type_id":6,"title":"Section","source":{"title":"Buffers:"}},{"id":1054724,"guid":"094F146929ED45149289291C4300CDE6","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Tricine SDSRunningBuffer- dilute from 10 X to 1X\u00a0<\/div><div class = \"text-block\">Composition                               10X\/L<\/div><div class = \"text-block\"><ul style = \"list-style-type:disc;\"><li style = \"counter-reset:ol0;list-style-type:disc;\">100 mM Tris base              (121 g)<\/li><li style = \"counter-reset:ol0;list-style-type:disc;\">100 mM Tricine                  (179 g)<\/li><li style = \"counter-reset:ol0;list-style-type:disc;\">0.1% SDS                             (10g)<\/li><\/ul><\/div><div class = \"text-block\">pH 8.3<\/div><div class = \"text-block\"><span>The pH of the 1X solution is 8.3.<\/span><span style = \"font-weight:bold;\"> Do not<\/span><span> use acid or base to adjust pH (Novex, 2003)<\/span><\/div><div class = \"text-block\">Stable 6 months at room temperature<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1092644,"guid":"D6A791203E4A11EBAD4F5551AAC39FD1","previous_id":1092642,"previous_guid":"C24955603E4A11EBAD4F5551AAC39FD1","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"B0B18EA3576F42269E56A4B5EAFD6429","order_id":1,"type_id":6,"title":"Section","source":{"title":"Buffers:"}},{"id":1054724,"guid":"8B423EE40E1B4D8D99C7DDEC151EC389","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Tris-GlycineTransferBuffer-(Novex, 2003)<\/div><div class = \"text-block\">\u00a0Composition                            10X\/L<\/div><div class = \"text-block\"><ul style = \"list-style-type:disc;\"><li style = \"counter-reset:ol0;list-style-type:disc;\">12 mM Tris Base              (14.56gm)<\/li><li style = \"counter-reset:ol0;list-style-type:disc;\">96 mM Glycine                 (72 g)<\/li><\/ul><\/div><div class = \"text-block\"><span>pH 8.3(1X solution is pH 8.3, <\/span><span style = \"font-weight:bold;\">do not<\/span><span> use acid or base to adjust)<\/span><\/div><div class = \"text-block\">Mix well and adjust the volume to 1 L. Store at room temperature, stable for 6 months.<\/div><\/div>"}},{"id":1054725,"guid":"9FD38978ECC7458483E84029691D970D","order_id":2,"type_id":26,"title":"notes","source":{"id":0,"parent_id":0,"uri":"","title":"","body":"<div class = \"text-blocks\"><div class = \"text-block\">Different concentrations found from different protocols e.g. this one was suggested for BioRad\u2019selectrophoresis transfer cells\u00a0<\/div><div class = \"text-block\">TowbinBuffer with SDS<\/div><div class = \"text-block\">25 mM Tris (3.03 gm\/L),192 mM Glycine (14.4g\/L), 20% MeOH (200 ml), SDS (0.1 g\/L)- do not use SDS for small protein<\/div><\/div>","created_on":0,"changed_on":0,"creator":{"name":" ","affiliation":null,"affiliations":[],"username":"","note":null,"link":null,"image":{"source":"","placeholder":""},"badges":[],"verified":0,"is_verified_user":false,"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false},"comments":[]}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1092645,"guid":"BBC521A03E4B11EBAD4F5551AAC39FD1","previous_id":1092644,"previous_guid":"D6A791203E4A11EBAD4F5551AAC39FD1","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"2DD7039541DA4D818910816E750197DE","order_id":1,"type_id":6,"title":"Section","source":{"title":"Running gel (See Invitrogen quick reference Cat. No. A25977, publication part no. 100025990)."}},{"id":1054724,"guid":"D41F9A1BEC0543ED8AD407483C2BC6A9","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">If running only one gel- make sure the unused chamber does not contain the cassette clamp.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1092647,"guid":"05C07E303E4C11EBAD4F5551AAC39FD1","previous_id":1092645,"previous_guid":"BBC521A03E4B11EBAD4F5551AAC39FD1","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"A4F118AA024C48B3873A797A2ACFB1BE","order_id":1,"type_id":6,"title":"Section","source":{"title":"Running gel (See Invitrogen quick reference Cat. No. A25977, publication part no. 100025990)."}},{"id":1054724,"guid":"97B51710998A40FEAD34AAD28B67862C","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Prepare buffer (dilute 10X Tricine SDS Running Buffer to 1X)<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1092654,"guid":"47B67AB03E4C11EBAD4F5551AAC39FD1","previous_id":1092647,"previous_guid":"05C07E303E4C11EBAD4F5551AAC39FD1","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"4065F205B7F84D17A86A3EB5ACEF869E","order_id":1,"type_id":6,"title":"Section","source":{"title":"Running gel (See Invitrogen quick reference Cat. No. A25977, publication part no. 100025990)."}},{"id":1054724,"guid":"633262A8AB3A454FB178F42951E5B398","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Plan to load 10\u00a0ul\u00a0and with 5\u00a0ug\u00a0of protein (might vary based on the concentration of your protein of interest).\u00a0Prepare 1X sample buffer for dilution of samples, if needed.\u00a0Allocate 12\u00a0ul\u00a0(sample in SB containing 0.5\u00a0ug\/ul) to an Eppendorf tube,\u00a0also prepare your standard (1-1.5\u00a0ul\u00a0of SeeBluePlus2 + 1X Sample buffer). Heat at 85 C for 2 minutes.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1092657,"guid":"668634803E4C11EBAD4F5551AAC39FD1","previous_id":1092654,"previous_guid":"47B67AB03E4C11EBAD4F5551AAC39FD1","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"58F1D4E7570E4839BC361AE8D30D3A94","order_id":1,"type_id":6,"title":"Section","source":{"title":"Running gel (See Invitrogen quick reference Cat. No. A25977, publication part no. 100025990)."}},{"id":1054724,"guid":"2A882C6A50AE4401B3EACB2CAAB44394","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Prepare gels.<\/div><div class = \"text-block\"><ul style = \"list-style-type:disc;\"><li style = \"counter-reset:ol0;list-style-type:disc;\">Remove the comb, and rinse wells three times using 1X Running Buffer.<\/li><li style = \"counter-reset:ol0;list-style-type:disc;\">Remove the white tape near the bottom of the gel cassettes and place the gels in the tank.<\/li><li style = \"counter-reset:ol0;list-style-type:disc;\">Load 10ul of your sample and standard.\u00a0Adjust gels as necessary.<\/li><li style = \"counter-reset:ol0;list-style-type:disc;\">Fill up the tank with Running buffer and Run (125 V constant ~90 minutes).<\/li><\/ul><\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1092660,"guid":"988065003E4C11EBAD4F5551AAC39FD1","previous_id":1092657,"previous_guid":"668634803E4C11EBAD4F5551AAC39FD1","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"F169D12D38B9464488723CCC5FD1EC6C","order_id":1,"type_id":6,"title":"Section","source":{"title":"Protein Transfer (see mini blot module Cat. No. B1000)"}},{"id":1054724,"guid":"664440E0770A40EF90082B2331830958","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Cut selected transfer membrane and filter\u00a0paper to the dimensions of the gel\u00a0<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#84CE84","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1092661,"guid":"C501D3C03E4C11EBAD4F5551AAC39FD1","previous_id":1092660,"previous_guid":"988065003E4C11EBAD4F5551AAC39FD1","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"F08DCF80E9414547B3AEC87C713FA519","order_id":1,"type_id":6,"title":"Section","source":{"title":"Protein Transfer (see mini blot module Cat. No. B1000)"}},{"id":1054724,"guid":"9174F7049DA9415CA6AC90B4644883D2","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span>PVDF membrane: Pre-wet PVDF membrane for 30 seconds in <\/span><span style = \"font-weight:bold;\">methanol<\/span><span>,\u00a0 ethanol, or isopropanol. Briefly rinse in deionized water, then place in a\u00a0shallow dish with 50 ml of 1X Transfer Buffer for several minutes.\u00a0\u00a0<\/span><\/div><div class = \"text-block\"><span>Standard PVDF has an unacceptable background signal in the IR.\u00a0 Handle the membranes with forceps at the very edges, and minimize contact with gloves. Damage to the membrane can show up on the image of the blot.\u00a0<\/span><span style = \"font-weight:bold;\">Use only dishes that are clean and have not contained Coomassie blue<\/span><span>\u2013 the instrument is very sensitive and will detect traces of blue dye on the blot.\u00a0 The blue dye in the pre-stained markers will be detected, which is convenient.\u00a0 The red marker in the set will not be detected.<\/span><\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#84CE84","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1092662,"guid":"E689CFC03E4C11EBAD4F5551AAC39FD1","previous_id":1092661,"previous_guid":"C501D3C03E4C11EBAD4F5551AAC39FD1","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"84A877F4FEDB430B8F2CA45F2A8968A5","order_id":1,"type_id":6,"title":"Section","source":{"title":"Protein Transfer (see mini blot module Cat. No. B1000)"}},{"id":1054724,"guid":"2C67D2C5587340DD80EF89FCCEE931AF","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Nitrocellulose: Place the membrane directly into a shallow dish\u00a0containing 50 ml of 1X Transfer Buffer for several minutes.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#84CE84","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1092663,"guid":"0E0F9C003E4D11EBAD4F5551AAC39FD1","previous_id":1092662,"previous_guid":"E689CFC03E4C11EBAD4F5551AAC39FD1","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"1E86F42971BC442C83AE6A36CEFABAB4","order_id":1,"type_id":6,"title":"Section","source":{"title":"Protein Transfer (see mini blot module Cat. No. B1000)"}},{"id":1054724,"guid":"85160E2C331A49308D44EF557E97C616","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Filter paper: Soak the filter paper briefly in 1X Transfer Buffer\u00a0immediately prior to use.\u00a0\u00a0<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#84CE84","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1092664,"guid":"1AF59C803E4D11EBAD4F5551AAC39FD1","previous_id":1092663,"previous_guid":"0E0F9C003E4D11EBAD4F5551AAC39FD1","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"3981D1983A7140B896D3A5B37C6343EA","order_id":1,"type_id":6,"title":"Section","source":{"title":"Protein Transfer (see mini blot module Cat. No. B1000)"}},{"id":1054724,"guid":"A17AEF86F59D4A7AAF20D1D409044F3F","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Gel: Use the gel immediately following the run.\u00a0 Do not soak the gel in\u00a0transfer buffer.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#84CE84","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1092666,"guid":"37C170003E4D11EBAD4F5551AAC39FD1","previous_id":1092664,"previous_guid":"1AF59C803E4D11EBAD4F5551AAC39FD1","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"10BD5B5DC761402ABB31112525C265EF","order_id":1,"type_id":6,"title":"Section","source":{"title":"Follow mini blot module protocol (20V- 50 minutes for PVDF membrane)"}},{"id":1054724,"guid":"BA79FE2737A34F3893669D59F6E256B1","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><ul style = \"list-style-type:disc;\"><li style = \"counter-reset:ol0;list-style-type:disc;\">After the transfer is complete, turn the power supply off, unplug the power leads, and remove the lid<\/li><li style = \"counter-reset:ol0;list-style-type:disc;\">Remove the blot module from the chamber and empty the transfer buffer, open module assembly, disassemble the blot sandwich, and carefully remove the membrane with blotting tweezers<\/li><li style = \"counter-reset:ol0;list-style-type:disc;\">Let the membrane dry on a filter paper (store or proceed to immunodetection)<\/li><\/ul><\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#FFED92","section_duration":0,"critical":null,"critical_id":null,"duration":0}],"document":"","materials":[],"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span style = \"font-weight:bold;\">Western Blot<\/span><\/div><\/div>","changed_on":1608837838}