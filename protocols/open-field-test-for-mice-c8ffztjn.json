[],\"data\":{}},{\"key\":\"co23k\",\"text\":\"White lights.\",\"type\":\"unordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","ownership_history":null,"parent_collections":[],"parent_protocols":[],"peer_reviewed":false,"protocol_references":"","public":true,"public_fork_note":"","published_on":1706710235,"references":[],"related_equipments":[],"related_materials":[],"reserved_doi":"","retraction_reason":null,"samples":{},"shared_access_id":265,"show_comparison":true,"sign_info":null,"space_access":{"can_view":false,"can_edit":false,"can_remove":false,"can_add":false,"can_publish":true,"can_get_doi":true,"can_share":false,"can_move":false,"can_move_outside":false,"can_transfer":false,"can_download":false,"limited_run":false,"limited_private_links":false,"limited_blind_links":false,"is_locked":false},"space_id":8782,"state_version_id":520,"stats":{"is_voted":false,"number_of_views":3,"number_of_steps":31,"number_of_bookmarks":0,"number_of_comments":0,"number_of_bookmarked_comments":0,"number_of_steps_comments":0,"number_of_protocol_comments":0,"number_of_exports":0,"number_of_runs":0,"number_of_votes":0,"number_of_reagents":0,"number_of_equipments":0,"number_of_collections":0,"number_of_forks":{"private":0,"public":1},"number_of_accessible_forks":1},"status":{"id":1,"info":"We use this protocol and it's working"},"steps":[{"id":1924359,"guid":"E17AAB5BB4314F90BC8FED480F9021D9","previous_id":0,"previous_guid":null,"section":"\u003cp\u003eBiobserve Viewer\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"47crq\",\"text\":\"Open Biobserve Viewer.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94407,"case_id":0,"critical_ids":"","duration":30,"original_id":0,"number":"1","cases":[],"critical":null},{"id":1924360,"guid":"CEEA09964ECD44AAAA5CEAA1A73A7A32","previous_id":1924359,"previous_guid":"E17AAB5BB4314F90BC8FED480F9021D9","section":"\u003cp\u003eBiobserve Viewer\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"39j45\",\"text\":\"Make sure that the arena is well positioned in the middle of the view of the webcam.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94407,"case_id":0,"critical_ids":"","duration":60,"original_id":0,"number":"2","cases":[],"critical":null},{"id":1924361,"guid":"37A164E51731453EB5A6DFB93148FB60","previous_id":1924360,"previous_guid":"CEEA09964ECD44AAAA5CEAA1A73A7A32","section":"\u003cp\u003eBiobserve Viewer\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"8spn5\",\"text\":\"Go to Configuration and Experiment. Set “stop experiment after: “ to 20 minutes. If you are alone, it is best to tick “Delayed start”, so you can start the recording at the moment when the animal is put into the arena.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94407,"case_id":0,"critical_ids":"","duration":30,"original_id":0,"number":"3","cases":[],"critical":null},{"id":1924362,"guid":"8DF67D0F0CFD4F35A4AADA2CDF61CE7B","previous_id":1924361,"previous_guid":"37A164E51731453EB5A6DFB93148FB60","section":"\u003cp\u003eBiobserve Viewer\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"53rsq\",\"text\":\"At Camera, go to Capture settings and disable auto focus.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94407,"case_id":0,"critical_ids":"","duration":30,"original_id":0,"number":"4","cases":[],"critical":null},{"id":1924363,"guid":"7C40A0BD1BC042A59F36E4EA6F7AAA43","previous_id":1924362,"previous_guid":"8DF67D0F0CFD4F35A4AADA2CDF61CE7B","section":"\u003cp\u003eBiobserve Viewer\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"ed9b\",\"text\":\"At Filters and Objects, calibrate the camera. It is easiest if you highlight the whole arena and tell the software its dimensions (50x50 cm).\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94407,"case_id":0,"critical_ids":"","duration":120,"original_id":0,"number":"5","cases":[],"critical":null},{"id":1924364,"guid":"A52C34DEFB5B40FC82609AF2A7C9BF68","previous_id":1924363,"previous_guid":"7C40A0BD1BC042A59F36E4EA6F7AAA43","section":"\u003cp\u003eBiobserve Viewer\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"ef8mg\",\"text\":\"If you click on the Area configuration button, you can select the tracking area. Make sure that the whole arena is within the tracking area. If you would like to measure the mouse’s average distance from the walls, then the tracking area should be equivalent to the floor of the arena.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94407,"case_id":0,"critical_ids":"","duration":120,"original_id":0,"number":"6","cases":[],"critical":null},{"id":1924365,"guid":"667EB3ABCAC74C8894BA91E7FD55A219","previous_id":1924364,"previous_guid":"A52C34DEFB5B40FC82609AF2A7C9BF68","section":"\u003cp\u003eBiobserve Viewer\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"bcgsi\",\"text\":\"You can make a background filter. With this, you can mask the walls, so the tracking system will not confuse the mouse with dark areas.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94407,"case_id":0,"critical_ids":"","duration":180,"original_id":0,"number":"7","cases":[],"critical":null},{"id":1924366,"guid":"71C01DD26B6542DE9D66CCE730D9EC92","previous_id":1924365,"previous_guid":"667EB3ABCAC74C8894BA91E7FD55A219","section":"\u003cp\u003eBiobserve Viewer\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"doc29\",\"text\":\"With a mouse which does not participate in your experiment, you can optimise the sensitivity of the background filter and the animal filter. Recommended values:\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"di00u\",\"text\":\"Background filter sensitivity: 20\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"edk4f\",\"text\":\"Animal filter: 35\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"dac0o\",\"text\":\"Min. animal size: 270 pixels\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94407,"case_id":0,"critical_ids":"","duration":180,"original_id":0,"number":"8","cases":[],"critical":null},{"id":1924367,"guid":"C14093860AB24886A5ED24DCF523D613","previous_id":1924366,"previous_guid":"71C01DD26B6542DE9D66CCE730D9EC92","section":"\u003cp\u003eBiobserve Viewer\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"eb9tn\",\"text\":\"Define the tracking zones in Zone Definition. I used the following zones:\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"tsht\",\"text\":\"50x50 cm is the whole arena\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"19hn5\",\"text\":\"25x25 cm in the middle is the center\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"67d85\",\"text\":\"12.5x12.5 cm in the middle is the minimal center\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94407,"case_id":0,"critical_ids":"","duration":300,"original_id":0,"number":"9","cases":[],"critical":null},{"id":1924368,"guid":"B2FD850466254B058B4B022D08CE4791","previous_id":1924367,"previous_guid":"C14093860AB24886A5ED24DCF523D613","section":"\u003cp\u003eBiobserve Viewer\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"fcrfc\",\"text\":\"Reference point is the body.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94407,"case_id":0,"critical_ids":"","duration":1,"original_id":0,"number":"10","cases":[],"critical":null},{"id":1924369,"guid":"192D94FC6DEE4210B16BE70C7D4AC43C","previous_id":1924368,"previous_guid":"B2FD850466254B058B4B022D08CE4791","section":"\u003cp\u003eBiobserve Viewer\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"e2r10\",\"text\":\"If you would like, you can define velocity classes.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94407,"case_id":0,"critical_ids":"","duration":180,"original_id":0,"number":"11","cases":[],"critical":null},{"id":1924370,"guid":"76D34CF8B3BF47F7B05FAC325C5A2997","previous_id":1924369,"previous_guid":"192D94FC6DEE4210B16BE70C7D4AC43C","section":"\u003cp\u003eBiobserve Viewer\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"2r60r\",\"text\":\"At Saving options, set the summary to 300 seconds (this will give you information about the mouse’s behaviour in 5-min bins).\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94407,"case_id":0,"critical_ids":"","duration":30,"original_id":0,"number":"12","cases":[],"critical":null},{"id":1924371,"guid":"9CF5A1E36F4948A9AD785D21E10EC498","previous_id":1924370,"previous_guid":"76D34CF8B3BF47F7B05FAC325C5A2997","section":"\u003cp\u003eBiobserve Viewer\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"fn347\",\"text\":\"Tick “Record movie” and set the frame rate to 25 fps.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94407,"case_id":0,"critical_ids":"","duration":30,"original_id":0,"number":"13","cases":[],"critical":null},{"id":1924372,"guid":"C1B5FD861DE344F8851B4CCE280D2571","previous_id":1924371,"previous_guid":"9CF5A1E36F4948A9AD785D21E10EC498","section":"\u003cp\u003eBiobserve Viewer\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"911jb\",\"text\":\"Define your output folders. The initial data path is to the folder where you want to save the videos and the .vrf files, and the initial export data path is where you will save the csv files.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94407,"case_id":0,"critical_ids":"","duration":120,"original_id":0,"number":"14","cases":[],"critical":null},{"id":1924373,"guid":"FA8146B7E127458ABB56FEC17C1CB80A","previous_id":1924372,"previous_guid":"C1B5FD861DE344F8851B4CCE280D2571","section":"\u003cp\u003eBiobserve Viewer\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"4rvhb\",\"text\":\"Save your settings.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94407,"case_id":0,"critical_ids":"","duration":1,"original_id":0,"number":"15","cases":[],"critical":null},{"id":1924376,"guid":"DAB5372E6BBA4E1B9FD066951F86EC1E","previous_id":1924373,"previous_guid":"FA8146B7E127458ABB56FEC17C1CB80A","section":"\u003cp\u003eBehaviour\u003c/p\u003e","section_color":"#94EBFF","section_duration":3870,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"etjfl\",\"text\":\"Transport the mice to the behaviour room and put the mice in individual cages.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94407,"case_id":0,"critical_ids":"","duration":300,"original_id":0,"number":"16","cases":[],"critical":null},{"id":1924377,"guid":"6505BDEFDB334F76A01C57D20C48469C","previous_id":1924376,"previous_guid":"DAB5372E6BBA4E1B9FD066951F86EC1E","section":"\u003cp\u003eBehaviour\u003c/p\u003e","section_color":"#94EBFF","section_duration":3870,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"2chou\",\"text\":\"Leave them in the lobby in darkness or with red light for at least 30 minutes.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94407,"case_id":0,"critical_ids":"","duration":1800,"original_id":0,"number":"17","cases":[],"critical":null},{"id":1924378,"guid":"193CBD94DD3C462EAE972EDB38DDBFBA","previous_id":1924377,"previous_guid":"6505BDEFDB334F76A01C57D20C48469C","section":"\u003cp\u003eBehaviour\u003c/p\u003e","section_color":"#94EBFF","section_duration":3870,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"7far3\",\"text\":\"Turn on the red lights in the room.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94407,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"18","cases":[],"critical":null},{"id":1924379,"guid":"CEBC2D203C82487FAC6172C3A2291D51","previous_id":1924378,"previous_guid":"193CBD94DD3C462EAE972EDB38DDBFBA","section":"\u003cp\u003eBehaviour\u003c/p\u003e","section_color":"#94EBFF","section_duration":3870,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"36noa\",\"text\":\"Place a 50x50 cm arena on the table.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94407,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"19","cases":[],"critical":null},{"id":1924380,"guid":"F4A74D0E057C4D1FBE62BA963189C337","previous_id":1924379,"previous_guid":"CEBC2D203C82487FAC6172C3A2291D51","section":"\u003cp\u003eBehaviour\u003c/p\u003e","section_color":"#94EBFF","section_duration":3870,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"fdnkf\",\"text\":\"Clean the arena and the table with 70% ethanol.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94407,"case_id":0,"critical_ids":"","duration":60,"original_id":0,"number":"20","cases":[],"critical":null},{"id":1924381,"guid":"6A802323F1FA4F85842E77497F17D4C0","previous_id":1924380,"previous_guid":"F4A74D0E057C4D1FBE62BA963189C337","section":"\u003cp\u003eBehaviour\u003c/p\u003e","section_color":"#94EBFF","section_duration":3870,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"3vcgo\",\"text\":\"Turn on the white lights above the arena. Set the light intensity in the bottom of the arena to 300 lux with the luxmeter.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94407,"case_id":0,"critical_ids":"","duration":120,"original_id":0,"number":"21","cases":[],"critical":null},{"id":1924382,"guid":"523850B3936D4AEAA1B10C3DC737A39F","previous_id":1924381,"previous_guid":"6A802323F1FA4F85842E77497F17D4C0","section":"\u003cp\u003eBehaviour\u003c/p\u003e","section_color":"#94EBFF","section_duration":3870,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"201k1\",\"text\":\"Bring a mouse to the behaviour room. Open the cage and gently get the animal out. Do not chase it around in the cage.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94407,"case_id":0,"critical_ids":"","duration":120,"original_id":0,"number":"22","cases":[],"critical":null},{"id":1924383,"guid":"452DA8BD6F554A23AC7B2DF2BF9B3DB6","previous_id":1924382,"previous_guid":"523850B3936D4AEAA1B10C3DC737A39F","section":"\u003cp\u003eBehaviour\u003c/p\u003e","section_color":"#94EBFF","section_duration":3870,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"bhrkc\",\"text\":\"Place the animal in the center of the arena. Depending on your handling protocol, you can put it in by the tail or in the palm of your hand.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94407,"case_id":0,"critical_ids":"","duration":30,"original_id":0,"number":"23","cases":[],"critical":null},{"id":1924384,"guid":"5AAAB82B57D94F3CA7A06F9A4C634444","previous_id":1924383,"previous_guid":"452DA8BD6F554A23AC7B2DF2BF9B3DB6","section":"\u003cp\u003eBehaviour\u003c/p\u003e","section_color":"#94EBFF","section_duration":3870,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"3oln6\",\"text\":\"Make sure to start the recording at the moment when you place the mouse in the arena. You can either do it by setting a delayed start in Biobserve, or with the help of another person.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":94407,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"24","cases":[],"critical":null},{"id":1924385,"guid":"5043DC085841