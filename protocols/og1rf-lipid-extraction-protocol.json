protocol_id":0,"components":[{"id":1054723,"guid":"78687214F3344A17AB60C60021262C08","order_id":1,"type_id":6,"title":"Section","source":{"title":"Steps"}},{"id":1054724,"guid":"84B66C02D8A54E77B6E99363AE327D3C","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Re-suspend\u00a0pellets\u00a0in 1mL of\u00a0PBS<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1081985,"guid":"B2EA1D802F6211EBB53D4DBAFDC0A7B1","previous_id":1081984,"previous_guid":"B2E192002F6211EBB53D4DBAFDC0A7B1","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"32E8C77BB71C4DFABA00437492769A6D","order_id":1,"type_id":6,"title":"Section","source":{"title":"Steps"}},{"id":1054724,"guid":"AF12AD0AFEDA4815A793444E7E4C61BA","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Add 100uL of stock lysozyme\u00a0(100mg\/mL\u00a0\u2013 stock made in water)\u00a0pre-warmed to 37oC.\u00a0\u00a0Incubate cells for 20\u00a0minutes at 37oC\u00a0in 50 mL conical.<\/div><div class = \"text-block\">Meanwhile, add 0.5g glass beads to the screw cap microcentrifuge tube.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1081986,"guid":"C9318BA02F6211EBB53D4DBAFDC0A7B1","previous_id":1081985,"previous_guid":"B2EA1D802F6211EBB53D4DBAFDC0A7B1","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"29D7334819E542BEB21113C79F745F16","order_id":1,"type_id":6,"title":"Section","source":{"title":"Steps"}},{"id":1054724,"guid":"8174875665884A42919B2ADACFB4E254","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Add\u00a0the lysozyme treated cells to screw cap\u00a0microcentrifuge tube\u00a0containing 0.5g of glass beads\u00a0(<100um).\u00a0\u00a0Bead beat for two intervals of\u00a01 minute each.\u00a0\u00a0Leave on ice at least 30 seconds between cycles.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1081987,"guid":"C93E83F02F6211EBB53D4DBAFDC0A7B1","previous_id":1081986,"previous_guid":"C9318BA02F6211EBB53D4DBAFDC0A7B1","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"93D6B4BA69064D808E78DA2DE3E4B550","order_id":1,"type_id":6,"title":"Section","source":{"title":"Steps"}},{"id":1054724,"guid":"C89789C1412B4DFBAA159C55AB744C95","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Transfer\u00a0mixture\u00a0to\u00a015 mL conical\u00a0tube\u00a0(vortex\u00a0the bead beating tube then pour contents into 15mL\u00a0conical).\u00a0 Add 1.5mL methanol and\u00a03mL chloroform.\u00a0\u00a0 (You can add this to the tubes ahead of time.)<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1081988,"guid":"C94959602F6211EBB53D4DBAFDC0A7B1","previous_id":1081987,"previous_guid":"C93E83F02F6211EBB53D4DBAFDC0A7B1","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"31485245CAD24CBD935B3CAAC494ECC9","order_id":1,"type_id":6,"title":"Section","source":{"title":"Steps"}},{"id":1054724,"guid":"CBDF95DE7B204E168D1B1ECA0326B9BA","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Then,\u00a0gently vortex mixture.\u00a0 Centrifuge for 5\u00a0minutes.<\/div><div class = \"text-block\">There will be two layers, separated by a white interface, and glass beads at the bottom of the tube.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1081989,"guid":"E07C87102F6211EBB53D4DBAFDC0A7B1","previous_id":1081988,"previous_guid":"C94959602F6211EBB53D4DBAFDC0A7B1","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"E610AFEB514F4F6082BC465AE30B91A4","order_id":1,"type_id":6,"title":"Section","source":{"title":"Steps"}},{"id":1054724,"guid":"27F49588377847BB808BBAC36D33ED2F","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Using a glass Pasteur pipet\u00a0\u2013 transfer the liquid above the white interface and the liquid below the white interface (leaving behind the beads at the bottom and as much of the interface as possible) to a new 15mL conical.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1081990,"guid":"E08D77002F6211EBB53D4DBAFDC0A7B1","previous_id":1081989,"previous_guid":"E07C87102F6211EBB53D4DBAFDC0A7B1","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"3379AA13E9804056B983D82DFD7E3AC0","order_id":1,"type_id":6,"title":"Section","source":{"title":"Steps"}},{"id":1054724,"guid":"0384018D6C424D71AE4CFB910C06D6D0","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Add 1.5mL of 0.9% NaCl\u00a0to\u00a0the\u00a0new 15mL conical.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1081991,"guid":"E0A285A02F6211EBB53D4DBAFDC0A7B1","previous_id":1081990,"previous_guid":"E08D77002F6211EBB53D4DBAFDC0A7B1","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"551EEE320A794EC8AB0647EEE3117E04","order_id":1,"type_id":6,