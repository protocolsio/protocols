id":"5894B3B926764383B3C6871F82A5B838","section":"\u003cp\u003ePreparation of chambered slides\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"7dhud\",\"text\":\"Wash cells once with PBS, then fix for 15 minutes using 4% PFA diluted in PBS. Wash cells once more with PBS.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90734,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"2","cases":[],"critical":null},{"id":1845949,"guid":"4892AE01DDB04ED1864DE81A0EA9CB0D","previous_id":1845947,"previous_guid":"BC2FD5A5A5AA43FD93822B633D8E49FB","section":"\u003cp\u003ePreparation of chambered slides\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"8lan9\",\"text\":\"Permeabilize cells for an additional 15 minutes using a 1x stock of SLO prepared in PBS + 10 mM TCEP. Prepare 1x stock according to the protocol in this article (https://doi.org/10.7554/eLife.20378)\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[{\"key\":0,\"offset\":162,\"length\":35}],\"data\":{}}],\"entityMap\":{\"0\":{\"type\":\"link\",\"mutability\":\"MUTABLE\",\"data\":{\"guid\":\"76E8849D800811EEA1FC0A58A9FEAC02\",\"url\":\"https://doi.org/10.7554/eLife.20378\"}}}}","data":null,"protocol_id":90734,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"3","cases":[],"critical":null},{"id":1845951,"guid":"4B11C5E9D9BF4E81AEDFEA2FC0CA1F37","previous_id":1845949,"previous_guid":"4892AE01DDB04ED1864DE81A0EA9CB0D","section":"\u003cp\u003ePreparation of chambered slides\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"condt\",\"text\":\"Remove SLO, wash cells with PBS once\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90734,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4","cases":[],"critical":null},{"id":1845952,"guid":"16304AB39B1643FAA17C7FA6DF18D722","previous_id":1845951,"previous_guid":"4B11C5E9D9BF4E81AEDFEA2FC0CA1F37","section":"\u003cp\u003ePreparation of chambered slides\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"601nl\",\"text\":\"Incubate permeabilized cells in 2% BSA in PBS for 1 H at RT to block\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90734,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"5","cases":[],"critical":null},{"id":1845953,"guid":"4D913C973266485DBF4DFC18FC5D31EB","previous_id":1845952,"previous_guid":"16304AB39B1643FAA17C7FA6DF18D722","section":"\u003cp\u003ePreparation of chambered slides\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"20eu2\",\"text\":\"Replace blocking solution with blocking solution + IFA competent primary antibody. For this study, used a 1:200 dilution of ab302494 anti pS72 Rab7 Rb antibody. Incubate at RT for 1 H\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90734,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"6","cases":[],"critical":null},{"id":1845958,"guid":"78DF91ADB0354F10B85B3DA1E40C1298","previous_id":1845953,"previous_guid":"4D913C973266485DBF4DFC18FC5D31EB","section":"\u003cp\u003ePreparation of chambered slides\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"egrdv\",\"text\":\"Wash cells 3 x 5 min with PBS to remove primary antibody\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90734,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"7","cases":[],"critical":null},{"id":1845959,"guid":"B808BE1361C841AF9F25519156F9E7C3","previous_id":1845958,"previous_guid":"78DF91ADB0354F10B85B3DA1E40C1298","section":"\u003cp\u003ePreparation of chambered slides\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"8a1ov\",\"text\":\"Replace PBS with a solution of fluorescently labelled secondary antibody diluted in 2% BSA in PBS. In this study, used ThermoFisher antibody A11008, Goat anti Rabbit Alexa 488. Incubate for 1 H in the dark.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90734,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"8","cases":[],"critical":null},{"id":1845964,"guid":"D4AA81836A694E918B1B0D983A7834C2","previous_id":1845959,"previous_guid":"B808BE1361C841AF9F25519156F9E7C3","section":"\u003cp\u003ePreparation of chambered slides\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"322jk\",\"text\":\"Wash cells with 3 x 5 min of PBS, then leave cells sitting under PBS. Image immediately.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90734,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"9","cases":[],"critical":null},{"id":1845965,"guid":"89CDCAD5A6B84BD4A2EDCF22C9A0E8AF","previous_id":1845964,"previous_guid":"D4AA81836A694E918B1B0D983A7834C2","section":"\u003cp\u003eImaging\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"8uau9\",\"text\":\"Prepare Confocal microscope with 60x immersion oil objective. Place slides on stage, and find focus.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90734,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"10","cases":[],"critical":null},{"id":1845967,"guid":"993C8525A86646728F3F223790763E69","previous_id":1845965,"previous_guid":"89CDCAD5A6B84BD4A2EDCF22C9A0E8AF","section":"\u003cp\u003eImaging\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"1s66f\",\"text\":\"Adjust laser power, pinhole, and gain for optimal brightness and resolution. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90734,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"11","cases":[],"critical":null},{"id":1845969,"guid":"011CC699708443989B25E40EF0B3A163","previous_id":1845967,"previous_guid":"993C8525A86646728F3F223790763E69","section":"\u003cp\u003eImaging\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"9funi\",\"text\":\"Capture each fluorescent channel individually in order to minimize risk of bleedthrough\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90734,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"12","cases":[],"critical":null},{"id":1845971,"guid":"E3C2ADCC7D4B4867B5067BE34D045F19","previous_id":1845969,"previous_guid":"011CC699708443989B25E40EF0B3A163","section":"\u003cp\u003eImaging\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"sb0k\",\"text\":\"In order to test for bleedthrough, check to make sure that when excitatory laser for a particular channel is dialed down to an intensity of 0, image is completely dark.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90734,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"13","cases":[],"critical":null},{"id":1845984,"guid":"2D36D9D3B2E14F5BBCD044F275A434DE","previous_id":1845971,"previous_guid":"E3C2ADCC7D4B4867B5067BE34D045F19","section":"\u003cp\u003eImaging\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"mfnu\",\"text\":\"Acquire images and export to FIJI for analysis\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90734,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"14","cases":[],"critical":null},{"id":1845985,"guid":"E158F0C80C2C4F8092CB8682A6C0BD6B","previous_id":1845984,"previous_guid":"2D36D9D3B2E14F5BBCD044F275A434DE","section":"\u003cp\u003eColocalization analysis \u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"lre8\",\"text\":\"Merge fluorescent channels into a single stack for colocalization analysis\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90734,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"15","cases":[],"critical":null},{"id":1845986,"guid":"144D6487BA274814AF3C007173C059AA","previous_id":1845985,"previous_guid":"E158F0C80C2C4F8092CB8682A6C0BD6B","section":"\u003cp\u003eColocalization analysis \u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"2qcvo\",\"text\":\"For each channel, measure the max brightness of a known dark region of the image. For this study, I used the nucleus of a cell as an expected dark. Use FIJI's math\\u003esubtract command to subtract this value from each pixel in the image. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"italic\",\"offset\":159,\"length\":4},{\"style\":\"italic\",\"offset\":164,\"length\":8}],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90734,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"16","cases":[],"critical":null},{"id":1845987,"guid":"4EA511B3F52B45CD9A2DA45862722AD2","previous_id":1845986,"previous_guid":"144D6487BA274814AF3C007173C059AA","section":"\u003cp\u003eColocalization analysis \u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"9mkuq\",\"text\":\"In each image, define ROIs around target cells. If a transient transfection was performed, cells should be expressing a moderate amount of the target protein.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90734,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"17","cases":[],"critical":null},{"id":1845988,"guid":"FFA173E3BFE64CDBB32282810A8CFE96","previous_id":1845987,"previous_guid":"4EA511B3F52B45CD9A2DA45862722AD2","section":"\u003cp\u003eColocalization analysis \u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"2t07e\",\"text\":\"Use the Coloc 2i plugin to calculate the Mander's colocalization coefficient with a Costes' threshold regression.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"italic\",\"offset\":8,\"length\":9}],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90734,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"18","cases":[],"critical":null},{"id":1845989,"guid":"C722D85A2759472D9261CA0AAC02015B","previous_id":1845988,"previous_guid":"FFA173E3BFE64CDBB32282810A8CFE96","section":"\u003cp\u003eColocalization analysis \u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"