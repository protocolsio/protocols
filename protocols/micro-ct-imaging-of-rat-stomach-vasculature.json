se,"is_confirmed":1,"is_declined":0,"requester":{"name":" ","affiliation":null,"affiliation_url":null,"username":null,"link":null},"protocol":{"id":0,"title":"Micro-CT imaging of rat stomach vasculature","title_html":"Micro-CT imaging of rat stomach vasculature","image":{"source":null,"placeholder":null},"doi":null,"doi_status":0,"uri":"micro-ct-imaging-of-rat-stomach-vasculature-bafnibme","type_id":1,"template_id":0,"published_on":null,"stats":{"number_of_views":0,"number_of_steps":0,"number_of_bookmarks":0,"number_of_comments":0,"number_of_exports":0,"number_of_runs":0,"number_of_votes":0,"is_voted":0},"parent_protocols":[],"parent_collections":[],"cited_protocols":[]},"created_on":1608146500,"resolve_on":0,"resolved_user":{"name":" ","affiliation":null,"affiliations":[],"username":null,"note":null,"link":null,"image":{"source":null,"placeholder":null},"badges":[],"verified":0,"is_verified_user":false,"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false},"shared":false}}],"is_owner":1,"has_subprotocols":0,"is_subprotocol":0,"is_bookmarked":0,"can_claim_authorship":0,"can_accept_authorship":0,"can_be_copied":1,"can_remove_fork":1,"fork_id":null,"url":"https:\/\/www.protocols.io\/view\/micro-ct-imaging-of-rat-stomach-vasculature-bafnibme","forks_count":{"private":0,"public":0},"access":{"can_view":1,"can_remove":0,"can_add":0,"can_edit":0,"can_publish":0,"can_get_doi":0,"can_share":1,"can_move":1,"can_move_outside":1,"can_transfer":1,"can_download":1,"is_locked":0},"guid":"FE359C401CEB11EA9D5C8522DB3DACA2","state_version_id":874,"steps":[{"id":859824,"guid":"14AF21701CED11EA9D5C8522DB3DACA2","previous_id":null,"previous_guid":null,"modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"EDC093350B2443D99522670A79C175BE","order_id":1,"type_id":6,"title":"Section","source":{"title":"Set-up"}},{"id":1054724,"guid":"870DF04E0AF34703A8954CF1D167F329","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Feed the rat per the details given in protocol \"Micro-CT imaging of iodine-stained rat stomach\".<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":[],"critical_id":null,"duration":0},{"id":859825,"guid":"14B081001CED11EA9D5C8522DB3DACA2","previous_id":1093760,"previous_guid":"2EFB18803FCF11EB94441DE7F0A47D27","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"E550F55B3DF74EAB91E1E811DA19356D","order_id":1,"type_id":6,"title":"Section","source":{"title":"Set-up"}},{"id":1054724,"guid":"61E6E0732AD34B70A7038F943CDC1547","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Weigh the rat to determine the appropriate dose of ketamine + xylazine cocktail. Prepared cocktail consists of 10 mL of 100 mg\/mL ketamine and 1 mL of 100 mg\/mL xylazine for a total volume of 11 mL. Accepted dose is 2-3 mL\/kg body weight, depending on the fed\/fasting state of the animal.<\/div><\/div>"}},{"id":1054725,"guid":"DE40F0303FCF11EB94441DE7F0A47D27","order_id":2,"type_id":20,"title":"reagents","source":{"id":182051,"mol_weight":0,"name":"Ketamine","linfor":"","url":"https:\/\/www.pattersonvet.com\/ProductItem\/078036637","sku":"07-803-6637","cas_number":"","rrid":"","public":0,"vendor":{"name":"Patterson Veterinary","affiliation":null,"affiliations":[],"username":null,"note":null,"link":"https:\/\/www.pattersonvet.com\/","image":{"source":"https:\/\/www.protocols.iohttps:\/\/www.protocols.io\/img\/vendors\/placeholder.png","placeholder":"https:\/\/www.protocols.iohttps:\/\/www.protocols.io\/img\/vendors\/placeholder.png"},"badges":[],"verified":0,"is_verified_user":false,"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false,"id":470},"can_edit":0,"stats":{"total_protocols":0}}},{"id":1054726,"guid":"004680A03FD011EB94441DE7F0A47D27","order_id":3,"type_id":20,"title":"reagents","source":{"id":193073,"mol_weight":0,"name":"Xylazine","linfor":"","url":"http:\/\/www.akorn.com\/prod_detail_veterinary.php?ndc=59399-110-20","sku":"NDC: 59399-110-20","cas_number":"","rrid":"","public":0,"vendor":{"name":"Akorn Animal Health","affiliation":null,"affiliations":[],"username":null,"note":null,"link":null,"image":{"source":"https:\/\/www.protocols.iohttps:\/\/www.protocols.io\/img\/vendors\/placeholder.png","placeholder":"https:\/\/www.protocols.iohttps:\/\/www.protocols.io\/img\/vendors\/placeholder.png"},"badges":[],"verified":0,"is_verified_user":false,"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false,"id":686},"can_edit":0,"stats":{"total_protocols":0}}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":[],"critical_id":null,"duration":0},{"id":859826,"guid":"14B22EB01CED11EA9D5C8522DB3DACA2","previous_id":859825,"previous_guid":"14B081001CED11EA9D5C8522DB3DACA2","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"A7E86534DA0949A08E0639D969BD0846","order_id":1,"type_id":6,"title":"Section","source":{"title":"Set-up"}},{"id":1054724,"guid":"6412C6B834354B428954A221BFE16EA9","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Prepare two syringes: one with a 23G needle to be used for intraperitoneal injection of ketamine + xylazine, and one with a 26G needle for intracardiac injection of heparin. 500 USP units of heparin are used for each rat.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":[],"critical_id":null,"duration":0},{"id":859827,"guid":"14B478A01CED11EA9D5C8522DB3DACA2","previous_id":859826,"previous_guid":"14B22EB01CED11EA9D5C8522DB3DACA2","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"1B409953F7D74148B1F33F4A579FBC28","order_id":1,"type_id":6,"title":"Section","source":{"title":"Set-up"}},{"id":1054724,"guid":"67412FD9ECED447799F14F9D5F9BBF14","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Mix the Microfil compound and diluent in an appropriate (at least 40 mL) container, preferably sealable and disposable (avoid using glass, as it is difficult to clean). The specific Microfil compound used is MV-122, due to its high concentration of radiopaque lead chromate. <\/div><div class = \"text-block\"><span>The preferred ratio of compound to diluent was determined (through trial and error) to be 3 mL of compound to 5 mL of diluent. To ensure thorough vascular fill, you must use a total volume (while maintaining the 3:5 ratio) that exceeds the estimated volume of blood in the rat. Gently shake or stir the solution to mix, but take care not to mix ao vigorously that bubbles are produced. <\/span><span style = \"font-weight:bold;\">DO NOT ADD THE CURING AGENT AT THIS POINT.   <\/span><span>If the ratio of compound to diluent is too high then penetration of the vasculature is poor; if the ratio of compound to diluent is too low then the material will tend to leak out of the stomach.<\/span><\/div><\/div>"}},{"id":1054725,"guid":"1B59CEB03FD011EB94441DE7F0A47D27","order_id":2,"type_id":20,"title":"reagents","source":{"id":348640,"mol_weight":0,"name":"Microfil MV-122","linfor":"","url":"https:\/\/www.flowtech-inc.com\/order\/","sku":"MV-122","cas_number":"","rrid":"","public":0,"vendor":{"name":"Flow Tech Inc","affiliation":null,"affiliations":[],"username":null,"note":null,"link":"https:\/\/www.flowtech-inc.com\/","image":{"source":"https:\/\/www.protocols.iohttps:\/\/www.protocols.io\/img\/vendors\/placeholder.png","placeholder":"https:\/\/www.protocols.iohttps:\/\/www.protocols.io\/img\/vendors\/placeholder.png"},"badges":[],"verified":0,"is_verified_user":false,"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false,"id":878},"can_edit":0,"stats":{"total_protocols":0}}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":[],"critical_id":null,"duration":0},{"id":859828,"guid":"BF2066A01CED11EA9D5C8522DB3DACA2","previous_id":859827,"previous_guid":"14B478A01CED11EA9D5C8522DB3DACA2","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"85A6061A579A458AB9132185753D1308","order_id":1,"type_id":6,"title":"Section","source":{"title":"Set-up"}},{"id":1054724,"guid":"7823E08FEF2F45BCAE28F0B3CB16A510","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Prepare the peristalsis pump (Masterflex series) with appropriate input and output tubing to accommodate an 18G blunted needle attached to a 1 mL syringe. Set the pump to 25 mL\/min or an equivalent flow rate.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":[],"critical_id":null,"duration":0},{"id":859829,"guid":"92788EB01CEE11EA9D5C8522DB3DACA2","previous_id":859828,"previous_guid":"BF2066A01CED11EA9D5C8522DB3DACA2","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"737230EFA5F848FF8F2DB44B7A802E78","order_id":1,"type_id":6,"title":"Section","source":{"title":"Perfusion"}},{"id":1054724,"guid":"C038A3DDED0D4F0D819EDB03CBBB82DC","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Administer the appropriate dose of ketamine\/xylazine intraperitoneally.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":[],"critical_id":null,"duration":0},{"id":859831,"guid":"0277AB601CEF11EA9D5C8522DB3DACA2","previous_id":859829,"previous_guid":"92788EB01CEE11EA9D5C8522DB3DACA2","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"6588EE6217D24144AE2D7E972BDCE250","order_id":1,"type_id":6,"title":"Section","source":{"title":"Perfusion"}},{"id":1054724,"guid":"75E4738967C04DBBB7106F76C9925A3D","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Expose the thoracic cavity of the rat. Take care not to expose the abdomen so that it can be kept at or as close to 37 \u00b0C as possible for the rest of the procedure.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":[],"critical_id":null,"duration":0},{"id":859832,"guid":"027F73901CEF11EA9D5C8522DB3DACA2","previous_id":859831,"previous_guid":"0277AB601CEF11EA9D5C8522DB3DACA2","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"CEA44DF6C0C645EAB158D717F005F03F","order_id":1,"type_id":6,"title":"Section","source":{"title":"Perfusion"}},{"id":1054724,"guid":"47176644B3B343ECB2D1A6F227075A41","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Turn the pump on to begin the flow of saline through an 18G blunted needle. Inject the heparin into the left ventricle of the rat. Immediately after, thread the 18G needle carefully through the left ventricle and into the aorta without piercing the interventricular septum. If the septum is pierced, the procedure may still continue, but the vascular fill may not be optimal due to partial redirection to the pulmonary circuit. Cut the right atrium and partially sever the inferior vena cava (IVC). Keep in mind that the IVC will need to be clamped later.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":[],"critical_id":null,"duration":0},{"id":859833,"guid":"028BCFA01CEF11EA9D5C8522DB3DACA2","previous_id":859850,"previous_guid":"C2413C601CF211EA9D5C8522DB3DACA2","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"2393BB98FC4D4961B32D2692C64D9701","order_id":1,"type_id":6,"title":"Section","source":{"title":"Perfusion"}},{"id":1054724,"guid":"FD313DEA4D4E4E28BD7ED789D3C449BB","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Make sure throughout the procedure to administer warm saline in and around the abdominal cavity of the rat at least once per minute to minimize cooling. Experience has shown that if the stomach cools too much little penetration of the forestomach vasculature occurs<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":[],"critical_id":null,"duration":0},{"id":859834,"guid":"029126D01CEF11EA9D5C8522DB3DACA2","previous_id":859833,"previous_guid":"028BCFA01CEF11EA9D5C8522DB3DACA2","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"59BB78E5247F40DEB3CD251CD5DA155F","order_id":1,"type_id":6,"title":"Section","source":{"title":"Perfusion"}},{"id":1054724,"guid":"714655B8E219456FAA1D2E3EE71C3B6C","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">After perfusion of around 200 mL saline, add the curing agent to the Microfil solution (assuming 12 mL of MV-122 compound and 20 mL of diluent), and immediately mix gently (avoid bubbles) but thoroughly for 1 to 2 minutes to achieve homogeneity. Start a 20-minute timer upon the addition of the curing agent, by which point significant polymerization will have occurred.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":[],"critical_id":null,"duration":0},{"id":859835,"guid":"0296A5101CEF11EA9D5C8522DB3DACA2","previous_id":859834,"previous_guid":"029126D01CEF11EA9D5C8522DB3DACA2","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"51F96FBC3DAF48BF8F66D8CAFFB74421","order_id":1,"type_id":6,"title":"Section","source":{"title":"Perfusion"}},{"id":1054724,"guid":"0D380F3F6FDD48F6B2A5681C695FEEB8","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">After 250 mL of saline has been perfused, stop the pump and move the input end of the tubing into the Microfil solution. Secure the tubing into the solution container. Lower the pump rate down to 2 mL\/min, and start the pump. It will likely take a couple of minutes to travel through the tubing before it reaches the rat, but filling of the aorta and coronary arteries should be visible almost immediately upon reaching the needle.While making sure to keep administering saline to the abdominal cavity, keep the pump running until the end of the 20 minute timer or until the Microfil solution is depleted, whichever comes first. Stop the pump, clamp both the perfusion line and the IVC (distal to the cut), and wait at least an additional 70 minutes before dissection to allow the Microfil compound to adequately cure.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":[],"critical_id":null,"duration":0},{"id":859837,"guid":"42AD3DC01CF011EA9D5C8522DB3DACA2","previous_id":859835,"previous_guid":"0296A5101CEF11EA9D5C8522DB3DACA2","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"E3B7AB2E37684AACB210166A9A107C74","order_id":1,"type_id":6,"title":"Section","source":{"title":"Dissection"}},{"id":1054724,"guid":"30DC813BF3E740B7A871D86C17EDA84B","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Open the abdomen and dissect out the stomach, leaving about a centimeter each of esophagus and duodenum. Make sure not to remove the gastroepiploic artery along the greater curvature of the corpus\/antrum, as it can serve as a useful axis for orientation during scanning.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#84CE84","section_duration":0,"critical":[],"critical_id":null,"duration":0},{"id":859838,"guid":"42B331301CF011EA9D5C8522DB3DACA2","previous_id":859837,"previous_guid":"42AD3DC01CF011EA9D5C8522DB3DACA2","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"D766AA8EA8034D1886A710E2F9A110D9","order_id":1,"type_id":6,"title":"Section","source":{"title":"Dissection"}},{"id":1054724,"guid":"9A2F0CFC6D104753933445CF3B625819","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Immediately place the stomach in a solution of 4% paraformaldehyde in 0.01M PBS. Allow at least 24 hours of fixation time before scanning.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#84CE84","section_duration":0,"critical":[],"critical_id":null,"duration":0},{"id":859840,"guid":"7DFAF9301CF011EA9D5C8522DB3DACA2","previous_id":1093795,"previous_guid":"7CE9E4703FD111EB94441DE7F0A47D27","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"7FAB6175EEC54C7189AD5422B83E6AC1","order_id":1,"type_id":6,"title":"Section","source":{"title":"Scanning Preparation"}},{"id":1054724,"guid":"619920A5E2AA487DB5F56411FC4FA557","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Prior to scanning, make a thick solution of gelatin in which the stomach can be suspended while scanning. The preferred concentration is 4% to 5% of type B gelatin in ultrapure water (40 g\/L to 50 g\/L). This concentration of gelatin is rigid enough to hold the stomach into place during scanning, but soft enough to allow the stomach to be manipulated into the appropriate orientation without the exertion of excess force on the tissue. The advantage of supporting the stomach in gelatin is that the stomach does not then rest on a rigid surface to which it will conform, hence distorting the shape of the stomach.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#FFED92","section_duration":0,"critical":[],"critical_id":null,"duration":0},{"id":859841,"guid":"7E0161D01CF011EA9D5C8522DB3DACA2","previous_id":859840,"previous_guid":"7DFAF9301CF011EA9D5C8522DB3DACA2","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"C1B113C192CE400CAADEC5D7ED30BE7C","order_id":1,"type_id":6,"title":"Section","source":{"title":"Scanning Preparation"}},{"id":1054724,"guid":"E0081ACF07F6466A9B928B092E985991","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">The gelatin solution should be made a day ahead of scanning to allow it to thoroughly cool. If you experience blurred scans when using gelatin, the gelatin can be refrigerated before scanning and between scans to avoid softening. An appropriate vessel for holding the stomach and gelatin is a 4-ounce, circular, transparent, polypropylene (PP) container with a sealable top.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#FFED92","section_duration":0,"critical":[],"critical_id":null,"duration":0},{"id":859842,"guid":"37399C801CF111EA9D5C8522DB3DACA2","previous_id":1093805,"previous_guid":"0B38B5803FD211EB94441DE7F0A47D27","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"FC1D4535E0734BEBB9E609D533038B66","order_id":1,"type_id":6,"title":"Section","source":{"title":"Scanning"}},{"id":1054724,"guid":"651DDB2C212C4005A1BBEB1293D93443","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">The instrument used for microCT scanning is <\/div><\/div>"}},{"id":1054725,"guid":"CB2BE52016D811EAA9BDFFB3E6DE460D","order_id":2,"type_id":28,"title":"equipment","source":{"brand":"Perkin Elmer","sku":"CLS149276","specifications":"","can_edit":false,"name":"Quantum GX2","type":"MicroCT imaging system","link":"https:\/\/www.perkinelmer.com\/product\/quantum-gx2-instrument-cls149276","file":null,"image":null}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#EA9F6C","section_duration":0,"critical":[],"critical_id":null,"duration":0},{"id":859843,"guid":"875741401CF111EA9D5C8522DB3DACA2","previous_id":859842,"previous_guid":"37399C801CF111EA9D5C8522DB3DACA2","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"63A3F25262E4483387CBF19848CBF0B7","order_id":1,"type_id":6,"title":"Section","source":{"title":"Scanning"}},{"id":1054724,"guid":"E416879CB9804498BD878492AE934DED","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Warm up the scanner. If possible, set the warmup voltage to 55 kV and current to 144 \u00b5A. <\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#EA9F6C","section_duration":0,"critical":[],"critical_id":null,"duration":0},{"id":859844,"guid":"8AFFAFD01CF111EA9D5C8522DB3DACA2","previous_id":859843,"previous_guid":"875741401CF111EA9D5C8522DB3DACA2","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"6A3924F7328A4BAFA86CB80E1321378C","order_id":1,"type_id":6,"title":"Section","source":{"title":"Scanning"}},{"id":1054724,"guid":"C2C5F1C765F14EF2BB7DC47ED466E32A","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Under a ventilated hood, remove the stomach from 4% PF solution and rinse with 10X PBS, saline, or a similar buffer. Partially fill a 4 oz. PP container \u00be with gelatin. Using a smooth tool and\/or gloved fingers, place the stomach into the container and manipulate it into the appropriate scanning orientation. Completely submerge the stomach so that it does not dry out. The standard orientation is ventral side up, esophagus facing the back of the scanner, and pylorus facing left. Make sure that the stomach is lying as flat as possible in the horizontal plane.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#EA9F6C","section_duration":0,"critical":[],"critical_id":null,"duration":0},{"id":859845,"guid":"930BEDB01CF111EA9D5C8522DB3DACA2","previous_id":859844,"previous_guid":"8AFFAFD01CF111EA9D5C8522DB3DACA2","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"C7170F58438F4462A730FEE97F6D511F","order_id":1,"type_id":6,"title":"Section","source":{"title":"Scanning"}},{"id":1054724,"guid":"73302B31154644B9B55A2190CF2A1C7E","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Open the door, place the stomach onto the bore between the beam and detector, and close the door. The Quantum GX2 will allow for fine adjustments to position (forward\/backward, left\/right, up\/down) that can be accessed in Live Mode. For the highest pixel\/voxel resolution, it is necessary to reduce the field of view (FOV) to as small as possible while still capturing the entire stomach. Using Live Mode allows the user to preview the scan by rotating the beam\/detector position around the stomach prior to actual scanning, so it is useful in determining the smallest possible field of view. With the exception of particularly large stomachs, most rat stomachs can fit within a 45 mm field of view (which yields 90 \u00b5m voxel resolution), but some may require a 60 mm FOV, or in extreme cases, a 72 mm FOV.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#EA9F6C","section_duration":0,"critical":[],"critical_id":null,"duration":0},{"id":859846,"guid":"A9282D201CF111EA9D5C8522DB3DACA2","previous_id":859845,"previous_guid":"930BEDB01CF111EA9D5C8522DB3DACA2","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"71F7F663E0F54910944A9A661D4F03CC","order_id":1,"type_id":6,"title":"Section","source":{"title":"Scanning"}},{"id":1054724,"guid":"EC657C0979924EA69DD8B3F1588F41F1","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Set the scanning parameters to High Resolution, with at least a 14 minute scan time.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#EA9F6C","section_duration":0,"critical":[],"critical_id":null,"duration":0},{"id":859847,"guid":"B4AA57E01CF111EA9D5C8522DB3DACA2","previous_id":859846,"previous_guid":"A9282D201CF111EA9D5C8522DB3DACA2","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"F7A14880FC0E4BA9B300DF5CFA18CA0C","order_id":1,"type_id":6,"title":"Section","source":{"title":"Scanning"}},{"id":1054724,"guid":"692D189676DA4BF79D858C6118829D5A","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Use Live Mode to view the orientation of the stomach, and use buttons located on the front of the scanner to alter translational position. It may also be necessary to remove the stomach and carefully manipulate it to achieve the correct position and alignment. After this is done, start the scan.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#EA9F6C","section_duration":0,"critical":[],"critical_id":null,"duration":0},{"id":859848,"guid":"C99EE0301CF111EA9D5C8522DB3DACA2","previous_id":859847,"previous_guid":"B4AA57E01CF111EA9D5C8522DB3DACA2","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"2727C590DF2A41269BBA678F857ADB31","order_id":1,"type_id":6,"title":"Section","source":{"title":"Scanning"}},{"id":1054724,"guid":"F94188D11E604B78A5A3705EC6F6498D","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">When scan is complete, transfer stomach to a PP container filled with saline\/PBS and rinse off all the gelatin. <\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#EA9F6C","section_duration":0,"critical":[],"critical_id":null,"duration":0},{"id":859849,"guid":"DF1395001CF111EA9D5C8522DB3DACA2","previous_id":859848,"previous_guid":"C99EE0301CF111EA9D5C8522DB3DACA2","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"5155E08B23EB4C1180A351B356162332","order_id":1,"type_id":6,"title":"Section","source":{"title":"Scanning"}},{"id":1054724,"guid":"AEF783A56056470886AE00BBC582715E","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Once back in the lab, remove tie-off threads and return all stomachs to 4% PF and restore at 4 \u00b0C.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#EA9F6C","section_duration":0,"critical":[],"critical_id":null,"duration":0},{"id":859850,"guid":"C2413C601CF211EA9D5C8522DB3DACA2","previous_id":859832,"previous_guid":"027F73901CEF11EA9D5C8522DB3DACA2","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"2BA75D65B3504AE3A086F5B685E8DA22","order_id":1,"type_id":6,"title":"Section","source":{"title":"Perfusion"}},{"id":1054724,"guid":"D4B4D95104F64FB0B19B233823BEAA6D","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Carefully insert an infant feeding tube into the stomach through the esophagus and leave in place through the perfusion to ensure that the lower esophageal sphincter remains open.  For a 300 g male Sprague-Dawley rat, the distance from mouth opening to the LES is about 125 mm.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":[],"critical_id":null,"duration":0},{"id":1093440,"guid":"6C11DEF03F3111EBBF17AD6DC82002E6","previous_id":859849,"previous_guid":"DF1395001CF111EA9D5C8522DB3DACA2","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"2E3627CAB16A4BD8B39E0CF09F71917D","order_id":1,"type_id":6,"title":"Section","source":{"title":"Scanning"}},{"id":1054724,"guid":"B04ED7F1985B4E0EB83BF6D7F6BF5275","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span>Once it has been confirmed that a good microCT scan of the vasculature has been obtained, the stomach as a whole can be imaged by soaking in an I<\/span><span style = \"vertical-align:sub;\">2<\/span><span> \/ KI solution (see this protocol \"Micro-CT imaging of iodine-stained rat stomach\") starting from step 18 of that protocol. If only light iodine staining of the complete stomach is wanted so that the vasculature remains visible, then reduce the soak time and\/or soak the stomach in PBS following the iodine soak to partially leach out the iodine. <\/span><\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#EA9F6C","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1093760,"guid":"2EFB18803FCF11EB94441DE7F0A47D27","previous_id":859824,"previous_guid":"14AF21701CED11EA9D5C8522DB3DACA2","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"109578FAB09B4A9A93AA270491939CED","order_id":1,"type_id":6,"title":"Section","source":{"title":"Set-up"}},{"id":1054724,"guid":"86F5EF3ACA9F4F69B1232BE0C9401D89","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Heat one liter of saline (either sterile or prepared with 1L ultrapure water + 8.7 g NaCl) to between 37 \u00b0C to 42 \u00b0C. <\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1093795,"guid":"7CE9E4703FD111EB94441DE7F0A47D27","previous_id":859838,"previous_guid":"42B331301CF011EA9D5C8522DB3DACA2","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"AA52CAF583C247E0AE46969A0B01732F","order_id":1,"type_id":6,"title":"Section","source":{"title":"Dissection"}},{"id":1054724,"guid":"5FB7D3BA0758464AB2BEBD6655705B74","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">After the tissue is fixed the stomach can be flushed. This is not required but can give better results if subsequent iodine staining is planned. To flush the stomach, hold the stomach under the surface of a container of saline and gently massage the stomach to push food out through the pylorus (which should be open due to the presence of the feeding tube during perfusion). If the contents just consist of Dietgel as intended, this should not be too difficult. Allow the stomach to refill with saline and repeat to remove the contents of the stomach.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#84CE84","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1093798,"guid":"CE957C303FD111EB94441DE7F0A47D27","previous_id":859841,"previous_guid":"7E0161D01CF011EA9D5C8522DB3DACA2","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"084B162CA20644B8B559BF7E93C4132A","order_id":1,"type_id":6,"title":"Section","source":{"title":"Scanning Preparation"}},{"id":1054724,"guid":"E170A6D8F2B74F3182F875FAA60002C5","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">An hour or so before scanning, the stomach needs to be filled full with saline and tied off with thread to ensure consistent stomach dimensions.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#FFED92","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1093800,"guid":"E104B9303FD111EB94441DE7F0A47D27","previous_id":1093798,"previous_guid":"CE957C303FD111EB94441DE7F0A47D27","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"050344FA325E48FBAFAC82712A67A98D","order_id":1,"type_id":6,"title":"Section","source":{"title":"Scanning Preparation"}},{"id":1054724,"guid":"4262F285E00B4988A4167AC4C1812681","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Tie off the esophagus with a bow of cotton thread, leaving preferably at least 1 cm of esophagus between the LES  and the bow.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#FFED92","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1093801,"guid":"ECF2C5203FD111EB94441DE7F0A47D27","previous_id":1093800,"previous_guid":"E104B9303FD111EB94441DE7F0A47D27","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"EDD6810F899F4279B540E18BE700382A","order_id":1,"type_id":6,"title":"Section","source":{"title":"Scanning Preparation"}},{"id":1054724,"guid":"B991BD2542F04048BA52B8A7958CE7D6","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Attach a 5 Fr feeding tube to a 10 or 20 mL syringe filled with PBS, and thread the tube through the duodenum, through the pylorus, and into the stomach, being careful not to damage or distort the stomach.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#FFED92","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1093803,"guid":"F8090EB03FD111EB94441DE7F0A47D27","previous_id":1093801,"previous_guid":"ECF2C5203FD111EB94441DE7F0A47D27","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"25843FA0C5904EC0A6EC01DA99E7EC1C","order_id":1,"type_id":6,"title":"Section","source":{"title":"Scanning Preparation"}},{"id":1054724,"guid":"9C060107BC764065A7FB3D0D51BE38FF","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Hold the duodenum (with tubing) skyward allowing liquids and gas to escape the stomach, and slowly (so as to not force the tubing off the needle) plunge the syringe to inflate the stomach. Continue to do this until bubbles no longer emerge to the surface. Slowly remove the tube while still plunging until the distal end of the tube is at the junction of your index finger and thumb, at the open end of the duodenum\u2014still ensure that no bubbles are present at this point, or you will have to reinsert the tube and repeat the saline injection.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#FFED92","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1093804,"guid":"01C82CB03FD211EB94441DE7F0A47D27","previous_id":1093803,"previous_guid":"F8090EB03FD111EB94441DE7F0A47D27","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"6E9483CD20FC4F97A5D0CF0EDFA788AD","order_id":1,"type_id":6,"title":"Section","source":{"title":"Scanning Preparation"}},{"id":1054724,"guid":"76154878E36A445493F228DAE7463639","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Using a small pair of hemostats, clamp the duodenum directly below where you are holding it, making sure not to clamp the end of the tube.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#FFED92","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1093805,"guid":"0B38B5803FD211EB94441DE7F0A47D27","previous_id":1093804,"previous_guid":"01C82CB03FD211EB94441DE7F0A47D27","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"1FFC4D17ACD74715917900C519C7C85D","order_id":1,"type_id":6,"title":"Section","source":{"title":"Scanning Preparation"}},{"id":1054724,"guid":"E0834B3C109B4213888A4C9BC557C569","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Tie a bow about 2-3 mm proximal to the clamp and then remove the clamp.  The stomach should now be fully inflated with saline. The purpose of this step is to ensure that the stomach has repeatable dimensions and is not partially collapsed or and doesn't contain air.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#FFED92","section_duration":0,"critical":null,"critical_id":null,"duration":0}],"document":"","materials":[{"id":159668,"mol_weight":0,"name":"Ketamine","linfor":"","url":"https:\/\/www.pattersonvet.com\/ProductItem\/078036637","sku":"07-803-6637","cas_number":"","rrid":"","public":0,"vendor":{"name":"Patterson Veterinary","affiliation":null,"affiliations":[],"username":null,"note":null,"link":"https:\/\/www.pattersonvet.com\/","image":{"source":"https:\/\/www.protocols.io\/img\/vendors\/placeholder.png","placeholder":"https:\/\/www.protocols.io\/img\/vendors\/placeholder.png"},"badges":[],"verified":0,"is_verified_user":false,"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false,"id":470},"can_edit":0,"stats":{"total_protocols":0}},{"id":193073,"mol_weight":0,"name":"Xylazine","linfor":"","url":"http:\/\/www.akorn.com\/prod_detail_veterinary.php?ndc=59399-110-20","sku":"NDC: 59399-110-20","cas_number":"","rrid":"","public":0,"vendor":{"name":"Akorn Animal Health","affiliation":null,"affiliations":[],"username":null,"note":null,"link":null,"image":{"source":"https:\/\/www.protocols.io\/img\/vendors\/placeholder.png","placeholder":"https:\/\/www.protocols.io\/img\/vendors\/placeholder.png"},"badges":[],"verified":0,"is_verified_user":false,"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false,"id":686},"can_edit":0,"stats":{"total_protocols":0}},{"id":203074,"mol_weight":0,"name":"Heparin","linfor":"","url":"https:\/\/www.henryscheinvet.com\/Product?sku=049130","sku":"049130","cas_number":"","rrid":"","public":0,"vendor":{"name":"Henry Schein Animal Health","affiliation":null,"affiliations":[],"username":null,"note":null,"link":"https:\/\/www.henryscheinvet.com\/","image":{"source":"https:\/\/www.protocols.io\/img\/vendors\/placeholder.png","placeholder":"https:\/\/www.protocols.io\/img\/vendors\/placeholder.png"},"badges":[],"verified":0,"is_verified_user":false,"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false,"id":690},"can_edit":0,"stats":{"total_protocols":0}},{"id":348640,"mol_weight":0,"name":"Microfil MV-122","linfor":"","url":"https:\/\/www.flowtech-inc.com\/order\/","sku":"MV-122","cas_number":"","rrid":"","public":0,"vendor":{"name":"Flow Tech Inc","affiliation":null,"affiliations":[],"username":null,"note":null,"link":"https:\/\/www.flowtech-inc.com\/","image":{"source":"https:\/\/www.protocols.io\/img\/vendors\/placeholder.png","placeholder":"https:\/\/www.protocols.io\/img\/vendors\/placeholder.png"},"badges":[],"verified":0,"is_verified_user":false,"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false,"id":878},"can_edit":0,"stats":{"total_protocols":0}},{"id":348641,"mol_weight":0,"name":"Microfil MV-Diluent","linfor":"","url":"https:\/\/www.flowtech-inc.com\/order\/","sku":"MV-Diluent","cas_number":"","rrid":"","public":0,"vendor":{"name":"Flow Tech Inc","affiliation":null,"affiliations":[],"username":null,"note":null,"link":"https:\/\/www.flowtech-inc.com\/","image":{"source":"https:\/\/www.protocols.io\/img\/vendors\/placeholder.png","placeholder":"https:\/\/www.protocols.io\/img\/vendors\/placeholder.png"},"badges":[],"verified":0,"is_verified_user":false,"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false,"id":878},"can_edit":0,"stats":{"total_protocols":0}}],"description":"<div class = \"text-blocks\"><div class = \"text-block\">This protocol describes how to prepare a rat stomach for micro-CT imaging of the vasculature.  A lead-containing silicone-based curable material is injected into the heart at perfusion and flows through the vasculature and cures in place.  Subsequent microCT imaging then provides a 3D description of the stomach vasculature.<\/div><\/div>","changed_on":1608146500}