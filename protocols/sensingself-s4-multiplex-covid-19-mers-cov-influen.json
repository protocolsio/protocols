:[{"id":1023610,"guid":"5828A4E0EF8C11EABC8A3963AB73116D","previous_id":null,"previous_guid":null,"modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"10E318D1DE584A869E4D677D162A0745","order_id":1,"type_id":6,"title":"Section","source":{"title":"SALIVA SAMPLE COLLECTION"}},{"id":1054724,"guid":"2937C21FC22047D09A0B5A769D48C2D9","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Saliva should be collected with the assistance of a healthcare worker or technician. <\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1023611,"guid":"5A3BCD70EF8C11EABC8A3963AB73116D","previous_id":1023610,"previous_guid":"5828A4E0EF8C11EABC8A3963AB73116D","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"F07D66CB41A340EC88FAB28976C091FF","order_id":1,"type_id":6,"title":"Section","source":{"title":"SALIVA SAMPLE COLLECTION"}},{"id":1054724,"guid":"D288A9D28D0D49B2AA4DD6DF35863DDB","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Before collection, clean hands using alcohol-based sanitizer or soap and water (no fragrances) and wear appropriate PPE (at minimum, gloves and a mask).<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1023612,"guid":"6084D780EF8C11EABC8A3963AB73116D","previous_id":1023611,"previous_guid":"5A3BCD70EF8C11EABC8A3963AB73116D","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"970C8EE397BF44F8B64CED213D9ABF13","order_id":1,"type_id":6,"title":"Section","source":{"title":"SALIVA SAMPLE COLLECTION"}},{"id":1054724,"guid":"930826844DAC43C6B7B0C4ADEE4B3C23","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">While preparing collection materials, direct the sample provider to begin pooling saliva in their mouth. Saliva production can be stimulated by thinking about food (favorite foods, upcoming meals, etc.) or about the saliva collection itself.<\/div><\/div>"}},{"id":1054725,"guid":"9F8F76E9AFE74F6CBB34C83CC970B7B3","order_id":2,"type_id":26,"title":"notes","source":{"id":0,"parent_id":0,"uri":"","title":"","body":"<div class = \"text-blocks\"><div class = \"text-block\">This protocol is intended for the collection of the normal saliva that naturally pools into the mouth. No coughing or sniffing prior to sample collection is required.\nIdeally, water should be avoided 10 minutes prior to collection. Other drinks, food, and nasal sprays should be avoided for half an hour before sample collection.<\/div><\/div>","created_on":0,"changed_on":0,"creator":{"name":" ","affiliation":null,"affiliations":[],"username":"","note":null,"link":null,"image":{"source":"","placeholder":""},"badges":[],"verified":0,"is_verified_user":false,"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false},"comments":[]}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1023613,"guid":"60DC9470EF8C11EABC8A3963AB73116D","previous_id":1023612,"previous_guid":"6084D780EF8C11EABC8A3963AB73116D","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"6E812A95F0104B40A164C08789B23911","order_id":1,"type_id":6,"title":"Section","source":{"title":"SALIVA SAMPLE COLLECTION"}},{"id":1054724,"guid":"62F566B3BCF145E988A1B677519EB1B1","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Once you have pooled saliva  in your mouth, gently expel saliva into the disposable paper cup.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1023614,"guid":"61311D10EF8C11EABC8A3963AB73116D","previous_id":1023613,"previous_guid":"60DC9470EF8C11EABC8A3963AB73116D","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"50AFDE1C385A4D65A37F173978759CA8","order_id":1,"type_id":6,"title":"Section","source":{"title":"SPUTUM SAMPLE COLLECTION"}},{"id":1054724,"guid":"E5AEA701A61E4678B6AE028DBA9CCE94","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Educate the patient about the difference between sputum and oral secretions (saliva).<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1023615,"guid":"616BB510EF8C11EABC8A3963AB73116D","previous_id":1023614,"previous_guid":"61311D10EF8C11EABC8A3963AB73116D","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"10B2335EB73A478982D7B8D7C771B864","order_id":1,"type_id":6,"title":"Section","source":{"title":"SPUTUM SAMPLE COLLECTION"}},{"id":1054724,"guid":"B5CC25EC39BB4543AC7FEB26BE6080AE","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Rinse the mouth with clean water. This is important to make sure there won\u2019t be mouth bacteria in the sputum collected. <\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1023616,"guid":"619A1810EF8C11EABC8A3963AB73116D","previous_id":1023615,"previous_guid":"616BB510EF8C11EABC8A3963AB73116D","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"D388C60372174AB9AA1FEFBB74544C60","order_id":1,"type_id":6,"title":"Section","source":{"title":"SPUTUM SAMPLE COLLECTION"}},{"id":1054724,"guid":"445D1360C86945869F08B8686681E48F","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Put one hand over the mouth with a tissue and put the other hand on your stomach.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1023617,"guid":"61B0D460EF8C11EABC8A3963AB73116D","previous_id":1023616,"previous_guid":"619A1810EF8C11EABC8A3963AB73116D","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"38140C23E9C34954B5B3165ABE4E54C4","order_id":1,"type_id":6,"title":"Section","source":{"title":"SPUTUM SAMPLE COLLECTION"}},{"id":1054724,"guid":"F5565DC758B544BB8F2C7E53C5A55AEB","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Cough deeply, so that you can really feel it in your stomach. Do not take shallow coughs from the throat or chest. <\/div><\/div>"}},{"id":1054725,"guid":"9DD6D439388D418DAC9648B54A93AB12","order_id":2,"type_id":26,"title":"notes","source":{"id":0,"parent_id":0,"uri":"","title":"","body":"<div class = \"text-blocks\"><div class = \"text-block\">If you are having trouble-<\/div><div class = \"text-block\">1. Get a pat- ask somebody to pat you solidly up and down your back to help you release the sputum.<\/div><div class = \"text-block\">2. Stretch and try again- sometimes it helps to relax a bit. When you are ready, rinse and spit and try again.<\/div><\/div>","created_on":0,"changed_on":0,"creator":{"name":" ","affiliation":null,"affiliations":[],"username":"","note":null,"link":null,"image":{"source":"","placeholder":""},"badges":[],"verified":0,"is_verified_user":false,"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false},"comments":[]}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1023618,"guid":"61CB3A30EF8C11EABC8A3963AB73116D","previous_id":1023617,"previous_guid":"61B0D460EF8C11EABC8A3963AB73116D","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"83396712C3154922A888C075A0C65214","order_id":1,"type_id":6,"title":"Section","source":{"title":"SPUTUM SAMPLE COLLECTION"}},{"id":1054724,"guid":"915C6BCEDC7B43E297DAF7E42DE436B7","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Once the sputum (phlegm) is in your mouth, release it into the disposable paper cup.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1023619,"guid":"61F15FD0EF8C11EABC8A3963AB73116D","previous_id":1023618,"previous_guid":"61CB3A30EF8C11EABC8A3963AB73116D","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"C05507A9B24F4AA8B4BDEDF32C5727A7","order_id":1,"type_id":6,"title":"Section","source":{"title":"STOOL SAMPLE COLLECTION"}},{"id":1054724,"guid":"7090A7E65EB44ADB8138AE3BF54EF421","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Use disposable gloves while handling a stool sample. Stools can contain material that spreads infection.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#84CE84","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1023620,"guid":"620758D0EF8C11EABC8A3963AB73116D","previous_id":1023619,"previous_guid":"61F15FD0EF8C11EABC8A3963AB73116D","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"107754F9448640A28429F51F133B01FE","order_id":1,"type_id":6,"title":"Section","source":{"title":"STOOL SAMPLE COLLECTION"}},{"id":1054724,"guid":"D69A2DB78B7A4180A679B3DABF8C4992","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Do not collect the stool from the toilet. The stool must not have any contact with urine.<\/div><\/div>"}},{"id":1054725,"guid":"D98F34FFC07E4973B8559F4C6FB31C11","order_id":2,"type_id":26,"title":"notes","source":{"id":0,"parent_id":0,"uri":"","title":"","body":"<div class = \"text-blocks\"><div class = \"text-block\">1. To ensure the stool does not come in contact with urine, pass urine before collecting the stool specimen.<\/div><div class = \"text-block\">2. To ensure that the stool does not come in contact with the toilet bowl or water, loosely drape a long section of plastic wrap over the rim on both sides of the toilet bowl creating a \u201cpocket\u201d in the centre.<\/div><\/div>","created_on":0,"changed_on":0,"creator":{"name":" ","affiliation":null,"affiliations":[],"username":"","note":null,"link":null,"image":{"source":"","placeholder":""},"badges":[],"verified":0,"is_verified_user":false,"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false},"comments":[]}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#84CE84","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1023621,"guid":"621DEE10EF8C11EABC8A3963AB73116D","previous_id":1023620,"previous_guid":"620758D0EF8C11EABC8A3963AB73116D","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"AF8F5B1FEECA49B983B2447771F59930","order_id":1,"type_id":6,"title":"Section","source":{"title":"STOOL SAMPLE COLLECTION"}},{"id":1054724,"guid":"8F9DF8E9A6824CA2903AA1F98535CE0F","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">With the help of a disposable sample extraction tube collect the required quantity of stool sample for further steps. <\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#84CE84","section_duration