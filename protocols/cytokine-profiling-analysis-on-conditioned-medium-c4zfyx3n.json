olor":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"chp66\",\"text\":\"Two days after medium change, a minimum of 200 µL of cell culture medium per\\nsample is collected to run duplicate wells without dilution.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90887,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"2","cases":[],"critical":null},{"id":1847734,"guid":"F8AD3B25787744F28998F1216186DC04","previous_id":1847733,"previous_guid":"27185F611B5E499C90D0B734D23ACA1A","section":"\u003cp\u003eSample collection\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"67bao\",\"text\":\"Conditioned medium is centrifuged at 10,000 X G for 10 min at room temperature to pellet out particulates.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90887,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"3","cases":[],"critical":null},{"id":1847735,"guid":"DD538D9584D84DBA83A7891BD91A44F3","previous_id":1847734,"previous_guid":"F8AD3B25787744F28998F1216186DC04","section":"\u003cp\u003eSample collection\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"3trcb\",\"text\":\"Supernatant is collected and immediately stored at −80°C.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90887,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4","cases":[],"critical":null},{"id":1847736,"guid":"516D81C73E4347CDBD51CAF3EF1F0527","previous_id":1847735,"previous_guid":"DD538D9584D84DBA83A7891BD91A44F3","section":"\u003cp\u003eSample collection\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"edd2u\",\"text\":\"Cells are then trypsinized by 0.05% Trypsin-EDTA for 5 min and centrifuged at 200 X G for 5 min. Decant the supernatant and resuspend cells in a small aliquot of culture medium.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90887,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"5","cases":[],"critical":null},{"id":1847737,"guid":"B3DBAB7A6BA34621B132346B90D26A1B","previous_id":1847736,"previous_guid":"516D81C73E4347CDBD51CAF3EF1F0527","section":"\u003cp\u003eSample collection\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"39qff\",\"text\":\"Determine cell number for each sample by a hemocytometer for normalization of cytokine levels.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90887,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"6","cases":[],"critical":null},{"id":1847738,"guid":"E137E955807A4197854C52893822D903","previous_id":1847737,"previous_guid":"B3DBAB7A6BA34621B132346B90D26A1B","section":"\u003cp\u003eSample preparation and measurement\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"1e7s7\",\"text\":\"Thaw samples on ice prior to loading a 96-well filter plate for assay.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90887,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"7","cases":[],"critical":null},{"id":1847739,"guid":"C2376B39879143DD9B34786487D8E0E2","previous_id":1847738,"previous_guid":"E137E955807A4197854C52893822D903","section":"\u003cp\u003eSample preparation and measurement\u003c/p\u003e","section_color":"#94EBFF","sectio