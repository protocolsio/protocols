k":null,"image":{"source":"https:\/\/s3.amazonaws.com\/protocols-files\/public\/6e13fe8d600b157fb23446500a1c2a29147857fa1e20e23e5c824ce71e1b0630\/bzf6bbrw7.jpg","placeholder":"https:\/\/s3.amazonaws.com\/protocols-files\/public\/6e13fe8d600b157fb23446500a1c2a29147857fa1e20e23e5c824ce71e1b0630\/bzf6bbrw7.jpg"},"badges":[],"verified":1,"is_verified_user":true,"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false}],"versions":[],"groups":[],"is_owner":1,"has_subprotocols":0,"is_subprotocol":0,"is_bookmarked":0,"can_claim_authorship":0,"can_accept_authorship":0,"can_be_copied":1,"can_remove_fork":1,"fork_id":null,"url":"https:\/\/www.protocols.io\/view\/zymoclean-gel-dna-recovery-chem-584-bmyzk7x6","forks_count":{"private":0,"public":0},"access":{"can_view":1,"can_remove":0,"can_add":0,"can_edit":0,"can_publish":0,"can_get_doi":0,"can_share":1,"can_move":1,"can_move_outside":1,"can_transfer":1,"can_download":1,"is_locked":0},"guid":"32BBE62004EC11EBA4613B31236B760E","state_version_id":143,"steps":[{"id":1044367,"guid":"752B0A4004EC11EBA4613B31236B760E","previous_id":null,"previous_guid":null,"modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"DA07833B649E40718B1B0EE882D21EC6","order_id":1,"type_id":6,"title":"Section","source":{"title":"Gel purification from agarose gel"}},{"id":1054724,"guid":"B80A0869896041879F05E91019BC337F","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Excise the DNA fragment from the agarose gel using a razor blade, scalpel or other device and transfer it into a 1.5 ml microcentrifuge tube. <\/div><\/div>"}},{"id":1054725,"guid":"D2E5A565E8BB4B818B24BB60350FAEFE","order_id":2,"type_id":26,"title":"notes","source":{"id":0,"parent_id":0,"uri":"","title":"","body":"<div class = \"text-blocks\"><div class = \"text-block\">The amount of agarose excised from the gel should be as small as possible. Be sure to excise the desired band with very little or no extra agarose gel.<\/div><\/div>","created_on":0,"changed_on":0,"creator":{"name":" ","affiliation":null,"affiliations":[],"username":"","note":null,"link":null,"image":{"source":"","placeholder":""},"badges":[],"verified":0,"is_verified_user":false,"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false},"comments":[]}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1044368,"guid":"752BF4A004EC11EBA4613B31236B760E","previous_id":1044367,"previous_guid":"752B0A4004EC11EBA4613B31236B760E","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"568EF20B8646401EA866B24F8889DB34","order_id":1,"type_id":6,"title":"Section","source":{"title":"Gel purification from agarose gel"}},{"id":1054724,"guid":"86AAB0C5D9204DD6BCEF4454FB861729","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Add 3 volumes of ADB to each volume of agarose excised from the gel (e.g., for 100 \u03bcl (mg) of agarose gel slice add 300 \u03bcl of ADB).<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1044369,"guid":"752CB7F004EC11EBA4613B31236B760E","previous_id":1044368,"previous_guid":"752BF4A004EC11EBA4613B31236B760E","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"BC2ABB2364A64C89A1D9DA8120258531","order_id":1,"type_id":6,"title":"Section","source":{"title":"Gel purification from agarose gel"}},{"id":1054724,"guid":"EFDE13D20772424CB1FF8BF7F3C825D4","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Incubate at 37-55 \uf0b0C for 5-10 minutes until the gel slice is completely dissolved2. For DNA fragments > 8 kb, following the incubation step, add one additional volume (equal to that of the gel slice) of water to the mixture for better DNA recovery (e.g., 100 \u03bcl agarose, 300 \u03bcl ADB, and 100 \u03bcl water).<\/div><\/div>"}},{"id":1054725,"guid":"20D9B5126BFB40D591BB566CEB15021A","order_id":2,"type_id":26,"title":"notes","source":{"id":0,"parent_id":0,"uri":"","title":"","body":"<div class = \"text-blocks\"><div class = \"text-block\">Do not incubate above 60\uf0b0C. It is important that the gel slice dissolve completely. This can be facilitated by gentle mixing during the incubation.<\/div><\/div>","created_on":0,"changed_on":0,"creator":{"name":" ","affiliation":null,"affiliations":[],"username":"","note":null,"link":null,"image":{"source":"","placeholder":""},"badges":[],"verified":0,"is_verified_user":false,"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false},"comments":[]}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1044370,"guid":"752DC96004EC11EBA4613B31236B760E","previous_id":1044369,"previous_guid":"752CB7F004EC11EBA4613B31236B760E","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"4458022FC62D4B70998715DC80DB3EAF","order_id":1,"type_id":6,"title":"Section","source":{"title":"Gel purification from agarose gel"}},{"id":1054724,"guid":"2AA64E8C895F420885AB7A24FA3CDDA7","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Transfer the melted agarose solution to a Zymo-Spin\u2122 Column in a provided Collection Tube.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1044371,"guid":"752EDAD004EC11EBA4613B31236B760E","previous_id":1044370,"previous_guid":"752DC96004EC11EBA4613B31236B760E","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"E32187987CF64F38A63A49C7FBAE4AB4","order_id":1,"type_id":6,"title":"Section","source":{"title":"Gel purification from agarose gel"}},{"id":1054724,"guid":"E5B9101DC4D540A1A298CE36849C86E1","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Centrifuge for 30-60 seconds. Discard the flow-through<\/div><div class = \"text-block\">.<\/div><\/div>"}},{"id":1054725,"guid":"B76E530FD2C846F7A92674EFB2DDE623","order_id":2,"type_id":26,"title":"notes","source":{"id":0,"parent_id":0,"uri":"","title":"","body":"<div class = \"text-blocks\"><div class = \"text-block\">Remove the flow-through by aspiration. Avoid contamination of the collection tube rim.<\/div><\/div>","created_on":0,"changed_on":0,"creator":{"name":" ","affiliation":null,"affiliations":[],"username":"","note":null,"link":null,"image":{"source":"","placeholder":""},"badges":[],"verified":0,"is_verified_user":false,"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false},"comments":[]}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1044372,"guid":"7530617004EC11EBA4613B31236B760E","previous_id":1044371,"previous_guid":"752EDAD004EC11EBA4613B31236B760E","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"D668B0DA704540B98E305B5F10B4FB30","order_id":1,"type_id":6,"title":"Section","source":{"title":"Gel purification from agarose gel"}},{"id":1054724,"guid":"3FB6146CF44D49598715F8766BB7EFC6","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Add 200 \u03bcl of DNA Wash Buffer to the column and centrifuge for 30 seconds. Discard the flow-through. Repeat the wash step.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1044373,"guid":"7531E81004EC11EBA4613B31236B760E","previous_id":1044372,"previous_guid":"7530617004EC11EBA4613B31236B760E","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"D7AC03428EF242E28090D36DF10ADD1F","order_id":1,"type_id":6,"title":"Section","source":{"title":"Gel purification from agarose gel"}},{"id":1054724,"guid":"E6B2E784B196419AA951B596F83B9157","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Add \u2265 6 \u03bcl DNA Elution Buffer or water directly to the column matrix. Place column into a 1.5 ml tube and centrifuge for 30-60 seconds to elute DNA. Ultra-pure DNA is now ready for use.<\/div><\/div>"}},{"id":1054725,"guid":"AEF06C4F610748A0900E6D3F1AAC26CF","order_id":2,"type_id":26,"title":"notes","source":{"id":0,"parent_id":0,"uri":"","title":"","body":"<div class = \"text-blocks\"><div class = \"text-block\">DNA Elution Buffer: 10 mM Tris-HCl, pH 8.5, 0.1 mM EDTA.<\/div><div class = \"text-block\">Elution of DNA from the column is dependent on pH and temperature. If water is used, make sure the pH is >6.0. Waiting 1 minute prior to elution may improve the yield of larger (> 6 kb) DNA. For even larger DNA (> 10 kb), the total yield may be improved by eluting the DNA with 60-70\u00b0C DNA Elution Buffer.<\/div><\/div>","created_on":0,"changed_on":0,"creator":{"name":" ","affiliation":null,"affiliations":[],"username":"","note":null,"link":null,"image":{"source":"","placeholder":""},"badges":[],"verified":0,"is_verified_user":false,"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false},"comments":[]}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0}],"document":null,"materials":[],"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span style = \"font-weight:bold;\">Product Description<\/span><\/div><div class = \"text-block\">The Zymoclean\u2122 Gel DNA Recovery Kit provides a hassle-free method for high yield recovery of pure DNA from agarose gels. Simply add the specially formulated Agarose Dissolving Buffer (ADB) to the gel slice containing your DNA sample, let dissolve, and then transfer to the supplied Zymo-Spin\u2122 Column. There is no need for organic denaturants or  chloroform. Instead, the product utilizes Fast-Spin column technology to yield high-quality DNA in just 15 minutes. DNA<\/div><div class = \"text-block\">purified using the Zymoclean\u2122 Gel DNA Recovery Kit is perfectly suited for use in DNA ligation reactions, sequencing, DNA labeling reactions, PCR, etc.<\/div><\/div>","changed_on":1601670517}