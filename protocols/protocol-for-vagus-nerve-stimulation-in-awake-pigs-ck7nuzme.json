nges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":74702,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"5","cases":[],"critical":null},{"id":1564440,"guid":"67461C40910711ED99A305BADB8B2175","previous_id":1564439,"previous_guid":"6617D570910711ED99A305BADB8B2175","section":"\u003cp\u003eSurgery\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"26i3k\",\"text\":\"Tunnel the cuffs and leads to an open incision halfway down the neck. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":74702,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"6","cases":[],"critical":null},{"id":1564442,"guid":"6A05B170910711ED99A305BADB8B2175","previous_id":1564440,"previous_guid":"67461C40910711ED99A305BADB8B2175","section":"\u003cp\u003eSurgery\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"v4bd\",\"text\":\"Position the pig supine with neck extended and make a single incision through the skin and superficial fat layers between the mandible and sternal notch. Use blunt dissection to expose the carotid sheath and isolate the vagus nerve from the carotid artery. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":74702,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"7","cases":[],"critical":null},{"id":1564443,"guid":"79B161A0910711ED99A305BADB8B2175","previous_id":1564442,"previous_guid":"6A05B170910711ED99A305BADB8B2175","section":"\u003cp\u003eSurgery\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"2m0a9\",\"text\":\"Use blunt dissection to locate the superior and recurrent laryngeal branches of the vagus nerve.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":74702,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"8","cases":[],"critical":null},{"id":1564445,"guid":"7A933620910711ED99A305BADB8B2175","previous_id":1564443,"previous_guid":"79B161A0910711ED99A305BADB8B2175","section":"\u003cp\u003eSurgery\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"jg4f\",\"text\":\"Use blunt dissection to expose as much length of the vagus trunk as possible. Ideally at least 8 cm from the superior laryngeal branch insertion point to a more caudal location.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":74702,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"9","cases":[],"critical":null},{"id":1564446,"guid":"7B43E880910711ED99A305BADB8B2175","previous_id":1564445,"previous_guid":"7A933620910711ED99A305BADB8B2175","section":"\u003cp\u003eSurgery\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"874j\",\"text\":\"Tunnel the leads and cuffs from the incision in the neck to where the vagus nerve is exposed. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":74702,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"10","cases":[],"critical":null},{"id":1564447,"guid":"7C0033A0910711ED99A305BADB8B2175","previous_id":1564446,"previous_guid":"7B43E880910711ED99A305BADB8B2175","section":"\u003cp\u003eSurgery\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"9vs1b\",\"text\":\"Place cuffs on vagus nerve such that there is at least 4 cm between each cuff, but ideally 5.5 cm.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":74702,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"11","cases":[],"critical":null},{"id":1564448,"guid":"8A7E1230910711ED99A305BADB8B2175","previous_id":1564447,"previous_guid":"7C0033A0910711ED99A305BADB8B2175","section":"\u003cp\u003eSurgery\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"blqa9\",\"text\":\"Sew EMG patch on laryngeal muscle and ground lead in a pocket next to the caudal cuff. Coil the ECG lead and place in a pocket on the right pectoral. Tack down the wires. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":74702,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"12","cases":[],"critical":null},{"id":1564449,"guid":"938F6400910711ED99A305BADB8B2175","previous_id":1564448,"previous_guid":"8A7E1230910711ED99A305BADB8B2175","section":"\u003cp\u003eSurgery\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"etr4s\",\"text\":\"Validate system works before closing incisions.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":74702,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"13","cases":[],"critical":null},{"id":1564450,"guid":"97C8EF00910711ED99A305BADB8B2175","previous_id":1564449,"previous_guid":"938F6400910711ED99A305BADB8B2175","section":"\u003cp\u003eSurgery\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"533qf\",\"text\":\"Close incisions.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":74702,"case_id":0,"criti