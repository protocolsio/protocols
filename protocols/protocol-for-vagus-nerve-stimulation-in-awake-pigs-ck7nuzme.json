anges\":[],\"data\":{}},{\"key\":\"9rpf1\",\"text\":\" Pulse width: 260us\",\"type\":\"unordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"bdnq4\",\"text\":\"Train duration: 3s\",\"type\":\"unordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"4p7nk\",\"text\":\"Stimulation shape: monophasic\",\"type\":\"unordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"3dp19\",\"text\":\"Polarity: cathodic\",\"type\":\"unordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":74702,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"16","cases":[],"critical":null},{"id":1573920,"guid":"31D2915098FB11EDA14EE51F07E2BD06","previous_id":1573919,"previous_guid":"198A984098FB11EDA14EE51F07E2BD06","section":"\u003cp\u003eStimulation Protocol\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"fbm7t\",\"text\":\"Conduct a grid search by doing the cartesian product of the frequency and current parameters. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"em0l3\",\"text\":\"Frequency: {2Hz, 5Hz, 10Hz, 20Hz, 30Hz}\",\"type\":\"unordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"d5lph\",\"text\":\"Current: {0.05mA, 0.5mA, 1mA, 1.5mA, 2.5mA}\",\"type\":\"unordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[{\"key\":0,\"offset\":10,\"length\":32}],\"data\":{}},{\"key\":\"ss6e\",\"text\":\"After each stimulation, wait for the physiological signals to return to baseline. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{\"0\":{\"type\":\"code_insert\",\"mutability\":\"MUTABLE\",\"data\":\"0.05mA, 0.5mA, 