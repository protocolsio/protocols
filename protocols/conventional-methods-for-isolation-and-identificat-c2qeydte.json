ce":"https://content.protocols.io/files/A917C6235E1C11EE9F150A58A9FEAC02-placeholder.png","placeholder":"https://content.protocols.io/files/A917C6235E1C11EE9F150A58A9FEAC02-placeholder.png"},"tech_support":{"email":null,"phone":null,"use_email":false,"hide_contact":false,"url":null}}],"guid":"4A20FA263EEF44948D6A1D6AEBA58FAF","guidelines":"","has_references":false,"has_step_reagents":false,"has_versions":false,"id":88550,"image":{"source":"https://www.protocols.io/img/default_protocol.png","webp_source":null,"placeholder":"https://www.protocols.io/img/default_protocol.png","webp_placeholder":null},"image_attribution":"","in_trash":false,"is_bookmarked":false,"is_contact_suspended":false,"is_content_confidential":false,"is_content_warning":false,"is_doi_reserved":false,"is_in_pending_publishing":false,"is_in_transfer":false,"is_owner":true,"is_research":true,"is_retracted":false,"is_shared_directly":false,"is_subprotocol":null,"is_unlisted":false,"item_id":1168088,"journal":null,"journals":[],"keywords":"","last_modified":1695928556,"link":"","location":null,"manuscript_citation":"","materials":[],"materials_text":"","ownership_history":null,"parent_collections":[],"parent_protocols":[],"peer_reviewed":false,"protocol_references":"","public":true,"public_fork_note":"","published_on":1695928556,"references":[],"related_equipments":[],"related_materials":[],"reserved_doi":"","retraction_reason":null,"samples":{},"shared_access_id":16,"show_comparison":false,"sign_info":null,"state_version_id":1128,"stats":{"is_voted":false,"number_of_views":0,"number_of_steps":13,"number_of_bookmarks":0,"number_of_comments":0,"number_of_bookmarked_comments":0,"number_of_steps_comments":0,"number_of_protocol_comments":0,"number_of_exports":0,"number_of_runs":0,"number_of_votes":0,"number_of_reagents":0,"number_of_equipments":0,"number_of_collections":0,"number_of_forks":{"private":0,"public":0},"number_of_accessible_forks":0},"status":{"id":1,"info":"We use this protocol and it's working"},"steps":[{"id":1803584,"guid":"458262DE026C49EF88705B546A56896E","previous_id":1803592,"previous_guid":"9DF949907F794C7CB938A91C8522DEBA","section":"\u003cp\u003eCulture and identification of \u003cem\u003eStreptococcus pyogenes\u003c/em\u003e\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"cqntl\",\"text\":\"Upon receipt, a beta-haemolytic colony was streaked onto a new 5% blood agar (HiMedia Laboratories Limited, Mumbai, In.) plate to obtain isolated colonies and incubated at 35-37ºC for 24 hours in an anerobic jar. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":88550,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"2","cases":[],"critical":null},{"id":1803592,"guid":"9DF949907F794C7CB938A91C8522DEBA","previous_id":0,"previous_guid":null,"section":"\u003cp\u003eCulture and identification of \u003cem\u003eStreptococcus pyogenes\u003c/em\u003e\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"ai2cr\",\"text\":\"S. pyogenes isolates that were presumptively identified were collected from the different hospitals and outpatient centres that formed part of the study. Isolates were inoculated to 5% sheep blood agar plates that were incubated overnight at 35ºC.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"italic\",\"offset\":0,\"length\":11}],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":88550,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"1","cases":[],"critical":null},{"id":1803615,"guid":"7F8681EAF16C4BFBB11D7550C4530AB2","previous_id":1803619,"previous_guid":"AC95C3A1039B495C9626F713C6728621","section":"\u003cp\u003eCulture and identification of \u003cem\u003eStreptococcus pyogenes\u003c/em\u003e\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":true,"step":"{\"blocks\":[{\"key\":\"c7dbl\",\"text\":\"For quality control purposes, S. pyogenes ATCC 19615 (positive control), S.agalactiae ATCC 12386 (negative control), Enterococcus faecalis ATCC 29212 (negative control), and an uninoculated blood agar plate were incubated alongside the test isolates.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"italic\",\"offset\":30,\"length\":11},{\"style\":\"italic\",\"offset\":73,\"length\":12},{\"style\":\"italic\",\"offset\":117,\"length\":21}],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":88550,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"2.2","cases":[],"critical":null},{"id":1803616,"guid":"9A612B3DAD2346408A444F0706299D47","previous_id":1803615,"previous_guid":"7F8681EAF16C4BFBB11D7550C4530AB2","section":"\u003cp\u003eCulture and identification of \u003cem\u003eStreptococcus pyogenes\u003c/em\u003e\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"bnkjm\",\"text\":\"Following incubation, the pyrrolidonylarylamidase (PYR) test was performed on all beta-haemolytic isolates based on the manufacturer's instructions.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":88550,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"3","cases":[],"critical":null},{"id":1803619,"guid":"AC95C3A1039B495C9626F713C6728621","previous_id":1803584,"previous_guid":"458262DE026C49EF88705B546A56896E","section":"\u003cp\u003eCulture and identification of \u003cem\u003eStreptococcus pyogenes\u003c/em\u003e\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":true,"step":"{\"blocks\":[{\"key\":\"41ger\",\"text\":\"A bacitracin antibiotic disc was placed into the inoculum of each isolate. Any zone of inhibition to the antibiotic after incubation was considered to be an indication of drug susceptibility.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":88550,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"2.1","cases":[],"critical":null},{"id":1803639,"guid":"7E2C4A5DCB02406E8383D5E0F53C9D21","previous_id":1803616,"previous_guid":"9A612B3DAD2346408A444F0706299D47","section":"\u003cp\u003eCulture and identification of \u003cem\u003eStreptococcus pyogenes\u003c/em\u003e\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":true,"step":"{\"blocks\":[{\"key\":\"eajc9\",\"text\":\"A PYR disc (HiMedia Laboratories Limited, Mumbai, In.) was placed on a glass slide on a petri dish and rehydrated with one drop of sterile deionized water. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":88550,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"3.1","cases":[],"critical":null},{"id":1803642,"guid":"D392573D166F491A998F32FB2B0DF18B","previous_id":1803639,"previous_guid":"7E2C4A5DCB02406E8383D5E0F53C9D21","section":"\u003cp\u003eCulture and identification of \u003cem\u003eStreptococcus pyogenes\u003c/em\u003e\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":true,"step":"{\"blocks\":[{\"key\":\"792q5\",\"text\":\"Using a sterile loop, 2-3 colonies were added to the suspension on the glass slide and the mixture incubated at room temperature for 2 minutes.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":88550,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"3.2","cases":[],"critical":null},{"id":1803643,"guid":"363EFF7F67F24CB6A16AD24BD80FA015","previous_id":1803644,"previous_guid":"325BBEB376054AA1A6115571A759CC6A","section":"\u003cp\u003eCulture and identification of \u003cem\u003eStreptococcus pyogenes\u003c/em\u003e\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"aqf3r\",\"text\":\"Beta-haemolytic colonies from the test samples and controls were also exposed to the Voges-Proskauer (VP) Test (BD, New Jersey, USA).\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":88550,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4","cases":[],"critical":null},{"id":1803644,"guid":"325BBEB376054AA1A6115571A759CC6A","previous_id":1803642,"previous_guid":"D392573D166F491A998F32FB2B0DF18B","section":"\u003cp\u003eCulture and identification of \u003cem\u003eStreptococcus pyogenes\u003c/em\u003e\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":true,"step":"{\"blocks\":[{\"key\":\"f5q8l\",\"text\":\"One drop of the PYR chromogenic solution was placed on the disc and observations recorded after 3 minutes. The formation of a red colour was considered a positive result\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":88550,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"3.3","cases":[],"critical":null},{"id":1803646,"guid":"F2EE54F4A534450AA281D59CF3087EB8","previous_id":1803643,"previous_guid":"363EFF7F67F24CB6A16AD24BD80FA015","section":"\u003cp\u003eCulture and identification of \u003cem\u003eStreptococcus pyogenes\u003c/em\u003e\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":true,"step":"{\"blocks\":[{\"key\":\"4aa6p\",\"text\":\"A colony was subcultured to tubes containing 4 mL of trypticase soy broth (HiMedia Laboratories Limited, Mumbai, In.) and incubated at 35-37ºC for 24 hours.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":88550,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4.1","cases":[],"critical":null},{"id":1803648,"guid":"B7793A1ECC7C405E923EDC54DEC8AB3E","previous_id":1803650,"previous_guid":"B543C89AD0F041B491B9F0AA65F4A8ED","section":"\u003cp\u003eCulture and identification of \u003cem\u003eStreptococcus pyogenes\u003c/em\u003e\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"dqvie\",\"text\":\"Confirmatory testing was performed using the Lancefield classification method with a Strep Grouping kit (Thermo Scientific Remel PathoDX, Remel Europe LTD, Kent, UK) following the manufacturer's instructions.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":88550,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"5","cases":[],"critical":null},{"id":1803649,"guid":"7B00EC2137684BAE99C6D8EBD4B28FEC","previous_id":1803646,"previous_guid":"F2EE54F4A534450AA281D59CF3087EB8","section":"\u003cp\u003eCulture and identification of \u003cem\u003eStreptococcus pyogenes\u003c/em\u003e\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":true,"step":"{\"blocks\":[{\"key\":\"8p1hl\",\"text\":\"Following incubation, 0.6 mL of alpha-naphthol solution and 0.2 mL of potassium hydroxide solution were added to the TSB tubes and mixed by vortexing for 10 sections.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":88550,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4.2","cases":[],"critical":null},{"id":1803650,"guid":"B543C89AD0F041B491B9F0AA65F4A8ED","previous_id":1803649,"previous_guid":"7B00EC2137684BAE99C6D8EBD4B28FEC","section":"\u003cp\u003eCulture and identification of \u003cem\u003eStreptococcus pyogenes\u003c/em\u003e\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":true,"step":"{\"blocks\":[{\"key\":\"fsmll\",\"text\":\"Observations were recorded within 5 minutes. QC observations were made using E. faecalis ATCC 29212 (positive) and S. pyogenes ATCC 19615. Tubes that showed a yellow-brown colour were considered VP negative and indicative of S. pyogenes. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"italic\",\"offset\":77,\"length\":11},{\"style\":\"italic\",\"offset\":115,\"length\":11},{\"style\":\"italic\",\"offset\":225,\"length\":11}],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":88550,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4.3","cases":[],"critical":null}],"template_id":1,"title":"Conventional methods for isolation and Identification of Streptococcus pyogenes","title_html":"Conventional methods for isolation and Identification of Streptococcus pyogenes","type_id":1,"units":[{"id":1,"type_id":3,"name":"µL","can_manage":0,"read_only":0},{"id":2,"type_id":3,"name":"mL","can_manage":0,"read_only":0},{"id":3,"type_id":3,"name":"L","can_manage":0,"read_only":0},{"id":4,"type_id":3,"name":"µg","can_manage":0,"read_only":0},{"id":5,"type_id":3,"name":"mg","can_manage":0,"read_only":0},{"id":6,"type_id":3,"name":"g","can_manage":0,"read_only":0},{"id":7,"type_id":3,"name":"kg","can_manage":0,"read_only":0},{"id":8,"type_id":3,"name":"ng","can_manage":0,"read_only":0},{"id":9,"type_id":3,"name":"Hz","can_manage":0,"read_only":0},{"id":10,"type_id":24,"name":"°C","can_manage":0,"read_only":0},{"id":11,"type_id":24,"name":"°К","can_manage":0,"read_only":0},{"id":12,"type_id":24,"name":"°F","can_manage":0,"read_only":0},{"id":13,"type_id":25,"name":"Mass Percent","can_manage":0,"read_only":0},{"id":14,"type_id":25,"name":"% volume","can_manage":0,"read_only":0},{"id":15,"type_id":25,"name":"Mass / % volume","can_manage":0,"read_only":0},{"id":16,"type_id":25,"name":"Parts per Million (PPM)","can_manage"