number_of_collections":0,"number_of_forks":{"private":0,"public":0},"number_of_accessible_forks":0},"status":{"id":1,"info":"We use this protocol and it's working"},"steps":[{"id":2046578,"guid":"DEAA3C0B4C2E46ADB9A66CF52E122047","previous_id":0,"previous_guid":null,"section":"\u003cp\u003eAnesthesia\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"55lv8\",\"text\":\"Weigh mice.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":99453,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"1","cases":[],"critical":null},{"id":2046580,"guid":"3FC54DA048BF4059818DDD1F14E0C879","previous_id":2046819,"previous_guid":"FBD2B46E957B494C8FE0233743A8EBA6","section":"\u003cp\u003ePost-surgery Procedure\u003c/p\u003e","section_color":"#FFED92","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"ej1kp\",\"text\":\"Close skin with non-absorbable sutures.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":99453,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"15","cases":[],"critical":null},{"id":2046582,"guid":"2A7F1CEE5989420D95D794366F95C6F9","previous_id":2046580,"previous_guid":"3FC54DA048BF4059818DDD1F14E0C879","section":"\u003cp\u003ePost-surgery Procedure\u003c/p\u003e","section_color":"#FFED92","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"4duhp\",\"text\":\"Give meloxicam 2mg/kg subcutaneously immediately after surgery and 24 hours post-surgery.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":99453,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"16","cases":[],"critical":null},{"id":2046583,"guid":"39280A96DF8F49E6A16F6BC0CD810517","previous_id":2046578,"previous_guid":"DEAA3C0B4C2E46ADB9A66CF52E122047","section":"\u003cp\u003eAnesthesia\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"dm808\",\"text\":\"Administer intraperitoneally a mixture of ketamine (Butler Animal Health Supply) and xylazine (Lloyd Laboratories) at concentrations of 110 and 10 mg/kg of body weight, respectively.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":99453,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"2","cases":[],"critical":null},{"id":2046584,"guid":"FDF9FE9AE02D4005BEC64E3BA2791C1E","previous_id":2046583,"previous_guid":"39280A96DF8F49E6A16F6BC0CD810517","section":"\u003cp\u003eAnesthesia\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"rcur\",\"text\":\"Administer desipramine 25mg/kg intraperitoneally 30 minutes prior to the intracerebral injection of 6-OHDA.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":99453,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"3","cases":[],"critical":null},{"id":2046746,"guid":"55549760D931473982661A0CB31DB992","previous_id":2046747,"previous_guid":"318B4698972A46AAA39D3CD46FE014C5","section":"\u003cp\u003ePost-surgery Procedure\u003c/p\u003e","section_color":"#FFED92","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"669ga\",\"text\":\"Remove sutures 15 days post-injections.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":99453,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"19","cases":[],"critical":null},{"id":2046747,"guid":"318B4698972A46AAA39D3CD46FE014C5","previous_id":2046748,"previous_guid":"F5EFF04AFFF8413DBC9A3F5117694978","section":"\u003cp\u003ePost-surgery Procedure\u003c/p\u003e","section_color":"#FFED92","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"8f7lc\",\"text\":\"*Note that mice will weaken and lose body weight. Extra care must be given until body weight stabilizes to ensure survival.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"dan32\",\"text\":\"Perform health checks twice a day for 15 days post-surgery or until the mice fully recover.\",\"type\":\"unordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"5m5lb\",\"text\":\"Check for any sign of distress, infection, and inflammation. \",\"type\":\"unordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"8erdj\",\"text\":\"Give 1-2mL of dextrose 0.83% S.C. every day for the first 15 days post-injection.\",\"type\":\"unordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"d79i9\",\"text\":\"Give diet supplements (e.g. condensed milk solution, moistened pellets).\",\"type\":\"unordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"8qo4t\",\"text\":\"Remove any bacterial plaques that form on genitals.\",\"type\":\"unordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":99453,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"18","cases":[],"critical":null},{"id":2046748,"guid":"F5EFF04AFFF8413DBC9A3F5117694978","previous_id":2046582,"previous_guid":"2A7F1CEE5989420D95D794366F95C6F9","section":"\u003cp\u003ePost-surgery Procedure\u003c/p\u003e","section_color":"#FFED92","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"f2vcq\",\"text\":\"Give 1-2mL of dextrose 0.833% S.C. This is made by diluting 5% stock dextrose 1:6.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":99453,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"17","cases":[],"critical":null},{"id":2046749,"guid":"AB327E35536F49CD97A2078F244D0809","previous_id":2046746,"previous_guid":"55549760D931473982661A0CB31DB992","section":"\u003cp\u003eApomorphine-induced Behavioral Test\u003c/p\u003e","section_color":"#EA9F6C","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"eao35\",\"text\":\"Mice were allowed 4 to 6 weeks to recover before being subjected to the apomorphine-induced a behavioral test to estimate the extent of dopamine depletion in the substantia nigra. See protocol on protocols.io for more details.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":99453,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"20","cases":[],"critical":null},{"id":2046750,"guid":"3F18BD3A8FD7443CA7BBF42E5DAB2797","previous_id":2046755,"previous_guid":"04E36767B8FF49B5A0F3D2AC723CF1B0","section":"\u003cp\u003eSurgical Procedure\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"7ho01\",\"text\":\"Place mouse into stereotactic frame (David Kopf Instruments) and place eye lubricant.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":99453,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"5","cases":[],"critical":null},{"id":2046751,"guid":"2D9EEF28D91D49CFA1C833A8C8D58507","previous_id":2046756,"previous_guid":"608804BE39D546CE887300A6B26FFF03","section":"\u003cp\u003eSurgical Procedure\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"8h16i\",\"text\":\"Move to injection coordinates and drill small hole into the skull.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"d2ghr\",\"text\":\"Coordinates: AP -1.1 mm, ML -1.1 mm, DV -5.0 mm relative to bregma and the dural surface\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":99453,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"9","cases":[],"critical":null},{"id":2046752,"guid":"FE76C727D3174735990802ADF8322D5C","previous_id":2046753,"previous_guid":"FAF232C2CE3E43D696B61827F352CA31","section":"\u003cp\u003eSurgical Procedure\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"6d239\",\"text\":\"Sterilize skull area and then cut open with scalpel.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":99453,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"7","cases":[],"critical":null},{"id":2046753,"guid":"FAF232C2CE3E43D696B61827F352CA31","previous_id":2046750,"previous_guid":"3F18BD3A8FD7443CA7BBF42E5DAB2797","section":"\u003cp\u003eSurgical Procedure\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"8rr39\",\"text\":\"Remove fur from skull area using either electric shaver or depilatory cream.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":99453,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"6","cases":[],"critical":null},{"id":2046755,"guid":"04E36767B8FF49B5A0F3D2AC723CF1B0","previous_id":2046584,"previous_guid":"FDF9FE9AE02D4005BEC64E3BA2791C1E","section":"\u003cp\u003e6-OHDA Preparation\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"2lk4g\",\"text\":\"Prepare 6-OHDA hydrobromide solution:\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"4htmg\",\"text\":\"6-OHDA hydrobromide (Sigma-Aldrich) in PBS with 0.1% ascorbate at a concentration of 3.0 mg/mL\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"efj01\",\"text\":\"\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"59mcj\",\"text\":\"Each mouse will be administered injected with a total volume of 0.6 μL.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"9b0di\",\"text\":\"*Note that this solution is light-sensitive and loses efficacy after 2 hours. Stock solution can be made and stored in   and used for 2 hours before discarding.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[{\"key\":0,\"offset\":119,\"length\":1}],\"data\":{}},{\"key\":\"5mpdk\",\"text\":\"\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{\"0\":{\"type\":\"temperature\",\"mutability\":\"MUTABLE\",\"data\":{\"error_temperature\":false,\"guid\":\"\",\"id\":0,\"label\":\"\",\"scientificNotation\":false,\"temperature\":\"-80\",\"unit\":10}}}}","data":null,"protocol_id":99453,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4","cases":[],"critical":null},{"id":2046756,"guid":"608804BE39D546CE887300A6B26FFF03","previous_id":2046752,"previous_guid":"FE76C727D3174735990802ADF8322D5C","section":"\u003cp\u003eSurgical Procedure\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"6t9h3\",\"text\":\"Adjust brain so that it is level. Locate bregma, and zero X and Y axes on stereotaxic frame.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":99453,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"8","cases":[],"critical":null},{"id":2046782,"guid":"5AEBA46C28E043F7A7A4E855D0CA7977","previous_id":2046751,"previous_guid":"2D9EEF28D91D49CFA1C833A8C8D58507","section":"\u003cp\u003eSurgical Procedure\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"avvnk\",\"text\":\"Zero the Z axis on the stereotaxic frame.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":99453,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"10","cases":[],"critical":null},{"id":2046816,"guid":"F3AFB4E76DE3471AAA9F270366EAB35A","previous_id":2046782,"previous_guid":"5AEBA46C28E043F7A7A4E855D0CA7977","section":"\u003cp\u003eSurgical Procedure\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"aof9f\",\"text\":\"Using a 10 μL stereotactic syringe with a 30 G needle attached to a micro-infusion pump (World Precision Instruments), withdraw the 6-OHDA solution.  \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":99453,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"11","cases":[],"critical":null},{"id":2046817,"guid":"8496216B06F34D408C5FDB25B6C97855","previous_id":2046816,"previous_guid":"F3AFB4E76DE3471AAA9F270366EAB35A","section":"\u003cp\u003eSurgical Procedure\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"9utee\",\"text\":\"With the loaded syringe, navigate the needle to the desired Z coordinate.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":99453,"case_id":0,"cr