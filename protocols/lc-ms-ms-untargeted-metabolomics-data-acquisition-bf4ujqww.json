section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"5vmgh\",\"text\":\"Metabolic extracts were analyzed four times using HILIC and RPLC separation in both positive and negative ionization modes. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":{}}","data":null,"protocol_id":36724,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"2","cases":[],"critical":null},{"id":934787,"guid":"4A808520900E11EA9AC9093A26B5EE99","previous_id":934785,"previous_guid":"2854F620900E11EA9AC9093A26B5EE99","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"2ke0o\",\"text\":\"Data were acquired on a Thermo Q Exactive HF mass spectrometer for HILIC (Thermo Fisher Scientific, Bremen, Germany) and a Thermo Q Exactive mass spectrometer for RPLC (Thermo Fisher Scientific, Bremen, Germany). \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":{}}","data":null,"protocol_id":36724,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"3","cases":[],"critical":null},{"id":934788,"guid":"4C4DA400900E11EA9AC9093A26B5EE99","previous_id":934787,"previous_guid":"4A808520900E11EA9AC9093A26B5EE99","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"folad\",\"text\":\"Both instruments were equipped with a HESI-II probe and operated in full MS scan mode. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":{}}","data":null,"protocol_id":36724,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4","cases":[],"critical":null},{"id":934789,"guid":"5A06D300900E11EA9AC9093A26B5EE99","previous_id":934788,"previous_guid":"4C4DA400900E11EA9AC9093A26B5EE99","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"ef64e\",\"text\":\"MS/MS data were acquired on quality control samples (QC) consisting of an equimolar mixture of all samples in the study. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":{}}","data":null,"protocol_id":36724,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"5","cases":[],"critical":null},{"id":934790,"guid":"61F88400900E11EA9AC9093A26B5EE99","previous_id":934789,"previous_guid":"5A06D300900E11EA9AC9093A26B5EE99","section":"\u003cp\u003eHILIC\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"fsv2v\",\"text\":\"HILIC experiments were performed using a ZIC-HILIC column 2.1 x 100 mm, 3.5 μm, 200Å (Merck Millipore, Darmstadt, Germany) and mobile phase solvents consisting of 10 mM ammonium acetate in 50/50 acetonitrile/water (A) and 10 mM ammonium acetate in 95/5 acetonitrile/water (B). \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":{}}","data":null,"protocol_id":36724,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"6","cases":[],"critical":null},{"id":934791,"guid":"6C369600900E11EA9AC9093A26B5EE99","previous_id":934790,"previous_guid":"61F88400900E11EA9AC9093A26B5EE99","section":"\u003cp\u003eRPLC\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"a6ni1\",\"text\":\"RPLC experiments were performed using a Zorbax SBaq column 2.1 x 50 mm, 1.7 μm, 100Å (Agilent Technologies, Palo Alto, CA) and mobile phase solvents consisting of 0.06% acetic acid in water (A) and 0.06% acetic acid in methanol (B). \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":{}}","data":null,"protocol_id":36724,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"7","cases":[],"critical":null},{"id":934792,"guid":"78E6B970900E11EA9AC9093A26B5EE99","previous_id":934791,"previous_guid":"6C369600900E11EA9AC9093A26B5EE99","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"8f5ib\",\"text\":\"Data quality was ensured by (i) injecting 6 and 12 pool samples to equilibrate the LC-MS system prior to running the sequence for RPLC and HILIC, respectively, (ii) injecting a pool sample every 10 injections to control for signal deviation with time, and (iii) checking mass accuracy, retention time and peak shape of internal standards in each sample.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":{}}","data":null,"protocol_id":36724,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"8","cases":[],"critical":null}],"table_of_contents":[{"title":"\u003cp\u003eHILIC\u003c/p\u003e","color":"#A492FF","first_step_guid":"61F88400900E11EA9AC9093A26B5EE99"},{"title":"\u003cp\u003eRPLC\u003c/p\u003e","color":"#94EBFF","first_step_guid":"6C369600900E11EA9AC9093A26B5EE99"}],"template_id":1,"title":"LC-MS/MS Untargeted Metabolomics Data Acquisition","title_html":"\u003cp\u003eLC-MS/MS Untargeted Metabolomics Data Acquisition\u003c/p\u003e","translations":[],"troubleshooting_items":[],"type_id":1,"units":[{"id":1,"type_id":3,"name":"µL","can_manage":0,"read_only":0},{"id":2,"type_id":3,"name":"mL","can_manage":0,"read_only":0},{"id":3,"type_id":3,"name":"L","can_manage":0,"read_only":0},{"id":4,"type_id":3,"name":"µg","can_manage":0,"read_only":0},{"id":5,"type_id":3,"name":"mg","can_manage":0,"read_only":0},{"id":6,"type_id":3,"name":"g","can_manage":0,"read_only":0},{"id":7,"type_id":3,"name":"kg","can_manage":0,"read_only":0},{"id":8,"type_id":3,"name":"ng","can_manage":0,"read_only":0},{"id":9,"type_id":3,"name":"Hz","can_manage":0,"read_only":0},{"id":10,"type_id":24,"name":"°C","can_manage":0,"read_only":0},{"id":11,"type_id":24,"name":"°К","can_manage":0,"read_only":0},{"id":12,"type_id":24,"n