arison":false,"sign_info":null,"space_access":{"can_view":false,"can_edit":false,"can_remove":false,"can_add":false,"can_publish":true,"can_get_doi":true,"can_share":false,"can_move":false,"can_move_outside":false,"can_transfer":false,"can_download":false,"limited_run":false,"limited_private_links":false,"limited_blind_links":false,"is_locked":false},"state_version_id":373,"stats":{"is_voted":false,"number_of_views":16,"number_of_steps":5,"number_of_bookmarks":0,"number_of_comments":0,"number_of_bookmarked_comments":0,"number_of_steps_comments":0,"number_of_protocol_comments":0,"number_of_exports":0,"number_of_runs":0,"number_of_votes":0,"number_of_reagents":0,"number_of_equipments":0,"number_of_collections":0,"number_of_forks":{"private":0,"public":0},"number_of_accessible_forks":0},"status":{"id":1,"info":"We use this protocol and it's working"},"steps":[{"id":1748146,"guid":"BE6207F02FD111EEAD5A77A80989FFF2","previous_id":0,"previous_guid":null,"section":"\u003cp\u003eImage analysis of plasma membrane contacts\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"1vrpc\",\"text\":\"Using the image analysis program FIJI, split the channels of the image containing the mitochondria (TMRE stain or overexpressed mito-BFP) and overexpressed VPS13A^Halo.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":85739,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"1","cases":[],"critical":null},{"id":1748147,"guid":"D6E13DA02FD111EEAD5A77A80989FFF2","previous_id":1748146,"previous_guid":"BE6207F02FD111EEAD5A77A80989FFF2","section":"\u003cp\u003eImage analysis of plasma membrane contacts\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"484ep\",\"text\":\"Threshold the mitochondria channel to include the mitochondria. Convert to a 32-bit image and convert the threshold to binary, setting values within the mitochondria to be NaN (not a number). This step generates a negative mitochondrial mask.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"ee4qn\",\"text\":\" \",\"type\":\"atomic\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[{\"key\":0,\"offset\":0,\"length\":1}],\"data\":{}},{\"key\":\"91sh5\",\"text\":\"\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{\"0\":{\"type\":\"notes\",\"mutability\":\"IMMUTABLE\",\"data\":{\"blocks\":[{\"data\":{},\"depth\":0,\"entityRanges\":[],\"inlineStyleRanges\":[],\"key\":\"97qk1\",\"text\":\"Due to the close proximity of mitochondria to the cell edge in K562 cells, a negative mask is used to exclude signal originating from the mitochondria.\",\"type\":\"unstyled\"}],\"entityMap\":{}}}}}","data":null,"protocol_id":85739,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"2","cases":[],"critical":null},{"id":1748148,"guid":"175F67302FD211EEAD5A77A80989FFF2","previous_id":1748147,"previous_guid":"D6E13DA02FD111EEAD5A77A80989FFF2","section":"\u003cp\u003eImage analysis of plasma membrane contacts\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"3gdel\",\"text\":\"Multiply the VPS13A^Halo channel and the negative mitochondrial mask into a 32-bit result image. This generates a mitochondria-subtracted VPS13A^Halo image.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":85739,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"3","cases":[],"critical":null},{"id":1748152,"guid":"64874B902FD211EEAD5A77A80989FFF2","previous_id":1748148,"previous_guid":"175F67302FD211EEAD5A77A80989FFF2","section":"\u003cp\u003eImage analysis of plasma membrane contacts\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"eqc45\",\"text\":\"Using the mitochondria-subtracted VPS13A^Halo image, apply a threshold to include signal present at the plasma membrane. Convert to a 32-bit image and convert threshold to binary, setting values outside of the VPS13A signal to be NaN. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"6fb6o\",\"text\":\"\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"6uams\",\"text\":\"The resulting image includes plasma membrane and excludes signal originating from the mitochondria or background.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":85739,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4","cases":[],"critical":null},{"id":1748157,"guid":"A07231602FD211EEAD5A77A80989FFF2","previous_id":1748152,"previous_guid":"64874B902FD211EEAD5A77A80989FFF2","section":"\u003cp\u003eImage analysis of plasma membrane contacts\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"5atcs\",\"text\":\"Trace the cell edge using the selection brush tool with a pixel size of 1. Apply the ROI to the image generated in step 4 and measure the \\\"% area\\\" (selected as \\\"Area fraction\\\" in the \\\"Set measurement\\\" window) of the cell edge length. The recorded value corresponds to the percent of plasma membrane length that contains VPS13A^Halo signal.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":85739,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"5","cases":[],"critical":null}],"template_id":1,"title":"Image analysis of plasma membrane contacts","title_html":"Image analysis of plasma memb