false,"limited_private_links":false,"limited_blind_links":false,"is_locked":false},"space_id":131436,"state_version_id":592,"stats":{"is_voted":false,"number_of_views":0,"number_of_steps":24,"number_of_bookmarks":0,"number_of_comments":0,"number_of_bookmarked_comments":0,"number_of_steps_comments":0,"number_of_protocol_comments":0,"number_of_exports":0,"number_of_runs":0,"number_of_votes":0,"number_of_reagents":0,"number_of_equipments":0,"number_of_collections":0,"number_of_forks":{"private":0,"public":0},"number_of_accessible_forks":0},"status":{"id":1,"info":"We use this protocol and it's working"},"steps":[{"id":2218885,"guid":"301F44A0B3DC47FAA4A882F23ED8604E","previous_id":0,"previous_guid":null,"section":"\u003cp\u003eNPC generation\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"71s8k\",\"text\":\"To prepare the iPSC’s for neural induction, iPSCs need to grow to 90 - 100% confluence.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":107048,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"1","cases":[],"critical":null},{"id":2218886,"guid":"3F152219C79541C798DB7D26A229E216","previous_id":2218885,"previous_guid":"301F44A0B3DC47FAA4A882F23ED8604E","section":"\u003cp\u003eNPC generation\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"2ot6v\",\"text\":\"As soon as the cells reach 90% confluence mTeSR media is switched to neural induction media (N2B27 supplemented with 10uM SB431542 and 1uM Dorsomorphin). \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":107048,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"2","cases":[],"critical":null},{"id":2218887,"guid":"F63B6570D2344F4D8B9AD33B04EB536A","previous_id":2218886,"previous_guid":"3F152219C79541C798DB7D26A229E216","section":"\u003cp\u003eNPC generation\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"8pgp1\",\"text\":\"Neural induction media is replaced every day.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":107048,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"3","cases":[],"critical":null},{"id":2218888,"guid":"B038448DCA1945A08C72F45FC0D068DB","previous_id":2218887,"previous_guid":"F63B6570D2344F4D8B9AD33B04EB536A","section":"\u003cp\u003eNPC generation\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"dbjif\",\"text\":\"After 10 days of neural induction a uniform neuroepithelial sheet should appear. 500ul of 10mg/ml dispase stock solution is added directly to the media of the well to break this sheet.  \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":107048,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4","cases":[],"critical":null},{"id":2218889,"guid":"30B5E289A8FC4CDCAF548308F7D2A1F8","previous_id":2218888,"previous_guid":"B038448DCA1945A08C72F45FC0D068DB","section":"\u003cp\u003eNPC generation\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"3g75r\",\"text\":\"The plate is left at 37°C until the neuroepithelial sheet lifts from the well. This can take 15-30 minutes. The sheet should come off in one piece.   \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":107048,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"5","cases":[],"critical":null},{"id":2218891,"guid":"C05329B772D54B7EA8FC303B95D3BCC1","previous_id":2219004,"previous_guid":"D6FA926D82E1405CBBE33DE9273C8E59","section":"\u003cp\u003eNPC generation\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"b1lcb\",\"text\":\"Cells are plated onto geltrex coated wells (pre filled with 2ml of the media) in a ratio of 1:2 and placed in the incubator overnight to allow the cells to attach. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":107048,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"9","cases":[],"critical":null},{"id":2218892,"guid":"466B5467AB2D4DF7BE3859B7DA118798","previous_id":2218891,"previous_guid":"C05329B772D54B7EA8FC303B95D3BCC1","section":"\u003cp\u003eNPC generation\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"7md16\",\"text\":\"Media is replaced with neural maintenance media (N2B27) the day after next very gently. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":107048,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"10","cases":[],"critical":null},{"id":2218895,"guid":"55180525E3614D08A66E4D67F13C0448","previous_id":2218889,"previous_guid":"30B5E289A8FC4CDCAF548308F7D2A1F8","section":"\u003cp\u003eNPC generation\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"bkqc1\",\"text\":\"The detached sheet can be collected gently from the well and placed into 10ml of PBS in a 15ml falcon. The cell sheet is left to sink to the bottom of the falcon tube, once this happens, PBS is removed and new PBS is added to the tube to wash the cells from remaining dispase. This PBS wash is repeated twice. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":107048,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"6","cases":[],"critical":null},{"id":2218953,"guid":"6ACE888D7C6446668FD303D418C7DABF","previous_id":2218895,"previous_guid":"55180525E3614D08A66E4D67F13C0448","section":"\u003cp\u003eNPC generation\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"18vb6\",\"text\":\"After the washes, PBS is removed and 2mL of neural induction media containing 10uM Y-27632 is added to the cells.  \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":107048,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"7","cases":[],"critical":null},{"id":2218982,"guid":"96AB1ECFE5AC42D2870C40DB9BC54ACD","previous_id":2218892,"previous_guid":"466B5467AB2D4DF7BE3859B7DA118798","section":"\u003cp\u003eNPC generation\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"52v6o\",\"text\":\"Neural maintenance media is changed every other day. Neural rosette structures should become obvious around day 15 after neural induction.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":107048,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"11","cases":[],"critical":null},{"id":2218983,"guid":"7E79BF1A730C424A9B1755AD5A07FC8F","previous_id":2218982,"previous_guid":"96AB1ECFE5AC42D2870C40DB9BC54ACD","section":"\u003cp\u003eNPC generation\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"b2a47\",\"text\":\"When cells become confluent, typically around day 20, or if the cells are too clumpy, cells should be split and expanded further by passaging with dispase as done previously.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":107048,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"12","cases":[],"critical":null},{"id":2218985,"guid":"61E1E362B05A4538B4DBCF4055C806B8","previous_id":2218983,"previous_guid":"7E79BF1A730C424A9B1755AD5A07FC8F","section":"\u003cp\u003eNPC generation\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"275sr\",\"text\":\"Between days 20-30 substantial neurogenesis should occur. When neurons have accumulated at the peripery of the rosettes, cells should be passaged using neat Accutase into single cells. This tends to occur close to day 30. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":107048,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"13","cases":[],"critical":null},{"id":2218988,"guid":"79EFBBA3878643EA8AE394FEE67AB369","previous_id":2218985,"previous_guid":"61E1E362B05A4538B4DBCF4055C806B8","section":"\u003cp\u003eNPC generation\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"chhn9\",\"text\":\"Cells are washed with PBS and 0.5 ml of Accutase per well of a 6 well plate is added. The plate is then incubated for 5 minutes at 37°C.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":107048,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"14","cases":[],"critical":null},{"id":2218990,"guid":"4E32FC3108D84E81AE4AECCA181612A1","previous_id":2218988,"previous_guid":"79EFBBA3878643EA8AE394FEE67AB369","section":"\u003cp\u003eNPC generation\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"1t0u9\",\"text\":\"Cell clumps are dissociated into a single cell suspension by pipetting them up and down 3 or 4 times with a P1000 in the Accutase solution. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":107048,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"15","cases":[],"critical":null},{"id":2218991,"guid":"86A9D7A8B5F64698918AD64D619ACF49","previous_id":2218990,"previous_guid":"4E32FC3108D84E81AE4AECCA181612A1","section":"\u003cp\u003eNPC generation\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"4cgl6\",\"text\":\"The Accutase solution containing the cells is diluted with 4 volumes of neural maintenance media and centrifuged at 300g for 3 mins at room temperature. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":107048,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"16","cases":[],"critical":null},{"id":2218992,"guid":"B65017D67E6D4652A9CB2245EBB0E910","previous_id":2218991,"previous_guid":"86A9D7A8B5F64698918AD64D619ACF49","section":"\u003cp\u003eNPC generation\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"7a21j\",\"text\":\"Supernatant is discarded and the cells are resuspended in neural maintenance media containing 10uM Y-27632 and plated in onto geltrex coated plates. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":107048,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"17","cases":[],"critical":null},{"id":2218993,"guid":"8625D4F501104424995968F6985F30AF","previous_id":2218992,"previous_guid":"B65017D67E6D4652A9CB2245EBB0E910","section":"\u003cp\u003eNPC generation\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"qmvi\",\"text\":\"Media si replaced the following day with neural maintenance media.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":107048,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"18","cases":[],"critical":null},{"id":2218994,"guid":"55FD1C2283F04409AFAB7F6F42FD3C88","previous_id":2218993,"previous_guid":"8625D4F501104424995968F6985F30AF","section":"\u003cp\u003eAstrocyte generation\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"bcur4\",\"text\":\"Media is switched to AM media the following day.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":107048,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"19","cases":[],"critical":null},{"id":2218995,"guid":"FE40DA8DC7E7451E928222F4DA0C2951","previous_id":2219006,"previous_guid":"8412BB78AC4841F5ABCEA0727D8808C4","section":"\u003cp\u003eAstrocyte generation\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"d4j3t\",\"text\":\"When cells reach 80% confluence they are split using Accutase as previously described.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":107048,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"21","cases":[],"critical":null},{"id":2218996,"guid":"C1022037F13E4D3D9C54531C8215BA9E","previous_id":2218995,"previous_guid":"FE40DA8DC7E7451E928222F4DA0C2951","section":"\u003cp\u003eAstrocyte generation\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"udb8\",\"text\":\"Cells are plated at a 15K cells/cm2 in AM media. Cells need to be split following the previous steps every 7-10 days at the same density (15K cells/cm2).\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":107048,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"22","cases":[],"critical":null},{"id":2219000,"guid":"F3DF4F4DA99D42928B0D70BDE9A7F165","previous_id":2218996,"previous_guid":"C1022037F13E4D3D9C54531C8215BA9E","section":"\u003cp\u003eAstrocyte generation\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"fq2n9\",\"text\":\"After 21-25 days in AM media, astrocyte-looking cells start to become obvious. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":107048,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"23","cases":[],"critical":null},{"id":2219002,"guid":"48BBFE6D22684BECA1C296AB32A8247F","previous_id":2219000,"previous_guid":"F3DF4F4DA99D42928B0D70BDE9A7F165","section":"\u003cp\u003eAstrocyte generation\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"dmreb\",\"text\":\"Astrocytes can then be maintained following the same steps - passages every 7-10 days at 15K cells/cm2 in AM - for around 100 days. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":107048,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"24","cases":[],"critical":null},{"id":2219004,"guid":"D6FA926D82E1405CBBE33DE9273C8E59","previous_id":2218953,"previous_guid":"6ACE888D7C6446668FD303D418C7DABF","section":"\u003cp\u003eNPC generation\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"bm6oh\",\"text\":\"The cell sheet is gently broken into around 15 smaller clumps by inverting the tube, trying not to break the cell clumps up too much as this can cause non-neuronal differentiation. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":107048,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"8","cases":[],"critical":null},{"id":2219006,"guid":"8412BB78AC4841F5ABCEA0727D8808C4","previous_id":2218994,"previous_guid":"55FD1C2283F04409AFAB7F6F42FD3C88","section":"\u003cp\u003eAstrocyte generation\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"59ldr\",\"text\":\"AM media is replaced every other day.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":107048,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"20","cases":[],"critical":null}],"template_id":0,"title":"Astrocyte generation ","title_html":"Astrocyte generation ","type_id":1,"units":[{"id":1,"type_id":3,"name":"µL","can_manage":0,"read_only":0},{"id":2,"type_id":3,"name":"mL","can_manage":0,"read_only":0},{"id":3,"type_id":3,"name":"L","can_manage":0,"read_only":0},{"id":4,"type_id":3,"name":"µg","can_manage":0,"read_only":0},{"id":5,"type_id":3,"name":"mg","can_manage":0,"read_only":0},{"id":6,"type_id":3,"name":"g","can_manage":0,"read_only":0},{"id":7,"type_id":3,"name":"kg","can_manage":0,"read_only":0},{"id":8,"type_id":3,"name":"ng","can_manage":0,"read_only":0},{"id":9,"type_id":3,"name":"Hz","can_manage":0,"read_only":0},{"id":10,"type_id":24,"name":"°C","can_manage":0,"read_only":0},{"id":11,"type_id":24,"name":"°К","can_manage":0,"read_only":0},{"id":12,"type_id":24,"name":"°F","can_manage":0,"read_only":0},{"id":13,"type_id":25,"name":"Mass Percent","can_manage":0,"read_only":0},{"id":14,"type_id":25,"name":"% volume","can_manage":0,"read_only":0},{"id":15,"type_id":25,"nam