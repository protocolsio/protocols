"id":1054725,"guid":"412CD7608ABA11EB90D19F417EB57138","order_id":2,"type_id":30,"title":"centrifuge","source":{"centrifuge":"300","duration":600,"error_centrifuge":false,"error_temperature":false,"guid":"412CD7608ABA11EB90D19F417EB57138","id":0,"label":"","pause":null,"preset":[],"scientificNotation":{"centrifuge":false,"temperature":false},"start":null,"temperature":"4","temperatureUnit":10,"unit":34}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":1800,"critical":null,"critical_id":null,"duration":600},{"id":1148410,"guid":"53FB99808ABA11EB90D19F417EB57138","previous_id":1148409,"previous_guid":"E89825008AB911EB90D19F417EB57138","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"EC3A9B073C8C4A149731DBB5C4C29511","order_id":1,"type_id":6,"title":"Section","source":{"title":"PROCESSING OF BAL TO RECOVER SINGLE CELLS."}},{"id":1054724,"guid":"6A271E1686F0490B84D059973FC059E1","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Remove supernatant and resuspend cell pellet in 10mL of pre-chilled RPMI supplemented with 2% heat-inactivated fetal calf serum (herein referred to as RPMI 2% FCS).<\/div><\/div>"}},{"id":1054725,"guid":"556EBBCDACD5490998CF98B7E6F7669C","order_id":2,"type_id":26,"title":"notes","source":{"id":0,"parent_id":0,"uri":"","title":"","body":"<div class = \"text-blocks\"><div class = \"text-block\"><span>Cell-free BAL supernatant can be stored at <\/span><a href=\"#\" style = \"text-decoration:underline;color:blue;cursor:pointer;\"><span style = \":;\"> <\/span><\/a><span>  for future proteomic analysis (e.g. quantification of cytokines)<\/span><\/div><\/div>","created_on":0,"changed_on":0,"creator":{"name":" ","affiliation":null,"affiliations":[],"username":"","note":null,"link":null,"image":{"source":"","placeholder":""},"badges":[],"verified":0,"is_verified_user":false,"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false},"comments":[]}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":1800,"critical":null,"critical_id":null,"duration":0},{"id":1148411,"guid":"11536E908ABB11EB90D19F417EB57138","previous_id":1148410,"previous_guid":"53FB99808ABA11EB90D19F417EB57138","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"2C6BD7860072468E9938C87D984733CD","order_id":1,"type_id":6,"title":"Section","source":{"title":"PROCESSING OF BAL TO RECOVER SINGLE CELLS."}},{"id":1054724,"guid":"90240EDC572D43ACAD462D2F4E970451","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Filter cell suspension through a 70-120\u00b5m cell strainer and centrifuge filtered cell suspension <\/div><div class = \"text-block\">.<\/div><\/div>"}},{"id":1054725,"guid":"CD0C9DDF8ABE11EBB46E0A58A9FEAC02","order_id":2,"type_id":30,"title":"centrifuge","source":{"centrifuge":"300","duration":600,"error_centrifuge":false,"error_temperature":false,"guid":"CD0C9DDF8ABE11EBB46E0A58A9FEAC02","id":0,"label":"","pause":null,"preset":[],"scientificNotation":{"centrifuge":false,"temperature":false},"start":null,"temperature":"4","temperatureUnit":10,"unit":34}},{"id":1054726,"guid":"7AC0415BD9A44A73B0811D4C4715758C","order_id":3,"type_id":26,"title":"notes","source":{"id":0,"parent_id":0,"uri":"","title":"","body":"<div class = \"text-blocks\"><div class = \"text-block\">In some cases, BAL samples may require a second filtering step to remove additional debris<\/div><\/div>","created_on":0,"changed_on":0,"creator":{"name":" ","affiliation":null,"affiliations":[],"username":"","note":null,"link":null,"image":{"source":"","placeholder":""},"badges":[],"verified":0,"is_verified_user":false,"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false},"comments":[]}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":1800,"critical":null,"critical_id":null,"duration":600},{"id":1148413,"guid":"ACAA78B08ABC11EB90D19F417EB57138","previous_id":1148411,"previous_guid":"11536E908ABB11EB90D19F417EB57138","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"A81D2312E90B4B2E92F70184493EDE31","order_id":1,"type_id":6,"title":"Section","source":{"title":"PROCESSING OF BAL TO RECOVER SINGLE CELLS."}},{"id":1054724,"guid":"357E19FA62F540389CDB659834864B2C","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Discard supernatant and resuspend cell suspension in  2mL chilled RPMI 2% FCS.   Remove 10\u00b5L for cell counting. Top up the cell suspension to 10mL RPMI 2% FCS and centrifuge <\/div><div class = \"text-block\"> while performing cell count.<\/div><\/div>"}},{"id":1054725,"guid":"01F33EF08ABE11EB90D19F417EB57138","order_id":2,"type_id":30,"title":"centrifuge","source":{"centrifuge":"300","duration":600,"error_centrifuge":false,"error_temperature":false,"guid":"01F33EF08ABE11EB90D19F417EB57138","id":0,"label":"","pause":null,"preset":[],"scientificNotation":{"centrifuge":false,"temperature":false},"start":null,"temperature":"4","temperatureUnit":10,"unit":34}},{"id":1054726,"guid":"9D4BB3694D964366A65FD6C2C706F96A","order_id":3,"type_id":26,"title":"notes","source":{"id":0,"parent_id":0,"uri":"","title":"","body":"<div class = \"text-blocks\"><div class = \"text-block\">Cell counting can be performed manually using a haemocytometer, or using an automated cell counter (although the accuracy of some automated cell counters is limited by their inability to count large alveolar macrophages)<\/div><\/div>","created_on":0,"changed_on":0,"creator":{"name":" ","affiliation":null,"affiliations":[],"username":"","note":null,"link":null,"image":{"source":"","placeholder":""},"badges":[],"verified":0,"is_verified_user":false,"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false},"comments":[]}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":1800,"critical":null,"critical_id":null,"duration":600},{"id":1148415,"guid":"4118B6508ABE11EB90D19F417EB57138","previous_id":1148413,"previous_guid":"ACAA78B08ABC11EB90D19F417EB57138","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"843B158DD3634AF5B33E6CDD24FF017A","order_id":1,"type_id":6,"title":"Section","source":{"title":"OPTION: Workflow for single cell analysis of fresh BAL samples. "}},{"id":1054724,"guid":"53815E74961D476AA4DD02388BE9D127","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">If samples will be cryopreserved for storage, proceed to step 9 of this protocol. <\/div><div class = \"text-block\">If samples will be processed for immediate analysis (e.g. flow cytometry, cell sorting, or sc-RNAseq of live single cells) the following steps can be performed: <\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#84CE84","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1148416,"guid":"9E6FE7E08AC011EB90D19F417EB57138","previous_id":1148425,"previous_guid":"29B18E108AC311EBA7D16520E7F91759","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"07D3EC17C75C4B8B91EE26DBF55E8799","order_id":1,"type_id":6,"title":"Section","source":{"title":"CRYOPRESERVATION OF BAL."}},{"id":1054724,"guid":"0A20C8831DA4463DB54DAAA4956E8139","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Discard supernatant and resuspend cells at a ratio of 1:1 in chilled RPMI 2% FCS and freeze solution (heat-inactivated FCS + 15% DMSO) such that cells are frozen between 1-10 million cells\/mL. <\/div><\/div>"}},{"id":1054725,"guid":"83AD9E3CCF814948951FE1E02EE92A40","order_id":2,"type_id":26,"title":"notes","source":{"id":0,"parent_id":0,"uri":"","title":"","body":"<div class = \"text-blocks\"><div class = \"text-block\">This step should be done on ice. Add freeze solution to cell suspension in a drop-by-drop manner.<\/div><\/div>","created_on":0,"changed_on":0,"creator":{"name":" ","affiliation":null,"affiliations":[],"username":"","note":null,"link":null,"image":{"source":"","placeholder":""},"badges":[],"verified":0,"is_verified_user":false,"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false},"comments":[]}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#FFED92","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1148417,"guid":"E0144C808AC111EBA7D16520E7F91759","previous_id":1148416,"previous_guid":"9E6FE7E08AC011EB90D19F417EB57138","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"8DABD8DA6366420C811E0A3716A1716B","order_id":1,"type_id":6,"title":"Section","source":{"title":"CRYOPRESERVATION OF BAL."}},{"id":1054724,"guid":"3797F27CC6E6449CAE2C3BB9033A6416","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Immediately place cryogenic vials into an isopropanol freezing container (e.g. Nalgene\u00ae Mr. Frosty) and transfer to <\/div><div class = \"text-block\"> overnight.<\/div><\/div>"}},{"id":1054725,"guid":"70E7F3B08AC211EBA7D16520E7F91759","order_id":2,"type_id":24,"title":"temperature","source":{"temperature":-80,"unit":"\u00b0C","title":""}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#FFED92","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1148418,"guid":"88D745708AC211EBA7D16520E7F91759","previous_id":1148417,"previous_guid":"E0144C808AC111EBA7D16520E7F91759","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"D697649AA71D4E6186D289C977E82BF5","order_id":1,"type_id":6,"title":"Section","source":{"title":"CRYOPRESERVATION OF BAL."}},{"id":1054724,"guid":"1E74D705B8D74387A34016319A85EFC0","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">For long term storage, transfer the vials of frozen BAL cells to liquid nitrogen.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#FFED92","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1148423,"guid":"F45E20708AC211EBA7D16520E7F91759","previous_id":1148415,"previous_guid":"4118B6508ABE11EB90D19F417EB57138","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"3DB87ACBD0D44FC1A59D5ED5B2127D2C","order_id":1,"type_id":6,"title":"Section","source":{"title":"OPTION: Workflow for single cell analysis of fresh BAL samples. "}},{"id":1054724,"guid":"B028BCD90DDA446FBF47B14D9D159EFC","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Resuspend cell suspension in chilled staining buffer for fixable viability staining according to manufacturers' instructions (e.g. the LIVE\/DEAD\u2122 Fixable Near-IR Stain from Invitrogen\/ThermoFisher). <\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#84CE84","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1148425,"guid":"29B18E108AC311EBA7D16520E7F91759","previous_id":1148423,"previous_guid":"F45E20708AC211EBA7D16520E7F91759","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"F070EF19E3A84D7AB684B59B5D090469","order_id":1,"type_id":6,"title":"Section","source":{"title":"OPTION: Workflow for single cell analysis of fresh BAL samples. "}},{"id":1054724,"guid":"A002706061084F45BECE1AB7ACADD378","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">For a protocol detailing flow cytometry analysis of fresh BAL samples, please see our article:<\/div><\/div>"}},{"id":1054725,"guid":"C796846917FA4E5DB44EE3836C457DB1","order_id":2,"type_id":17,"title":"result","source":{"body":"<div class = \"text-blocks\"><div class = \"text-block\">Following processing of n=11 pediatric BAL samples as described in this protocol, we achieved a median viability of 78.9% (range 72-88.8%) (determined by live\/dead staining using flow cytometry).<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#84CE84","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1148426,"guid":"B70D4C908AC311EBA7D16520E7F91759","previous_id":1148418,"previous_guid":"88D745708AC211EBA7D16520E7F91759","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"3A8EF30CF5724CBC865C37CD4C5FA637","order_id":1,"type_id":6,"title":"Section","source":{"title":"THAWING OF CRYOPRESERVED BAL FOR SINGLE CELL ANALYSIS."}},{"id":1054724,"guid":"F4CF67FB46034EB48A8E94243DE4BD3E","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Warm thaw media (RPMI + 10% heat-inactivated FCS + 25U\/mL Benzonase) to <\/div><div class = \"text-block\">  in a water bath. <\/div><\/div>"}},{"id":1054725,"guid":"42F556D08AC411EBA7D16520E7F91759","order_id":2,"type_id":24,"title":"temperature","source":{"temperature":37,"unit":"\u00b0C","title":""}},{"id":1054726,"guid":"53B9D29549D7483B86FFFB5AFE314970","order_id":3,"type_id":26,"title":"notes","source":{"id":0,"parent_id":0,"uri":"","title":"","body":"<div class = \"text-blocks\"><div class = \"text-block\">For every sample to be thawed, place 8mL of warmed thaw media into a 15mL tube.<\/div><\/div>","created_on":0,"changed_on":0,"creator":{"name":" ","affiliation":null,"affiliations":[],"username":"","note":null,"link":null,"image":{"source":"","placeholder":""},"badges":[],"verified":0,"is_verified_user":false,"research_interests":null,"blocked_by