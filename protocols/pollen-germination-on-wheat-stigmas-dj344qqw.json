of_protocol_comments":0,"number_of_exports":0,"number_of_runs":0,"number_of_votes":0,"number_of_reagents":0,"number_of_equipments":0,"number_of_collections":0,"number_of_forks":{"private":0,"public":0},"number_of_accessible_forks":0},"status":{"id":1,"info":"We use this protocol and it's working"},"steps":[{"id":2199371,"guid":"C39686195FAD46BABF98078A6EBE8600","previous_id":0,"previous_guid":null,"section":"\u003cp\u003eHand pollination\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"drvlr\",\"text\":\"Please refer to wheat-training.com for detailed explanations on how to perform hand pollinations. Link to pdf: https://www.wheat-training.com/wp-content/uploads/Wheat_growth/pdfs/How-to-cross-wheat-pdf.pdf\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[{\"key\":0,\"offset\":111,\"length\":94}],\"data\":{}}],\"entityMap\":{\"0\":{\"type\":\"link\",\"mutability\":\"MUTABLE\",\"data\":{\"guid\":\"93A1BFC8616311EF822F0A58A9FEAC02\",\"url\":\"https://www.wheat-training.com/wp-content/uploads/Wheat_growth/pdfs/How-to-cross-wheat-pdf.pdf\"}}}}","data":null,"protocol_id":106332,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"1","cases":[],"critical":null},{"id":2199372,"guid":"6453F34A01654189B453E8D01EC0F422","previous_id":2199371,"previous_guid":"C39686195FAD46BABF98078A6EBE8600","section":"\u003cp\u003eCarpel dissection and fixation\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"9fcnj\",\"text\":\"Using a pair of tweezers, dissect carpels 4.5 h after pollination to allow sufficient time for pollen tube\\nemergence. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":106332,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"2","cases":[],"critical":null},{"id":2199374,"guid":"1C6AC8B18D284C2C89D8D2DC9841C847","previous_id":2199372,"previous_guid":"6453F34A01654189B453E8D01EC0F422","section":"\u003cp\u003eCarpel dissection and fixation\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"1folr\",\"text\":\"Store samples in a fixative solution of 95% ethanol and absolute acetic acid (75% v/v) and kept at 4 °C until sample preparation for fluorescence microscopy.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":106332,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"3","cases":[],"critical":null},{"id":2199375,"guid":"CF2031DAD8A04895A2B7E033D6D42CB0","previous_id":2199374,"previous_guid":"1C6AC8B18D284C2C89D8D2DC9841C847","section":"\u003cp\u003eAniline blue staining of pollinated stigmas\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"5tp0u\",\"text\":\"On the day of sampling, prepare a solution of 0.1% aniline blue in 0.1 M K3PO4. You can prepare a stock solution of 1% aniline blue dissolved in 1x PBS. The stock solution should be kept in the fridge at\\n4 °C. Use tin foil to avoid exposure to light. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"sub\",\"offset\":74,\"length\":1},{\"style\":\"sub\",\"offset\":77,\"length\":1}],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":106332,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4","cases":[],"critical":null},{"id":2199451,"guid":"2E921DA4A30B4EB4A531AAF0D347B65E","previous_id":2199375,"previous_guid":"CF2031DAD8A04895A2B7E033D6D42CB0","section":"\u003cp\u003eAniline blue staining of pollinated stigmas\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"8t0nv\",\"text\":\"Wash fixed samples three times for 5 minutes in sterile water and transferred to 0.1% aniline blue\\nsolution and kept overnight at 4 °C. Use tin foil to avoid exposure to light. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":106332,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"5","cases":[],"critical":null},{"id":2199452,"guid":"BF60FEE4E5AB41B29C37E6A317B6D5A4","previous_id":2199451,"previous_guid":"2E921DA4A30B4EB4A531AAF0D347B65E","section":"\u003cp\u003eAniline blue staining of pollinated stigmas\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"4adb1\",\"text\":\"Without washing the samples, dissect out the ovary using a sharp razor blade. Try not to damage the stigma. Leave the remaining stigmatic tissue to dry at 45 °C in a hot plate for a few minutes until most of the aniline blue solution is evaporated. Cover the hot plate with an opaque lid to avoid exposure to light. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":106332,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"6","cases":[],"critical":null},{"id":2199469,"guid":"0DECF2E7FBEC4823B788E534E85A4FD6","previous_id":2199452,"previous_guid":"BF60FEE4E5AB41B29C37E6A317B6D5A4","section":"\u003cp\u003eAniline blue staining of pollinated stigmas\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"5rpul\",\"text\":\"Use Vectashield (catalogue No. H-1000-1, 2BSCIENTIFIC LTD) as an antifade mounting media to\\npreserve fluorescence. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":106332,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"7","cases":[],"critical":null},{"id":2199471,"guid":"466B81BC0B7A4A43BB4DA7A5BEE2429F","previous_id":2199469,"previous_guid":"0DECF2E7FBEC4823B788E534E85A4FD6","section":"\u003cp\u003eAniline blue staining of pollinated stigmas\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"81nbd\",\"text\":\"Happy microscopy.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":106332,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"8","cases":[],"critical":null}],"template_id":0,"title":"Pollen germination on wheat stigmas ","title_html":"Pollen germination on wheat stigmas ","type_id":1,"units":[{"id":1,"type_id":3,"name":"µL","can_manage":0,"read_only":0},{"id":2,"type_id":3,"name":"mL","can_manage":0,"read_only":0},{"id":3,"type_id":3,"name":"L","can_manage":0,"read_only":0},{"id":4,"type_id":3,"name":"µg","can_manage":0,"read_only":0},{"id":5,"type_id":3,"name":"mg","can_manage":0,"read_only":0},{"id":6,"type_id":3,"name":"g","can_manage":0,"read_only":0},{"id":7,"type_id":3,"name":"kg","can_manage":0,"read_only":0},{"id":8,"type_id":3,"name":"ng","can_manage":0,"read_only":0},{"id":9,"type_id":3,"name":"Hz","can_manage":0,"read_only":0},{"id":10,"type_id":24,"name":"°C","can_manage":0,"read_only":0},{"id":11,"type_id":24,"name":"°К","can_manage":0,"read_only":0},{"id":12,"type_id":24,"name":"°F","can_manage":0,"read_only":0},{"id":13,"type_id":25,"name":"Mass Percent","can_manage":0,"read_only":0},{"id":14,"type_id":25,"name":"% volume","can_manage":0,"read_only":0},{"id":15,"type_id":25,"name":"Mass / % volume","can_manage":0,"read_only":0},{"id":16,"type_id":25,"name":"Parts per Million (PPM)","can_manage":0,"read_only":0},{"id":17,"type_id":25,"name":"Parts per Billion (PPB)","can_manage":0,"read_only":0},{"id":18,"type_id":25,"name":"Parts per Trillion (PPT)","can_manage":0,"read_only":0},{"id":19,"type_id":25,"name":"Mole Fraction","can_manage":0,"read_only":0},{"id":20,"type_