bookmarked":0,"can_claim_authorship":0,"can_accept_authorship":0,"can_be_copied":1,"can_remove_fork":1,"fork_id":null,"url":"https:\/\/www.protocols.io\/view\/human-cd8-t-cell-transduction-and-rapid-expansion-sxvefn6","forks_count":{"private":0,"public":0},"access":{"can_view":1,"can_remove":0,"can_add":0,"can_edit":0,"can_publish":0,"can_get_doi":0,"can_share":1,"can_move":1,"can_move_outside":1,"can_transfer":1,"can_download":1,"is_locked":0},"guid":"8A4D0FC4B9DA11E99CA40242AC110005","state_version_id":0,"steps":[{"id":657626,"guid":"BDAC0B602F754058A693DCB59FBF8905","previous_id":null,"previous_guid":null,"modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"1E7B148449184F30B9316DFD7C0B53C3","order_id":1,"type_id":6,"title":"Section","source":{"title":"Day 0 - Lentiviral supernatant production and transduction"}},{"id":1054724,"guid":"16E276FF590F43A08C17A92CF22CCAFB","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span style = \"font-weight:bold;\">Prepare packaging cell line:<\/span><\/div><div class = \"text-block\"><span style = \"font-weight:bold;\">1.<\/span><span> Resuspend 3x10<\/span><span style = \"vertical-align:super;\">6<\/span><span> 293T cells with 10 mL LCL media and plate in a 10cm plate.<\/span><\/div><div class = \"text-block\"><span style = \"font-weight:bold;\">2.<\/span><span> Incubate at 37C overnight.<\/span><\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":null,"section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":657627,"guid":"CF6CD04377D341829D9F07EE2ED59195","previous_id":657626,"previous_guid":"BDAC0B602F754058A693DCB59FBF8905","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"0ECABB560DA245A39D5C0C62A396DE00","order_id":1,"type_id":6,"title":"Section","source":{"title":"Day 1 - Lentiviral supernatant production and transduction"}},{"id":1054724,"guid":"561520A6C86547EFB8E822495B2FA048","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span style = \"font-weight:bold;\">Transfect 293T:<\/span><\/div><div class = \"text-block\"><span style = \"font-weight:bold;\">1.<\/span><span> Thaw tubes of DNA. And prepare 5mL polypropylene tubes (1\/plate).<\/span><\/div><div class = \"text-block\"><span style = \"font-weight:bold;\">2.<\/span><span> Add 280.7 uL EC Buffer.<\/span><\/div><div class = \"text-block\"><span style = \"font-weight:bold;\">3. <\/span><span>Add 2.5ug of M-Mix (2.6ul) and 1.5ug of DNA to EC Buffer.<\/span><\/div><div class = \"text-block\"><span style = \"font-weight:bold;\">4. <\/span><span>Add 32uL of Enhancer and vortex for 1 second.<\/span><\/div><div class = \"text-block\"><span style = \"font-weight:bold;\">5.<\/span><span> Incubate 5 min at Room Temp.<\/span><\/div><div class = \"text-block\"><span style = \"font-weight:bold;\">6.<\/span><span> Add 60uL of Effectene. Vortex for 10 seconds.<\/span><\/div><div class = \"text-block\"><span style = \"font-weight:bold;\">7.<\/span><span> Incubate 10 min at Room Temp.<\/span><\/div><div class = \"text-block\"><span style = \"font-weight:bold;\">8. <\/span><span>Aspirate media from 293T plates. Add 7mL of CTL media to each plate.<\/span><\/div><div class = \"text-block\"><span style = \"font-weight:bold;\">9. <\/span><span>Add 3 mL of CTL media to each tube of Effectene mixture. Pipette up and down to mix, then add slowly (dropwise) to each 293T plate.<\/span><\/div><div class = \"text-block\"><span style = \"font-weight:bold;\">10.<\/span><span> Swirl gently to mix and return to 37C incubator overnight.<\/span><\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":null,"section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":657628,"guid":"951074E49F9840D6A37F9ACCAF3FA0AD","previous_id":657627,"previous_guid":"CF6CD04377D341829D9F07EE2ED59195","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"35156EA191864752A7D6445A1A10CE63","order_id":1,"type_id":6,"title":"Section","source":{"title":"Day 2 - Lentiviral supernatant production and transduction"}},{"id":1054724,"guid":"1F79242963CB4A1481D124E0EA1C3D02","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Aspirate media from the dish and replace with 10mL fresh CTL media. Incubate at 37C.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":null,"section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":657629,"guid":"07E50AFB32834CD5B54E66C5F1126FF7","previous_id":657628,"previous_guid":"951074E49F9840D6A37F9ACCAF3FA0AD","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"8B00A0B3FE7A44C6AA729E8F71B0A048","order_id":1,"type_id":6,"title":"Section","source":{"title":"Day 3 - Lentiviral supernatant production and transduction"}},{"id":1054724,"guid":"9B1A12519CC64040B0900A43179DF07E","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span style = \"font-weight:bold;\">Activate and transduce T cells:<\/span><\/div><div class = \"text-block\"><span style = \"font-weight:bold;\">1.<\/span><span> Collect the viral supernatant and filter thru a 0.45um filter (Thermo scientific Nalgene syringe filter cat #190-9945).<\/span><\/div><div class = \"text-block\"><span style = \"font-weight:bold;\">2. <\/span><span>Add 10mL fresh CTL media to plates and return to 37C. Alternatively, use frozen viral supernatant.<\/span><\/div><div class = \"text-block\"><span style = \"font-weight:bold;\">3.<\/span><span> Thaw CD8+ T cells and count. (Quick thaw at 37C and drop-wise (slow) dilution).<\/span><\/div><div class = \"text-block\"><span style = \"font-weight:bold;\">4.<\/span><span> Resuspend 2x10<\/span><span style = \"vertical-align:super;\">7<\/span><span> T cells with 2x10<\/span><span style = \"vertical-align:super;\">7<\/span><span> anti-CD3 and anti-CD28 beads (1:1 T cell to bead ratio) (Dynabeads Cat#111.41D from Invitrogen).<\/span><\/div><div class = \"text-block\"><span>\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0 Starting bead concentration = 1x10<\/span><span style = \"vertical-align:super;\">8<\/span><span> beads\/ml.<\/span><\/div><div class = \"text-block\"><span style = \"font-weight:bold;\">5.<\/span><span>\u00a0 Wash Dynabeads with 10mL CTL media.<\/span><\/div><div class = \"text-block\"><span style = \"font-weight:bold;\">6.<\/span><span> Dilute the cells to a concentration of 5x10<\/span><span style = \"vertical-align:super;\">6<\/span><span> cells\/mL, add IL-2 at 50U\/mL and transfer into T25 flasks.<\/span><\/div><div class = \"text-block\"><span style = \"font-weight:bold;\">7.<\/span><span> Incubate at 37C for ~4 hours<\/span><\/div><div class = \"text-block\"><span style = \"font-weight:bold;\">8. <\/span><span>Spin down T cells at 369<\/span><span style = \"font-style:italic;\">xg<\/span><span> for 4-5 minutes at 4C and re-suspend in 10mL CTL media. Recount T cells post-stimulation.<\/span><\/div><div class = \"text-block\"><span style = \"font-weight:bold;\">9.<\/span><span> Aliquot T cells into 15mL tubes (1 per transduction condition) and spin down at 369<\/span><span style = \"font-style:italic;\">xg<\/span><span> for 4-5 minutes at 4C.<\/span><\/div><div class = \"text-block\"><span style = \"font-weight:bold;\">10.<\/span><span> Spin down cells at 369<\/span><span style = \"font-style:italic;\">xg<\/span><span> for 4-5 minutes at 4C.<\/span><\/div><div class = \"text-block\"><span style = \"font-weight:bold;\">11.<\/span><span> Resuspend cells in viral supernatant and add 50 U\/mL IL-2 and 5ug\/mL Polybrene. Transfer 2mL per well to a 12-well plate.<\/span><\/div><div class = \"text-block\"><span style = \"font-weight:bold;\">12. <\/span><span>Spin cells at 1265<\/span><span style = \"font-style:italic;\">xg<\/span><span> at 30C for 90 minutes.<\/span><\/div><div class = \"text-block\"><span style = \"font-weight:bold;\">13. <\/span><span>Transfer plates to 37C, 5% CO2 incubator overnight.<\/span><\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":null,"section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":657630,"guid":"BEBCB49BF49F44008DE252C0C08D2C06","previous_id":657629,"previous_guid":"07E50AFB32834CD5B54E66C5F1126FF7","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"D7A70A7CBC4248D69DE0977B325412D9","order_id":1,"type_id":6,"title":"Section","source":{"title":"Day 4 - Lentiviral supernatant production and transduction"}},{"id":1054724,"guid":"877B288FB0564123B3FDA6C365CD4336","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span style = \"font-weight:bold;\">Transduce cells a second time:<\/span><\/div><div class = \"text-block\"><span style = \"font-weight:bold;\">1.<\/span><span> Collect and combine T cells. Spin down cells at 369<\/span><span style = \"font-style:italic;\">xg<\/span><span> for 4-5 minutes at 4C.<\/span><\/div><div class = \"text-block\"><span style = \"font-weight:bold;\">2.<\/span><span> Resuspend cells in viral supernatant and add 50 U\/mL IL-2 and 5ug\/mL Polybrene. Transfer 2mL per well to a 12-well plate.<\/span><\/div><div class = \"text-block\"><span style = \"font-weight:bold;\">3. <\/span><span>Spin cells at 1265<\/span><span style = \"font-style:italic;\">xg<\/span><span> at 30C for 90 minutes.<\/span><\/div><div class = \"text-block\"><span style = \"font-weight:bold;\">4. <\/span><span>Transfer plates to 37C, 5% CO2 incubator overnight.<\/span><\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":null,"section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":657631,"guid":"04A04CF717F24D16B669264C4A701638","previous_id":657630,"previous_guid":"BEBCB49BF49F44008DE252C0C08D2C06","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"7CA924B05CB7404981E5A4CDC579C832","order_id":1,"type_id":6,"title":"Section","source":{"title":"Day 5 - Lentiviral supernatant production and transduction"}},{"id":1054724,"guid":"086A61D7FB3F4C9EA7227139978EAD01","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Add 2 mL per well fresh CTL media containing 100U\/mL IL-2.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":null,"section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":657632,"guid":"36F337A074BE4AA4A32481E84A5F834A","previous_id":657631,"previous_guid":"04A04CF717F24D16B669264C4A701638","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"33B04F108D784366B3135DA003F24796","order_id":1,"type_id":6,"title":"Section","source":{"title":"Day 6 - Lentiviral supernatant production and transduction"}},{"id":1054724,"guid":"268CBDEBB2DC4CE087A7DDA1777CF929","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span style = \"font-weight:bold;\">Remove Dynabeads:<\/span><\/div><div class = \"text-block\"><span style = \"font-weight:bold;\">1.<\/span><span> Combine T cells into a 15mL tube and spin down at 369<\/span><span style = \"font-style:italic;\">xg<\/span><span> for 4-5 minutes at 4C.<\/span><\/div><div class = \"text-block\"><span style = \"font-weight:bold;\">2.<\/span><span> Rinse the wells with 5mL CTL media and re-suspend the cells with this. Mix well to break up any clumps.<\/span><\/div><div class = \"text-block\"><span style = \"font-weight:bold;\">3.<\/span><span> Use the Dyanlmagnet to remove Dynabeads. Let tube sit for ~2 minutes so the beads aggregate along the side of the tube.<\/span><\/div><div class = \"text-block\"><span style = \"font-weight:bold;\">4. <\/span><span>Gently remove the media containing the cells, careful not to disrupt the beads. Wash the beads one more time with 5mL CTL media.<\/span><\/div><div class = \"text-block\"><span style = \"font-weight:bold;\">5.<\/span><span> Collect T cells. Spin down cells at 369<\/span><span style = \"font-style:italic;\">xg<\/span><span> for 4-5 minutes at 4C.<\/span><\/div><div class = \"text-block\"><span style = \"font-weight:bold;\">6.<\/span><span> Resuspend cells in 8 mL fresh CTL media containing 1:1000 dilution of IL-2. Transfer to one well of a 6-well plate.<\/span><\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":null,"section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":657633,"guid":"0D4E8EC597D647A1935B8D72483FE533","previous_id":657632,"previous_guid":"36F337A074BE4AA4A32481E84A5F834A","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"58C05EFD6F424A34B79B8D97514C95EE","order_id":1,"type_id":6,"title":"Section","source":{"title":"Day 8 - Lentiviral supernatant production and transduction"}},{"id":1054724,"guid":"0D47E37003094218A4B55EE903BF7FCD","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Without disturbing cells on the bottom of the well, remove \u00bd of the CTL media. Add fresh CTL media containing 100 U\/mL IL-2.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":null,"section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":657634,"guid":"9148B3C2DC2342A9B09F469B81FEF80C","previous_id":657633,"previous_guid":"0D4E8EC597D647A1935B8D72483FE533","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"CDEFAF3AAD0E48C890FCB548C6B84E90","order_id":1,"type_id":6,"title":"Section","source":{"title":"Day 11 - Lentiviral supernatant production and transduction"}},{"id":1054724,"guid":"817B6AA5941644BAA7A054BDA8C5002D","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Without disturbing cells on the bottom of the well, remove \u00bd of the CTL media. Add fresh CTL media containing 100 U\/mL IL-2.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":null,"section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":657635,"guid":"0A57C569735D421E934619BCAC0E756D","previous_id":657634,"previous_guid":"9148B3C2DC2342A9B09F469B81FEF80C","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"60044999DB7B4407908DE4CFF7C896E1","order_id":1,"type_id":6,"title":"Section","source":{"title":"Day 13 - Lentiviral supernatant production and transduction"}},{"id":1054724,"guid":"A88155736A3D4828A30E8318651462CD","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Without disturbing cells on the bottom of the well, remove \u00bd of the CTL media. Add fresh CTL media containing 100 U\/mL IL-2.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":null,"section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":660167,"guid":"D9AB7F8A420F4C1C86D751F6829A0FB5","previous_id":657635,"previous_guid":"0A57C569735D421E934619BCAC0E756D","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"8920B761F1934B2293DA1092405C9313","order_id":1,"type_id":6,"title":"Section","source":{"title":"Day 0 - Rapid Expansion Protocol (REP)"}},{"id":1054724,"guid":"194F78E81A094F02B02F870CB014C5B2","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Prepare items for REP mix:<\/div><div class = \"text-block\">1. Thaw mixed donor PBMC, count and irradiate at 4000 rad.<\/div><div class = \"text-block\">2. Count and irradiate LCL cells at 8000 rad.<\/div><div cl