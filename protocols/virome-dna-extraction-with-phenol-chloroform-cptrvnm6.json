tion":null,"manuscript_citation":"{\"blocks\":[{\"key\":\"bvlpq\",\"text\":\"\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","materials":[],"materials_text":"","ownership_history":[{"name":"Adair  Borges","affiliation":null,"affiliation_url":null,"username":"adair--borges","link":"","image":{"source":"/img/avatars/001.png","placeholder":"/img/avatars/001.png","webp_source":""},"change_time":1677002215},{"name":"Arcadia Science","affiliation":null,"affiliation_url":null,"username":"arcadia-science","link":"https://www.arcadiascience.com/","image":{"source":"https://content.protocols.io/files/ksusbx3af.jpg","placeholder":"https://content.protocols.io/files/ksusbx3af.jpg","webp_source":"https://content.protocols.io/files/ksurbx3af.webp"},"change_time":1678326512}],"parent_collections":[{"id":77739,"title":"Protocol collection: Sampling and characterizing cheese rind microbial communities","title_html":null,"image":{"source":"https://content.protocols.io/files/kv8vbw2qp.jpg","webp_source":null,"placeholder":"https://content.protocols.io/files/kv8vbw2qp.jpg","webp_placeholder":null},"doi":"dx.doi.org/10.17504/protocols.io.n2bvj8j5xgk5/v1","uri":"protocol-collection-sampling-and-characterizing-ch-cp6jvrcn","published_on":1678327779,"modified_on":0,"version_class":0,"version_id":0,"version_code":"","version_uri":"","created_on":1677545101,"categories":null,"creator":{"name":"Arcadia Science","affiliation":null,"affiliation_url":null,"username":"arcadia-science","link":"https://www.arcadiascience.com/","image":{"source":"https://content.protocols.io/files/ksusbx3af.jpg","placeholder":"https://content.protocols.io/files/ksusbx3af.jpg","webp_source":"https://content.protocols.io/files/ksurbx3af.webp"}},"stats":null}],"parent_protocols":[],"peer_reviewed":false,"protocol_references":"","public":true,"public_fork_note":"","published_on":1678327673,"references":[],"related_equipments":[],"related_materials":[],"reserved_doi":"","retraction_reason":null,"samples":{},"shared_access_id":16,"show_comparison":true,"sign_info":null,"state_version_id":149,"stats":{"is_voted":false,"number_of_views":5,"number_of_steps":17,"number_of_bookmarks":0,"number_of_comments":0,"number_of_bookmarked_comments":0,"number_of_steps_comments":0,"number_of_protocol_comments":0,"number_of_exports":0,"number_of_runs":0,"number_of_votes":0,"number_of_reagents":0,"number_of_equipments":0,"number_of_collections":1,"number_of_forks":{"private":0,"public":0},"number_of_accessible_forks":0},"status":{"id":1,"info":"We use this protocol and it's working"},"steps":[{"id":1605772,"guid":"211A74D9B21111ED88870A58A9FEAC02","previous_id":null,"previous_guid":null,"section":"\u003cp\u003eDegradation of host nucleic acids\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"5kgeq\",\"text\":\"To degrade any non-encapsulated nucleic acids, treat the concentrated phage lysate with 10 µg/mL RNase A (NEB T3018) and 10 µg/mL DNase I (NEB M0303). Digest for 1 h at room temperature or 4 °C overnight. Store phage at 4 °C. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":77393,"case_id":0,"critical_ids":"","duration":0,"original_id":1534794,"number":"1","cases":[],"critical":null},{"id":1605773,"guid":"211ABA76B21111ED88870A58A9FEAC02","previous_id":1605772,"previous_guid":"211A74D9B21111ED88870A58A9FEAC02","section":"\u003cp\u003ePhage DNA extraction (using phenol-chloroform)\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"19tgc\",\"text\":\"Strip away phage capsids with 1 µl of Proteinase K (NEB P8107S) + 0.05% SDS. Incubate at 55 °C for 60 min on a Thermomixer at 250 rpm. If you only have a static heat block, vortex every 10 min.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":77393,"case_id":0,"critical_ids":"","duration":0,"original_id":1534797,"number":"2","cases":[],"critical":null},{"id":1605774,"guid":"211B31E2B21111ED88870A58A9FEAC02","previous_id":1605773,"previous_guid":"211ABA76B21111ED88870A58A9FEAC02","section":"\u003cp\u003ePhage DNA extraction (using phenol-chloroform)\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"743mf\",\"text\":\"In fume hood, add an equal volume of phenol – chloroform – isoamyl alcohol mixture, pH 8 (Sigma Aldrich 77618) to the digested phage. If the volume of phage is low, consider bringing up the volume to 500 µl with 1× TE buffer. Incubate at room temperature for 10 min, vortexing regularly to keep phenol mixed in.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":77393,"case_id":0,"critical_ids":"","duration":0,"original_id":1534802,"number":"3","cases":[],"critical":null},{"id":1605775,"guid":"211BA8ABB21111ED88870A58A9FEAC02","previous_id":1605774,"previous_guid":"211B31E2B21111ED88870A58A9FEAC02","section":"\u003cp\u003ePhage DNA extraction (using phenol-chloroform)\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"1jgq3\",\"text\":\"Spin sample at maximum speed at 4 °C for 10 min. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":77393,"case_id":0,"critical_ids":"","duration":0,"original_id":1534812,"number":"4","cases":[],"critical":null},{"id":1605776,"guid":"211C1DC5B21111ED88870A58A9FEAC02","previous_id":1605775,"previous_guid":"211BA8ABB21111ED88870A58A9FEAC02","section":"\u003cp\u003ePhage DNA extraction (using phenol-chloroform)\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"2kvb6\",\"text\":\"In the fume hood, transfer the aqueous layer (top layer) to a new tube. Make sure to avoid the interface, and do not transfer any any debris or organics to the new tube. Add equal volume of chloroform to the sample and invert to mix. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":77393,"case_id":0,"critical_ids":"","duration":0,"original_id":1534814,"number":"5","cases":[],"critical":null},{"id":1605777,"guid":"211C91ADB21111ED88870A58A9FEAC02","previous_id":1605776,"previous_guid":"211C1DC5B21111ED88870A58A9FEAC02","section":"\u003cp\u003ePhage DNA extraction (using phenol-chloroform)\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"3ckv4\",\"text\":\"Spin sample at maximum speed at 4 °C for 5 min. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":77393,"case_id":0,"critical_ids":"","duration":0,"original_id":1534820,"number":"6","cases":[],"critical":null},{"id":1605778,"guid":"211D05C2B21111ED88870A58A9FEAC02","previous_id":1605777,"previous_guid":"211C91ADB21111ED88870A58A9FEAC02","section":"\u003cp\u003ePhage DNA extraction (using phenol-chloroform)\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"11r15\",\"text\":\"Repeat steps 5 and 6, extracting the aqueous layer with more chloroform. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":77393,"case_id":0,"critical_ids":"","duration":0,"original_id":1534827,"number":"7","cases":[],"critical":null},{"id":1605779,"guid":"211D7B6FB21111ED88870A58A9FEAC02","previous_id":1605778,"previous_guid":"211D05C2B21111ED88870A58A9FEAC02","section":"\u003cp\u003ePhage DNA extraction (using phenol-chloroform)\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"e0n4q\",\"text\":\"Transfer aqueous layer to new tube. You can perform all subsequent steps on the benchtop. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":77393,"case_id":0,"critical_ids":"","duration":0,"original_id":1534828,"number":"8","cases":[],"critical":null},{"id":1605780,"guid":"211DF018B21111ED88870A58A9FEAC02","previous_id":1605779,"previous_guid":"211D7B6FB21111ED88870A58A9FEAC02","section":"\u003cp\u003eIsopropanol precipitation \u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"c01ei\",\"text\":\"Add a 10% volume of 3 M sodium acetate to the sample, and an equal volume of isopropanol. If you anticipate a low DNA yield, you can use glycogen (like GlycoBlue, ThermoFischer AM9515) as a coprecipitant at 50–150 µg/mL. Invert to mix, and move to 4 °C for 2 h (can be left overnight). \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":77393,"case_id":0,"critical_ids":"","duration":0,"original_id":1534830,"number":"9","cases":[],"critical":null},{"id":1605781,"guid":"211E633AB21111ED88870A58A9FEAC02","previous_id":1605780,"previous_guid":"211DF018B21111ED88870A58A9FEAC02","section":"\u003cp\u003eIsopropanol precipitation \u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"6nhdu\",\"text\":\"Spin sample at maximum speed at 4 °C for 30 minutes.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":77393,"case_id":0,"critical_ids":"","duration":0,"original_id":1534832,"number":"10","cases":[],"critical":null},{"id":1605782,"guid":"211ED9EBB21111ED88870A58A9FEAC02","previous_id":1605781,"previous_guid":"211E633AB21111ED88870A58A9FEAC02","section":"\u003cp\u003eIsopropanol precipitation \u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"7gptc\",\"text\":\"DNA will be a glassy smear or pellet on the side of the tube. Carefully pipette off and discard the liquid.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":77393,"case_id":0,"critical_ids":"","duration":0,"original_id":1534833,"number":"11","cases":[],"critical":null},{"id":1605783,"guid":"211F517DB21111ED88870A58A9FEAC02","previous_id":1605782,"previous_guid":"211ED9EBB21111ED88870A58A9FEAC02","section":"\u003cp\u003eIsopropanol precipitation \u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"edh5p\",\"text\":\"Wash pellet with 1 mL freshly made 70% ethanol (room temperature, not cold). DNA will start to turn white. Check to make sure the DNA isn’t stuck on the sides of the tube. If it is, wash it down with the 70% EtOH.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":77393,"case_id":0,"critical_ids":"","duration":0,"original_id":1534837,"number":"12","cases":[],"critical":null},{"id":1605784,"guid":"211FC493B21111ED88870A58A9FEAC02","previous_id":1605783,"previous_guid":"211F517DB21111ED88870A58A9FEAC02","section":"\u003cp\u003eIsopropanol precipitation \u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"9msup\",\"text\":\"Spin sample at maximum speed at 4 °C for 5 min.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":77393,"case_id":0,"critical_ids":"","duration":0,"original_id":1534839,"number":"13","cases":[],"critical":null},{"id":1605785,"guid":"2120370AB21111ED88870A58A9FEAC02","previous_id":1605784,"previous_guid":"211FC493B21111ED88870A58A9FEAC02","section":"\u003cp\u003eIsopropanol precipitation \u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"afvaj\",\"text\":\"Carefully pipette off and discard the liquid, and then wash with 500 µl of 70% ethanol. Spin sample at maximum speed at 4 °C for 5 min.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":77393,"case_id":0,"critical_ids":"","duration":0,"original_id":1534840,"number":"14","cases":[],"critical":null},{"id":1605786,"guid":"2120AB62B21111ED88870A58A9FEAC02","previous_id":1605785,"previous_guid":"2120370AB21111ED88870A58A9FEAC02","section":"\u003cp\u003eIsopropanol precipitation \u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"2rddg\",\"text\":\"Carefully pipette off and discard the liquid. Do short spins, and pipette with smaller and smaller pipette volumes until all the ethanol is removed. Let dry for 5 min or less.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":77393,"case_id":0,"critical_ids":"","duration":0,"original_id":1534841,"number":"15","cases":[],"critical":null},{"id":1605787,"guid":"21212081B21111ED88870A58A9FEAC02","previous_id":1605786,"previous_guid":"2120AB62B21111ED88870A58A9FEAC02","section":"\u003cp\u003eIsopropanol precipitation \u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"fo0gh\",\"text\":\"Resuspend pellet in 30 µl of nuclease-free water. It can help to warm the sample to 50 °C. The volume of water can be adjusted if you anticipate a large DNA yield.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":