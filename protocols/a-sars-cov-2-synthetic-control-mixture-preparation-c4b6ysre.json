public":true,"public_fork_note":"","published_on":1701965900,"references":[],"related_equipments":[],"related_materials":[],"reserved_doi":"","retraction_reason":null,"samples":{},"shared_access_id":265,"show_comparison":false,"sign_info":null,"space_access":{"can_view":false,"can_edit":false,"can_remove":false,"can_add":false,"can_publish":true,"can_get_doi":true,"can_share":false,"can_move":false,"can_move_outside":false,"can_transfer":false,"can_download":false,"limited_run":false,"limited_private_links":false,"limited_blind_links":false,"is_locked":false},"state_version_id":1657,"stats":{"is_voted":false,"number_of_views":5,"number_of_steps":21,"number_of_bookmarks":0,"number_of_comments":0,"number_of_bookmarked_comments":0,"number_of_steps_comments":0,"number_of_protocol_comments":0,"number_of_exports":0,"number_of_runs":0,"number_of_votes":0,"number_of_reagents":0,"number_of_equipments":0,"number_of_collections":0,"number_of_forks":{"private":0,"public":0},"number_of_accessible_forks":0},"status":{"id":1,"info":"We use this protocol and it's working"},"steps":[{"id":1834831,"guid":"40A7944D98064CE290CBA2484537C79F","previous_id":1834990,"previous_guid":"575BA5256A294A4180EDFA510FA4515A","section":"\u003cp\u003eSelection of Synthetic Controls \u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"an5eh\",\"text\":\"Synthetic controls chosen for Ferdous et al. 2023  - Control 15 (Alpha-103909), Control 17 (Gamma-104044), Control 23 (Delta-104533), Control 48 (Omicron - BA.1 lineage-105204), Control 51 (B.1.1.529+BA.2-England-105346), Control 2 (Wuhan hu-1 from china-102024), Control 6 (Wuhan hu-1 from California-102918), Control 50 (B.1.1.529+BA.2-Australia-105345), Control 64 (BA.5-England-106196), Control 62 (B.2.12.1-Denmark-105865), Control 66 (BA.4-Texas-106198), Control 67 (BA.4-California-106199), Control 63 (B.2.12.1-USA-105857), Control 65 (BA.5-USA-106197) and Control 19 (Iota-104529). \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90206,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"2","cases":[],"critical":null},{"id":1834832,"guid":"018B984DD0A64B51813D19043369BEDC","previous_id":1835538,"previous_guid":"60D1EA0DEA5F4891872A77C52660FB4F","section":"\u003cp\u003eDilution of Synthetic Controls \u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"am730\",\"text\":\"Dilution can be modified based on the specific concentration requirement of the research design. For this dataset, dilute the controls in a 1:100 ratio as each control is provided in a standard format, which consists of 100ul at a concentration of approximately one million copies per microliter. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90206,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4","cases":[],"critical":null},{"id":1834854,"guid":"9B099E5A1F29478AB769EE0EF3BDB9B9","previous_id":1835536,"previous_guid":"3AD90914724541459BAA9E4482BED8AC","section":"\u003cp\u003eCreating Proper Documentation\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"3a019\",\"text\":\"It is crucial to maintain proper documentation while making the control mixture to make sure that the ground truth is completely error-free.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90206,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"10","cases":[],"critical":null},{"id":1834855,"guid":"DA2685EC3A824980A15F7B6F8A72B757","previous_id":1849348,"previous_guid":"C8D1F1FDC87D4E878C82F060B8FBBACC","section":"\u003cp\u003ePreparation of Synthetic Control Mixtures\u003c/p\u003e","section_color":"#FFED92","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"624d2\",\"text\":\"Take out all the diluted controls from the freezer and thaw them in room temperature. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90206,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"14","cases":[],"critical":null},{"id":1834990,"guid":"575BA5256A294A4180EDFA510FA4515A","previous_id":0,"previous_guid":null,"section":"\u003cp\u003eSelection of Synthetic Controls \u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"a78jm\",\"text\":\"Synthetic RNA controls were selected from Twist Biosciences (TwistBioscienceSyntheticViralControl). For this project, 15 different synthetic controls were chosen, however, any combination of controls can be ordered and combined for a similar analysis.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[{\"key\":0,\"offset\":61,\"length\":36}],\"data\":{}}],\"entityMap\":{\"0\":{\"type\":\"link\",\"mutability\":\"MUTABLE\",\"data\":{\"guid\":\"5C504F07B1884805886A9ADF6E15518F\",\"url\":\"https://www.twistbioscience.com/products/ngs/synthetic-viral-controls?tab=sars-cov-2-controls\"}}}}","data":null,"protocol_id":90206,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"1","cases":[],"critical":null},{"id":1835531,"guid":"2E303CC24253428B959F2815B53976B5","previous_id":1834832,"previous_guid":"018B984DD0A64B51813D19043369BEDC","section":"\u003cp\u003eDilution of Synthetic Controls \u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"8761j\",\"text\":\"Take 15 1.5ml eppendorf tubes and sterilize them with UV radiation.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90206,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"5","cases":[],"critical":null},{"id":1835533,"guid":"11645D2DA2CE4BA19A878AED48DE996E","previous_id":1835531,"previous_guid":"2E303CC24253428B959F2815B53976B5","section":"\u003cp\u003eDilution of Synthetic Controls \u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"efl55\",\"text\":\"Label  the tubes as \\\"Diluted Control x-date\\\" with respective control name and date.  \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90206,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"6","cases":[],"critical":null},{"id":1835534,"guid":"B55CF54583E74CF3A8EAB37D849C254E","previous_id":1835533,"previous_guid":"11645D2DA2CE4BA19A878AED48DE996E","section":"\u003cp\u003eDilution of Synthetic Controls \u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"37n9a\",\"text\":\"Add 99ul Nuclease free water to the tubes.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"2grr8\",\"text\":\"Add 1ul of each control in respective tubes . \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90206,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"7","cases":[],"critical":null},{"id":1835535,"guid":"473E188AD12340108743CC40A1B5FB70","previous_id":1835534,"previous_guid":"B55CF54583E74CF3A8EAB37D849C254E","section":"\u003cp\u003eDilution of Synthetic Controls \u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"c8h1\",\"text\":\"Mix properly with gentle vortexing. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90206,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"8","cases":[],"critical":null},{"id":1835536,"guid":"3AD90914724541459BAA9E4482BED8AC","previous_id":1835535,"previous_guid":"473E188AD12340108743CC40A1B5FB70","section":"\u003cp\u003eDilution of Synthetic Controls \u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"7clcp\",\"text\":\"This diluted controls will be used as our stock to make control mixtures. Store the controls in -80 unless used immediately. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90206,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"9","cases":[],"critical":null},{"id":1835538,"guid":"60D1EA0DEA5F4891872A77C52660FB4F","previous_id":1834831,"previous_guid":"40A7944D98064CE290CBA2484537C79F","section":"\u003cp\u003eSelection of Synthetic Controls \u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"7cs6k\",\"text\":\"Store the controls in -80 unless used immediately. It is crucial to minimize the number of freeze and thaw cycles with these RNA controls. So plan accordingly.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90206,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"3","cases":[],"critical":null},{"id":1835545,"guid":"5426417573984746B73192B1592345EF","previous_id":1835549,"previous_guid":"E197ABF77205494CBA8C14F772AF218D","section":"\u003cp\u003eCreating Proper Documentation\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"d5ku0\",\"text\":\"Fill in the information accordingly. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90206,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"12","cases":[],"critical":null},{"id":1835549,"guid":"E197ABF77205494CBA8C14F772AF218D","previous_id":1834854,"previous_guid":"9B099E5A1F29478AB769EE0EF3BDB9B9","section":"\u003cp\u003eCreating Proper Documentation\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"3ps07\",\"text\":\"We created a spreadsheet with columns \\\"Mixture name\\\"(Name of the mixture), \\\"Controls\\\"(controls that are added in this mixture), and \\\"Amount\\\"(Amount of each control added in this mixture). \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90206,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"11","cases":[],"critical":null},{"id":1835570,"guid":"5CEBCF5EDD5A45A0AC089E6F97A9EBF0","previous_id":1834855,"previous_guid":"DA2685EC3A824980A15F7B6F8A72B757","section":"\u003cp\u003ePreparation of Synthetic Control Mixtures\u003c/p\u003e","section_color":"#FFED92","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"a90nt\",\"text\":\"Take 45 eppendorf tubes and decontaminate them with UV. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"8pgof\",\"text\":\"Label them as the Mixtures names and organize them in eppendorf tube holder. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90206,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"15","cases":[],"critical":null},{"id":1835574,"guid":"75E1492C0F124F86A4C816AAAACF6446","previous_id":1835570,"previous_guid":"5CEBCF5EDD5A45A0AC089E6F97A9EBF0","section":"\u003cp\u003ePreparation of Synthetic Control Mixtures\u003c/p\u003e","section_color":"#FFED92","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"ets11\",\"text\":\"As per the spreadsheet above, take the tube of Mixture \\\"All\\\" and add all the control in 0.5ul amount.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"4orch\",\"text\":\"Add 0.5 ul of NFW to that solution as well. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"ej2ve\",\"text\":\"Remember to change tips in between adding controls. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90206,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"16","cases":[],"critical":null},{"id":1835580,"guid":"285464742A6D4F84BF755B4C2DD85524","previous_id":1835574,"previous_guid":"75E1492C0F124F86A4C816AAAACF6446","section":"\u003cp\u003ePreparation of Synthetic Control Mixtures\u003c/p\u003e","section_color":"#FFED92","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"61fm8\",\"text\":\"Close the lid and mix by gentle vortexing. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"c2djs\",\"text\":\"Mixtue \\\"All\\\" is now completely prepared. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90206,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"17","cases":[],"critical":null},{"id":1835582,"guid":"5B11AAB77E3A4CC484B3528243C44BEC","previous_id":1835580,"previous_guid":"285464742A6D4F84BF755B4C2DD85524","section":"\u003cp\u003ePreparation of Synthetic Control Mixtures\u003c/p\u003e","section_color":"#FFED92","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"c3efc\",\"text\":\"Prepare rest of the mixtures as per the spreadsheet. \",\"type\":\"unstyled\",\"