an_accept_authorship":0,"can_be_copied":1,"can_remove_fork":1,"fork_id":null,"url":"https:\/\/www.protocols.io\/view\/t-cell-cytotoxicity-xcelligence-platform-sqkeduw","forks_count":{"private":0,"public":0},"access":{"can_view":1,"can_remove":0,"can_add":0,"can_edit":0,"can_publish":0,"can_get_doi":0,"can_share":1,"can_move":1,"can_move_outside":1,"can_transfer":1,"can_download":1,"is_locked":0},"guid":"897AF3C6B9DA11E99CA40242AC110005","state_version_id":0,"steps":[{"id":655725,"guid":"F37297F17FA44FDD87846FC0629AC76E","previous_id":null,"previous_guid":null,"modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"9B95A3771DDA47929AF132E7AD0060BB","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"844F81C0FC8B44C38A4044100EA06970","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Add 50ul media to each well of the E-VIEW plate (use 'inverse pipetting' technique to avoid bubbles in the wells), place the plate in the xCELLigence MP cradle, and run the setup step of the xCELLigence software to confirm all the wells and connections on the cradle are functioning properly.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":null,"section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":655726,"guid":"D34E499C2A44440090F1A0E581613E6A","previous_id":655725,"previous_guid":"F37297F17FA44FDD87846FC0629AC76E","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"7DB56F16E1904A4BAFEABA0ABFA97EC0","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"58ABFD8ADEB34C7794E5EA25CAA7DF05","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Resuspend adherent tumor target cells at 10e5 cells\/50ul T cell media.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":null,"section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":655727,"guid":"8C8AED02F4CE4F019C100256BBA02E5A","previous_id":655726,"previous_guid":"D34E499C2A44440090F1A0E581613E6A","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"8FCA808E12B14502BFD7E28B21091B95","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"8F94AD700A344BAB93A8756BD15ED4D4","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Remove the plate from the cradle and plate 10e5 adherent tumor cells (target cells) to grow overnight in (100uL total volume per well) in plastic 96 well VIEW-E plates. Use 'inverse pipetting' technique to avoid bubbles in the wells. Let plate sit in the hood for 30 minutes for cells to settle. Return each plate to the xCELLigence cradle. Begin recording impedance values at desired time interval.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":null,"section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":655728,"guid":"0D522962D08E45AF857307BA1B1DFBC4","previous_id":655727,"previous_guid":"8C8AED02F4CE4F019C100256BBA02E5A","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"02DBB73BE8994DDA8A394979A09F8244","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"0DA5D446A79B4573A4666FF7697ADB60","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Grow cells to confluence (determine confluence using cell index readout on the xCELLigence MP platform using RTCA data acquisition software).<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":null,"section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":655729,"guid":"7E7CEC81C81E4714A27258018BF65368","previous_id":655728,"previous_guid":"0D522962D08E45AF857307BA1B1DFBC4","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"6889B4BFCD34460BA53745E490D35C1A","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"89A6DDE78356418B89B39FF71B0364B0","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Prepare effector cells at desired concentrations for assy. Pause impedance readings and remove the plate from the cradle. Add transduced T cells, cytolysis control, or media only for growth control, in 50ul volume, to each well (use 'inverse pipetting' technique to avoid bubbles in the wells).<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":null,"section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":655730,"guid":"075EABF4D6094A808378A754DE49CD96","previous_id":655729,"previous_guid":"7E7CEC81C81E4714A27258018BF65368","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"068B6512C4F14BFCA9EB07C21B12361C","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"2E916B3DB9F14B1EB82271A44C901052","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Return E-VIEW plate to the xCELLigence cradle and resume recording impedance values until desired stopping point. If running an assay for more than 2-3 days, pause the instrument, remove the plates and add additional media (25-50ul) so that the wells do not dry out.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":null,"section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":655731,"guid":"05D4827B6DB843908DC849CC9A841B85","previous_id":655730,"previous_guid":"075EABF4D6094A808378A754DE49CD96","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"16A8446C23C348398BE9738D3C6A67C5","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"EA3F4F3A4B3644448EBD327275BB0E95","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Use RTCA and xIMT software for analysis.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":null,"section_duration":0,"critical":null,"critical_id":null,"duration":0}],"document":"","materials":[{"id":127867,"mol_weight":0,"name":"xCELLigence RTCA MP \u2013 Bundle (complete system)","linfor":"","url":"https:\/\/www.aceabio.com\/products\/rtca-mp\/","sku":"0088060140","cas_number":"","rrid":"","public":0,"vendor":{"name":"ACEA Biosciences, Inc","affiliation":null,"affiliations":[],"username":null,"note":null,"link":"https:\/\/www.aceabio.com\/","image":{"source":"https:\/\/www.protocols.io\/img\/vendors\/placeholder.png","placeholder":"https:\/\/www.protocols.io\/img\/vendors\/placeholder.png"},"badges":[],"verified":0,"is_verified_user":false,"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false,"id":405},"can_edit":0,"stats":{"total_protocols":0}},{"id":127870,"mol_weight":0,"name":"E-Plate View 96 (6 plates)","linfor":"","url":"https:\/\/www.aceabio.com\/products\/rtca-mp\/","sku":"0647248001","cas_number":"","rrid":"","public":0,"vendor":{"name":"ACEA Biosciences, Inc","affiliation":null,"affiliations":[],"username":null,"note":null,"link":"https:\/\/www.aceabio.com\/","image":{"source":"https:\/\/www.protocols.io\/img\/vendors\/placeholder.png","placeholder":"https:\/\/www.protocols.io\/img\/vendors\/placeholder.png"},"badges":[],"verified":0,"is_verified_user":false,"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false,"id":405},"can_edit":0,"stats":{"total_protocols":0}},{"id":127873,"mol_weight":0,"name":"xIMT Immunotherapy Software","linfor":"","url":"https:\/\/www.aceabio.com\/products\/rtca-mp\/","sku":"8100004","cas_number":"","rrid":"","public":0,"vendor":{"name":"ACEA Biosciences, Inc","affiliation":null,"affiliations":[],"username":null,"note":null,"link":"https:\/\/www.aceabio.com\/","image":{"source":"https:\/\/www.protocols.io\/img\/vendors\/placeholder.png","placeholder":"https:\/\/www.protocols.io\/img\/vendors\/placeholder.png"},"badges":[],"verified":0,"is_verified_user":false,"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false,"id":405},"can_edit":0,"stats":{"total_protocols":0}}],"description":"<div class = \"text-blocks\"><div class = \"text-block\">This protocol outlines the steps for preparing cellular cytotoxicity assays using the xCELLigence platform.<\/div><div class = \"text-block\">In brief: For impedance assays, 10e5 tumor cells were plated overnight in 100mL media in plastic 96 well VIEW-E plates (ACEA) and grown to confluence (measured using the cell index readout on xCELLigence MP platform, RTCA software, ACEA). Transduced T cells were added at varying E:T ratios (as indicated), and cell index was monitored for up to 5 days. Impedance data was analyzed using xIMT software (v1.2, ACEA).\u00a0 \u00a0<\/div><\/div>","changed_on":1625097723}