":null,"space_access":{"can_preview":false,"can_view":false,"can_edit":false,"can_remove":false,"can_add":false,"can_publish":true,"can_get_doi":true,"can_share":false,"can_share_organization":false,"can_move":false,"can_move_outside":false,"can_transfer":false,"can_download":false,"limited_run":false,"limited_private_links":false,"limited_blind_links":false,"is_locked":false},"space_id":22979,"state_version_id":1,"stats":{"is_voted":false,"number_of_views":201,"number_of_steps":11,"number_of_bookmarks":0,"number_of_comments":0,"number_of_bookmarked_comments":0,"number_of_steps_comments":0,"number_of_protocol_comments":0,"number_of_exports":5,"number_of_runs":0,"number_of_votes":0,"number_of_reagents":0,"number_of_equipments":0,"number_of_collections":0,"number_of_forks":{"private":0,"public":0},"number_of_accessible_forks":0},"status":{"id":1,"info":""},"steps":[{"id":1162538,"guid":"43916A2650444A14B23A53A562E7F946","previous_id":null,"previous_guid":null,"section":"\u003cp\u003eMinced Digestion Device Operation\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"66i2i\",\"text\":\"Affix 0.05” ID tubing (Saint-Gobain, Malvern, PA) to Minced Digestion Device inlet and outlet hose barbs.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":{}}","data":null,"protocol_id":49176,"case_id":0,"critical_ids":"","duration":0,"original_id":1162518,"number":"1","cases":[],"critical":null},{"id":1162539,"guid":"30034288B7F84B97B68DB79496B3E6B9","previous_id":1162538,"previous_guid":"43916A2650444A14B23A53A562E7F946","section":"\u003cp\u003eMinced Digestion Device Operation\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"5sj0d\",\"text\":\"Connect tubing to an Ismatec peristaltic pump (Cole-Parmer, Werheim, Germany) with 2.62 mm ID tubing (Saint-Gobain, Malvern, PA).\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":{}}","data":null,"protocol_id":49176,"case_id":0,"critical_ids":"","duration":0,"original_id":1162519,"number":"2","cases":[],"critical":null},{"id":1162540,"guid":"67127880F07547C58C8AB317C709B0F9","previous_id":1162539,"previous_guid":"30034288B7F84B97B68DB79496B3E6B9","section":"\u003cp\u003eMinced Digestion Device Operation\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"4mel\",\"text\":\"Incubate devices and tubing with SuperBlock (PBS) blocking buffer (Thermo Fisher Scientific, Waltham, MA) at room temperature for 15 min to reduce non-specific binding of cells to channel walls.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":{}}","data":null,"protocol_id":49176,"case_id":0,"critical_ids":"","duration":0,"original_id":1162520,"number":"3","cases":[],"critical":null},{"id":1162541,"guid":"40959CD7233A456DB6A62B53945130A4","previous_id":1162540,"previous_guid":"67127880F07547C58C8AB317C709B0F9","section":"\u003cp\u003eMinced Digestion Device Operation\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"jvdj\",\"text\":\"Flush out SuperBlock with PBS+1% BSA (PBS+).\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":{}}","data":null,"protocol_id":49176,"case_id":0,"critical_ids":"","duration":0,"original_id":1162521,"number":"4","cases":[],"critical":null},{"id":1162542,"guid":"99114A00423E43909AEE6B9BCA596147","previous_id":1162541,"previous_guid":"40959CD7233A456DB6A62B53945130A4","section":"\u003cp\u003eMinced Digestion Device Operation\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"8mdtf\",\"text\":\"Load minced tissues into the device tissue chamber through the luer inlet port.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":{}}","data":null,"protocol_id":49176,"case_id":0,"critical_ids":"","duration":0,"original_id":1162522,"number":"5","cases":[],"critical":null},{"id":1162543,"guid":"1D9E4F60FD4644EBB6DE65FEC05FBE52","previous_id":1162542,"previous_guid":"99114A00423E43909AEE6B9BCA596147","section":"\u003cp\u003eMinced Digestion Device Operation\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"1tv2f\",\"text\":\"Prime device and tubing with 0.25% collagenase type I solution (StemCell Technologies, Vancouver, BC), and close off the luer port using a stopcock.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":{}}","data":null,"protocol_id":49176,"case_id":0,"critical_ids":"","duration":0,"original_id":1162523,"number":"6","cases":[],"critical":null},{"id":1162544,"guid":"F78D63BD8AA24B77B28E8165FC3D86F2","previous_id":1162543,"previous_guid":"1D9E4F60FD4644EBB6DE65FEC05FBE52","section":"\u003cp\u003eMinced Digestion Device Operation\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"2uc14\",\"text\":\"Place the experimental setup (device, tubing, and peristaltic pump) inside a 37°C incubator to maintain optimal enzymatic activity.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":{}}","data":null,"protocol_id":49176,"case_id":0,"critical_ids":"","duration":0,"original_id":1162524,"number":"7","cases":[],"critical":null},{"id":1162545,"guid":"74FD5BCD7AD0400C87BF41569B2DE10B","previous_id":1162544,"previous_guid":"F78D63BD8AA24B77B28E8165FC3D86F2","section":"\u003cp\u003eMinced Digestion Device Operation\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"543ma\",\"text\":\"Recirculate the collagenase solution through the device and tubing using the peristaltic pump at a flow rate of 10 or 20 mL/min for the desired length of time.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":{}}","data":null,"protocol_id":49176,"case_id":0,"critical_ids":"","duration":0,"original_id":1162525,"number":"8","cases":[],"critical":null},{"id":1162547,"guid":"B9037FEA75FA49EAACDE5FBCBB466789","previous_id":1162545,"previous_guid":"74FD5BCD7AD0400C87BF41569B2DE10B","section":"\u003cp\u003eIntegrated Dissociation/Filter Device Operation\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"6qpnj\",\"text\":\"Following processing with the minced digestion device, connect 0.05” ID tubing (Saint-Gobain, Malvern, PA) from the outlet of the minced digestion device to the inlet of the integrated dissociation and filtration device. If recirculation is intended, connect tubing from the cross-flow outlet to the peristaltic pump, while the outlet of the integrated device is closed off with a stopcock.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":{}}","data":null,"protocol_id":49176,"case_id":0,"critical_ids":"","duration":0,"original_id":1162530,"number":"9","cases":[],"critical":null},{"id":1162548,"guid":"3AC9151704C34685AB217873BA3E32CC","previous_id":1162547,"previous_guid":"B9037FEA75FA49EAACDE5FBCBB466789","section":"\u003cp\u003eIntegrated Dissociation/Filter Device Operation\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"ds02u\",\"text\":\"Pump fluid through the dissociation component at 10 mL/min flow rate. For final collection of the sample, close off the cross-flow outlet with a stopcock, and pump sample through at 10 mL/min and collect from the effluent outlet.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":{}}","data":null,"protocol_id":49176,"case_id":0,"critical_ids":"","duration":0,"original_id":1162531,"number":"10","cases":[],"critical":null},{"id":1162549,"guid":"37C480C5B10A446AA28CD2A40F0C7271","previous_id":1162548,"previous_guid":"3AC9151704C34685AB217873BA3E32CC","section":"\u003cp\u003eIntegrated Dissociation/Filter Device Operation\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"cac1i\",\"text\":\"Following all experiments, flush out devices with 2 mL PBS+ to collect any remaining cells. For time interval recovery, follow each PBS+ wash with repriming of the device and tubing with collagenase solution, and reconnect the outlet of the minced digestion device to the peristaltic pump for continued recirculation until the next collection period.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":{}}","data":null,"protocol_id":49176,"case_id":0,"critical_ids":"","duration":0,"original_id":1162532,"number":"11","cases":[],"critical":null}],"table_of_contents":[{"title":"\u003cp\u003eMinced Digestion Device Operation\u003c/p\u003e","color":"#A492FF","first_step_guid":"43916A2650444A14B23A53A562E7F946"},{"title":"\u003cp\u003eIntegrated Dissociation/Filter Device Operation\u003c/p\u003e","color":"#94EBFF","first_step_guid":"B9037FEA75FA49EAACDE5FBCBB466789"}],"template_id":1,"title":"Microfluidic device platform operation for processing tissue into single cells ","title_html":"\u003cp\u003eMicrofluidic device platform operation for processing tissue into single cells \u003c/p\u003e","translations":[],"troubleshooting_items":[],"type_id":1,"units":[{"id":1,"type_id":3,"name":"µL","can_manage":0,"read_only":0},{"id":2,"type_id":3,"name":"mL","can_manage":0,"read_only":0},{"id":3,"type_id":3,"name":"L","can_manage":0,"read_only":0},{"id":4,"type_id":3,"name":"µg","can_manage":0,"read_only":0}