erences":"","public":true,"public_fork_note":"","published_on":1709785331,"references":[],"related_equipments":[],"related_materials":[],"reserved_doi":"10.17504/protocols.io.n92ldmq2nl5b/v1","retraction_reason":null,"samples":{},"shared_access_id":0,"show_comparison":false,"sign_info":null,"space_access":{"can_view":false,"can_edit":false,"can_remove":false,"can_add":false,"can_publish":true,"can_get_doi":true,"can_share":false,"can_move":false,"can_move_outside":false,"can_transfer":false,"can_download":false,"limited_run":false,"limited_private_links":false,"limited_blind_links":false,"is_locked":false},"space_id":34457,"state_version_id":393,"stats":{"is_voted":false,"number_of_views":0,"number_of_steps":14,"number_of_bookmarks":0,"number_of_comments":0,"number_of_bookmarked_comments":0,"number_of_steps_comments":0,"number_of_protocol_comments":0,"number_of_exports":0,"number_of_runs":0,"number_of_votes":0,"number_of_reagents":0,"number_of_equipments":0,"number_of_collections":1,"number_of_forks":{"private":0,"public":0},"number_of_accessible_forks":0},"status":{"id":1,"info":""},"steps":[{"id":1899828,"guid":"719E5B7035004D74940864E75E20877B","previous_id":0,"previous_guid":null,"section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"bi0is\",\"text\":\"Wash adherent cells to be analyzed twice with PBS, then scrape into microcentrifuge tubes. Pellet cells via tabletop centrifuge, aspirate supernatant, and lyse for 30 minutes on a rocker at 4 C (lysis buffer: 50 mM HEPES 7.4, 150 mM NaCl, 0.5% NP-40 detergent, 1 mM TCEP, cOmplete protease inhibitor tablet).\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90735,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"1","cases":[],"critical":null},{"id":1899829,"guid":"C558E1F77EDC4992841ED48CFBB34F5D","previous_id":1899828,"previous_guid":"719E5B7035004D74940864E75E20877B","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"uidm\",\"text\":\"Pellet cells post lysis and recover supernatant\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90735,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"2","cases":[],"critical":null},{"id":1899830,"guid":"9FA33546E0344CC5AA6FD75429E422DC","previous_id":1899829,"previous_guid":"C558E1F77EDC4992841ED48CFBB34F5D","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"f4aoc\",\"text\":\"Measure protein content via a BCA assay. Briefly, reconstitute BCA detection reagent, dispense ~100 uL into each well of a 96 well plate, and prepare BSA stock solutions at 1, 2, and 3 mg/mL diluted in PBS. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"6r9hd\",\"text\":\"\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"70agh\",\"text\":\"Set up a standard curve using the BSA samples, as well as blanks for lysis buffer and PBS. To prepare samples, add 2 uL of sample to the prepared 100 uL of BCA detection reagent.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"feme2\",\"text\":\"\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"9lgfs\",\"text\":\"Incubate sample at 37 C for 30 minutes, then scan on a plate reader using a BCA detection protocol. Calculate standard curve, and use to determine protein concentration in each sample. Check that BSA standard curve is linear, and that analyzed samples fall within the standard curve linear range.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90735,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"3","cases":[],"critical":null},{"id":1899831,"guid":"614204C47C8A4AF79749C4079B221557","previous_id":1899830,"previous_guid":"9FA33546E0344CC5AA6FD75429E422DC","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"8271a\",\"text\":\"Load 20 µg of samples in each lane of an SDS-PAGE, and run at 120 V for about 1.5 H. Run a prestained ladder\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90735,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4","cases":[],"critical":null},{"id":1899832,"guid":"56202DE8C71744DD8EB1EAAF25FE1552","previous_id":1899831,"previous_guid":"614204C47C8A4AF79749C4079B221557","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"bt8jh\",\"text\":\"Recover gels and incubate in transfer buffer (48 mM Tris Base, 39 mM glycine, 0.037% SDS, 20% methanol) for 5 minutes. Also incubate a nitrocellulose sheet in transfer buffer.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90735,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"5","cases":[],"critical":null},{"id":1899833,"guid":"49AA4B7BA2D74CCB8C06B32692DDE04B","previous_id":1899832,"previous_guid":"56202DE8C71744DD8EB1EAAF25FE1552","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"bfi7c\",\"text\":\"Set up TransBlot apparatus, keeping transfer sandwich moist in transfer buffer. Transfer using standard protocol for 1 gel. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90735,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"6","cases":[],"critical":null},{"id":1899834,"guid":"6C1E642E9EB8412EA1F32AC59D0D9BAA","previous_id":1899833,"previous_guid":"49AA4B7BA2D74CCB8C06B32692DDE04B","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"1njsn\",\"text\":\"Post transfer, use MW markers to cut strip at expected MW\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90735,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"7","cases":[],"critical":null},{"id":1899835,"guid":"F98983E82E8F4F588F2401B307CE6468","previous_id":1899834,"previous_guid":"6C1E642E9EB8412EA1F32AC59D0D9BAA","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"4r8pj\",\"text\":\"Incubate strips in 5% (m/v) powdered milk in TBST at RT for 1 H or at 4 C while rocking to block.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90735,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"8","cases":[],"critical":null},{"id":1899836,"guid":"B9379C3F99BA41F28F150F6172CCEBBE","previous_id":1899835,"previous_guid":"F98983E82E8F4F588F2401B307CE6468","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"a42kj\",\"text\":\"Dilute primary antibodies according to manufacturer instructions in blocker buffer, incubate at RT for 1 H in a rocker.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90735,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"9","cases":[],"critical":null},{"id":1899837,"guid":"17B8147AA5A747AF84B98B41E091E8FB","previous_id":1899836,"previous_guid":"B9379C3F99BA41F28F150F6172CCEBBE","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"etoib\",\"text\":\"Wash strips 3 x 5 min in TBST\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90735,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"10","cases":[],"critical":null},{"id":1899838,"guid":"F0287E71B9B34510B1B572A4FEFB33C9","previous_id":1899837,"previous_guid":"17B8147AA5A747AF84B98B41E091E8FB","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"8rj29\",\"text\":\"Dilute secondary antibodies conjugated to HRP according to manufacturer instructions in blocking buffer, incubate at RT for 1 H in a rocker.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90735,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"11","cases":[],"critical":null},{"id":1899839,"guid":"0380ADFA8B60432A8732E755CAD162D6","previous_id":1899838,"previous_guid":"F0287E71B9B34510B1B572A4FEFB33C9","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"e9771\",\"text\":\"Wash strips 3x5 min in TBST.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90735,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"12","cases":[],"critical":null},{"id":1899840,"guid":"FFECDD92FA0C4D0E9C15C91C07C8C9CE","previous_id":1899839,"previous_guid":"0380ADFA8B60432A8732E755CAD162D6","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"8rf25\",\"text\":\"Pick up strips and dry corner using kimwipe. Lays strips on saran wrap when excess buffer has been removed.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90735,"case_id":0,"crit