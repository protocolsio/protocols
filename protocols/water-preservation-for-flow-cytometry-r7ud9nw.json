ts":{"is_voted":false,"number_of_views":27,"number_of_steps":6,"number_of_bookmarks":0,"number_of_comments":0,"number_of_bookmarked_comments":0,"number_of_steps_comments":0,"number_of_protocol_comments":0,"number_of_exports":2,"number_of_runs":1,"number_of_votes":0,"number_of_reagents":5,"number_of_equipments":0,"number_of_collections":0,"number_of_forks":{"private":1,"public":0},"number_of_accessible_forks":0},"status":{"id":1,"info":"We use this protocol and it's working"},"steps":[{"id":650615,"guid":"C3A47034441A4E12B21E4FF973E96D81","previous_id":null,"previous_guid":null,"section":"Glutaraldehyde preparation","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"7gds2\",\"text\":\"For 1.5 mL sample volume:\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"italic\",\"offset\":0,\"length\":25}],\"entityRanges\":[],\"data\":{}},{\"key\":\"3m76\",\"text\":\"Aliquot 7.5 µL of 25% aqueous gluteraldehyde into each cryovial. Store aliquoted glutaraldehyde at 4°C until use.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"5coeu\",\"text\":\" \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"afa9u\",\"text\":\" \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[{\"key\":0,\"offset\":0,\"length\":1}],\"data\":{}},{\"key\":\"4vp22\",\"text\":\" \",\"type\":\"atomic\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[{\"key\":1,\"offset\":0,\"length\":1}],\"data\":{}}],\"entityMap\":{\"0\":{\"type\":\"reagents\",\"mutability\":\"\",\"data\":{\"id\":41990,\"mol_weight\":0,\"name\":\"AQUEOUS GLUTARALDEHYDE 25% EM\",\"linfor\":\"\",\"url\":\"\",\"sku\":\"50-262-19\",\"cas_number\":\"\",\"rrid\":\"\",\"public\":1,\"can_edit\":0,\"guid\":\"7729394A431342D696B831A9578F47C8\",\"entries\":null,\"vendor\":{\"id\":31,\"name\":\"Fisher Scientific\",\"link\":\"https://www.fishersci.com/us/en/home.html\",\"is_requested\":false,\"image\":{\"source\":\"/img/vendors/placeholder.png\",\"placeholder\":\"/img/vendors/placeholder.png\"}}}},\"1\":{\"type\":\"notes\",\"mutability\":\"\",\"data\":{\"blocks\":[{\"data\":[],\"depth\":0,\"entityRanges\":[],\"inlineStyleRanges\":[],\"key\":\"v6d1\",\"text\":\"We sample for FCM in triplicate, as most samples can only be completely defrosted and analyzed once.\",\"type\":\"unstyled\"}],\"entityMap\":{}}}}}","data":null,"protocol_id":14292,"case_id":0,"critical_ids":"","duration":0,"original_id":641249,"number":"1","cases":[],"critical":null},{"id":650616,"guid":"A89B7D381CB74C88B99AEA9D6364913F","previous_id":650642,"previous_guid":"29EA24B695C94688B937009D1A87ECF7","section":"Sample preservation","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"6iesh\",\"text\":\"Using a sterile 10-ml syringe (see bleaching protocol in Step 2 if syringe is being reused) and the prepared filter cartridge, aliquot 1.5 mL of your 20 um-filtered seawater sample into each cryovial. Invert to mix. Label your cryovials with a ultra fine point Sharpie marker.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":14292,"case_id":0,"critical_ids":"","duration":0,"original_id":641267,"number":"4","cases":[],"critical":null},{"id":650617,"guid":"272FB74AF5E046A5859B04FEDB4C35EC","previous_id":650616,"previous_guid":"A89B7D381CB74C88B99AEA9D6364913F","section":"Sample preservation","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"9fv00\",\"text\":\"Incubate at room temperature in the dark for 15 minutes.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]},{\"key\":\"bfn6p\",\"text\":\" \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[{\"key\":0,\"offset\":0,\"length\":1}],\"data\":[]}],\"entityMap\":{\"0\":{\"type\":\"duration\",\"mutability\":\"MUTABLE\",\"data\":{\"duration\":\"900\",\"guid\":\"7590AD3B6A164427B1EA556C85FD5E23\",\"id\":\"17645\",\"label\":\"fixation\"}}}}","data":null,"protocol_id":14292,"case_id":0,"critical_ids":"","duration":0,"original_id":641268,"number":"5","cases":[],"critical":null},{"id":650618,"guid":"9864E0DD6F8E49E6BC9CF202F98B837E","previous_id":650617,"previous_guid":"272FB74AF5E046A5859B04FEDB4C35EC","section":"Sample preservation","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"drj0m\",\"text\":\"Snap-freeze the samples in liquid nitrogen. Store at -80°C.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":{}}","data":null,"protocol_id":14292,"case_id":0,"critical_ids":"","duration":0,"original_id":641269,"number":"6","cases":[],"critical":null},{"id":650622,"guid":"E8271277CFE44034A6FA56B7C48CC2AF","previous_id":650615,"previous_guid":"C3A47034441A4E12B21E4FF973E96D81","section":"Filtration preparation","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"101nq\",\"text\":\"Clean Swinnex filter holders by soaking in 10% bleach for 1-24 hrs. Then rinse thoroughly with MilliQ water. Allow to dry completely or use immediately. Syringes can also be bleached in the same manner and reused to reduce plastic waste.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"bold\",\"offset\":72,\"length\":17}],\"entityRanges\":[],\"data\":{}},{\"key\":\"irau\",\"text\":\" \",\"type\":\"atomic\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[{\"key\":0,\"offset\":0,\"length\":1}],\"data\":{}},{\"key\":\"c7vs4\",\"text\":\"\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"78otq\",\"text\":\" \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[{\"key\":1,\"offset\":