/avatars\/002.png"},"badges":[],"verified":0,"is_verified_user":false,"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false},{"name":"Feng Chen","affiliation":"Washington University, Saint Louis","affiliations":[],"username":"feng-chen1","note":"","link":null,"image":{"source":"\/img\/avatars\/006.png","placeholder":"\/img\/avatars\/006.png"},"badges":[],"verified":1,"is_verified_user":true,"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false}],"versions":[],"groups":[],"is_owner":1,"has_subprotocols":0,"is_subprotocol":0,"is_bookmarked":0,"can_claim_authorship":0,"can_accept_authorship":0,"can_be_copied":1,"can_remove_fork":1,"fork_id":null,"url":"https:\/\/www.protocols.io\/view\/wu-sc-prep-protocol-for-solid-tumors-v2-1-bsnqnddw","forks_count":{"private":0,"public":0},"access":{"can_view":1,"can_remove":0,"can_add":0,"can_edit":0,"can_publish":0,"can_get_doi":0,"can_share":1,"can_move":1,"can_move_outside":1,"can_transfer":1,"can_download":1,"is_locked":0},"guid":"C6A06F70753D11EBA4EB8981B5FF8412","state_version_id":54,"steps":[{"id":1131592,"guid":"3097DB60753F11EBA4EB8981B5FF8412","previous_id":null,"previous_guid":null,"modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"D0E59ED3F17049CA83F67C49C6B17C2B","order_id":1,"type_id":6,"title":"Section","source":{"title":"Reagents"}},{"id":1054724,"guid":"879F58106BA94217B225A2A5BC69B5FC","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">-<\/div><div class = \"text-block\"><a href=\"https:\/\/www.miltenyibiotec.com\/_Resources\/Persistent\/3136316fa227c4b10d8821618b1678f6df7d59e3\/DS_Tumor%20Dissociation%20Kit_h.pdf\" style = \"text-decoration:underline;color:blue;cursor:pointer;\"><span style = \":;\">Human tumor dissociation kit<\/span><\/a><\/div><div class = \"text-block\">(Miltenyi Biotec #130-095-929)<\/div><div class = \"text-block\">-PBS<\/div><div class = \"text-block\">-PBS, 0.5% BSA (keep at 4\u00b0C)<\/div><div class = \"text-block\">-(optional) Red Blood Cell (RBC) Lysis Solution ACK buffer Fisher (A1049201)<\/div><div class = \"text-block\">-Falcon 15mL\/50mL Conical Centrifuge Tubes (Corning Life Sciences DL, Fisher Scientific)<\/div><div class = \"text-block\">-Large filter: Fisherbrand Cell Strainers. Nylon mesh 40um (Fischer Scientific, Cat # 22-363-547)<\/div><div class = \"text-block\">-Smaller filter: PluriSelect Mini-Strainer 40 uM (Cat #43-10040-60)<\/div><div class = \"text-block\">-Petri dishes <\/div><div class = \"text-block\">-LoBind Protein or Genomic Microcentrifuge Tubes, Eppendorf\u00ae, Volume=2 ml Fisher 13-698-792<\/div><div class = \"text-block\">-All steps should be performed under sterile conditions<\/div><div class = \"text-block\">-For tissue fragments >1000mg, scale up reagent volumes (volumes listed are for 1 g tissue)<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1131593,"guid":"593C5E10753F11EBA4EB8981B5FF8412","previous_id":1131592,"previous_guid":"3097DB60753F11EBA4EB8981B5FF8412","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"F0D1B623F5E4477E81262E368ACF29F2","order_id":1,"type_id":6,"title":"Section","source":{"title":"Octo Dissociation"}},{"id":1054724,"guid":"831FED7BE1B84310B2A4FE4E451A9971","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Mince tissue into small pieces with razor blade in petri dish. <\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1131595,"guid":"813DEA00753F11EBA4EB8981B5FF8412","previous_id":1131593,"previous_guid":"593C5E10753F11EBA4EB8981B5FF8412","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"3CD28B76602F4626A62423F6F56DA9CF","order_id":1,"type_id":6,"title":"Section","source":{"title":"Octo Dissociation"}},{"id":1054724,"guid":"14400128603948DBB2D63036494908A3","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Resuspend tissue in enzyme mix (2.5 mL DMEM to 100 uL of Enzyme H, 50 uL Enzyme R, and 12.5 uL enzyme A) in C-Tube<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1131596,"guid":"865F3CF0753F11EBA4EB8981B5FF8412","previous_id":1131595,"previous_guid":"813DEA00753F11EBA4EB8981B5FF8412","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"A93099612F284710A4AFD7D41A935EE8","order_id":1,"type_id":6,"title":"Section","source":{"title":"Octo Dissociation"}},{"id":1054724,"guid":"0F2394BAFFDF4EDB821339E70325D644","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Incubate on gentleMACS\u2122 Octo Dissociator -  37h_TDK_1 setting (incubation can be shortened if tissue is very soft ~20-25 min). Check sample every 10-15 minutes. <\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1131597,"guid":"AD8F4810753F11EBA4EB8981B5FF8412","previous_id":1131596,"previous_guid":"865F3CF0753F11EBA4EB8981B5FF8412","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"30BF8474E5F14AE785BC7E9364A96E7D","order_id":1,"type_id":6,"title":"Section","source":{"title":"Octo Dissociation"}},{"id":1054724,"guid":"3E6C3F3F4E054855B42A400C24795B11","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">After dissociation, filter cells through 40um filter and spin down in 15ml tube at 450g for 5 min at 4 degrees<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1131598,"guid":"B3CA2150753F11EBA4EB8981B5FF8412","previous_id":1131597,"previous_guid":"AD8F4810753F11EBA4EB8981B5FF8412","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"6E39261EF66D45B2BEA086E65D3160B7","order_id":1,"type_id":6,"title":"Section","source":{"title":"Octo Dissociation"}},{"id":1054724,"guid":"9C293A1169654F3CB33E01030DA106B9","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">If there is any red visible in the cell pellet proceed to ACK RBC lysis step below (Refer to bottom of protocol for examples).<\/div><div class = \"text-block\">RBC ACK Lysis Solution<\/div><div class = \"text-block\">-Slowly resuspend cell pellet in 500 uL-5mL (5ml for larger pellets) of RBC ACK Lysis Solution<\/div><div class = \"text-block\">-Incubate on ice for 30 sec to 5 min (longer incubation for more RBCs)<\/div><div class = \"text-block\">-Quench reaction with 10mL of BSA\/PBS buffer<\/div><div class = \"text-block\">-Centrifuge at 450g 5min at 4 degrees <\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1131599,"guid":"CABAEED0753F11EBA4EB8981B5FF8412","previous_id":1131598,"previous_guid":"B3CA2150753F11EBA4EB8981B5FF8412","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"E61E37BE99754F20B58C23BB9626D054","order_id":1,"type_id":6,"title":"Section","source":{"title":"Octo Dissociation"}},{"id":1054724,"guid":"C10B9FF1B76242438D1DE9908E767EDF","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Do 1-3 1mL washes of 0.5% BSA in PBS on ice (spin at 450g 5min at 4 degrees) to remove dead cells and debris. Do washes in 2mL tube. After each wash aspirate off BSA\/PBS and leave ~50-100 uL of BSA\/PBS buffer. Resuspend cells using 1000 uL wide-bore pipette tip and count 10 uL of sample using a hematocytometer and trypan blue.<\/div><div class = \"text-block\">After first wash take 10 uL of sample and visualize under microscope (for smaller amounts take 2 uL of sample and 8 uL of wash buffer). If after 1 wash there are too many dead cells \u2013 consider using dead cell removal kit or debris removal kit: <\/div><div class = \"text-block\">Dead Cell Removal: <\/div><div class = \"text-block\"><a href=\"https:\/\/www.miltenyibiotec.com\/US-en\/products\/macs-sample-preparation\/removal-reagents\/dead-cell-removal-kit.html\" style = \"text-decoration:underline;color:blue;cursor:pointer;\"><span style = \":;\">https:\/\/www.miltenyibiotec.com\/US-en\/products\/macs-sample-preparation\/removal-reagents\/dead-cell-removal-kit.html<\/span><\/a><\/div><div class = \"text-block\">Debris Removal: <\/div><div class = \"text-block\"><a href=\"https:\/\/www.miltenyibiotec.com\/US-en\/products\/macs-sample-preparation\/removal-reagents\/debris-removal-solution.html\" style = \"text-decoration:underline;color:blue;cursor:pointer;\"><span style = \":;\">https:\/\/www.miltenyibiotec.com\/US-en\/products\/macs-sample-preparation\/removal-reagents\/debris-removal-solution.html<\/span><\/a><\/div><div class = \"text-block\">oNote: For Debris removal \u2013 use slow speed 450-500 g for all spinning steps.<\/div><div class = \"text-block\">If cell suspension looks pretty clean after first wash, then second wash should be clean to load. <\/div><div class = \"text-block\">oIf you have approximately 400-2000 cells\/uL then continue on to GEMs if doing single cell. Load 20,000 cells. <\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0}],"document":"","materials":[],"description":"<div class = \"text-blocks\"><div class = \"text-block\">Single-cell Isolation for solid tumors  Ding Lab 2021<\/div><\/div>","changed_on":1614278670}