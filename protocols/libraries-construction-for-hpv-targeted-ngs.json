protocols":[]},"created_on":1623709817,"resolve_on":0,"resolved_user":{"name":" ","affiliation":null,"affiliations":[],"username":null,"note":null,"link":null,"image":{"source":null,"placeholder":null},"badges":[],"verified":0,"is_verified_user":false,"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false},"shared":false}}],"is_owner":1,"has_subprotocols":0,"is_subprotocol":0,"is_bookmarked":0,"can_claim_authorship":0,"can_accept_authorship":0,"can_be_copied":1,"can_remove_fork":1,"fork_id":null,"url":"https:\/\/www.protocols.io\/view\/libraries-construction-for-hpv-targeted-ngs-bvttn6nn","forks_count":{"private":0,"public":0},"access":{"can_view":1,"can_remove":0,"can_add":0,"can_edit":0,"can_publish":0,"can_get_doi":0,"can_share":1,"can_move":1,"can_move_outside":1,"can_transfer":1,"can_download":1,"is_locked":0},"guid":"2C2BFF80CD5411EB826C038820FF6507","state_version_id":252,"steps":[{"id":1196016,"guid":"ED22B0C0CD5511EB826C038820FF6507","previous_id":null,"previous_guid":null,"modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"A5AB0961A7FE41BFBC52185A0499C9E9","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"BEAAB687ED914A8798760C44833C397F","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">A first PCR with PGMY09\/11primers was performed using 50\u03bcl mixture containing 3mM MgCl2, 10 \u03bcmol of each primer, 1.5 mM of dNTP (dATP, dCTP, TTP, dGTP), 5\u03bcl of the Taq DNA polymerase buffer, 1U of Taq DNA Polymerase, and 10 \u03bcl of DNA preparation was aliquoted. The PCR cycling parameters were composed of a 10 minutes initial denaturation at 94\u00b0C, followed by 30 amplification cycles of 30 s at 94\u00b0C, 1 min at 50\u00b0C and 1 min at 72\u00b0C, and a final extension step for 7 minutes at 72\u00b0C.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1196017,"guid":"ED23E940CD5511EB826C038820FF6507","previous_id":1196016,"previous_guid":"ED22B0C0CD5511EB826C038820FF6507","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"BDB6CB7B21604D399832D863BDE8F1DC","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"18F66936F68A43BCA384828EEB75F2FA","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">2.5 \u00b5l from the PYGMY PCR were amplified by a second PCR using 25\u00b5l mixture. PCR mixture contained 12.5\u00b5L Kapa Mix(Kapa Taq Extra Hot Start Mix, TXHSRMK Roche), 10\u00b5l of each designed primers: (P5-GP5+:\tTCGTCGGCAGCGTCAGATGTGTATAAGAGACAG TTT GTT ACT GTG GTA GAT ACCAC\/ P7-GP6+:\tGTCTCGTGGGCTCGGAGATGTGTATAAGAGACAG GAA AAA TAA ACT GTA AAT CAT ATT). The PCR cycling parameters were composed of a 5 minutes of initial denaturation at 95\u00b0C, followed by 14 amplification cycles of 30 seconds at 94\u00b0C, 1 minutes at 55\u00b0C and 1 minutes at 72\u00b0C, and a final extension step for 5 minutes at 72\u00b0C.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1196018,"guid":"ED24D3A0CD5511EB826C038820FF6507","previous_id":1196017,"previous_guid":"ED23E940CD5511EB826C038820FF6507","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"BBFB9D1CD90F408DBC00CC8059DC780F","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"0D6A6AD14FD84794A0D2E0A054F85352","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">The size of PCR products (~200 pub) was evaluated with the 2100 Bio analyzer system (Cat. no. G2940CA; Agilent Technologies, Santa Clara, CA, USA).<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1196019,"guid":"ED25BE01CD5511EB826C038820FF6507","previous_id":1196018,"previous_guid":"ED24D3A0CD5511EB826C038820FF6507","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"83156477777A43F69814BEA6A9EA362E","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"47118F98F290439DBAD2A4AB5D54B1FD","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">samples were purified using the Agent court\u00ae Apure\u00ae XP beads (Cat. no. A63881; Beckman Coulter Genomics, Danvers, MA, USA).<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1196020,"guid":"ED26F680CD5511EB826C038820FF6507","previous_id":1196019,"previous_guid":"ED25BE01CD5511EB826C038820FF6507","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"31F69C0DEE0A41DFA5A242D455A11AFC","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"9306E1D7937A41C684BAEBA4F52CDABA","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">96 different indexes (NextEra XT Index Kit) were added using a PCR reaction (40\u00b5l). PCR mixture contained <\/div><div class = \"text-block\">5\u00b5l of the previous  purified PCR product, 25 \u00b5l of Kapa Mix and 5\u00b5l of each index. The PCR cycling parameters were composed of a  3 minutes of initial denaturation at 95\u00b0C, followed by 8 amplification cycles of 30 seconds at 94\u00b0C, 1 minutes at 55\u00b0C and 30 seconds at 72\u00b0C, and a final extension step for 5 minutes at 72\u00b0C.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1196021,"guid":"ED285610CD5511EB826C038820FF6507","previous_id":1196020,"previous_guid":"ED26F680CD5511EB826C038820FF6507","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"140B40966D3F45B6BC334E75C8683C32","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"CDC8424809DE4225BA81C95972623887","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Libraries were purified<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1196022,"guid":"ED2A51E0CD5511EB826C038820FF6507","previous_id":1196021,"previous_guid":"ED285610CD5511EB826C038820FF6507","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"7E2EF97B76E244F0AC93C05CB58A67CC","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"EF5AF5B6F6514151A7F673623E714992","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Librairies were quantified using Real Time PCR.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1196023,"guid":"ED2C4DB0CD5511EB826C038820FF6507","previous_id":1196022,"previous_guid":"ED2A51E0CD5511EB826C038820FF6507","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"0B574A4440334A8B97A98E8C832FA2D9","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"9C3F3B8CE8E04903AF39AD9EFC913501","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Librairies were pooled at equimolar ratios (4nM) to yield one sequencing sample.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1196024,"guid":"ED2E7091CD5511EB826C038820FF6507","previous_id":1196023,"previous_guid":"ED2C4DB0CD5511EB826C038820FF6507","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"CB3D6908955C4F3C91D760CC85C8F5A5","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"D0566520403040BABDC6539FB1F2699B","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">In preparation for cluster generation and sequencing, pooled libraries are denatured with NaOH, diluted with hybridization buffer, and then heat denatured before MiSeq sequencing. The sequencing was performed using the Miseq V2 Kit.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0}],"document":"","materials":[],"description":"<div class = \"text-blocks\"><div class = \"text-block\">This protocol allow the construction of libraries for targeted NGS. Customized primers were designed and indexes were used to permit a one run sequencing.<\/div><\/div>","changed_on":1623709817}