uration":0},{"id":1111255,"guid":"22C2E7405C3611EBAE7D0D411F390185","previous_id":1111254,"previous_guid":"DE9789A05C3411EBAE7D0D411F390185","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"5A9917FBD86848608EB8B6B05EB6D18B","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"759689CC16E3438F812C750190177A8C","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Tamp the soil down firmly with your hands. Trying to make as level of a surface as possible.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1111256,"guid":"2FF029F05C3611EBAE7D0D411F390185","previous_id":1111255,"previous_guid":"22C2E7405C3611EBAE7D0D411F390185","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"E2447D0D440646449F2C4EFC773297A5","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"2A730423810847B8808B378C6F0990A1","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Using the super fine (0.5 GPM @ 40 PSI) fogg-IT drench nozzle, soak the soil until saturated to remove any air bubbles.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1111257,"guid":"79EF30005C3611EBAE7D0D411F390185","previous_id":1111264,"previous_guid":"B63E39605C3711EBAE7D0D411F390185","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"1775DDBDBF8C4784A0E7E562EEFA49DB","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"4DC1C0F4DA674D6D8D12306AFD09BFEA","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span>Pour 50-300 seeds from stock vial into a pour boat.  Using the pour boat, carefully sprinkle the seeds on top of the soil.  Sprinkling them in a spiral shape starting on the outside and working in to ensure even coverage across the pot and even spacing of seeds. Seed 5 to 10x more seeds than will be transplanted. <\/span><span style = \"font-weight:bold;\">Do not cover seeds with soil.<\/span><\/div><\/div>"}},{"id":1054725,"guid":"D1271F314E65413A921ED849EDACED45","order_id":2,"type_id":26,"title":"notes","source":{"id":0,"parent_id":0,"uri":"","title":"","body":"<div class = \"text-blocks\"><div class = \"text-block\">Nicotiana tabacum seed requires light in order to germinate. The seeds are very miniscule and need to be scattered on the soil surface in order to avoid being covered.<\/div><\/div>","created_on":0,"changed_on":0,"creator":{"name":" ","affiliation":null,"affiliations":[],"username":"","note":null,"link":null,"image":{"source":"","placeholder":""},"badges":[],"verified":0,"is_verified_user":false,"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false},"comments":[]}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":[{"id":12,"title":"Critical step","icon":"CriticalIcon"}],"critical_id":null,"duration":0},{"id":1111258,"guid":"14DE6D105C3711EBAE7D0D411F390185","previous_id":1111262,"previous_guid":"6D84ABA05C3711EBAE7D0D411F390185","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"22347882EE58496B890ED612D5719C75","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"3719514EB5D8416EA71B6BE15F615370","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Place azalea pots in a propagation tray with no drain holes.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1111259,"guid":"337F4EB05C3711EBAE7D0D411F390185","previous_id":1111258,"previous_guid":"14DE6D105C3711EBAE7D0D411F390185","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"BB3B22478CAD415894C3C4AED4F5A498","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"F9FC1FFA3D9841618DC91347DE6659C7","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Fill the tray with approximately 1 inch of water.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1111261,"guid":"662046805C3711EBAE7D0D411F390185","previous_id":1111259,"previous_guid":"337F4EB05C3711EBAE7D0D411F390185","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"EB6C3E32C8C345FC81D6CC60A8BD0E15","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"A5879EA91D6943898642809399AF2D06","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Cover with a humidity dome. Leave the vents on the humidity dome half open, half closed.<\/div><div style = \"text-align :; float : ;\"><img style = \"\" src = \"https:\/\/s3.amazonaws.com\/protocols-files\/files\/czncbh8gf.jpg\" \/><\/div><div style = \"text-align :; float : ;\"><img style = \"\" src = \"https:\/\/s3.amazonaws.com\/protocols-files\/files\/czngbh8gf.jpg\" \/><\/div><\/div>"}},{"id":1054725,"guid":"B43735D86C0C4FB38EA1585689862915","order_id":2,"type_id":1,"title":"description","source":{"description":"<div style = \"text-align :; float : ;\"><img style = \"\" src = \"https:\/\/s3.amazonaws.com\/protocols-files\/files\/czncbh8gf.jpg\" \/><\/div>"}},{"id":1054726,"guid":"30B4E164B05341D0B973B2C24AF4D23C","order_id":3,"type_id":1,"title":"description","source":{"description":"<div style = \"text-align :; float : ;\"><img style = \"\" src = \"https:\/\/s3.amazonaws.com\/protocols-files\/files\/czngbh8gf.jpg\" \/><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1111262,"guid":"6D84ABA05C3711EBAE7D0D411F390185","previous_id":1111257,"previous_guid":"79EF30005C3611EBAE7D0D411F390185","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"CC8ED66142364503A5CAED2D27531197","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"81F55AF15A374C76ADE2384C4BAEDDAD","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Label pot twice. Once with sharpie on a plastic stake in the pot and once on the exterior of the pot using a sharpie and labelling tape or a laser printed label.  <\/div><\/div>"}},{"id":1054725,"guid":"9092E908562A494688936DD49D901225","order_id":2,"type_id":26,"title":"notes","source":{"id":0,"parent_id":0,"uri":"","title":"","body":"<div class = \"text-blocks\"><div class = \"text-block\">Laser printed labels are more robust.  Handwritten sharpie labels tend to fade over the growing season.<\/div><\/div>","created_on":0,"changed_on":0,"creator":{"name":" ","affiliation":null,"affiliations":[],"username":"","note":null,"link":null,"image":{"source":"","placeholder":""},"badges":[],"verified":0,"is_verified_user":false,"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false},"comments":[]}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1111264,"guid":"B63E39605C3711EBAE7D0D411F390185","previous_id":1111256,"previous_guid":"2FF029F05C3611EBAE7D0D411F390185","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"0973001A902F4D78B4C4730DC41A0B67","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"151F4E9AF5AC4D22837921D45F22F270","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Tamp the soil lightly and relevel.  If the soil settles more than about an inch, add additional soil and rewet.  Do not overpack the soil.<\/div><\/div>"}},{"id":1054725,"guid":"14C33330A805469BA62857E870B79ACF","order_id":2,"type_id":26,"title":"notes","source":{"id":0,"parent_id":0,"uri":"","title":"","body":"<div class = \"text-blocks\"><div class = \"text-block\">A level surface is important because tobacco seeds are tiny and round.  If surface is not level, seeds will roll towards each other or the sides of the pot and cause overcrowding during germination.<\/div><\/div>","created_on":0,"changed_on":0,"creator":{"name":" ","affiliation":null,"affiliations":[],"username":"","note":null,"link":null,"image":{"source":"","placeholder":""},"badges":[],"verified":0,"is_verified_user":false,"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false},"comments":[]}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1111265,"guid":"26A65BA05C3911EBAE7D0D411F390185","previous_id":1111261,"previous_guid":"662046805C3711EBAE7D0D411F390185","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"90922F5BC12A4D1D9F8F64E4F05AACFA","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"F0335E52211D4238A28B544D72DB029E","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Grow at <\/div><div class = \"text-block\"> , <\/div><div class = \"text-block\"> , 300-750PAR, and light cycle 12 hours on\/12 hours off. Check daily and add water to keep tray filled with approximately 1\" water.<\/div><\/div>"}},{"id":1054725,"guid":"B32E9D805C3911EBAE7D0D411F390185","order_id":2,"type_id":24,"title":"temperature","source":{"temperature":23,"unit":"\u00b0C","title":""}},{"id":1054726,"guid":"261C21B910184FAFAB49D2C33319F57D","order_id":3,"type_id":26,"title":"notes","source":{"id":0,"parent_id":0,"uri":"","title":"","body":"<div class = \"text-blocks\"><div class = \"text-block\">Do not allow soil to dry out during germination. The water level can be maintained at 1\u201d of water in the tray without any disease\/damping off issues.<\/div><\/div>","created_on":0,"changed_on":0,"creator":{"name":" ","affiliation":null,"affiliations":[],"username":"","note":null,"link":null,"image":{"source":"","placeholder":""},"badges":[],"verified":0,"is_verified_user":false,"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false},"comments":[]}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":[{"id":12,"title":"Critical step","icon":"CriticalIcon"}],"critical_id":null,"duration":0},{"id":1111266,"guid":"F42468105C3911EBAE7D0D411F390185","previous_id":1111265,"previous_guid":"26A65BA05C3911EBAE7D0D411F390185","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"9B22F5BB461643C5A137E3D163BC44DE","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"4E8FE0388EF148D89E1C153B67A5163A","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Transplant seedlings when they have one to two true leaves.<\/div><\/div>"}},{"id":1054725,"guid":"BC6F87761475428D9CAF5569A59104FD","order_id":2,"type_id":26,"title":"notes","source":{"id":0,"parent_id":0,"uri":"","title":"","body":"<div class = \"text-blocks\"><div class = \"text-block\">Seedling will be ready to transplant in approximately 10 to 14 days.  Do not wait too long to transplant. The root system begins to develop rapidly after the first true leaves.  When the true leaves first appear, there is typically one branch in the root system making it easy to remove from the initial media if care is taken. Damaging roots during transplant will drastically affect development and result in non-uniform plants.<\/div><\/div>","created_on":0,"changed_on":0,"creator":{"name":" ","affiliation":null,"affiliations":[],"username":"","note":null,"link":null,"image":{"source":"","placeholder":""},"badges":[],"verified":0,"is_verified_user":false,"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false},"comments":[]}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":[{"id":12,"title":"Critical step","icon":"CriticalIcon"}],"critical_id":null,"duration":950400}],"document":"","materials":[],"description":"<div class = \"text-blocks\"><div class = \"text-block\">Protocol for planting, germinating, and growing tobacco seedlings in the UIUC RIPE greenhouse.  Has been verified on Petite Havana and Samson cultivars.  <\/div><div class = \"text-block\">For additional information on growing tobacco for research purposes refer to UIUC RIPE greenhouse guide \"Growing Tobacco (Nicotiana tabacum) under Greenhouse Conditions, RIPE Project UIUC Field and Greenhouse, 2020\"<\/div><\/div>","changed_on":1612997804}