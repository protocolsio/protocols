de":"dexf3fjn","version_class":0,"parent_id":null,"parent_uri":null,"is_same_owner":false,"is_parent_public":false,"has_pending_merge_request":false,"has_approved_merge_request":false,"merge_request":null},"creator":{"name":"Andreas Novotny","username":"andreas-novotny1","affiliation":"University of British Columbia","affiliation_url":"ubc.ca","link":"https://linktr.ee/andreasnovotny","is_verified_user":true,"image":{"source":"https://content.protocols.io/files/qnxpcbk77.jpg","webp_source":null,"placeholder":"https://content.protocols.io/files/qnxpcbk77.jpg","webp_placeholder":null}},"image":{"source":"https://content.protocols.io/files/qm9pcbk77.jpg","webp_source":null,"placeholder":"https://content.protocols.io/files/qm9pcbk77.jpg","webp_placeholder":null},"uri":"12s-rrna-gene-metabarcoding-library-prep-dual-pcr-dexf3fjn","private_link":null,"blind_link":null}}],"created_on":1718826173,"creator":{"name":"Andreas Novotny","affiliation":"University of British Columbia","affiliation_url":"ubc.ca","username":"andreas-novotny1","link":"https://linktr.ee/andreasnovotny","image":{"source":"https://content.protocols.io/files/qnxpcbk77.jpg","placeholder":"https://content.protocols.io/files/qnxpcbk77.jpg","webp_source":"https://content.protocols.io/files/qnxncbk77.webp"},"badges":[{"id":3,"name":"Power author!","image":{"source":"/img/badges/silver.svg","placeholder":"/img/badges/silver.svg"}},{"id":6,"name":"Science accelerator","image":{"source":"/img/badges/accelerator.svg","placeholder":"/img/badges/accelerator.svg"}}],"affiliations":[{"affiliation":"University of British Columbia","url":"ubc.ca","job_title":"Postdoctoral researcher","is_default":true},{"affiliation":"Hakai Institute","url":"hakai.org","job_title":"Postdoctoral researcher","is_default":false},{"affiliation":"Stockholm University","url":"su.se","job_title":"","is_default":false}]},"cross_cloud_origin":null,"description":"{\"blocks\":[{\"key\":\"643i7\",\"text\":\"This is a collection of protocols used to analyze environmental DNA (eDNA) of seawater samples.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"ecm4t\",\"text\":\"As part of the Hakai Institute Ocean Observing Program, biomolecular samples have been collected weekly from 0 m to near bottom (260 m), to genetically characterize plankton communities in the Northern Salish Sea since 2015. These protocols are developed to work across all domains of life, from viruses to prokaryotes to eukaryotes, allowing for both amplicon sequencing and shotgun sequencing.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"a1uf\",\"text\":\"\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"ece44\",\"text\":\"This pipeline includes the following steps:\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"bold\",\"offset\":0,\"length\":43},{\"style\":\"italic\",\"offset\":0,\"length\":43}],\"entityRanges\":[],\"data\":{}},{\"key\":\"6vks7\",\"text\":\"1. Seawater filtration: Our standard protocol for filtering seawater on sterivex filters. Alternatives may be used.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"bold\",\"offset\":0,\"length\":24}],\"entityRanges\":[],\"data\":{}},{\"key\":\"2hp1t\",\"text\":\"\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"cv8o1\",\"text\":\"2. DNA extraction from sterivex filters: Here we offer two alternatives:\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"bold\",\"offset\":0,\"length\":41}],\"entityRanges\":[],\"data\":{}},{\"key\":\"9hjnl\",\"text\":\"The robust Phenol Chloroform extraction protocol is used in our long-term monitoring program.\",\"type\":\"unordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"ehj2b\",\"text\":\"A kit alternative requires less handling of toxic chemicals, and are used for stand-alone projects.\",\"type\":\"unordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"1cjn9\",\"text\":\"\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"e2ar7\",\"text\":\"3. DNA metabarcoding, library prep, and sequencing: With the options of analyzing different marker genes:\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"bold\",\"offset\":0,\"length\":34},{\"style\":\"bold\",\"offset\":35,\"length\":17}],\"entityRanges\":[],\"data\":{}},{\"key\":\"7esgk\",\"text\":\"18S rRNA nuclear gene: Diversity of Eukaryotes. (Balzano et al 2015)\",\"type\":\"unordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"6l5bc\",\"text\":\"COI mitochondrial gene: Diversity of marine invertebrates. (Leray et al 2013)\",\"type\":\"unordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"4pf8n\",\"text\":\"12S rRNA mitochondrial gene: Diversity of fish. (Miya et al 2015)\",\"type\":\"unordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"8rvpp\",\"text\":\"\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"58f3k\",\"text\":\"4. Bioinformatic analyses\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"bold\",\"offset\":0,\"length\":25}],\"entityRanges\":[],\"data\":{}},{\"key\":\"b9d3n\",\"text\":\"Examples bioinformatic analysis for the different genes can be found on our GitHub page: https://github.com/hakaigenomics\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[{\"key\":0,\"offset\":89,\"length\":32}],\"data\":{}},{\"key\":\"cjbhc\",\"text\":\"\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"em95l\",\"