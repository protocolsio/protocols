l},{"id":1706219,"guid":"320E47B0070D11EE930EFF781E771D0B","previous_id":1706218,"previous_guid":"2FD93F40070D11EE930EFF781E771D0B","section":"\u003cp\u003eDNA Extraction\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"4t8c8\",\"text\":\"Heat samples to 100 degrees C for 15 minutes, or until tissues are thoroughly dissolved.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":83164,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4","cases":[],"critical":null},{"id":1706221,"guid":"340E5D70070D11EE930EFF781E771D0B","previous_id":1706230,"previous_guid":"AA9D5130070D11EE930EFF781E771D0B","section":"\u003cp\u003ePCR amplification\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"bid31\",\"text\":\"Create master mix. Scale depending on how many reactions you are conducting.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"7bumi\",\"text\":\"\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":83164,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"7","cases":[],"critical":null},{"id":1706229,"guid":"A63C6810070D11EE930EFF781E771D0B","previous_id":1706219,"previous_guid":"320E47B0070D11EE930EFF781E771D0B","section":"\u003cp\u003eDNA Extraction\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"e0brv\",\"text\":\"Add 50uL of 30% TRIS to each sample and centrifuge on high speed for 6 minutes.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":83164,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"5","cases":[],"critical":null},{"id":1706230,"guid":"AA9D5130070D11EE930EFF781E771D0B","previous_id":1706229,"previous_guid":"A63C6810070D11EE930EFF781E771D0B","section":"\u003cp\u003eDNA Extraction\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"aeigd\",\"text\":\"Withdraw 200uL of supernatant from each sample, store in 1mL tube. Use 1uL of this DNA solution for each reaction.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":83164,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"6","cases":[],"critical":null},{"id":1706231,"guid":"F6653060070D11EE930EFF781E771D0B","previous_id":1706221,"previous_guid":"340E5D70070D11EE930EFF781E771D0B","section":"\u003cp\u003ePCR amplification\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":true,"step":"{\"blocks\":[{\"key\":\"es8u9\",\"text\":\"To create the mix, for each reaction add in order:\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"f1va\",\"text\":\"- 17 uL of autoclaved water\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"fvmup\",\"text\":\"- 5 uL of GoTAQ buffer\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"ffgpr\",\"text\":\"- 0.5uL of dNTP\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"2p483\",\"text\":\"- 0.5uL of each primer\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"dbftr\",\"text\":\"- 0.5uL of TAQ\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":83164,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"7.1","cases":[],"critical":null},{"id":1706233,"guid":"4FB8ED00070E11EE930EFF781E771D0B","previous_id":1706231,"previous_guid":"F6653060070D11EE930EFF781E771D0B","section":"\u003cp\u003ePCR amplification\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"8fa1\",\"text\":\"Shake gently and add 24uL of this solution to DNA in a PCR plate.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":83164,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"8","cases":[],"critical":null},{"id":1706234,"guid":"66C1AF50070E11EE930EFF781E771D0B","previous_id":1706233,"previous_guid":"4FB8ED00070E11EE930EFF781E771D0B","section":"\u003cp\u003ePCR amplification\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"8q0b4\",\"text\":\"Thermal cycle; the PCR protocol will vary according to primers, but cycling between 60 and 94 degrees Celsius has been effective for our lab.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":83164,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"9","cases":[],"critical":null},{"id":1706235,"guid":"B6D89D50070E11EE930EFF781E771D0B","previous_id":1706234,"previous_guid":"66C1AF50070E11EE930EFF781E771D0B","section":"\u003cp\u003eGel electrophoresis\u003c/p\u003e","section_color":"#FFED92","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"af255\",\"text\":\"Prepare gel:\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":83164,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"10","cases":[],"critical":null},{"id":1706236,"guid":"CA8D79B0070E11EE930EFF781E771D0B","previous_id":1706235,"previous_guid":"B6D89D50070E11EE930EFF781E771D0B","section":"\u003cp\u003eGel electrophoresis\u003c/p\u003e","section_color":"#FFED92","section_duration":0,"is_substep":true,"step":"{\"blocks\":[{\"key\":\"9s70e\",\"text\":\"- Heat 3g of agarose in 200mL TAE buffer solution.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"415sa\",\"text\":\"- Once cooled to 55 degrees C, add 12uL SYBR Safe DNA Gel Stain\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"bt9k0\",\"text\":\"- Pour the gel onto the mold, add combs, and allow it to solidify for 15 minutes.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":83164,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"10.1","cases":[],"critical":null},{"id":1706237,"guid":"29C4EEE0070F11EE930EFF781E771D0B","previous_id":1706236,"previous_guid":"CA8D79B0070E11EE930EFF781E771D0B","section":"\u003cp\u003eGel electrophoresis\u003c/p\u003e","section_color":"#FFED92","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"1ikc1\",\"text\":\"Place solid gel into the electrophoresis chamber. Load PCR product from the plate into the wells, one per well, leaving a gap between each well. Add a ladder before the first and after the last well, or as desired.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":83164,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"11","cases":[],"critical":null},{"id":1706238,"guid":"7B3AF7B0070F11EE930EFF781E771D0B","previous_id":1706237,"previous_guid":"29C4EEE0070F11EE930EFF781E771D0B","section":"\u003cp\u003eGel electrophoresis\u003c/p\u003e","section_color":"#FFED92","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"323tn\",\"text\":\"Switch on the electrical current (around 90 volts is recomm