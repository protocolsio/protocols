ccept_authorship":0,"can_be_copied":1,"can_remove_fork":1,"fork_id":null,"url":"https:\/\/www.protocols.io\/view\/human-pluripotent-stem-cell-culture-br9cm92w","forks_count":{"private":0,"public":0},"access":{"can_view":1,"can_remove":0,"can_add":0,"can_edit":0,"can_publish":0,"can_get_doi":0,"can_share":1,"can_move":1,"can_move_outside":1,"can_transfer":1,"can_download":1,"is_locked":0},"guid":"9C832FE06AFF11EBA98D07E9020DF081","state_version_id":279,"steps":[{"id":1122360,"guid":"7F2F4B306B0011EBA98D07E9020DF081","previous_id":null,"previous_guid":null,"modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"FD84CAE212DB4DBEA8F99E106B45185D","order_id":1,"type_id":6,"title":"Section","source":{"title":"Thaw frozen cell from liquid nitrogen tank"}},{"id":1054724,"guid":"20FE1677FB1F48AFA81334F6A0842EE7","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><br\/><br\/><\/div>"}},{"id":1054725,"guid":"CF1C86C7B3B74D8094DDE06ECBF9E119","order_id":2,"type_id":19,"title":"safety","source":{"body":"<div class = \"text-blocks\"><div class = \"text-block\">IMPORTANT, ALWAYS WEAR SAFETY GOGGLES WHEN THAWING<\/div><div class = \"text-block\">CELLS!!<\/div><\/div>","link":""}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1122361,"guid":"92F0AAB06B0011EBA98D07E9020DF081","previous_id":1122360,"previous_guid":"7F2F4B306B0011EBA98D07E9020DF081","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"D1544ECBDF2A4D57A42CA246D59782F9","order_id":1,"type_id":6,"title":"Section","source":{"title":"Thaw frozen cell from liquid nitrogen tank"}},{"id":1054724,"guid":"0DF9ACC375B94B3090923155BF653D75","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Take a vial of frozen cells from liquid nitrogen tank. If you have many vials you need to take out of the tank and thaw at a time, you can put them on dry ice while you are looking for other vials.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1122362,"guid":"970CC2A06B0011EBA98D07E9020DF081","previous_id":1122361,"previous_guid":"92F0AAB06B0011EBA98D07E9020DF081","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"BCDA3CE4D19A4CAC88ED90A28FC6F48E","order_id":1,"type_id":6,"title":"Section","source":{"title":"Thaw frozen cell from liquid nitrogen tank"}},{"id":1054724,"guid":"2228AFA3DE864C04A9307782D58CD522","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Put the vials in <\/div><div class = \"text-block\"><span> water bath with gentle shaking. Do not at any point let water get above 2\/3 the height of the vial to prevent water from getting into the vial. <\/span><span style = \"font-weight:bold;\">Check the vial periodically.<\/span><span> Take vial out when there is only a small chunk of ice left. <\/span><\/div><br\/><\/div>"}},{"id":1054725,"guid":"BC2E87306B0011EBA98D07E9020DF081","order_id":2,"type_id":24,"title":"temperature","source":{"temperature":37,"unit":"\u00b0C","title":""}},{"id":1054726,"guid":"81D85BC0168042BB835694CFC2C19BF0","order_id":3,"type_id":19,"title":"safety","source":{"body":"<div class = \"text-blocks\"><div class = \"text-block\">This is the step a defective cryovial might explode! So be extra careful and always wear eye protection!<\/div><\/div>","link":""}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1122363,"guid":"9A62A4606B0011EBA98D07E9020DF081","previous_id":1122362,"previous_guid":"970CC2A06B0011EBA98D07E9020DF081","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"D93EF597282C4B9DAA457D6D62EC31BF","order_id":1,"type_id":6,"title":"Section","source":{"title":"Thaw frozen cell from liquid nitrogen tank"}},{"id":1054724,"guid":"C6F070F0328F4B81BE1861AD6BAD272A","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Spray the vial with <\/div><div class = \"text-block\">. Wipe dry. In the hood, open the vial and take the cells out of the vial with a P1000 into a 15 ml conical tube.<\/div><\/div>"}},{"id":1054725,"guid":"EF8B7C006B0011EBA98D07E9020DF081","order_id":2,"type_id":25,"title":"concentration","source":{"concentration":70,"unit":null,"title":"EtOH"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":[{"id":3,"title":"Pipetting","icon":"PipettingIcon"}],"critical_id":null,"duration":0},{"id":1122365,"guid":"9DF42F906B0011EBA98D07E9020DF081","previous_id":1122363,"previous_guid":"9A62A4606B0011EBA98D07E9020DF081","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"D7C74540D2ED4D30BD43404463DC1439","order_id":1,"type_id":6,"title":"Section","source":{"title":"Thaw frozen cell from liquid nitrogen tank"}},{"id":1054724,"guid":"361A36BFC7DF46058C4C63ED9D264A5A","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Slowly add <\/div><div class = \"text-block\"> to <\/div><div class = \"text-block\"> to the conical tube with gentle agitation.<\/div><div class = \"text-block\">Add the first ml drop wise. This is to reduce the osmotic pressure the cells will experience. Do not add thawed cells directly to fresh media!<\/div><\/div>"}},{"id":1054725,"guid":"107256006B0111EBA98D07E9020DF081","order_id":2,"type_id":3,"title":"amount","source":{"amount":5,"unit":"mL","title":""}},{"id":1054726,"guid":"0E0BDD506B0111EBA98D07E9020DF081","order_id":3,"type_id":3,"title":"amount","source":{"amount":10,"unit":"mL","title":"fresh E8"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":[{"id":3,"title":"Pipetting","icon":"PipettingIcon"}],"critical_id":null,"duration":0},{"id":1122366,"guid":"A24672B06B0011EBA98D07E9020DF081","previous_id":1122365,"previous_guid":"9DF42F906B0011EBA98D07E9020DF081","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"540511E5F97E40ED8F8C529AFA426260","order_id":1,"type_id":6,"title":"Section","source":{"title":"Thaw frozen cell from liquid nitrogen tank"}},{"id":1054724,"guid":"7661536567154F08A2ED2EB8298187A1","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Pellet cells in the conical tube at <\/div><div class = \"text-block\">. Take off supernatant.<\/div><div class = \"text-block\">Resuspend the cell pellet in appropriate amount of media.<\/div><\/div>"}},{"id":1054725,"guid":"2FF0C6606B0111EBA98D07E9020DF081","order_id":2,"type_id":30,"title":"centrifuge","source":{"centrifuge":"150","duration":180,"guid":"2FF0C6606B0111EBA98D07E9020DF081","id":0,"label":"","pause":null,"preset":[],"start":null,"temperature":"","temperatureUnit":10,"unit":34}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":180},{"id":1122367,"guid":"A74DFC106B0011EBA98D07E9020DF081","previous_id":1122366,"previous_guid":"A24672B06B0011EBA98D07E9020DF081","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"26B4C7854F5847B08E3C12DCFF12693B","order_id":1,"type_id":6,"title":"Section","source":{"title":"Thaw frozen cell from liquid nitrogen tank"}},{"id":1054724,"guid":"22CA0D320417480795B8CE019056D1D6","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Plate the cells onto a matrigel coated plate and put the plate back into the incubator.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":[{"id":1,"title":"Incubation","icon":"IncubationIcon"}],"critical_id":null,"duration":0},{"id":1122368,"guid":"AA5C9BA06B0011EBA98D07E9020DF081","previous_id":1122367,"previous_guid":"A74DFC106B0011EBA98D07E9020DF081","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"17F54249289B424F87DF88F3BB01B449","order_id":1,"type_id":6,"title":"Section","source":{"title":"Thaw frozen cell from liquid nitrogen tank"}},{"id":1054724,"guid":"5CF86BBF447D4A2BB8FBBD9FD17FCC59","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Cells should attach within an hour of plating.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1122369,"guid":"AD73C6B06B0011EBA98D07E9020DF081","previous_id":1122368,"previous_guid":"AA5C9BA06B0011EBA98D07E9020DF081","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"4F86D74CCD0243FB8D83FBC53AE8C857","order_id":1,"type_id":6,"title":"Section","source":{"title":"Thaw frozen cell from liquid nitrogen tank"}},{"id":1054724,"guid":"9B12B3D1783443538DC1FFD8B3B2CF1C","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Check cells the next day. Some floating dead cells are OK. The survival is normally 40-70% depending on the freeze.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1122372,"guid":"6135FEC06B0111EBA98D07E9020DF081","previous_id":1122369,"previous_guid":"AD73C6B06B0011EBA98D07E9020DF081","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"167130EC86E74861A68C070189064A62","order_id":1,"type_id":6,"title":"Section","source":{"title":"Regular maintenance of human pluripotent stem cells"}},{"id":1054724,"guid":"B5F6C3676351433A9EE63A313AF6A9BF","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Feed cells with E8 medium everyday. It is OK to double feed (feed twice the amount of media) and let the cells go without feeding for a couple days only when the cells are not too dense (eg. Right after a split).<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1122373,"guid":"6DDD48906B0111EBA98D07E9020DF081","previous_id":1122372,"previous_guid":"6135FEC06B0111EBA98D07E9020DF081","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"98B087CCBD59411C9A5A6EAF5BA62FD8","order_id":1,"type_id":6,"title":"Section","source":{"title":"Regular maintenance of human pluripotent stem cells"}},{"id":1054724,"guid":"BC16B5EFBE4744E8BCACD59FED8B493E","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">When cells are 70-90% confluent, they need to be split.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1122374,"guid":"7165FA206B0111EBA98D07E9020DF081","previous_id":1122373,"previous_guid":"6DDD48906B0111EBA98D07E9020DF081","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"2F2AE78FE13D46FA9ACEDBDE0EFD2437","order_id":1,"type_id":6,"title":"Section","source":{"title":"Regular maintenance of human pluripotent stem cells"}},{"id":1054724,"guid":"28BCFA67CE82439E85C50E979FF5DA01","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">To split cells, follow the steps below. The volume is for one well of a 6-well plate. Change the volume of solution according to your culture vessel.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1122375,"guid":"80E20ED06B0111EBA98D07E9020DF081","previous_id":1122374,"previous_guid":"7165FA206B0111EBA98D07E9020DF081","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"512E442633F24888AFD4E3EBCF88935F","order_id":1,"type_id":6,"title":"Section","source":{"title":"Regular maintenance of human pluripotent stem cells"}},{"id":1054724,"guid":"88765A701BB043FE9F611915DF542FB4","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Take off media and rinse with <\/div><div class = \"text-block\">. Take off the PBS+EDTA and add another <\/div><div class = \"text-block\">. Let the plate sit in the hood for <\/div><div class = \"text-block\">-<\/div><div class = \"text-block\">.<\/div><br\/><\/div>"}},{"id":1054725,"guid":"E53E80706B0111EBA98D07E9020DF081","order_id":2,"type_id":3,"title":"amount","source":{"amount":1,"unit":"mL","title":"PBS+EDTA (0.5 mM EDTA final concentration)"}},{"id":1054726,"guid":"D2A21AA06C4E11EBA796A714FA997B99","order_id":3,"type_id":3,"title":"amount","source":{"amount":1,"unit":"mL","title":" PBS+EDTA"}},{"id":1054727,"guid":"F0A9A0C06B0111EBA98D07E9020DF081","order_id":4,"type_id":4,"title":"duration","source":{"duration":180,"title":""}},{"id":1054728,"guid":"F4299FC06B0111EBA98D07E9020DF081","order_id":5,"type_id":4,"title":"duration","source":{"duration":300,"title":""}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":[{"id":3,"title":"Pipetting","icon":"PipettingIcon"},{"id":5,"title":"Wash","icon":"WashIcon"}],"critical_id":null,"duration":300},{"id":1122376,"guid":"859C14706B0111EBA98D07E9020DF081","previous_id":1122375,"previous_guid":"80E20ED06B0111EBA98D07E9020DF081","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"DCA050E0F3BD4EEBAE03517723F35383","order_id":1,"type_id":6,"title":"Section","source":{"title":"Regular maintenance of human pluripotent stem cells"}},{"id":1054724,"guid":"685CECC97AD04E3398091E875502D202","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Look under the microscope. The cells within a colony should start to separate from each other. If they are still attached to each other, wait for another <\/div><div class = \"text-block\"> -<\/div><div class = \"text-block\">  and check again.<\/div><br\/><\/div>"}},{"id":1054725,"guid":"755B35606C4F11EBA796A714FA997B99","order_id":2,"type_id":4,"title":"duration","source":{"duration":120,"title":""}},{"id":1054726,"guid":"7A8EB0C06C4F11EBA796A714FA997B99","order_id":3,"type_id":4,"title":"duration","source":{"duration":180,"title":""}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":[{"id":9,"title":"Imaging","icon":"ImagingIcon"}],"critical_id":null,"duration":180},{"id":1122377,"guid":"88DAC4B06B0111EBA98D07E9020DF081","previous_id":1122376,"previous_guid":"859C14706B0111EBA98D07E9020DF081","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"6641B06D47D545EE8045260EEC42321D","order_id":1,"type_id":6,"title":"Section","source":{"title":"Regular maintenance of human pluripotent stem cells"}},{"id":1054724,"guid":"A1E816C27415492980C4511D1C3F8E22","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Carefully take the PBS+EDTA off the plate. If the cells are incubated too long with EDTA, they might come off the plate at this step. So be careful not to suck up your cells.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1122378,"guid":"8C03A3006B0111EBA98D07E9020DF081","previous_id":1122377,"previous_guid":"88DAC4B06B0111EBA98D07E9020DF081","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"AB6CA625073945FBA2849DC9906DFB18","order_id":1,"type_id":6,"title":"Section","source":{"title":"Regular maintenance of human pluripotent stem cells"}},{"id":1054724,"guid":"960B80D6B89340418C74A2081E756197","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Resuspend cells with <\/div><div class = \"text-block\"> using a 5 ml pipette. Dislodge the cells only with media. DO NOT scrape cells off the plate with pipettes. Pipette cells up and down several times to break up cell clumps.<\/div><\/div>"}},{"id":1054725,"guid":"325E75906B0211EBA98D07E9020DF081","order_id":2,"type_id":3,"title":"amount","source":{"amount":3,"unit":"mL","title":"fresh E8"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":[{"id":3,"title":"Pipetting","icon":"PipettingIcon"}],"critical_id":null,"duration":0},{"id":1122379,"guid":"8F47D1806B0111EBA98D07E9020DF081","previous_id":1122378,"previous_guid":"8C03A3006B0111EBA98D07E9020DF081","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"C85695BC80AD435BBC6350AA9E0E6AB6","order_id":1,"type_id":6,"title":"Section","source":{"title":"Regular maintenance of human pluripotent stem cells"}},{"id":1054724,"guid":"2B9D95F8B0644E4FA3A6208FDD50E95B","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Aliquot resuspended cells into another plate with fresh E8.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1122380,"guid":"921A2B606B0111EBA98D07E9020DF081","previous_id":1122379,"previous_guid":"8F47D1806B0111EBA98D07E9020DF081","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"730D744B31BA4C3FA59211E0D11DCA2E","order_id":1,"type_id":6,"title":"Section","source":{"title":"Regular maintenance of human pluripotent stem cells"}},{"id":1054724,"guid":"28227C5131A24CCCA6B63825D60333CE","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">I normally split cells at series of different ratios (1:6, 1:12 and 1:20).<\/div><br\/><\/div>"}},{"id":1054725,"guid":"BFBAA9FD79DD4DC49A2C3F06BC334843","order_id":2,"type_id":26,"title":"notes","source":{"id":0,"parent_id":0,"uri":"","title":"","body":"<div class = \"text-blocks\"><div class = \"text-block\">A 1:6 split will normally give you 50-70% confluency after 2 days. This is good for electroporation. A 1:20 split will give you 70-90% confluency after 4-5 days and this is good for regular maintenance especially for double feeding over the weekend.<\/div><\/div>","created_on":0,"changed_on":0,"creator":{"name":"","affiliation":null,"affiliations":[],"username":"","note":null,"link":null,"image":{"source":"","placeholder":""},"badges":[],"verified":0,"is_verified_user":false,"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false},"comments":[]}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1122381,"guid":"955B73B06B0111EBA98D07E9020DF081","previous_id":1122380,"previous_guid":"921A2B606B0111EBA98D07E9020DF081","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"BC21C03721904562886DAA9757150F2C","order_id":1,"type_id":6,"title":"Section","source":{"title":"Freezing cells"}},{"id":1054724,"guid":"B821A67E9B7A4907843AF6327690AA9E","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Split cells at 1:3 or 1:6 two days before freeze. Cells 2 days after split will give the best survival after thaw. You can still freeze cells 4 or 5 days after split if you have to. But the survival will be significantly reduced especially if the cells are confluent.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#84CE84","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1122382,"guid":"9E578FD06B0111EBA98D07E9020DF081","previous_id":1122381,"previous_guid":"955B73B06B0111EBA98D07E9020DF081","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"58395937F2D747B8BE9D3A20F8D0056F","order_id":1,"type_id":6,"title":"Section","source":{"title":"Freezing cells"}},{"id":1054724,"guid":"BE0E92CFEA9E43B9AF3A4EB216515DDF","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Label appropriate number of cryovials you need for freezing. Also make sure you have appropriate number of Mr. Frosty.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#84CE84","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1122383,"guid":"A41830006B0111EBA98D07E9020DF081","previous_id":1122382,"previous_guid":"9E578FD06B0111EBA98D07E9020DF081","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"139DDDF71F454139A4704FE93384B823","order_id":1,"type_id":6,"title":"Section","source":{"title":"Freezing cells"}},{"id":1054724,"guid":"FFFCC9954A164BEB8B68A0BD28421C77","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Treat cells with EDTA just like what you would do when you split cells.<\/div><div class = \"text-block\">Resuspend cells with fresh E8 in half the volume you want to freeze your cells in.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#84CE84","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1122384,"guid":"A886FBD06B0111EBA98D07E9020DF081","previous_id":1122383,"previous_guid":"A41830006B0111EBA98D07E9020DF081","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"C3201C91F1D54835995EACF3B9C74136","order_id":1,"type_id":6,"title":"Section","source":{"title":"Freezing cells"}},{"id":1054724,"guid":"8256CF3AA00943E5AAA0BF4BC8B02A32","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Prepare <\/div><div class = \"text-block\"> by mixing 8 parts of E8 with 2 parts of DMSO.<\/div><\/div>"}},{"id":1054725,"guid":"3566B7106B0311EBA98D07E9020DF081","order_id":2,"type_id":25,"title":"concentration","source":{"concentration":20,"unit":null,"title":"DMSO in E8"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#84CE84","section_duration":0,"critical":[{"id":6,"title":"Mix","icon":"MixIcon"}],"critical_id":null,"duration":0},{"id":1122385,"guid":"ABF547906B0111EBA98D07E9020DF081","previous_id":1122384,"previous_guid":"A886FBD06B0111EBA98D07E9020DF081","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"0F608499B2064A96A062F034EF2ABFE1","order_id":1,"type_id":6,"title":"Section","source":{"title":"Freezing cells"}},{"id":1054724,"guid":"289FA2F273994616B11E3AC879E6488A","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Slowly add the same volume of <\/div><div class = \"text-block\"> to the resuspended cells with gentle mixing. This will give <\/div><div class = \"text-block\"> final concentration in the freezing solution.<\/div><\/div>"}},{"id":1054725,"guid":"576E91706B0311EBA98D07E9020DF081","order_id":2,"type_id":25,"title":"concentration","source":{"concentration":20,"unit":null,"title":"DMSO in E8"}},{"id":1054726,"guid":"73D895E06B0311EBA98D07E9020DF081","order_id":3,"type_id":25,"title":"concentration","source":{"concentration":10,"unit":null,"title":"DMSO"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#84CE84","section_duration":0,"critical":[{"id":3,"title":"Pipetting","icon":"PipettingIcon"}],"critical_id":null,"duration":0},{"id":1122386,"guid":"AF39EB406B0111EBA98D07E9020DF081","previous_id":1122385,"previous_guid":"ABF547906B0111EBA98D07E9020DF081","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"889DAF6C608C465787864F1F0AA866B4","order_id":1,"type_id":6,"title":"Section","source":{"title":"Freezing cells"}},{"id":1054724,"guid":"55EDBA54D9C14586AD96355A12E09E2B","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Aliquot the cells into appropriate number of cryovials.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#84CE84","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1122387,"guid":"B2D53A706B0111EBA98D07E9020DF081","previous_id":1122386,"previous_guid":"AF39EB406B0111EBA98D07E9020DF081","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"CAF9FAB675974802A6DF5CD540BEC1D3","order_id":1,"type_id":6,"title":"Section","source":{"title":"Freezing cells"}},{"id":1054724,"guid":"DB33FBA18A9549058A1B48B0F044732B","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Put vials into Mr. Frosty and place the Mr. Frosty in a <\/div><div class = \"text-block\"> freezer.<\/div><\/div>"}},{"id":1054725,"guid":"800817A06B0311EBA98D07E9020DF081","order_id":2,"type_id":24,"title":"temperature","source":{"temperature":-80,"unit":"\u00b0C","title":""}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#84CE84","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1122388,"guid":"B6B9EEB06B0111EBA98D07E9020DF081","previous_id":1122387,"previous_guid":"B2D53A706B0111EBA98D07E9020DF081","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"80E5F51E33004B5B8A584C99B06A7E88","order_id":1,"type_id":6,"title":"Section","source":{"title":"Freezing cells"}},{"id":1054724,"guid":"2A3F4A6D913F44B0A70312F6B25AD803","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">The next day, transfer all the vials to liquid nitrogen tank for long-term storage. Human pluripotent stem cells are good for several months at <\/div><div class = \"text-block\">. But it is general not recommended to keep your frozen cells at <\/div><div class = \"text-block\"> for an extended period of time.<\/div><\/div>"}},{"id":1054725,"guid":"8C1348806B0311EBA98D07E9020DF081","order_id":2,"type_id":24,"title":"temperature","source":{"temperature":-80,"unit":"\u00b0C","title":""}},{"id":1054726,"guid":"9882B9706B0311EBA98D07E9020DF081","order_id":3,"type_id":24,"title":"temperature","source":{"temperature":-80,"unit":"\u00b0C","title":""}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#84CE84","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1122389,"guid":"BB2649806B0111EBA98D07E9020DF081","previous_id":1122388,"previous_guid":"B6B9EEB06B0111EBA98D07E9020DF081","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"39F41C56288549C59391C4F19F4B6611","order_id":1,"type_id":6,"title":"Section","source":{"title":"Freezing cells"}},{"id":1054724,"guid":"0DB9A13D7D4C433DAB2BEF80E0273ECA","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">(Optional). Thaw one vial of cells to check on the survival rate. Repeat the freezing if survival is not optimal.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#84CE84","section_duration":0,"critical":[{"id":11,"title":"Optional step","icon":"OptionalIcon"}],"critical_id":null,"duration":0},{"id":1122390,"guid":"BE7351A06B0111EBA98D07E9020DF081","previous_id":1122389,"previous_guid":"BB2649806B0111EBA98D07E9020DF081","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"4DC14388FE5349D8AAC4695206E100CB","order_id":1,"type_id":6,"title":"Section","source":{"title":"Freezing cells"}},{"id":1054724,"guid":"FA52D8FC54D24F8F9B6FDC63B248D4DE","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Generally, a 70-80% confluent well in a 6-well plate can give you 2-3 vials cells.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#84CE84","section_duration":0,"critical":null,"critical_id":null,"duration":0}],"document":"","materials":[],"description":"<div class = \"text-blocks\"><div class = \"text-block\">This protocol is about human pluripotent stem cell culture.<\/div><br\/><br\/><div class = \"text-block\"><span style = \"font-weight:bold;\">Some facts about human pluripotent stem cells<\/span><\/div><div class = \"text-block\"><ol style = \"list-style-type: decimal;\"><li style = \"counter-reset:ol0;\">They attach to several extra cellular matrices, including Matrigel, laminin, vitronectin, fibronectin and high-density RGD peptides. The best and most natural ECM for human PSC is Laminin 511 or 521.<\/li><li style = \"counter-reset:ol0;\">Human PSCs don\u2019t like to be in single cells. They always grow the best in colonies.<\/li><li style = \"counter-reset:ol0;\">The doubling time of human PSC in E8 medium is 16-20 hours.<\/li><li style = \"counter-reset:ol0;\">Cells are normally split every 4-5 days.<\/li><li style = \"counter-reset:ol0;\">One well of a 6-well plate at 90% confluency will give you roughly 1.5 to 2 million cells.<\/li><\/ol><\/div><\/div>","changed_on":1628796472}