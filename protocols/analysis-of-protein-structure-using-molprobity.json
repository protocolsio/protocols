nterests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false}],"versions":[],"groups":[],"is_owner":1,"has_subprotocols":0,"is_subprotocol":0,"is_bookmarked":0,"can_claim_authorship":0,"can_accept_authorship":0,"can_be_copied":1,"can_remove_fork":1,"fork_id":null,"url":"https:\/\/www.protocols.io\/view\/analysis-of-protein-structure-using-molprobity-bgmaju2e","forks_count":{"private":0,"public":0},"access":{"can_view":1,"can_remove":1,"can_add":0,"can_edit":0,"can_publish":0,"can_get_doi":1,"can_share":1,"can_move":1,"can_move_outside":1,"can_transfer":1,"can_download":1,"is_locked":0},"guid":"D4194FC099F411EAA33E690FD5C04F5D","state_version_id":1511,"steps":[{"id":944719,"guid":"DCA719B099F411EAA33E690FD5C04F5D","previous_id":944720,"previous_guid":"F9F3B5F099F411EAA33E690FD5C04F5D","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"D9FEDA369C7F4C80A469FD6DA217AD4F","order_id":1,"type_id":6,"title":"Section","source":{"title":"Prepare file"}},{"id":1054724,"guid":"ECBE96E2C36D45DAAA63F3F307A736D6","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Navigate to the <\/div><div class = \"text-block\"><a href=\"http:\/\/molprobity.biochem.duke.edu\/index.php\" style = \"text-decoration:underline;color:blue;cursor:pointer;\"><span style = \":;\">Molprobity<\/span><\/a><\/div><div class = \"text-block\"> home page<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":944720,"guid":"F9F3B5F099F411EAA33E690FD5C04F5D","previous_id":null,"previous_guid":null,"modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"7BF6E26DC6F54367967B606FB777512B","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"0A649555B723487183977DABBA27F2AA","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">This protocol describes submitting a .pdb file to the Molprobity server hosted at Duke University. <\/div><div class = \"text-block\">Molprobity has now been incorporated into SWISS-Model results as well. The analysis aspects of either method are comparable and the analysis section can be applied to data from this latter instance.  See the sections on the multi-criterion table and Ramachandran plot in this case.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":944722,"guid":"5335984099F511EAA33E690FD5C04F5D","previous_id":944719,"previous_guid":"DCA719B099F411EAA33E690FD5C04F5D","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"252A7076EF654B66A62FFB3E4AD28CC3","order_id":1,"type_id":6,"title":"Section","source":{"title":"Prepare file"}},{"id":1054724,"guid":"7F214C7B8101451996931DC670840B7E","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Upload the .pdb file and press the upload file button. <\/div><div style = \"text-align :; float : ;\"><img style = \"\" src = \"https:\/\/s3.amazonaws.com\/protocols-files\/public\/5917da60180611bbcc4c7255f94ad072ace49c2e3114d13b111e60a40a9364c1\/b9frisgw.png\" \/><\/div><\/div>"}},{"id":1054725,"guid":"10B446A4601243B88527B51EE7FD7605","order_id":2,"type_id":1,"title":"description","source":{"description":"<div style = \"text-align :; float : ;\"><img style = \"\" src = \"https:\/\/s3.amazonaws.com\/protocols-files\/public\/5917da60180611bbcc4c7255f94ad072ace49c2e3114d13b111e60a40a9364c1\/b9frisgw.png\" \/><\/div>"}},{"id":1054726,"guid":"679B498C1FE24E7FB54C37FDD1E6F4B9","order_id":3,"type_id":26,"title":"notes","source":{"id":0,"parent_id":0,"uri":"","title":"","body":"<div class = \"text-blocks\"><div class = \"text-block\">The server will then process the file. This can take a few seconds to a few minutes depending on size of the molecule, format, etc.<\/div><\/div>","created_on":0,"changed_on":0,"creator":{"name":" ","affiliation":null,"affiliations":[],"username":"","note":null,"link":null,"image":{"source":"","placeholder":""},"badges":[],"verified":0,"is_verified_user":false,"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false},"comments":[]}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":944723,"guid":"E577EAA099F511EAA33E690FD5C04F5D","previous_id":944722,"previous_guid":"5335984099F511EAA33E690FD5C04F5D","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"77B36DABDAEE42AFBA59EDDED5920D29","order_id":1,"type_id":6,"title":"Section","source":{"title":"Prepare file"}},{"id":1054724,"guid":"5CDDA5A760644567A80DFC7263A852C1","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">If successful, the a screen similar to the following will appear. Press Continue to move to the analysis phase.<\/div><div style = \"text-align :; float : ;\"><img style = \"\" src = \"https:\/\/s3.amazonaws.com\/protocols-files\/public\/5917da60180611bbcc4c7255f94ad072ace49c2e3114d13b111e60a40a9364c1\/b9fpisgw.png\" \/><\/div><\/div>"}},{"id":1054725,"guid":"2904E84A3C5649CCBBE06146A8E87D17","order_id":2,"type_id":1,"title":"description","source":{"description":"<div style = \"text-align :; float : ;\"><img style = \"\" src = \"https:\/\/s3.amazonaws.com\/protocols-files\/public\/5917da60180611bbcc4c7255f94ad072ace49c2e3114d13b111e60a40a9364c1\/b9fpisgw.png\" \/><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":944724,"guid":"36A9E59099F611EAA33E690FD5C04F5D","previous_id":945102,"previous_guid":"7D19A8609A0C11EABB46E54918AE30CD","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"4119F53E4FCC4372B575EF8329076387","order_id":1,"type_id":6,"title":"Section","source":{"title":"Prepare file"}},{"id":1054724,"guid":"DC542AC601314C72BBFBD5CC0A722C17","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">In the analysis menu, select Add Hydrogens.<\/div><\/div>"}},{"id":1054725,"guid":"AB676619F6184F53B75FD6292F58C350","order_id":2,"type_id":26,"title":"notes","source":{"id":0,"parent_id":0,"uri":"","title":"","body":"<div class = \"text-blocks\"><div class = \"text-block\">Adding hydrogens is not required, but will improve the analysis reliability. Most homology modeling programs do not include hydrogens because hydrogens are not observed (for the most past) in X-ray crystallography experiments. <\/div><\/div>","created_on":0,"changed_on":0,"creator":{"name":" ","affiliation":null,"affiliations":[],"username":"","note":null,"link":null,"image":{"source":"","placeholder":""},"badges":[],"verified":0,"is_verified_user":false,"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false},"comments":[]}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":944918,"guid":"5FFB37209A0311EABB46E54918AE30CD","previous_id":944724,"previous_guid":"36A9E59099F611EAA33E690FD5C04F5D","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"E1091DBBAD6E4B33A4B892F3F3403EFB","order_id":1,"type_id":6,"title":"Section","source":{"title":"Prepare file"}},{"id":1054724,"guid":"0B406C15ED7742C49E89BB75EA89029E","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">The defaults are fine for most analyses. Press Start adding H to begin adding hydrogens to the structure.<\/div><div class = \"text-block\"><ol style = \"list-style-type: decimal;\"><li style = \"counter-reset:ol0;\">Flips optimizes hydrogen bonding, which can be good in models. If you have a crystal structure, then maybe run the analysis twice with and without flips and see the difference.<\/li><li style = \"counter-reset:ol0;\">Electron cloud is appropriate for most structures.<\/li><\/ol><\/div><div style = \"text-align :; float : ;\"><img style = \"\" src = \"https:\/\/s3.amazonaws.com\/protocols-files\/public\/5917da60180611bbcc4c7255f94ad072ace49c2e3114d13b111e60a40a9364c1\/b9f3isgw.png\" \/><\/div><\/div>"}},{"id":1054725,"guid":"45CE68ADCF9047EE94FD624D64FB582E","order_id":2,"type_id":1,"title":"description","source":{"description":"<div style = \"text-align :; float : ;\"><img style = \"\" src = \"https:\/\/s3.amazonaws.com\/protocols-files\/public\/5917da60180611bbcc4c7255f94ad072ace49c2e3114d13b111e60a40a9364c1\/b9f3isgw.png\" \/><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":944929,"guid":"C59ABAA09A0411EABB46E54918AE30CD","previous_id":945111,"previous_guid":"9C3124209A0D11EABB46E54918AE30CD","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"23DE6651DD824CA8981AC6A37D4A495F","order_id":1,"type_id":6,"title":"Section","source":{"title":"Analyze the structure"}},{"id":1054724,"guid":"0ACDBBC1E15D49BBB8434A24F3B151A5","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">When the addition and optimization is complete. The screen with show the options below. Select Analyze all-atom contacts and geometry.<\/div><div style = \"text-align :; float : ;\"><img style = \"\" src = \"https:\/\/s3.amazonaws.com\/protocols-files\/public\/5917da60180611bbcc4c7255f94ad072ace49c2e3114d13b111e60a40a9364c1\/b9f9isgw.png\" \/><\/div><\/div>"}},{"id":1054725,"guid":"4F8792C8042340FFA90FA7AADD3B9DA5","order_id":2,"type_id":1,"title":"description","source":{"description":"<div style = \"text-align :; float : ;\"><img style = \"\" src = \"https:\/\/s3.amazonaws.com\/protocols-files\/public\/5917da60180611bbcc4c7255f94ad072ace49c2e3114d13b111e60a40a9364c1\/b9f9isgw.png\" \/><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":944930,"guid":"E58B4DC09A0411EABB46E54918AE30CD","previous_id":944929,"previous_guid":"C59ABAA09A0411EABB46E54918AE30CD","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"39C6B0E6CB2E4C4884ACD91652AE02DF","order_id":1,"type_id":6,"title":"Section","source":{"title":"Analyze the structure"}},{"id":1054724,"guid":"0455D4367A214342A1F51F98F26C4C25","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">The menu on the next page allows for selection parameters to analyze. In general the selections shown below are appropriate for analysis of protein homology models.<\/div><div class = \"text-block\">Press Run programs to perform analyses and wait about 60 seconds.<\/div><div style = \"text-align :; float : ;\"><img style = \"\" src = \"https:\/\/s3.amazonaws.com\/protocols-files\/public\/5917da60180611bbcc4c7255f94ad072ace49c2e3114d13b111e60a40a9364c1\/b9gbisgw.png\" \/><\/div><\/div>"}},{"id":1054725,"guid":"FE6A4982065E4C659D21B73EA512683A","order_id":2,"type_id":1,"title":"description","source":{"description":"<div style = \"text-align :; float : ;\"><img style = \"\" src = \"https:\/\/s3.amazonaws.com\/protocols-files\/public\/5917da60180611bbcc4c7255f94ad072ace49c2e3114d13b111e60a40a9364c1\/b9gbisgw.png\" \/><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":944931,"guid":"3B7133D09A0511EABB46E54918AE30CD","previous_id":944930,"previous_guid":"E58B4DC09A0411EABB46E54918AE30CD","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"7B7C8CB9900147D2925D56AC1B8832B6","order_id":1,"type_id":6,"title":"Section","source":{"title":"Analyze the structure"}},{"id":1054724,"guid":"90CCA06388764208BDFE81059FFF012C","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">The results screen brings up many tables and files.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":944932,"guid":"B0103E709A0511EABB46E54918AE30CD","previous_id":944931,"previous_guid":"3B7133D09A0511EABB46E54918AE30CD","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"BB8293F6C20C4FF4B504F751E48A895E","order_id":1,"type_id":6,"title":"Section","source":{"title":"Analyze the structure"}},{"id":1054724,"guid":"A8427E7244974BDF9F09DB96CBED9E41","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">The summary table is at the top of the screen and should appear similar as that shown below.<\/div><div style = \"text-align :; float : ;\"><img style = \"\" src = \"https:\/\/s3.amazonaws.com\/protocols-files\/public\/5917da60180611bbcc4c7255f94ad072ace49c2e3114d13b111e60a40a9364c1\/b9gdisgw.png\" \/><\/div><\/div>"}},{"id":1054725,"guid":"2109122110CF422FBCDB3243D17BCD1D","order_id":2,"type_id":1,"title":"description","source":{"description":"<div style = \"text-align :; float : ;\"><img style = \"\" src = \"https:\/\/s3.amazonaws.com\/protocols-files\/public\/5917da60180611bbcc4c7255f94ad072ace49c2e3114d13b111e60a40a9364c1\/b9gdisgw.png\" \/><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":944933,"guid":"D08CE4F09A0511EABB46E54918AE30CD","previous_id":944932,"previous_guid":"B0103E709A0511EABB46E54918AE30CD","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"6A97290063B641C9ADC0F9CFD71C614D","order_id":1,"type_id":6,"title":"Section","source":{"title":"Analyze the structure"}},{"id":1054724,"guid":"45EE6E94D74341E7BCD4570C92C31D1E","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span style = \"font-style:italic;font-weight:bold;\">Score descriptions:<\/span><\/div><div class = \"text-block\"><ul style = \"list-style-type:disc;\"><li style = \"counter-reset:ol0;list-style-type:disc;\"><span style = \"font-weight:bold;\">Clashscore, all atoms<\/span><span>: This score rates the sterics, lower number is better, while higher percentile is better. The percentile is the value that should be reported.<\/span><\/li><li style = \"counter-reset:ol0;list-style-type:disc;\"><span style = \"font-weight:bold;\">Rotamers<\/span><span>: Refers to the geometry of the amino acid side chains. The number indicates the number of amino acids in the poor or favored category and the percentage of amino acids that fall into those categories. If there is a high percentage of poor rotamers (>1.5%), this can be concerning.<\/span><\/li><li style = \"counter-reset:ol0;list-style-type:disc;\"><span style = \"font-weight:bold;\">Ramachandran<\/span><span>: Shows number of amino acids with poor or favored phi\/psi angles. Phi\/Psi angles are the dihedral angles in the protein backbone.  Only certain angles are typically found in proteins.<\/span><\/li><li style = \"counter-reset:ol0;list-style-type:disc;\"><span style = \"font-weight:bold;\">Rama distribution Z-score<\/span><span>: Can show over fitting of angles to unrealistic levels. A value -4 < x < 2 is considered appropriate for normal structures. This pre-print provides more details: <\/span><\/li><li style = \"counter-reset:ol0;list-style-type:disc;\"><a href=\"https:\/\/www.biorxiv.org\/content\/10.1101\/2020.03.26.010587v1\" style = \"text-decoration:underline;color:blue;cursor:pointer;\"><span style = \":;\">Link<\/span><\/a><\/li><li style = \"counter-reset:ol0;list-style-type:disc;\"><span style = \"font-weight:bold;\">Molprobity score<\/span><span>: Combines all the geometric scores into a single value to suggest quality. Lower values and higher percentiles are better.<\/span><\/li><li style = \"counter-reset:ol0;list-style-type:disc;\"><span style = \"font-weight:bold;\">C\u03b2, Bad bonds, band angles, cis-Prolines<\/span><span>: Indicate number of amino acids with poor geometry or chemical parameters. <\/span><\/li><li style = \"counter-reset:ol0;list-style-type:disc;\"><span style = \"font-weight:bold;\">CaBLAM outliers<\/span><span>: C-Alpha Based Low-resolution Annotation Method, looks at the backbone geometry.<\/span><\/li><\/ul><\/div><div class = \"text-block\"><span style = \"font-weight:bold;\">Rama distribution Z-score<\/span><span>: Can show over fitting of angles to unrealistic levels. A value -4 < x < 2 is considered appropriate for normal structures. This pre-print provides more details: <\/span><\/div><div class = \"text-block\"><a href=\"https:\/\/www.biorxiv.org\/content\/10.1101\/2020.03.26.010587v1\" style = \"text-decoration:underline;color:blue;cursor:pointer;\"><span style = \":;\">Link<\/span><\/a><\/div><div class = \"text-block\"><ul style = \"list-style-type:disc;\"><li style = \"counter-reset:ol0;list-style-type:disc;\"><span style = \"font-weight:bold;\">Molprobity score<\/span><span>: Combines all the geometric scores into a single value to suggest quality. Lower values and higher percentiles are better.<\/span><\/li><li style = \"counter-reset:ol0;list-style-type:disc;\"><span style = \"font-weight:bold;\">C\u03b2, Bad bonds, band angles, cis-Prolines<\/span><span>: Indicate number of amino acids with poor geometry or chemical parameters. <\/span><\/li><li style = \"counter-reset:ol0;list-style-type:disc;\"><span style = \"font-weight:bold;\">CaBLAM outliers<\/span><span>: C-Alpha Based Low-resolution Annotation Method, looks at the backbone geometry.<\/span><\/li><\/ul><\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":945062,"guid":"4D10F9909A0A11EABB46E54918AE30CD","previous_id":944933,"previous_guid":"D08CE4F09A0511EABB46E54918AE30CD","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"A5E362789DDC4E87B40CFF1D6ACF56C7","order_id":1,"type_id":6,"title":"Section","source":{"title":"Multi-criterion table"}},{"id":1054724,"guid":"11A062669D634064A5470B3E27BB38E3","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">At the bottom of the page are several additional files that can be viewed for deeper analysis. <\/div><div style = \"text-align :; float : ;\"><img style = \"\" src = \"https:\/\/s3.amazonaws.com\/protocols-files\/public\/5917da60180611bbcc4c7255f94ad072ace49c2e3114d13b111e60a40a9364c1\/b9gjisgw.png\" \/><\/div><\/div>"}},{"id":1054725,"guid":"FE2A459B9954478188D99A758D0A325D","order_id":2,"type_id":1,"title":"description","source":{"description":"<div style = \"text-align :; float : ;\"><img style = \"\" src = \"https:\/\/s3.amazonaws.com\/protocols-files\/public\/5917da60180611bbcc4c7255f94ad072ace49c2e3114d13b111e60a40a9364c1\/b9gjisgw.png\" \/><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#84CE84","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":945064,"guid":"6A599E309A0A11EABB46E54918AE30CD","previous_id":945062,"previous_guid":"4D10F9909A0A11EABB46E54918AE30CD","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"DB12602313C7463E9BEE040C5AD844CF","order_id":1,"type_id":6,"title":"Section","source":{"title":"Multi-criterion table"}},{"id":1054724,"guid":"C6152902535E4472899E6A74731F78A4","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">The Multi-Criterion chart is the highest level of detail and resolution and shows the residue level problems in the protein. <\/div><div class = \"text-block\"><ul style = \"list-style-type:disc;\"><li style = \"counter-reset:ol0;list-style-type:disc;\">Boxes shown in pink indicate the bad parameter at that amino acid position. <\/li><li style = \"counter-reset:ol0;list-style-type:disc;\">More than 5 consecutive amino acids with bad parameters may mean an issue with that part of the protein and should be inspected manually as well as reported.<\/li><\/ul><\/div><div style = \"text-align :; float : ;\"><img style = \"\" src = \"https:\/\/s3.amazonaws.com\/protocols-files\/public\/5917da60180611bbcc4c7255f94ad072ace49c2e3114d13b111e60a40a9364c1\/b9gmisgw.png\" \/><\/div><\/div>"}},{"id":1054725,"guid":"CDD4471373BB49D98E3A25C39AEDC5A8","order_id":2,"type_id":1,"title":"description","source":{"description":"<div style = \"text-align :; float : ;\"><img style = \"\" src = \"https:\/\/s3.amazonaws.com\/protocols-files\/public\/5917da60180611bbcc4c7255f94ad072ace49c2e3114d13b111e60a40a9364c1\/b9gmisgw.png\" \/><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#84CE84","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":945066,"guid":"78C6DAF09A0A11EABB46E54918AE30CD","previous_id":945064,"previous_guid":"6A599E309A0A11EABB46E54918AE30CD","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"D5E389E17962497CB354E2A26DE2657C","order_id":1,"type_id":6,"title":"Section","source":{"title":"Multi-criterion table"}},{"id":1054724,"guid":"73B524D40CB342D9ADEFBAA162A76300","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Save the multi-criterion chart as a PDF. Upload 