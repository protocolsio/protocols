duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"58gim\",\"text\":\"Set up rotarod equipment as needed and clean thoroughly.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":97805,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"1","cases":[],"critical":null},{"id":2004456,"guid":"C88767C31963465382B43675F2C0B6A2","previous_id":2004455,"previous_guid":"F8951BB7D4F54977ADCFEBD52025888E","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"7r28r\",\"text\":\"Turn on rotarod using green button and log into computer. Open software directly from desktop once machine is ON and connected.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":97805,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"2","cases":[],"critical":null},{"id":2004457,"guid":"0FBE3FC890C143C18FA18DA70EB76937","previous_id":2004456,"previous_guid":"C88767C31963465382B43675F2C0B6A2","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"5lp69\",\"text\":\"Specify settings:\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"adn9\",\"text\":\"a.    Protocol: Increasing speed\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"an73t\",\"text\":\"b.    Speed: 4.0- 40 RPM\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"fl8fu\",\"text\":\"c.    File name [EXP NAME].txt                \\n       i.     Important: save file as .txt to then convert to excel once saved.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"daqv8\",\"text\":\"d.    Subject ID: animal number \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":97805,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"3","cases":[],"critical":null},{"id":2004458,"guid":"1739E645947D4E26BDA178C9EDFAD3AA","previous_id":2004457,"previous_guid":"0FBE3FC890C143C18FA18DA70EB76937","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"7go7s\",\"text\":\"Place mouse on rotarod—can place up to five animals at a time. Make sure mice are all facing the same direction (i.e. wall, AWAY from experimenter) before starting. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":97805,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4","cases":[],"critical":null},{"id":2004460,"guid":"29EB903DEF914CA39AA40BA0B0D41F5B","previous_id":2004458,"previous_guid":"1739E645947D4E26BDA178C9EDFAD3AA","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"fbm6q\",\"text\":\"Press RESET to start. IMPORTANT. IF YOU DO NOT PRESS RESET, THE SPEED SETTINGS WON’T APPLY.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":97805,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"5","cases":[],"critical":null},{"id":2004461,"guid":"30C2C4862B9C4F3D8474D24C9CC772D4","previous_id":2004460,"previous_guid":"29EB903DEF914CA39AA40BA0B0D41F5B","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"fergq\",\"text\":\"Record behavior for 5 minutes or until mouse falls. If mouse falls \\u003c5s, place back on rod, reset zone using button, and continue recording. If mouse stays static (not walking/running but gripping rod) and spins more than three times, remove and mark as failed trial.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"7cic5\",\"text\":\"a.    IMPORTANT: The sensors register when a mouse falls and stops experiment for that zone. You must remove the fallen mouse as quickly as possible so that it doesn’t intercept another zone. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":97805,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"6","cases":[],"critical":null},{"id":2004462,"guid":"8974A0A109474EE4B2E30FFC819C3B4C","previous_id":2004461,"previous_guid":"30C2C4862B9C4F3D8474D24C9CC772D4","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"5ng3f\",\"text\":\"Place mice back in home cage and leave to rest for one minute before starting next trial. Clean rod in the meantime (within cage, use only water and/or kimwipe. Between cages, clean rod with ethanol and\\nwater).\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":97805,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"7","cases":[],"critical":null},{"id":2004463,"guid":"65881FF94D034D87BBD2EE06428FB387","previous_id":2004462,"previous_guid":"8974A0A109474EE4B2E30FFC819C3B4C","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"3lld3\",\"text\":\"Perform five 5-minute (300 second) trials per cage. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":97805,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"8","cases":[],"critical":null}],"template_id":0,"title":"Rotarod Test","title_html":"Rotarod Test","type_id":1,"units":[{"id":1,"type_id":3,"name":"µL","can_manage":0,"read_only":0},{"i