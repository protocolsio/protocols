2,"stats":{"is_voted":false,"number_of_views":59,"number_of_steps":11,"number_of_bookmarks":0,"number_of_comments":0,"number_of_bookmarked_comments":0,"number_of_steps_comments":0,"number_of_protocol_comments":0,"number_of_exports":2,"number_of_runs":0,"number_of_votes":0,"number_of_reagents":2,"number_of_equipments":0,"number_of_collections":0,"number_of_forks":{"private":0,"public":0},"number_of_accessible_forks":0},"status":{"id":1,"info":"We use this protocol and itâ€™s working"},"steps":[{"id":868642,"guid":"4BE08D4033EB11EA87ED29896152C525","previous_id":0,"previous_guid":null,"section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"eqvnm\",\"text\":\"Sedate adult pig (25 to 50 kg) with Telezol (500 mg, intramuscular). Administer atropine (1 mg, subcutaneous), to control salivary and bronchial secretions, and (200 mg, intravenous), to facilitate tracheal intubation. Anesthetize the animal with isoflurane (2-4%, inhalation). \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":{}}","data":null,"protocol_id":31722,"case_id":0,"critical_ids":"0","duration":0,"original_id":0,"number":"1","cases":[],"critical":null},{"id":868643,"guid":"4BE177A033EB11EA87ED29896152C525","previous_id":868642,"previous_guid":"4BE08D4033EB11EA87ED29896152C525","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"7jmnk\",\"text\":\"Shave the chest area and wash with betadine antiseptic solution. Check for loss of corneal and pain reflexes to ensure that the animal is in a deep anesthetic plane. Open the chest cavity by peeling back the skin over the left side of the chest wall and then making an incision between the 4th and 5th ribs. Use rib spreaders to hold open the incision.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":{}}","data":null,"protocol_id":31722,"case_id":0,"critical_ids":"0","duration":0,"original_id":0,"number":"2","cases":[],"critical":null},{"id":868644,"guid":"4BE213E033EB11EA87ED29896152C525","previous_id":868643,"previous_guid":"4BE177A033EB11EA87ED29896152C525","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"eimr2\",\"text\":\"Open the pericardium and remove the heart, placing it in ice cold cardioplegia solution containing (in mM) NaCl 130, KCl 27, MgSO4 1.2, KH2PO4 1.2, glucose 50, HEPES 6 (pH 7.4). Rinse the heart free of blood and transport to the laboratory in cardioplegia solution on ice. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"sub\",\"offset\":129,\"length\":1},{\"style\":\"sub\",\"offset\":138,\"length\":1},{\"style\":\"sub\",\"offset\":141,\"length\":1}],\"entityRanges\":[],\"data\":[]}],\"entityMap\":{}}","data":null,"protocol_id":31722,"case_id":0,"critical_ids":"0","duration":0,"original_id":0,"number":"3","cases":[],"critical":null},{"id":868645,"guid":"4BE2B02033EB11EA87ED29896152C525","previous_id":868644,"previous_guid":"4BE213E033EB11EA87ED29896152C525","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"rgko\",\"text\":\"Dissect the heart, removing a section of the left ventricular free wall supplied by the left anterior descending (LAD) coronary artery. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":{}}","data":null,"protocol_id":31722,"case_id":0,"critical_ids":"0","duration":0,"original_id":0,"number":"4","cases":[],"critical":null},{"id":868646,"guid":"4BE39A8033EB11EA87ED29896152C525","previous_id":868653,"previous_guid":"89A4836033EC11EA87ED29896152C525","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"700gp\",\"text\":\"Ligate any leaking arteries at the periphery of the free wall in order to maintain perfusion pressure throughout  the bulk of the tissue. Continue perfusion using, constant pressure, until the effluent is free of any remaining blood (about 10 minutes).\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":{}}","data":null,"protocol_id":31722,"case_id":0,"critical_ids":"0","duration":0,"original_id":0,"number":"6","cases":[],"critical":null},{"id":868647,"guid":"4BE484E033EB11EA87ED29896152C525","previous_id":868646,"previous_guid":"4BE39A8033EB11EA87ED29896152C525","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"al1gf\",\"text\":\"Switch perfusion to Ca2+-free solution containing collagenase  type 2 (~200 U/ml) and protease type XIV (~1 mg/ml) for 20 minutes.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":{}}","data":null,"protocol_id":31722,"case_id":0,"critical_ids":"0","duration":0,"original_id":0,"number":"7","cases":[],"critical":null},{"id":868648,"guid":"4BE5965033EB11EA87ED29896152C525","previous_id":868647,"previous_guid":"4BE484E033EB11EA87ED29896152C525","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"65v6c\",\"text\":\"Stop enzyme digestion of the tissue by perfusing with enzyme-free KB solution containing (in mM) K-glutamate 110, KH2PO4 10, KCl 25, MgSO4 1.2, taurine 20, creatine 5, EGTA 0.5, glucose 20, HEPES 5 (pH 7.2) for 2 minutes.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"sub\",\"offset\":116,\"length\":1},{\"style\":\"sub\",\"offset\":119,\"length\":1},{\"style\":\"sub\",\"offset\":137,\"length\":1}],\"entityRanges\":[],\"data\":[]}],\"entityMap\":{}}","data":null,"protocol_id":31722,"case_id":0,"critical_ids":"0","duration":0,"original_id":0,"number":"8","cases":[],"critical":null},{"id":868649,"guid":"4BE6CED033EB11EA87ED29896152C525","previous_id":868648,"previous_guid":"4BE5965033EB11EA87ED29896152C525","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"1ae90\",\"text\":\"Remove the tissue from the Langendorff apparatus and separate into different regions as necessary. The top half can be used to obtain myocytes from the base of the free wall, while the bottom half can be used to obtain myocytes from the apex.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":{}}","data":null,"protocol_id":31722,"case_id":0,"critical_ids":"0","duration":0,"original_id":0,"number":"9","cases":[],"critical":null},{"id":868650,"guid":"4BE8557033EB11EA87ED29896152C525","previous_id":868649,"previous_guid":"4BE6CED033EB11EA87ED29896152C525","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"59ndu\",\"text\":\"Cut the tissue into small pieces, mince with scissors, and then filter through a 500 Âµm polyester mesh. Collect the filtrate containing isolated myocytes. Allow the cells to settle in KB solution maintained at 37Â°C for 25 minutes.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":{}}","data":null,"protocol_id":31722,"case_id":0,"critical_ids":"0","duration":0,"original_id":0,"number":"10","cases":[],"critical":null},{"id":868652,"guid":"4BEB62B033EB11EA87ED29896152C525","previous_id":868650,"previous_guid":"4BE8557033EB11EA87ED29896152C525","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"1lsdn\",\"text\":\"Aspirate the supernatant and then resuspend the pellet of isolated myocytes in Ca2+ containing solution for use in experiments.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"sup\",\"offset\":81,\"length\":2}],\"entityRanges\":[],\"data\":[]}],\"entityMap\":{}}","data":null,"protocol_id":31722,"case_id":0,"critical_ids":"0","duration":0,"original_id":0,"number":"11","cases":[],"critical":null},{"id":868653,"guid":"89A4836033EC11EA87ED29896152C525","previous_id":868645,"previous_guid":"4BE2B02033EB11EA87ED29896152C525","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"9pe8h\",\"text\":\"Attache the ventricular free wall to a Langendorff apparatus by cannulating the proximal end of the LAD coronary artery, and perfuse the tissue with Ca2+-free solution containing (in mM) NaCl 140, KCl 5.4, MgCl2 2.5, glucose 11, HEPES 5.5 (pH 7.4) maintained at 37oC,.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"sup\",\"offset\":151,\"length\":2},{\"style\":\"sup\",\"offset\":264,\"length\":1},{\"style\":\"sub\",\"offset\":210,\"length\":1}],\"entityRanges\":[],\"data\":[]}],\"entityMap\":{}}","data":null,"protocol_id":31722,"case_id":0,"critical_ids":"0","duration":0,"original_id":0,"number":"5","cases":[],"critical":null}],"template_id":1,"title":"Isolation of Adult Pig Ventricular Myocytes","title_html":"\u003cp\u003eIsolation of Adult Pig Ventricular Myocytes\u003c/p\u003e","type_id":1,"units":[{"id":1,"type_id":3,"name":"ÂµL","can_manage":0},{"id":2,"type_id":3,"name":"mL","can_manage":0},{"id":3,"type_id":3,"name":"L","can_manage":0},{"id":4,"type_id":3,"name":"Âµg","can_manage":0},{"id":5,"type_id":3,"name":"mg","can_manage":0},{"id":6,"type_id":3,"name":"g","can_manage":0},{"id":7,"type_id":3,"name":"kg","can_manage":0},{"id":8,"type_id":3,"name":"ng","can_manage":0},{"id":9,"type_id":3,"name":"Hz","can_manage":0},{"id":10,"type_id":24,"name":"Â°C","can_manage":0},{"id":11,"type_id":24,"name":"Â°Ðš","can_manage":0},{"id":12,"type_id":24,"name":"Â°F","can_manage":0},{"id":13,"type_id":25,"name":"Mass Percent","can_manage":0},{"id":14,"type_id":25,"name":"% volume","can_manage":0},{"id":15,"type_id":25,"name":"Mass / % volume","can_manage":0},{"id":16,"type_id":25,"name":"Parts per Million (PPM)","can_manage":0},{"id":17,"type_id":25,"name":"Parts per Billion (PPB)","ca