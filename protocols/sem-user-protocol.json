ml":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1091217,"guid":"36EB0C303BE711EBB0A56F1D09043992","previous_id":1091216,"previous_guid":"3044EE003BE711EBB0A56F1D09043992","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"2690BAD464A74FAC91EF5D34AD488F0B","order_id":1,"type_id":6,"title":"Section","source":{"title":"How to:"}},{"id":1054724,"guid":"DDE30DF115594DFB915BC0D9DA80A9E4","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">The Transfer button will blink and when done will remain solid green.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1091218,"guid":"3DACC8B03BE711EBB0A56F1D09043992","previous_id":1091217,"previous_guid":"36EB0C303BE711EBB0A56F1D09043992","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"6DBC035EFD69458D829701C5E3710058","order_id":1,"type_id":6,"title":"Section","source":{"title":"How to:"}},{"id":1054724,"guid":"7A805623FFE0492490590C961C9C1245","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Push the large rod into the chamber \u2013 there will be some resistance when the sample holder is on the stage \u2013 push gently and then once in place, unscrew the rod from the sample holder \u2013 be sure to look at the monitor to ensure that the sample is correctly positioned and is not moving when you start to unscrew the rod.<\/div><\/div>"}}],"cases":[],"data":null,"html":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":1091219,"guid":"45450F103BE711EBB0A56F1D09043992","previous_id":1091218,"previous_guid":"3DACC8B03BE711EBB0A56F1D09043992","modified_on":0,"protocol_id":0,"compone