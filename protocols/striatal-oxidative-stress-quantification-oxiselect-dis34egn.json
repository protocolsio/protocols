:false,"item_id":1283217,"journal":null,"journals":[],"keywords":"","last_modified":1723174476,"link":"","location":null,"manuscript_citation":"","materials":[],"materials_text":"","ownership_history":null,"parent_collections":[],"parent_protocols":[],"peer_reviewed":false,"protocol_references":"","public":true,"public_fork_note":"","published_on":1723174476,"references":[],"related_equipments":[],"related_materials":[],"reserved_doi":"","retraction_reason":null,"samples":{},"shared_access_id":0,"show_comparison":false,"sign_info":null,"space_access":{"can_view":false,"can_edit":false,"can_remove":false,"can_add":false,"can_publish":true,"can_get_doi":true,"can_share":false,"can_move":false,"can_move_outside":false,"can_transfer":false,"can_download":false,"limited_run":false,"limited_private_links":false,"limited_blind_links":false,"is_locked":false},"space_id":119040,"state_version_id":474,"stats":{"is_voted":false,"number_of_views":1,"number_of_steps":14,"number_of_bookmarks":0,"number_of_comments":0,"number_of_bookmarked_comments":0,"number_of_steps_comments":0,"number_of_protocol_comments":0,"number_of_exports":0,"number_of_runs":0,"number_of_votes":0,"number_of_reagents":0,"number_of_equipments":0,"number_of_collections":0,"number_of_forks":{"private":0,"public":0},"number_of_accessible_forks":0},"status":{"id":1,"info":"We use this protocol and it's working"},"steps":[{"id":2177433,"guid":"90E37681480E45AFBEE7461AF5BBE352","previous_id":2177449,"previous_guid":"17D3AB378B37447A9B440E3472374999","section":"\u003cp\u003eSample preparation\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"1ish8\",\"text\":\"Samples are diluted (1:25) in PBS. Different dilutions should be tested for different tissues and experimental conditions. Prepare enough for 110 microliters of diluted samples (50 ul x duplicates +spare volume).\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":105019,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"3","cases":[],"critical":null},{"id":2177446,"guid":"CB1E62F03B404691A856CAC34D3A04CC","previous_id":0,"previous_guid":null,"section":"\u003cp\u003eOxidative stress quantification(OxiSelect™ In Vitro ROS/RNS Assay Kit (Green Fluorescence))\u003c/p\u003e","section_color":"#A492FF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"9ha7k\",\"text\":\"H2O2 is quantified using OxiSelect™ In Vitro ROS/RNS Assay Kit (Green Fluorescence) from CellBio Labs (Cat # STA-347-5) according to the manufacturer's intructions.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"sub\",\"offset\":1,\"length\":1},{\"style\":\"sub\",\"offset\":3,\"length\":2}],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":105019,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"1","cases":[],"critical":null},{"id":2177449,"guid":"17D3AB378B37447A9B440E3472374999","previous_id":2177446,"previous_guid":"CB1E62F03B404691A856CAC34D3A04CC","section":"\u003cp\u003eSample preparation\u003c/p\u003e","section_color":"#94EBFF","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"9sp6q\",\"text\":\"Following dissection, whole striatal tissue is homogenized in 100 µL PBS using a tissue homogenizer.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":105019,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"2","cases":[],"critical":null},{"id":2177453,"guid":"691B5531D82A4420B689279DC0569318","previous_id":2177459,"previous_guid":"16C6A6C255A44F52984537DA5409DA12","section":"\u003cp\u003eStandards Preparation (please refer to manufacturer protocol attached)\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"bsa2m\",\"text\":\"To prepare the Hydrogen Peroxide standards, first perform a 1:4400 dilution of the stock Hydrogen Peroxide in deionized water. Use only enough for immediate applications (e.g., Add 5 μL of Hydrogen Peroxide to 22 mL deionized water). This solution has a concentration of 2 mM.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":105019,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"5","cases":[],"critical":null},{"id":2177454,"guid":"80E318556B2A4F4BAB9BDEB328F4BF70","previous_id":2177453,"previous_guid":"691B5531D82A4420B689279DC0569318","section":"\u003cp\u003eStandards Preparation (please refer to manufacturer protocol attached)\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"2567a\",\"text\":\"Use the 2 mM H2O2 solution to prepare standards in the concentration range of 0 μM – 20 μM by further diluting in PBS (see Table 3 in attached manual). H2O2 diluted solutions and standards should be prepared fresh. Use the table below as a reference guide only. The volumes and concentrations of the standard may be adjusted by the user.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":105019,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"6","cases":[],"critical":null},{"id":2177455,"guid":"851A089DB58148F081E08C6DD11D9356","previous_id":2177458,"previous_guid":"9C36E664418748BC84D0BBDB33EF116B","section":"\u003cp\u003eAssay (please refer to manufacturer protocol attached)\u003c/p\u003e","section_color":"#EA9F6C","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"cd10d\",\"text\":\"Prepare and mix all reagents thoroughly before use. Each sample, including unknown(s) and standard(s), should be assayed in duplicate or triplicate.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":105019,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"10","cases":[],"critical":null},{"id":2177456,"guid":"6D97D90B48564C6BB35A58778356A490","previous_id":2177454,"previous_guid":"80E318556B2A4F4BAB9BDEB328F4BF70","section":"\u003cp\u003ePrepare other reagents (please refer to manufacturer protocol attached)\u003c/p\u003e","section_color":"#FFED92","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"e5ple\",\"text\":\"1X Stabilization Solution: Dilute the 10X Stabilization Solution 1:10 by adding 1.5 mL of solution to 13.5 mL of deionized water. Stir or vortex to homogeneity. Store the solution at 4ºC.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":105019,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"7","cases":[],"critical":null},{"id":2177457,"guid":"D5FF8B0A2F75437186A7B4F5E25A8534","previous_id":2177456,"previous_guid":"6D97D90B48564C6BB35A58778356A490","section":"\u003cp\u003ePrepare other reagents (please refer to manufacturer protocol attached)\u003c/p\u003e","section_color":"#FFED92","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"lfps\",\"text\":\"1X Catalyst: Prior to use, dilute the 250X Catalyst 1:250 in PBS. Vortex thoroughly. Prepare only enough for immediate applications (e.g., add 10 μL of Catalyst to 2.49 mL PBS for 50 wells).\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":105019,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"8","cases":[],"critical":null},{"id":2177458,"guid":"9C36E664418748BC84D0BBDB33EF116B","previous_id":2177457,"previous_guid":"D5FF8B0A2F75437186A7B4F5E25A8534","section":"\u003cp\u003ePrepare other reagents (please refer to manufacturer protocol attached)\u003c/p\u003e","section_color":"#FFED92","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"6qmcm\",\"text\":\"DCFH Solution: Prepare only enough DCFH Solution for immediate applications in an amber tube or aluminum foil covered tube. Prepare DCFH Solution by diluting the stock solution of DCF-DiOxyQ 1:5 with Priming Reagent (e.g., for 50 assays, add 25 μL DCF-DiOxyQ to 100 μL Priming Reagent). Vortex to homogeneity. Incubate the solution for 30 minutes at room temperature. Next, dilute the reaction 1:40 with 1X Stabilization Solution (e.g., for 50 assays, add 125 μL DCF-DiOxyQ/ Priming Reagent reaction to 4.875 mL of Stabilization Solution). Vortex to homogeneity. Protect the solution from light. This solution is now stable in the DCFH form and ready to use. The solution may be stored at -20ºC for up to one week when protected from light.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":105019,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"9","cases":[],"critical":null},{"id":2177459,"guid":"16C6A6C255A44F52984537DA5409DA12","previous_id":2177433,"previous_guid":"90E37681480E45AFBEE7461AF5BBE352","section":"\u003cp\u003eStandards Preparation (please refer to manufacturer protocol attached)\u003c/p\u003e","section_color":"#84CE84","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"1eol8\",\"text\":\"Due to light-induced auto-oxidation, the stock DCFH solutions must be protected from light.  \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":105019,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4","cases":[],"critical":null},{"id":2177460,"guid":"29459414C13A4744B66390DDD4DABF04","previous_id":2177455,"previous_guid":"851A089DB58148F081E08C6DD11D9356","section":"\u003cp\u003eAssay (please refer to manufacturer protocol attached)\u003c/p\u003e","section_color":"#EA9F6C","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"6t96g\",\"text\":\"Add 50 μL of unknown sample or hydrogen peroxide standard to wells of a 96-well plate suitable for fluorescence measurement\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":105019,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"11","cases":[],"critical":null},{"id":2177461,"guid":"87580009BC1E4FECAA75606A3697EDEA","previous_id":2177460,"previous_guid":"29459414C13A4744B66390DDD4DABF04","section":"\u003cp\u003eAssay (please refer to manufacturer protocol attached)\u003c/p\u003e","section_color":"#EA9F6C","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"39g51\",\"text\":\"Add 50 μL of Catalyst to each well. Mix well and incubate 5 minutes at room temperature.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":105019,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"12","cases":[],"critical":null},{"id":2177462,"guid":"C00BE34DC5CF41E192107C50B11224A1","previous_id":2177461,"previous_guid":"87580009BC1E4FECAA75606A3697EDEA","section":"\u003cp\u003eAssay (please refer to manufacturer protocol attached)\u003c/p\u003e","section_color":"#EA9F6C","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"8tgrg\",\"text\":\"Add 100 μL of DCFH solution to each well. Cover the plate reaction wells to protect them from light and incubate at room temperature for 15-45 minutes.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":105019,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"13","cases":[],"critical":null},{"id":2177463,"guid":"F1691