ame":"Karim Menassa","affiliation":"Medical International Technologies (MIT Canada) Inc., Montreal, Quebec, Canada.","affiliation_url":null,"username":"","link":null,"image":{"source":"","placeholder":"","webp_source":""},"note":"","is_verified_user":false},{"name":"Monzur Murshed","affiliation":"Faculty of Medicine and Health Sciences, McGill University, Montreal, Quebec, Canada.; Shriners Hospital for Children, McGill University, Montreal, Quebec, Canada.; Faculty of Dental Medicine and Oral Health Sciences, McGill University, Montreal, Quebec, Canada. ","affiliation_url":null,"username":"","link":null,"image":{"source":"","placeholder":"","webp_source":""},"note":"Principal Investigator and Corresponding Author","is_verified_user":false}],"before_start":"","book_chapter":null,"can_accept_authorship":false,"can_be_copied":true,"can_claim_authorship":false,"can_manage_keywords":true,"can_remove_fork":false,"can_sign":false,"child_steps":{},"cited_protocols":[],"collection_items":[],"created_on":1687885603,"creator":{"name":"nathan.liu","affiliation":"","affiliation_url":null,"username":"n4tle152z105tle1","link":null,"image":{"source":"/img/avatars/006.png","placeholder":"/img/avatars/006.png","webp_source":""},"badges":[{"id":2,"name":"Author","image":{"source":"/img/badges/bronze.svg","placeholder":"/img/badges/bronze.svg"}}],"affiliations":[]},"description":"{\"blocks\":[{\"key\":\"21gtk\",\"text\":\"Transfection, a non-viral method of nucleic acid delivery, often shows poor efficiency in vivo. The needle-based in vivo delivery of transfection reagents can be invasive. Here, we report a non-invasive in vivo gene delivery protocol via the needle-free MED-JET H4 MULTIJET (MJH4M) devices using glucose-based and commercial transfection reagents. The objective of this study is to compare the relative transfection efficiencies of the needle-free system to that of the needle-based delivery method. The highest transfection efficiency was noted using 5% glucose as a delivery agent; a 15-fold increase was observed using MJH4M compared to delivery with needles. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"style\":\"italic\",\"offset\":87,\"length\":7},{\"style\":\"italic\",\"offset\":113,\"length\":7},{\"style\":\"italic\",\"offset\":203,\"length\":7}],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","disclaimer":"","document":"","documents":[],"doi":"dx.doi.org/10.17504/protocols.io.kxygx3ekog8j/v1","doi_status":1,"ethics_statement":null,"fork_id":null,"fork_info":null,"forks":[],"funders":[],"groups":[],"guid":"6064C2A0150511EE9FEB3343C0F80031","guidelines":"","has_references":false,"has_step_reagents":false,"has_versions":false,"id":84108,"image":{"source":"https://content.protocols.io/files/m3hqb7baf.jpg","webp_source":"https://content.protocols.io/files/m3hpb7baf.webp","placeholder":"https://content.protocols.io/files/mzygb7baf.jpg","webp_placeholder":""},"image_attribution":"","in_trash":false,"is_bookmarked":false,"is_contact_suspended":false,"is_content_confidential":false,"is_content_warning":false,"is_doi_reserved":false,"is_in_pending_publishing":false,"is_in_transfer":false,"is_owner":true,"is_research":true,"is_retracted":false,"is_shared_directly":false,"is_subprotocol":null,"is_unlisted":false,"item_id":1133915,"journal":null,"journals":[],"keywords":"","last_modified":1688408470,"link":"","location":null,"manuscript_citation":"","materials":[],"materials_text":"","ownership_history":null,"parent_collections":[],"parent_protocols":[],"peer_reviewed":false,"protocol_references":"","public":true,"public_fork_note":"","published_on":1688408470,"references":[],"related_equipments":[],"related_materials":[],"reserved_doi":"","retraction_reason":null,"samples":{},"shared_access_id":0,"show_comparison":false,"sign_info":null,"state_version_id":26,"stats":{"is_voted":false,"number_of_views":19,"number_of_steps":10,"number_of_bookmarks":0,"number_of_comments":0,"number_of_bookmarked_comments":0,"number_of_steps_comments":0,"number_of_protocol_comments":0,"number_of_exports":0,"number_of_runs":0,"number_of_votes":0,"number_of_reagents":0,"number_of_equipments":0,"number_of_collections":0,"number_of_forks":{"private":0,"public":0},"number_of_accessible_forks":0},"status":{"id":1,"info":"We use this protocol and it's working"},"steps":[{"id":1721492,"guid":"FD4E9300150C11EE9FEB3343C0F80031","previous_id":0,"previous_guid":null,"section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"4sile\",\"text\":\"Prepare transfection reagent by diluting renilla-luciferase plasmid DNA (pR-Luc) in 5% glucose (w/v) to a final concentration of 0.1 μg/μl.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":84108,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"1","cases":[],"critical":null},{"id":1721493,"guid":"FD4F7D60150C11EE9FEB3343C0F80031","previous_id":1721492,"previous_guid":"FD4E9300150C11EE9FEB3343C0F80031","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"44un2\",\"text\":\"Insert the transfection reagent cartridge into the MED-JET H4 MULTIJET (MJH4M) device.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":84108,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"2","cases":[],"critical":null},{"id":1721494,"guid":"FD4FCB81150C11EE9FEB3343C0F80031","previous_id":1721493,"previous_guid":"FD4F7D60150C11EE9FEB3343C0F80031","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"3t9j5\",\"text\":\"MJH4M device parameters were set to 80 psi and 75 µl.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":84108,"case_id":0,"critical_ids":"