status":0,"uri":"case-processing-sop-for-thymus-bbgmiju6","type_id":1,"published_on":null,"stats":{"number_of_views":0,"number_of_steps":0,"number_of_bookmarks":0,"number_of_comments":0,"number_of_exports":0,"number_of_runs":0,"number_of_votes":0,"is_voted":0},"parent_protocols":[],"parent_collections":[],"cited_protocols":[]},"created_on":1582829714,"resolve_on":0,"resolved_user":{"name":" ","affiliation":null,"affiliations":[],"username":null,"link":null,"image":{"source":null,"placeholder":null},"badges":[],"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false},"shared":false}}],"is_owner":1,"has_subprotocols":0,"is_subprotocol":0,"is_bookmarked":0,"can_be_copied":1,"can_remove_fork":1,"fork_id":null,"forks_count":{"private":0,"public":0},"access":{"can_view":1,"can_remove":0,"can_add":1,"can_edit":0,"can_publish":0,"can_get_doi":0,"can_share":1,"can_move":1,"can_move_outside":1,"can_transfer":1,"can_download":1,"is_locked":0},"guid":"1259A660389111EAB06229DBA453A19D","state_version_id":540,"steps":[{"id":870472,"guid":"5DEB2160389311EAB06229DBA453A19D","previous_id":null,"previous_guid":null,"modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"93108C4593BB4E358963573E566DE180","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"8F1C7C3A15B446799CDABBAA926AF85A","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">The tissues received will be identified as follows: Thymus (TH).<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":870473,"guid":"90EEF9B0389311EAB06229DBA453A19D","previous_id":870472,"previous_guid":"5DEB2160389311EAB06229DBA453A19D","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"3939EA833B0E48FA98D5622E51335B91","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"AD81E07D96A640B6B5660F30300DE46F","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Sample prepared from tissues will be identified using the following nomenclature, abbreviations, and formats: Formalin Fixed Paraffin Embedded---FFPE (paraffin block)<\/div><div class = \"text-block\">Formalin Fixed Frozen Embedded---Fixed OCT Block<\/div><div class = \"text-block\">Fresh Frozen Embedded---Fresh OCT Block <\/div><div class = \"text-block\">Fresh Tissue piece cut to size---Tissue (in 4% PFA or complete medium in 50 cc conical)<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":870474,"guid":"967995C0389311EAB06229DBA453A19D","previous_id":870473,"previous_guid":"90EEF9B0389311EAB06229DBA453A19D","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"9F1B8EA663E343F0AFB6C755A9AC0CA8","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"230712D5E4094425A5FAC8A0E8772206","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Case Number Assignment: HuBMAP donor tissues will be assigned case numbers adhering to the following template: two digit year identifier, hyphen, sequential 3 digit number beginning with 001.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":870475,"guid":"9D5E1C80389311EAB06229DBA453A19D","previous_id":870474,"previous_guid":"967995C0389311EAB06229DBA453A19D","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"70D9BACB4E81452497E5FCE131F14061","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"74AC088B47C94FE28B88F657127BC78D","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Labeling Samples: <\/div><div class = \"text-block\">    a. Cassettes for paraffin embedding and Fixed OCT block fixation step<\/div><div class = \"text-block\">        i. Generate label using cassette printer or print legibly in pencil<\/div><div class = \"text-block\">        ii. Line #1: Case ID + Sample type abbreviation <\/div><div class = \"text-block\">        iii. Line #2: Block number. Where applicable, block # is determined by common coordinate region followed by   position moving lateral (hilar capsule) to medial (central) in 1centimeter cubes. <\/div><div class = \"text-block\">    b. Cryomolds for fixed or fresh frozen OCT blocks<\/div><div class = \"text-block\">        i. Print on mold legibly using permanent ink, apply printed label to bottom<\/div><div class = \"text-block\">        ii. Line #1 as for cassettes<\/div><div class = \"text-block\">        iii. Line #2 as for cassettes + FX if prepared to receive fixed frozen tissue<\/div><div class = \"text-block\">    c. Dishes, foil wraps and containers (culture, petri or conical)<\/div><div class = \"text-block\">        i. Print legibly using permanent ink or apply printed label<\/div><div class = \"text-block\">        ii. Case ID + Sample type abbreviation + common coordinate location if relevant<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":870476,"guid":"AD19D240389311EAB06229DBA453A19D","previous_id":870475,"previous_guid":"9D5E1C80389311EAB06229DBA453A19D","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"22990B38271B4A2A9DF2CC4CC3CA7B17","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"703DCCE305A0489CA3EC1470EE8BF9B6","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Processing Records: <\/div><div class = \"text-block\">    a. Tissue handling data will be recorded on the Case Worksheet form.<\/div><div class = \"text-block\">    b. Required fields during case processing include case identification, date received, any shipment or packaging, anomalies, processing date and time (start and end), staff, tissues received, aliquot types and numbers, and tissue quality notations.<\/div><div class = \"text-block\">    c. Photo documentation should be employed to verify harvests are in keeping with common coordinate guidelines.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":870477,"guid":"B4B043E0389311EAB06229DBA453A19D","previous_id":870476,"previous_guid":"AD19D240389311EAB06229DBA453A19D","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"C157B75DD62C4CF9896CD8A2F745ADAD","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"CC712CFF5D5A47B1A26986974024150A","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Tissue Dissection: <\/div><div class = \"text-block\">    a. Photo document intact organ from all angles. Orient with tissue horns at top, cardiac surface faced down. <\/div><div class = \"text-block\">    b. Collect a 0.5 cm cube of tissue for RNA QC purposes and flash freeze in liquid nitrogen. Use RNAse free conditions and collect from an interior, non-Common Coordinate site.<\/div><div class = \"text-block\">    c. Harvest Common Coordinate tissue regions (Appendix). Cut 1 cm thick cross sectional slabs and photo document location of origin by sliding slabs slightly to right of residual organ. Use ruler or grid to map locations.<\/div><div class = \"text-block\">    d. Cut a 1 cm wide strip through the center of each slab. Photo document position prior to harvesting strip.<\/div><div class = \"text-block\"><span>    e. Cut strips into individual blocks of 1 cm<\/span><span style = \"vertical-align:super;\">3<\/span><span> tissue. A 1 cm X 1 cm tissue matrix may be used to assist. Photo document cuts. Maintain orientation and order.<\/span><\/div><div style = \"text-align :; float : ;\"><img style = \"\" src = \"https:\/\/s3.amazonaws.com\/protocols-files\/public\/a0c10c37b52cf6183976da1c5d7dca79a399e980e833f43795570be378a73b69\/bzke8u96.png\" \/><\/div><div class = \"text-block\"><span>    f.  Place the Diagram Worksheet (Appendix) designated 1 cm<\/span><span style = \"vertical-align:super;\">3<\/span><span> cut block into the cassettes labeled for NBF (p), maintaining orientation. Immerse in NBF, room temperature with agitation, at a volume of at least 20 mls per cassette added. *cassettes are moved to 70% ethanol at 20-24 hours of fixation, and will be processed within 3 days.<\/span><\/div><div class = \"text-block\"><span>    g. Place the Diagram Worksheet (Appendix) designated 1 cm<\/span><span style = \"vertical-align:super;\">3<\/span><span> cut block into the cassettes labeled for PFA (p) maintaining orientation. Immerse in 4% PFA at a volume of at least 20 mls per cassette added. *cassettes are moved to 70% ethanol at 20-24 hours of fixation, and will be processed within 3 days.<\/span><\/div><div class = \"text-block\"><span>    h. Place the Diagram Worksheet (Appendix) designated 1 cm<\/span><span style = \"vertical-align:super;\">3<\/span><span> cut block into the cassette labeled for FX frozen OCT, maintaining orientation. Immerse in 4% PFA at a volume of at least 20 mls per cassette added. Complete processing and embedding following  SOP:<\/span><\/div><div class = \"text-block\"><a href=\"https:\/\/dx.doi.org\/10.17504\/protocols.io.basniede\" style = \"text-decoration:underline;color:blue;cursor:pointer;\"><span style = \":;\">https:\/\/dx.doi.org\/10.17504\/protocols.io.basniede<\/span><\/a><\/div><div class = \"text-block\"><span>    i. Place the Diagram Worksheet (Appendix) designated 1 cm<\/span><span style = \"vertical-align:super;\">3<\/span><span> cut block for Fresh OCT into a labeled, OCT filled cryomold maintaining orientation. Complete processing and embedding following  SOP: <\/span><\/div><div class = \"text-block\"><a href=\"https:\/\/dx.doi.org\/10.17504\/protocols.io.bcwsixee\" style = \"text-decoration:underline;color:blue;cursor:pointer;\"><span style = \":;\">https:\/\/dx.doi.org\/10.17504\/protocols.io.bcwsixee<\/span><\/a><\/div><div class = \"text-block\">  Hold and transport prepared blocks on dry ice.  <\/div><div class = \"text-block\"><span>    j. Place the Diagram Worksheet (Appendix) designated 1cm<\/span><span style = \"vertical-align:super;\">3<\/span><span> block of tissue into 4% PFA for CLARITY analysis. Use a ratio of at least 20 volumes of fixative to volume of tissue. Fix tissue for 20-24 hours at room temperature with gentle agitation. * Tissue is then transferred into hydrogel solution and incubated 5 days at 4C.<\/span><\/div><div class = \"text-block\"><span>    k. Prepare the Diagram Worksheet (Appendix) designated 1 cm<\/span><span style = \"vertical-align:super;\">3<\/span><span> cut block of  tissue to be used for 10X analysis.<\/span><\/div><div class = \"text-block\">        i. Place the tissue into a 50 ml tubes containing 25 ml complete DMEM\/F12 media. Store in the refrigerator until                          released for further isolation.<\/div><div class = \"text-block\">    l. If the tissue is selected to be used for collaboration projects identify protocol B on the work sheet and follow protocols as provided by the requesting lab(s).<\/div><\/div>"}},{"id":1054725,"guid":"717BF9DCAE624541BF7342C43152B09E","order_id":2,"type_id":1,"title":"description","source":{"description":"<div style = \"text-align :; float : ;\"><img style = \"\" src = \"https:\/\/s3.amazonaws.com\/protocols-files\/public\/a0c10c37b52cf6183976da1c5d7dca79a399e980e833f43795570be378a73b69\/bzke8u96.png\" \/><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":870478,"guid":"3076B530389511EAB06229DBA453A19D","previous_id":870477,"previous_guid":"B4B043E0389311EAB06229DBA453A19D","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"0B64244516854145A609246D7B3F167A","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"0B3B4336A46C4E398EBF208DC89C5F5B","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Sample Archiving: <\/div><div class = \"text-block\">    a. All materials obtained for this program will be inventoried by the University of Florida Tissue Mapping Center (UF TMC) and archived in the Organ Processing and Pathology Core (OPPC) until use.<\/div><div class = \"text-block\">    b. Samples will be released for use or shipped to collaborator(s) upon request by the Principle Investigator.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":870479,"guid":"3EBBD080389511EAB06229DBA453A19D","previous_id":870478,"previous_guid":"3076B530389511EAB06229DBA453A19D","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"54EC48579B34440396FB5E95246591ED","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"D5B0921CF0694F76952FACABB72A56E0","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Appendix referenced above. <\/div><\/div>"}},{"id":1054725,"guid":"867E05438E6B4807A0733FA895A04610","order_id":2,"type_id":27,"title":"documents","source":{"id":33448,"title":"SOP Appendix for Thymus ","title_html":"<p>SOP Appendix for Thymus <\/p>","image":{"source":"https:\/\/s3.amazonaws.com\/protocols-files\/public\/e90837a0e