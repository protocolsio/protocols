d it’s working"},"steps":[{"id":1270748,"guid":"E71BA110311311EC8AF67F9FCACC1E0E","previous_id":0,"previous_guid":null,"section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"9lak1\",\"text\":\"Prepared the CS solution to dissolving CS in an aqueous acetic acid solution at an ambient temperature overnight.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":54265,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"1","cases":[],"critical":null},{"id":1270749,"guid":"E71DC3F0311311EC8AF67F9FCACC1E0E","previous_id":1270748,"previous_guid":"E71BA110311311EC8AF67F9FCACC1E0E","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"e0q7b\",\"text\":\"Added Tween 80 to the solution and Stirred at 45C for 2 hours\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":54265,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"2","cases":[],"critical":null},{"id":1270750,"guid":"E71EFC70311311EC8AF67F9FCACC1E0E","previous_id":1270749,"previous_guid":"E71DC3F0311311EC8AF67F9FCACC1E0E","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"3ic8v\",\"text\":\"Dropped CEO in to the solution for 30 mins\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":54265,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"3","cases":[],"critical":null},{"id":1270751,"guid":"E7205C00311311EC8AF67F9FCACC1E0E","previous_id":1270750,"previous_guid":"E71EFC70311311EC8AF67F9FCACC1E0E","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"23csj\",\"text\":\"Weight ratios of CS to CEO 1:1\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":54265,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4","cases":[],"critical":null},{"id":1270752,"guid":"E7219480311311EC8AF67F9FCACC1E0E","previous_id":1270751,"previous_guid":"E7205C00311311EC8AF67F9FCACC1E0E","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"f6rf8\",\"text\":\"Added drop-wise of TPP solution into o/w emulsion (for CEO-CSN) or CS solution (for CSN) under constant stirring at room temperature for 60 mins.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":54265,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"5","cases":[],"critical":null},{"id":1270753,"guid":"E722CD00311311EC8AF67F9FCACC1E0E","previous_id":1270752,"previous_guid":"E7219480311311EC8AF67F9FCACC1E0E","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"f89l7\",\"text\":\"Nanoparticles were centrifuged at 27,000g for 14 mins\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":54265,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"6","cases":[],"critical":null},{"id":1270755,"guid":"E7264F70311311EC8AF67F9FCACC1E0E","previous_id":1270753,"previous_guid":"E722CD00311311EC8AF67F9FCACC1E0E","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"b9mb3\",\"text\":\"Prepared separately by dispersing 0.15% (w/v) of each one in distilled water and kept at 4 C for two coating solutions of CSN and CEO-CSN) with final concentration of 1.5 g/L.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":5426