":null,"space_access":{"can_view":false,"can_edit":false,"can_remove":false,"can_add":false,"can_publish":true,"can_get_doi":true,"can_share":false,"can_move":false,"can_move_outside":false,"can_transfer":false,"can_download":false,"limited_run":false,"limited_private_links":false,"limited_blind_links":false,"is_locked":false},"state_version_id":329,"stats":{"is_voted":false,"number_of_views":3,"number_of_steps":12,"number_of_bookmarks":0,"number_of_comments":0,"number_of_bookmarked_comments":0,"number_of_steps_comments":0,"number_of_protocol_comments":0,"number_of_exports":0,"number_of_runs":0,"number_of_votes":0,"number_of_reagents":0,"number_of_equipments":0,"number_of_collections":0,"number_of_forks":{"private":0,"public":0},"number_of_accessible_forks":0},"status":{"id":1,"info":"We use this protocol and it's working"},"steps":[{"id":1843282,"guid":"14B0EF31081043F7A8F8FF9D6C2C3483","previous_id":0,"previous_guid":null,"section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"b7klm\",\"text\":\"Unbox the PingRID unit. Save the cardboard tag from the packaging with the PingRID serial number on it. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90651,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"1","cases":[],"critical":null},{"id":1843289,"guid":"BF38C6DE3AAF4B64A8760D95C8D69F7F","previous_id":1843282,"previous_guid":"14B0EF31081043F7A8F8FF9D6C2C3483","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"ejkaj\",\"text\":\"Record the PingRID, SSID, and Password information from the device (included in the packaged materials) with the drone's stored records and registration information. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90651,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"2","cases":[],"critical":null},{"id":1843313,"guid":"D718687073904EE18A320A8A3B2EDC66","previous_id":1843289,"previous_guid":"BF38C6DE3AAF4B64A8760D95C8D69F7F","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"20d9s\",\"text\":\"Charge the PingRID unit using the provided USB-C cable. Test the unit to ensure that it will charge, turn on, and operate. The RID unit's orange charging light will illuminate when the device is charging and will extinguish when the device is fully charged. The device's green GPS lock light will flash slowly if it is acquiring GPS signal, remain solid green if a GPS lock has been acquired, give 2 rapid flashes if a takeoff is detected, and give 4 rapid flashes if an error occurs. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90651,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"3","cases":[],"critical":null},{"id":1843322,"guid":"235B8E83C258498198B8C78A255A3924","previous_id":1843313,"previous_guid":"D718687073904EE18A320A8A3B2EDC66","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"fvqm1\",\"text\":\"Log into the FAA's Drone Zone website (https://faadronezone-operator.faa.gov/) \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}},{\"key\":\"9bh5\",\"text\":\"\\n\\n\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90651,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"4","cases":[],"critical":null},{"id":1843325,"guid":"55A082AF19D9490D9144D1BBDC753E16","previous_id":1843322,"previous_guid":"235B8E83C258498198B8C78A255A3924","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"5bqt3\",\"text\":\"Launch the Drown Owner and Operator's Dashboard. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90651,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"5","cases":[],"critical":null},{"id":1843326,"guid":"B5DB306AAE35464188B3952090E0995B","previous_id":1843325,"previous_guid":"55A082AF19D9490D9144D1BBDC753E16","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"ap7r5\",\"text\":\"Click \\\"Manage Device Inventory\\\" \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90651,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"6","cases":[],"critical":null},{"id":1843327,"guid":"6C73BA27E52C42D59F6C5F7F7CF9F1C3","previous_id":1843326,"previous_guid":"B5DB306AAE35464188B3952090E0995B","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"ac7h\",\"text\":\"Select the drone that you want to attach to the RID device to from the list of drone's that are registered to you. Click the \\\"three dot\\\" menu button on the right side of the dashboard's menu and select the EDIT\\\" option to access the device's FAA Registration Records. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90651,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"7","cases":[],"critical":null},{"id":1843330,"guid":"6D5BF2A6BB0E441E9814C9EB19FE74F4","previous_id":1843327,"previous_guid":"6C73BA27E52C42D59F6C5F7F7CF9F1C3","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"3ulkb\",\"text\":\"In the \\\"Does your drone have remote ID?\\\" tab, select yes. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90651,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"8","cases":[],"critical":null},{"id":1843331,"guid":"1398193DF72143C187963559423225AC","previous_id":1843330,"previous_guid":"6D5BF2A6BB0E441E9814C9EB19FE74F4","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"a92eu\",\"text\":\"Enter the remote ID serial number from the device's packaging and select the UAS type as \\\"Remote ID Broadcast Module\\\" in the menu that opens. Save and exit this menu. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90651,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"9","cases":[],"critical":null},{"id":1843332,"guid":"015652EFCABB40BA9406A350EF50ED89","previous_id":1843331,"previous_guid":"1398193DF72143C187963559423225AC","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"9rj41\",\"text\":\"Review the drone registration information in the Device Inventory. The records should reflect that a remote ID device is provided for this drone. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":null,"protocol_id":90651,"case_id":0,"critical_ids":"","duration":0,"original_id":0,"number":"10","cases":[],"critical":null},{"id":1843334,"guid":"CDA289A960C846B6B93B3306BDB602AF","previous_id":1843332,"previous_guid":"015652EFCABB40BA9406A350EF50ED89","section":"","section_color":"","section_duration":0,"is_substep":false,"step":"{\"blocks\":[{\"key\":\"1d4h7\",\"text\":\"Attach the RID to the drone using the provided velcro and store the drone and RID device until ready to be used. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","data":nul